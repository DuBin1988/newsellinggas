<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.非流程超用气收费" FontFamily="Microsoft YaHei">

	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0,0,-101,0">
			<!--卡对象-->
		<oo:NewGeneralICCard Name="card"  ></oo:NewGeneralICCard> 
		
			<!--限购月数-->
			<o:GeneralObject Name="xgys" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='限购月数' "   ></o:GeneralObject>
			<!--非民用限购参数对象-->
        	<o:GeneralObject Name="gsxiangou" EntityType="t_gsxiangou" WebClientInfo="{StaticResource dbclient}"
                  Path="{m:Exp Str=$one/from t_gsxiangou where f_fanwei &lt;\= \{kbusers.f_flownum\} and f_daofanwei &gt; \{kbusers.f_flownum\}}">
			</o:GeneralObject>
			<!--民用壁挂炉用户采暖季参数对象-->
        	<o:GeneralObject Name="cainuanji" EntityType="t_addons" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=$one/from cainuanji }">
			</o:GeneralObject>	
			<!--历史交费列表-->
			<o:ObjectList Name="kbsellgas" WebClientInfo="{StaticResource dbclient}"
										Path="{m:Exp Str=kbusers.f_userid !\= null:$from t_sellinggas where f_userid\=\'\{kbfee.f_userid\}\' order by f_deliverydate desc \, id desc $\,$abc$}"
										IsOld="{m:Exp Str=SaveAction.State\=\=$End$}" />
			<!--本月有效费列表,目的是计算用户本月已经购买的气量-->
			<o:ObjectList Name="kbbysellgas" WebClientInfo="{StaticResource dbclient}"
										Path="{m:Exp Str=kbusers.f_userid !\= null:$from t_sellinggas where f_userid\=\'\{kbfee.f_userid\}\' and f_payfeetype \=\'卡表收费\' and f_payfeevalid\=\'有效\' and DateDiff(d\,f_deliverydate\,\'\{SysTime.Today.ToString($yyyy-MM-dd$)\}\') &lt;\=\{xgys.value\}*30$\,$abc$ }"
										/>
			<!--付款方式列表-->
			<o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
			<!--打开窗口对象-->
			<o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" />
			
			<!--混合气价对象 -->
        	<o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=$one/from t_hunhe }">
			</o:GeneralObject>	
			
			<!--单值对象-->
			<o:GeneralObject Name="fbffobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用低保户月维护费' "   ></o:GeneralObject>
			<o:GeneralObject Name="mwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月维护费' "   ></o:GeneralObject>
			<o:GeneralObject Name="fwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用月维护费' "   ></o:GeneralObject>
        	<o:GeneralObject Name="mxggasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月限购气量'"   ></o:GeneralObject>
			<o:GeneralObject Name="mzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用单次最大购气量'"   ></o:GeneralObject>
			<o:GeneralObject Name="fzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用单次最大购气量'"   ></o:GeneralObject>	
			<o:GeneralObject Name="mygqlobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用购气量' "   ></o:GeneralObject>
			<o:GeneralObject Name="fapiaogeshi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='发票格式'"   ></o:GeneralObject>	
			
			<!--用户对象-->
			<o:GeneralObject Name="kbusers" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=kbfee1.DataContext.f_userid \!\= null:$one/from t_userfiles where f_userid\=\'\{kbfee1.DataContext.f_userid\}\'$\,card.CardId \!\= $$ and card.Factory \!\=$$ and card.Factory \!\=$TianQingSI$:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,card.CardId \!\= $$ and card.Factory \!\=$$ and card.Factory \=\=$TianQingSI$:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,$abc$}"  IsInit="{m:Exp Str=zzprint.State\=\=$End$}">
        		<o:PropertySetter PropertyName="f_cumulativepurchase" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee1.DataContext.f_cumulativepurchase\,null}"/>
        		<o:PropertySetter PropertyName="f_zherownum" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_zherownum.ToInt()\,null}"/>
			</o:GeneralObject>
			
			<!--卡表收费对象 -->
        	<o:GeneralObject Name="kbfee" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
              IsInit="{m:Exp Str=zzprint.State\=\=$End$}">
        	<o:PropertySetter PropertyName="f_zherownum" Value="{m:Exp Str=kbusers !\= null and kbusers.f_zherownum !\= null and kbusers.f_zherownum.ToInt()^24:kbusers.f_zherownum+1\,1}"/>
			</o:GeneralObject>
				<b:HQLAction Name="UpdateBeginFeeAction"   WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_userfiles  set f_beginfee\= \'\{kbfee.f_beginfeehx\}\' where f_userid\=\'\{kbusers.f_userid\}\'}"/> 
			
		    <!--卡表保存对象 -->
			 <b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
				<b:BatchInfo Source="{m:Exp Str=kbusers}" MethodName="SaveToJson"/>
				<!--更新维护费开始日期-->
			   <b:BatchInfo Source="{m:Exp Str=UpdateBeginFeeAction}" MethodName="InvokeToJson"/>
				<b:BatchInfo Source="{m:Exp Str=kbfee1.DataContext}" MethodName="SaveToJson"/>
				<!--发票对象-->
				<b:BatchInfo Source="{m:Exp Str=fpinfosobj}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
			<!--发票对象-->
        	<o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" IsInit="{m:Exp Str=zzprint.State\=\=$End$}">
        		<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=kbfee1.DataContext.f_username}"/>
				<o:PropertySetter PropertyName="f_invoicenum" Value="{m:Exp Str=kbfee1.DataContext.f_invoicenum}"/>
				<o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbfee1.DataContext.f_userid}" />
				<o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str=kbfee1.DataContext.f_cardid}"/>
				<o:PropertySetter PropertyName="f_gas" Value="{m:Exp Str=kbfee1.DataContext.f_pregas}" />
				<o:PropertySetter PropertyName="f_money" Value="{m:Exp Str=kbfee1.DataContext.f_preamount}" />
				<o:PropertySetter PropertyName="f_repairfee" Value="{m:Exp Str=kbfee1.DataContext.f_amountmaintenance}" />
				<o:PropertySetter PropertyName="f_totalfee" Value="{m:Exp Str=kbfee1.DataContext.f_totalcost}" />
				<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=kbfee1.DataContext.f_sgnetwork}" />
				<o:PropertySetter PropertyName="f_sgoperator" Value="{m:Exp Str=kbfee1.DataContext.f_sgoperator}" />
				<o:PropertySetter PropertyName="f_fapiaotime" Value="{m:Exp Str=kbfee1.DataContext.f_deliverytime}" />
				<o:PropertySetter PropertyName="f_fapiaodate" Value="{m:Exp Str=kbfee1.DataContext.f_deliverydate}" />
				<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=kbfee1.DataContext.f_filiale}" />
				<o:PropertySetter PropertyName="f_fapiaotype" Default="购气"  />
				<o:PropertySetter PropertyName="f_fapiaostatue" Default="已用"  />
			</o:GeneralObject>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.35*"/>
				<ColumnDefinition Width="0.65*"/>
			</Grid.ColumnDefinitions >
			<c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
				<toolkit:DockPanel >
					<toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" toolkit:DockPanel.Dock="Top" Margin="0,0,0,0" >
						<a:Data.Context>
							<o:SearchObject />        	
						</a:Data.Context>
						<Grid>
							<Grid.RowDefinitions>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
                            <RowDefinition Height="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="75"/>
								<ColumnDefinition Width="140"/>
								<ColumnDefinition Width="75"/>
								<ColumnDefinition Width="140"/>
							</Grid.ColumnDefinitions>
						<TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,6,0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_userid" Operator="f_userid = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap" Grid.Row="3" Text="卡　　号：" Margin="5,6,0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Row="3" Grid.Column="1" Text="{m:Bind Path=f_cardid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_cardid" Operator="f_cardid = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<Button Content="查  询" x:Name="searchbutton" Grid.Row="3" Grid.Column="3" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,0,5,0" HorizontalAlignment="Right" Width="50" VerticalAlignment="Top" />
						<TextBlock TextWrapping="Wrap" Text="用户姓名：" Margin="0,6" HorizontalAlignment="Right" Grid.Column="2"/>
						<TextBox TextWrapping="Wrap" Grid.Column="3" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_username" Operator="f_username like '%this%'"/>               
							</a:Data.Prop>
						</TextBox>
                            <TextBlock TextWrapping="Wrap" Text="交费日期从:" Margin="0,6" HorizontalAlignment="Right" Grid.Row="4"/>
                            <sdk:DatePicker x:Name="tj_qbaz1" SelectedDate="{m:Bind Path=f_cydate2}"  a:ControlAttach.DefaultButton="searchbutton" Grid.Row="4" Grid.Column="3"  Width="Auto" Margin="0,2,0,1" Height="25" >
                                <a:Data.Prop>
                                    <!-- Value="{m:Exp Str=SysTime.Today}"-->
                                    <o:PropertySetter PropertyName="f_cydate2" 
											Operator="f_deliverydate^= substring('this',1,10)" Value="{m:Exp Str=SysTime.Today}"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock TextWrapping="Wrap" Text="到:" Margin="0,6" HorizontalAlignment="Right" Grid.Row="4" Grid.Column="2"/>
                            <sdk:DatePicker x:Name="tj_qbaz2" SelectedDate="{m:Bind Path=f_cydate1}"  a:ControlAttach.DefaultButton="searchbutton" Grid.Row="4" Grid.Column="1"  Width="Auto" Margin="0,2,0,1" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cydate1"  
											Operator="f_deliverydate  &gt;= substring('this',1,10)" Value="{m:Exp Str=SysTime.Today}"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock TextWrapping="Wrap" Text="小区名称：" Margin="0,6" HorizontalAlignment="Right" Grid.Row="1"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_districtname}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" Grid.Row="1" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_districtname" Operator="f_districtname like '%this%'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap" Text="楼    号：" Margin="0,6" HorizontalAlignment="Right" Grid.Column="2" Grid.Row="1"/>
						<TextBox TextWrapping="Wrap" Grid.Column="3" Text="{m:Bind Path=f_cusDom}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" Grid.Row="1" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_cusDom" Operator="f_cusDom = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap" Text="单    元：" Margin="0,6" HorizontalAlignment="Right" Grid.Row="2"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_cusDy}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" Grid.Row="2" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_cusDy" Operator="f_cusDy = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap" Text="门 牌 号：" Margin="0,6" HorizontalAlignment="Right" Grid.Column="2" Grid.Row="2"/>
						<TextBox TextWrapping="Wrap" Grid.Column="3" Text="{m:Bind Path=f_apartment}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" Grid.Row="2" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_apartment" Operator="f_apartment = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
					</Grid>
						<!--<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="100"/>
								<ColumnDefinition Width="50*"/>
								<ColumnDefinition Width="50*"/>
							</Grid.ColumnDefinitions>
							<Grid.RowDefinitions>
								<RowDefinition Height="28"/>
								<RowDefinition Height="28"/>
							</Grid.RowDefinitions>
						<TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,6,0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_userid" Operator="f_userid like '%this%'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="卡　　号：" Margin="5,6,0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_cardid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_cardid" Operator="f_cardid like '%this%'"/>              
							</a:Data.Prop>
						</TextBox>
						<Button Content="查  询" x:Name="searchbutton" Grid.Row="1" Grid.Column="2" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,-5,5,0" HorizontalAlignment="Right" Width="50" />
					</Grid>-->
					</toolkit:DockPanel>
					
					<toolkit:DockPanel   Background="#FFC3E4FD" toolkit:DockPanel.Dock="Bottom">
						<sdk:DataPager x:Name="pager" Source="{Binding Path=ItemsSource.Count,ElementName=userfiles,Converter={StaticResource DataPagerConverter}}"    Margin="0" PageSize="18" Height="24" toolkit:DockPanel.Dock="Top"/>
						<TextBlock TextWrapping="Wrap" Margin="5" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000" toolkit:DockPanel.Dock="Bottom" Height="27"/>
						
					</toolkit:DockPanel>
					
					<sdk:DataGrid x:Name="userfiles" Margin="0,0,0,2"  
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
						<a:Data.Items>
							<o:PagedObjectList x:Name="userfileslist" WebClientInfo="{StaticResource dbclient}"		
								Path="{m:Exp Str=$from t_sellinggas where \{userfilesearch.DataContext.Condition\} and f_sgoperator \=\'\{LoginUser.name\}\' and f_filiale \=\'\{LoginUser.f_fengongsi\}\'}"
								IsOld="{m:Exp Str='SaveAction.State\=\=$End$ '}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="f_times"/>
						</a:Data.Items>
						<sdk:DataGrid.Columns>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="名称" Binding="{Binding f_username}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户编号" Binding="{Binding f_userid}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="卡  号" Binding="{Binding f_cardid}"/>
							<sdk:DataGridTextColumn Header="购气量" Binding="{Binding f_pregas}"/>
							<sdk:DataGridTextColumn Header="金额" Binding="{Binding f_preamount}"/>
							<!--sdk:DataGridTextColumn Header="地  址" Binding="{Binding f_address}"/>
							<sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/-->
							<sdk:DataGridTextColumn Header="交费日期" Binding="{Binding f_deliverydate,StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="交费时间" Binding="{Binding f_deliverytime,StringFormat=hh:mm:ss tt}"/>
							<sdk:DataGridTextColumn Header="操作员" Binding="{Binding f_sgoperator}"/>
						</sdk:DataGrid.Columns>
					</sdk:DataGrid>
					
					
					<!--提交按钮-->
				</toolkit:DockPanel>
			</c:BusyIndicatorEx>
			
			<Grid Grid.Column="1" Margin="0,0,0,0">
			 <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=kbfee1.DataContext.IsBusy or SaveAction.IsBusy}"  Grid.Column="1" Margin="8,0,0,0">
				<toolkit:DockPanel>
					<StackPanel toolkit:DockPanel.Dock="Top">
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
							<Grid.Background>
						<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
							<GradientStop Color="#FFF2F8FF" Offset="0"/>
							<GradientStop Color="#FFDDF2FD" Offset="1"/>
						</LinearGradientBrush>
					</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="用户/单位名称：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="地　　址：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" RenderTransformOrigin="1.638,0.44"/>
							<TextBlock Text="小区名称：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表品牌：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表型号：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Margin="0" Grid.Row="5" TextWrapping="Wrap" Text="气价类型：" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="气　　价：" Grid.Column="4"  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Margin="0" Height="25"/>
							<TextBlock Text="上次总累计购气量："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Margin="0" Height="25"/>
							<TextBlock Text="限购气量：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Grid.Column="4" Height="25"/>
							<TextBlock Text="混合气价：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4"  Height="25"/>
							<StackPanel Grid.ColumnSpan="4" Grid.Column="1" Visibility="{m:Exp Str=kbfee1.DataContext.f_gasproperties\=\=$混合型$:$Visible$\,$Collapsed$}" Margin="0,2,-11,2" Grid.Row="4" Orientation="Horizontal" Height="25"  >
								<TextBlock Text="民用气价" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="0,2" />
								<TextBox Text="{m:Bind Path=f_myprice}" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_myprice" Value="{m:Exp Str=kbusers.f_myprice}" />
									</a:Data.Prop>
