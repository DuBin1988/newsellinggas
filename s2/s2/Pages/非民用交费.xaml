<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:bb="clr-namespace:Com.Aote.Behaviors"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13.333"
	x:Class="Com.Aote.Pages.非民用交费" FontFamily="Microsoft YaHei" d:DesignWidth="800" d:DesignHeight="600">

	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0,0,-101,0" Height="600" VerticalAlignment="Bottom">
			<!--打开窗口对象-->
			<o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" />			
			<o:ChildWindowObj Name="openkafei" CWName="/Pages/滞纳金登录" />
			
			<!--付款方式列表-->
			<o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>

			<!--抄表的未交费记录-->
			<o:ObjectList Name="kbsellgas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=
				$from t_handplan where f_username\=\'\{kbfee.f_username\}\' and shifoujiaofei\=\'否\' and lastrecord is not null and f_state\=\'已抄表\' order by id$}"
				IsClear="{m:Exp Str=kbfee.id\=\=null}">
				<!--滞纳金天数-->
				<o:PropertySetter PropertyName="f_zhinajin`" NoDependValue="{m:Exp Str=SysTime.Today.Subtract(this.f_endjfdate.ToDate()).Days\>0:SysTime.Today.Subtract(this.f_endjfdate.ToDate()).Days\,0}"/>
				<!--滞纳金-->
				<o:PropertySetter PropertyName="f_zhinajin" 
					NoDependValue="{m:Exp Str=(this.oughtfee*this.f_zhinajintianshu*(single[data.name\=\=$非民用滞纳金比率$].ToObjectList().First().value.ToDouble())).MathRound(2)}"/>
			</o:ObjectList>

			<!--机表收费对象-->
        	<o:GeneralObject Name="kbfee" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
            IsInit="{m:Exp Str=
			cancle\=>Click!\=null |or|
			SaveAction\=\=$Error$ |or| 
			print.State\=\=$End$ or print.State\=\=$Cancle$ |or| 
			print1.State\=\=$End$ or print1.State\=\=$Cancle$}">
				<!--滞纳金-->
				<o:PropertySetter PropertyName="f_zhinajin1" NoDependValue="{m:Exp Str=kbsellgas[True].Sum(f_zhinajin)}" Default="0"/>
				<o:PropertySetter PropertyName="f_zhinajin" NoDependValue="{m:Exp Str='openkafei.Result!=null:openkafei.Result\,kbfee.f_zhinajin1!=null:kbfee.f_zhinajin1\,0'}" Default="0"/>
				<!--抄表记录id号-->
				<o:PropertySetter PropertyName="f_useful" NoDependValue="{m:Exp Str=kbsellgas[True].ToString(id)}"/>
				<!--起始抄表日期-->
				<o:PropertySetter PropertyName="lastinputdate" NoDependValue="{m:Exp Str=kbsellgas.First().scinputdate\=>DataLoaded}" />
				<!--维护费单价-->
				<o:PropertySetter PropertyName="f_unitfee" NoDependValue="{m:Exp Str=(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu!\=$True$):single[data.name\=\=$民用月维护费$].ToObjectList().First().value\,(kbfee.f_usertype\=\=$民用$ and kbfee.f_dibaohu\=\=$True$):single[data.name\=\=$民用低保户月维护费$].ToObjectList().First().value\,0}"/>
				<!--维护费金额-->
				<o:PropertySetter PropertyName="f_amountmaintenance" 
					NoDependValue="{m:Exp Str=this.f_usertype\=\=$民用$ and this.f_endfee!\=null:kbfee.f_repairnum*kbfee.f_unitfee\,this.f_usertype\=\=$非民用$ and this.f_endfee!\=null:kbfee.f_repairnum*kbfee.f_feiweihufei\,0}"/>
				<!--非民用计算回写月个数-->
				<o:PropertySetter PropertyName="f_repairnumfei"  
					NoDependValue="{m:Exp Str=kbfee.f_usertype\=\=$非民用$:kbfee.f_repairnum.ToInt()*12\,0}" />
				<!--计算维护费的中间变量-->
				<o:PropertySetter PropertyName="f_beginfeebj" NoDependValue="{m:Exp Str=kbfee.f_beginfee !\= null:(SysTime.Today>kbfee.f_beginfee.ToDate():kbfee.f_beginfee.ToDate()\,null)\,null}"/>
				<!--计算出的维护费交费年数-->
				<o:PropertySetter PropertyName="f_weihunians" NoDependValue="{m:Exp Str= kbfee.f_beginfee !\=null:(SysTime.Today.Subtract(kbfee.f_beginfee.ToDate()).Days >0:SysTime.Today.Subtract(kbfee.f_beginfee.ToDate()).Days/365\,0)\,0}"/>
				<!--计算出的维护费交费月数小计-->
				<o:PropertySetter PropertyName="f_weihuheji" NoDependValue="{m:Exp Str=kbfee.f_weihunians!\=null and kbfee.f_weihuyue!\=null:(kbfee.f_weihunians.ToInt()+kbfee.f_weihuyue.ToInt())*12-1\,0}"/>
				<!--计算出的维护费交费月数-->
				<o:PropertySetter PropertyName="f_weihuyue" NoDependValue="{m:Exp Str= kbfee.f_beginfee !\=null:((kbfee.f_beginfee.ToDate().Year\=\=SysTime.Today.Year and kbfee.f_beginfee.ToDate().Month>SysTime.Today.Month):0\,1)\,0}"/>
				<!--维修费回写-->
				<o:PropertySetter PropertyName="f_beginfeehx" NoDependValue="{m:Exp Str='(kbfee.f_beginfee !\=null and kbfee.f_usertype\=\=$民用$):kbfee.f_beginfee.ToDate().AddMonths(kbfee.f_repairnum.ToInt())\,(kbfee.f_beginfee !\=null and kbfee.f_usertype\=\=$非民用$):kbfee.f_beginfee.ToDate().AddMonths(kbfee.f_repairnumfei.ToInt())\,kbfee.f_beginfee'}"/>
				<!--本次总累计购气量-->
				<o:PropertySetter PropertyName="f_cumulativepurchase" NoDependValue="{m:Exp Str=kbfee.f_upbuynum+kbfee.f_pregas}"/>
				<!--本次表累计购气量-->
				<o:PropertySetter PropertyName="f_metergasnums" NoDependValue="{m:Exp Str=kbfee.f_premetergasnums+kbfee.f_pregas}" />
				<!--是否结账-->
				<o:PropertySetter PropertyName="f_jiezhangstate" Default="未结账"/>
				<!--交费类型-->
				<o:PropertySetter PropertyName="f_payfeetype" Default="机表收费"/>
				<!--购气有效类型-->
				<o:PropertySetter PropertyName="f_payfeevalid" Default="有效"/>
				<!--分公司编号-->
				<o:PropertySetter PropertyName="f_fengongsinum" Default="{m:Exp Str=LoginUser.f_fengongsinum}"/>
				<!--公司类型，分为天然气公司、银行 -->
				<o:PropertySetter PropertyName="f_comtype" Default="天然气公司" Value="天然气公司" NotNull="True"/>
			</o:GeneralObject>
			
			<!--发票对象-->
        	<o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
			Path="{m:Exp Str=$one/from t_fapiaoinfos where f_invoicenum\=\'\{kbfee1.DataContext.f_invoicenum\}\'}"  
			IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| print.State\=\=$End$ |or| print1.State\=\=$End$}">
        		<o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbfee1.DataContext.f_userid}" />
			</o:GeneralObject>

			<!--更新抄表记录为已交-->
			<b:HQLAction Name="UpdateHandplan" 
			HQL="{m:Exp Str=$update t_handplan set shifoujiaofei\=\'是\' where id in (\{kbfee.f_useful\})}"/>
		    <!--保存对象 -->
			<b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
				<!--更新抄表记录为已交-->
				<b:BatchInfo Source="{m:Exp Str=UpdateHandplan}" MethodName="InvokeToJson"/>
				<!--交费对象-->
				<b:BatchInfo Source="{m:Exp Str=kbfee}" MethodName="SaveToJson"/>
				<!--发票对象-->
				<b:BatchInfo Source="{m:Exp Str=fpinfosobj}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>

			<!--打印完成后，重新获取发票号-->
            <b:ScriptAction Name="loadfapiao" CanRun="{m:Exp Str=print.State\=\=$End$ |or| print1.State\=\=$End$}" Start="{m:Exp Str=LoginUserfapiao.IsOld\=True}"></b:ScriptAction>
		    <o:SearchObject Name="search"/>
			
			<!--用户列表-->
			<o:PagedObjectList Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
		       Path="{m:Exp Str='$from t_unit where {search.Condition}'}"
		       PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"/>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.35*"/>
				<ColumnDefinition Width="0.65*"/>
			</Grid.ColumnDefinitions >
			
			<Grid x:Name="LayoutRoot1" Background="White" d:LayoutOverrides="Width">
				<Grid.RowDefinitions>
					<RowDefinition Height="0.6*"/>
					<RowDefinition Height="0.4*"/>
				</Grid.RowDefinitions>
				<c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" Grid.Column="0" Margin="0" Grid.RowSpan="1" >
					<toolkit:DockPanel>
						<StackPanel Orientation="Horizontal" x:Name="userfilesearch" DataContext="{m:Exp Str=search}" VerticalAlignment="Stretch" toolkit:DockPanel.Dock="Top" Margin="10,10,0,10">
							<TextBlock TextWrapping="Wrap" Text="单位名称：" Margin="0" Height="25" FontSize="13.333" HorizontalAlignment="Right" VerticalAlignment="Center" d:LayoutOverrides="Height" />
							<TextBox TextWrapping="Wrap" Grid.Row="0" Grid.Column="1" Text="{m:Bind Path=f_unitname}" Width="100" Margin="5,0,0,0" Height="25" FontSize="13.333" VerticalAlignment="Stretch" d:LayoutOverrides="Height" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_unitname"  Operator="f_unitname like '%this%'"/>          
								</a:Data.Prop>
							</TextBox>
							<Button Content="查  询" x:Name="searchbutton" Grid.Column="2" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="20,0,0,0" Width="70"  FontSize="13.333" Height="25"/>
						</StackPanel>
						<sdk:DataGrid x:Name="userfiles" Margin="0,-5,0,25"  FontSize="13.333" ItemsSource="{m:Exp Str=userfileslist}"
							SelectedItem="{m:Exp Str=userfileslist[data.f_userid\=\=kbfee.f_userid].First()}"
							AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
							<sdk:DataGrid.Columns>
								<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
								<sdk:DataGridTextColumn Header="单位编号" Binding="{Binding f_unitid}"/>
								<sdk:DataGridTextColumn d:IsLocked="True" Header="单位名称" Binding="{Binding f_unitname}"/>	
								<sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>							
								<sdk:DataGridTextColumn Header="气表个数" Binding="{Binding f_watchnum}"/>
							</sdk:DataGrid.Columns>
						</sdk:DataGrid>
					</toolkit:DockPanel>
				</c:BusyIndicatorEx>
				<c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=kbsellgas.IsBusy}" Grid.Row="1" >
					<ScrollViewer ScrollViewer.HorizontalScrollBarVisibility="Auto" ScrollViewer.VerticalScrollBarVisibility="Auto" Height="240" VerticalAlignment="Top">
						<sdk:DataGrid SelectionMode="Extended" AutoGenerateColumns="False" x:Name="dataGrid1" 
							ItemsSource="{m:Exp Str=kbsellgas}" IsReadOnly="True" CanUserSortColumns="False" IsHitTestVisible="False"
							ScrollViewer.HorizontalScrollBarVisibility="Visible" ScrollViewer.VerticalScrollBarVisibility="Visible">
							<sdk:DataGrid.Columns>
								<sdk:DataGridTextColumn Header="计划月份" Binding="{Binding f_handdate}"/>
								<sdk:DataGridTextColumn Header="用气量" Binding="{Binding oughtamount}"/>
								<sdk:DataGridTextColumn Header="本月金额" Binding="{Binding oughtfee}"/>
								<sdk:DataGridTextColumn Header="滞纳金" Binding="{Binding f_zhinajin}"/>
								<sdk:DataGridTextColumn Header="抄表日期" Binding="{Binding lastinputdate, StringFormat=yyyy-MM-dd}"/>
								<sdk:DataGridTextColumn Header="上次抄表底数" Binding="{Binding lastinputgasnum}"/>
								<sdk:DataGridTextColumn Header="本次抄表底数" Binding="{Binding lastrecord}"/>
								<sdk:DataGridTextColumn Header="气价1" Binding="{Binding linjienqj}"/>
								<sdk:DataGridTextColumn Header="金额1" Binding="{Binding ljnyqje}"/>
								<sdk:DataGridTextColumn Header="气价2" Binding="{Binding linjiewqj}"/>
								<sdk:DataGridTextColumn Header="金额2" Binding="{Binding ljwyqje}"/>
								<sdk:DataGridTextColumn Header="交费截止日期" Binding="{Binding f_endjfdate, StringFormat=yyyy-MM-dd}"/>
								<sdk:DataGridTextColumn Header="滞纳金天数" Binding="{Binding f_zhinajintianshu}"/>
								<sdk:DataGridTextColumn Header="网　　点" Binding="{Binding f_network}"/>
								<sdk:DataGridTextColumn Header="操 作 员" Binding="{Binding f_operator}"/>
								<sdk:DataGridTextColumn Header="录入日期" Binding="{Binding f_inputdate, StringFormat=yyyy-MM-dd}"/>
							</sdk:DataGrid.Columns>
						</sdk:DataGrid>
					</ScrollViewer>
				</c:BusyIndicatorEx>
			</Grid>
			
			<Grid Grid.Column="1" Margin="5,0,0,0">
		 	<c:BusyIndicatorEx
			BusyContent="{m:Exp Str=$正在访问服务器，请稍等...$}" 
			IsBusy="{m:Exp Str=SaveAction.IsBusy}"  Grid.Column="1" Margin="8,0,0,0"> 
				<toolkit:DockPanel>
					<StackPanel toolkit:DockPanel.Dock="Top">
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
							<Grid.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FFF2F8FF" Offset="0"/>
									<GradientStop Color="#FFDDF2FD" Offset="1"/>
								</LinearGradientBrush>
							</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="300"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="用户/单位名称：" Margin="0,6,0,0" TextWrapping="Wrap"  TextAlignment="Right" Height="25" VerticalAlignment="Top" d:LayoutOverrides="Height"/>
							<TextBlock Text="地　　址：" Margin="-1,9,1,0" TextWrapping="Wrap" TextAlignment="Right" Height="25" RenderTransformOrigin="1.638,0.44" VerticalAlignment="Top" d:LayoutOverrides="Height" Grid.Row="1"/>
							<TextBlock Text="用 气 量：" Margin="0,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Width="130" Height="25"/>
							<TextBlock Text="用气金额：" Margin="3,6,1,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Grid.Column="4" Height="25"/>
							<TextBlock Text="付款方式：" Margin="0,0,0,6" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
							<TextBlock Text="收　　款：" Grid.Column="2" Margin="0,0,0,6" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
							<TextBlock Text="应交金额：" Margin="0,0,0,6" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
							<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="1" Margin="0,6,0,0" TextWrapping="Wrap" Grid.ColumnSpan="5" Height="25" VerticalAlignment="Top" HorizontalAlignment="Stretch" Width="Auto" d:LayoutOverrides="Height"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=userfiles.SelectedItem.f_unitname}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Margin="-1,8,1,0" Grid.Row="1" TextWrapping="Wrap" Grid.ColumnSpan="5" Height="25" VerticalAlignment="Top" d:LayoutOverrides="Height" HorizontalAlignment="Stretch" Width="Auto">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_address" NoDependValue="{m:Exp Str=userfiles.SelectedItem.f_address}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_pregas}" FontWeight="Bold" Foreground="Red" Grid.Column="1" Margin="0,6" TextWrapping="Wrap" Grid.ColumnSpan="1" Grid.Row="3" Height="25" VerticalAlignment="Stretch" IsReadOnly="True" Grid.RowSpan="1" HorizontalAlignment="Stretch" Width="Auto">
								<!--用气量-->
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_pregas" NoDependValue="{m:Exp Str=kbsellgas[True].Sum(oughtamount)}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_preamount}"  Grid.Column="5" Margin="-1,6,1,6" TextWrapping="Wrap" Grid.ColumnSpan="1" IsReadOnly="True" Grid.Row="3" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1">
								<!--用气金额-->
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_preamount" 
										NoDependValue="{m:Exp Str=kbsellgas[True].Sum(oughtfee)}"/>
								</a:Data.Prop>
							</TextBox>							
							<ComboBox DisplayMemberPath="name" SelectedValuePath="name" ItemsSource="{m:Exp Str=paylist}" SelectedValue="{m:Bind Path=f_payment}"  Grid.Column="1" Margin="0,0,0,6" Grid.Row="4" HorizontalAlignment="Stretch" Width="Auto" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height" > 
								<a:Data.Prop>
									<o:PropertySetter  PropertyName="f_payment"
										Default="{m:Exp Str=paylist.Count &gt; 0 :paylist.First().name\,null}" />
								</a:Data.Prop>
							</ComboBox>
							<TextBox x:Name="shoukuan" a:ControlAttach.FocusToOnEnter="save22" Text="{m:Bind Path=f_grossproceeds}" FontWeight="Bold" Foreground="Red" Grid.Column="3" Margin="0,0,0,6" Grid.Row="4" TextWrapping="Wrap" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_grossproceeds" NoDependValue="{m:Exp Str=kbsellgas\=&gt;DataLoaded!\=null:data.f_totalcost\,null}" NotNull="True" />
								</a:Data.Prop>
							</TextBox>	
							<TextBox Text="{m:Bind Path=f_totalcost}" x:Name="save22" FontWeight="Bold" Foreground="Red" IsReadOnly="True" Grid.Column="5" Margin="0,0,0,6" Grid.Row="4" TextWrapping="Wrap" Height="25" VerticalAlignment="Bottom" Grid.ColumnSpan="1" d:LayoutOverrides="Height" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_totalcost"
									NoDependValue="{m:Exp Str=(kbfee.f_preamount+kbfee.f_amountmaintenance+kbfee.f_zhinajin-kbfee.f_zhye)&gt;0:kbfee.f_preamount+kbfee.f_amountmaintenance+kbfee.f_zhinajin-kbfee.f_zhye\,0}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBlock Margin="0,6" Grid.Row="6" TextWrapping="Wrap" Text="网　　点：" TextAlignment="Right" Height="25"/>
							<TextBox Text="{m:Bind Path=f_sgnetwork}" Grid.Column="1" Margin="0,6" Grid.Row="6" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_sgnetwork"  
										Default="{m:Exp Str=LoginUser.f_parentname}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBlock Grid.Column="2" Margin="0,6" Grid.Row="6" TextWrapping="Wrap" Text="操  作 员："  TextAlignment="Right" Height="25"/>
							<TextBox Text="{m:Bind Path=f_sgoperator}" Grid.Column="3" Margin="0,6" Grid.Row="6" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_sgoperator"  
										Default="{m:Exp Str=LoginUser.name}"/>
								</a:Data.Prop>
							</TextBox>
							<TextBlock Grid.Column="4" Margin="0,6" Grid.Row="6" TextWrapping="Wrap" Text="交费日期：" TextAlignment="Right" Height="25"/>
							<sdk:DatePicker SelectedDate="{m:Bind Path=f_deliverydate}" IsEnabled="False" Grid.Column="5" Margin="0,6" Grid.Row="6"  IsTodayHighlighted="True" IsDropDownOpen="False" SelectedDateFormat="Short" VerticalAlignment="Stretch" Height="25" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_deliverydate" 
										Default="{m:Exp Str=SysTime.Today}"/>
								</a:Data.Prop>
							</sdk:DatePicker>
							<!--表安装位置-->
							<!--上次表累计购气量-->
							<TextBlock Margin="0,6" Grid.Row="7" TextWrapping="Wrap" Text="分 公 司：" TextAlignment="Right" Height="25"/>
							<TextBox Text="{m:Bind Path=f_filiale}" Grid.Column="1" Margin="0,6" Grid.Row="7" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_filiale" 
										Default="{m:Exp Str=LoginUser.f_fengongsi}"/>
								</a:Data.Prop>
							</TextBox>
							<StackPanel Orientation="Horizontal" Grid.Column="4" Grid.ColumnSpan="2" Height="37" VerticalAlignment="Top"/>
							<TextBlock Grid.Column="4" Margin="0,6" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Height="25" HorizontalAlignment="Right" Width="130"><Run Text="交费"/><Run Text="时间"/><Run Text="："/></TextBlock>
							<toolkit:TimePicker Value="{m:Bind Path=f_deliverytime}" IsEnabled="False" Grid.Column="5" Margin="0,6" Grid.Row="7"   IsDropDownOpen="False"  VerticalAlignment="Stretch" Height="25" >
