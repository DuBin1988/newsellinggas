<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.民用业务受理">
	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad Margin="0,0,-101,0">
			<!--用户对象-->
			<o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
					EntityType="t_shoulilist"  Source="{m:Exp Str=userfiles.SelectedItem}" 
					IsInit="{m:Exp Str=SaveAction.State\=\=$End$}"        	>
			</o:GeneralObject>
			<!--小区对象areas.f_usernumber-->
        	<o:GeneralObject Name="areas" EntityType="t_area" WebClientInfo="{StaticResource dbclient}"
                  Path="{m:Exp Str=$one/from t_area where id\=\'\{areafiles.SelectedItem.id\}\'}" 
				  IsInit="{m:Exp Str='SaveAction.State==$End'}" >
			      <!--编号加1回写到小区的当前编号>
				  <o:PropertySetter  PropertyName="f_usernumber" Value="{m:Exp Str=userfile.DataContext.f_usernumberjia.ToString()}" /-->
			</o:GeneralObject>
			<b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
				<!--对象-->
				<b:BatchInfo Source="{m:Exp Str=userfile.DataContext}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.25*"/>
				<ColumnDefinition Width="0.75*"/>
			</Grid.ColumnDefinitions>
			<toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
				<toolkit:DockPanel >
					<!--提交按钮-->
					<toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Height="45" >
						<TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="183" HorizontalAlignment="Stretch" Foreground="#FF140000" Margin="30,0,0,0" Height="22" VerticalAlignment="Bottom"/>
						<StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
							</StackPanel>
					</toolkit:DockPanel>
					<Grid x:Name="userfilesearch" toolkit:DockPanel.Dock="Top" Margin="0,0,0,1" Background="#FFCFEFFF"  >
						<Grid.RowDefinitions>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
						</Grid.RowDefinitions>
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="100"/>
							<ColumnDefinition Width="50*"/>
							<ColumnDefinition Width="50*"/>
						</Grid.ColumnDefinitions>
						<a:Data.Context>
							<o:SearchObject />        	
						</a:Data.Context>
						
							<TextBlock TextWrapping="Wrap" Text="受理编号：" Margin="0,4" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_shoulicode}" Width="Auto" Margin="0" Height="25" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_shoulicode"  Operator="f_shoulicode like '%this%'"/>          
								</a:Data.Prop>
							</TextBox>
						
						
							<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" Margin="0,4" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_username}" Width="Auto" Margin="0" Height="25" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username"  Operator="f_username like '%this%'"/>          
								</a:Data.Prop>
							</TextBox>
					
					
							<TextBlock TextWrapping="Wrap" Text="受理日期：" Grid.Row="2" Margin="0,4" HorizontalAlignment="Right"/>
							
							<sdk:DatePicker SelectedDate="{m:Bind Path=f_shoulidate}" Margin="0" Grid.Row="2"  Grid.Column="1" Height="25" VerticalAlignment="Stretch">
										<a:Data.Prop>
											<o:PropertySetter PropertyName="f_shoulidate" 
												Operator=" (f_shoulidate, StringFormat=yyyy-MM-dd)  = cast('this' as date)"/>
										</a:Data.Prop>
							</sdk:DatePicker>
							<Button Content="查  询" x:Name="searchbutton" Grid.Column="2" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,-9,8,4" Grid.Row="3" HorizontalAlignment="Right" Width="50"/>
						
						
							<TextBlock TextWrapping="Wrap" Text="合同编号：" Grid.Row="3" Margin="0,4" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_hetongnum}" Grid.Column="1" Grid.Row="3" Width="Auto" Margin="0" Height="25" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_hetongnum"  Operator="f_hetongnum like '%this%'"/>          
								</a:Data.Prop>
							</TextBox>
						
					</Grid>
					<sdk:DataGrid x:Name="userfiles" Margin="0,1" IsReadOnly="True"
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" Height="Auto">
						<a:Data.Items>
							<!--and f_shoulidate\!\=null and f_userid\=\=null and  f_filiale \=\'\{LoginUser.f_fengongsi\}\'-->
							<o:PagedObjectList x:Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
								Path="{m:Exp Str=$from t_shoulilist where \{userfilesearch.DataContext.Condition\} and f_shoulidate is not null }"
								IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"/>
						</a:Data.Items>
						<sdk:DataGrid.Columns>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn Header="受理编号" Binding="{Binding f_shoulicode}"/>
							<sdk:DataGridTextColumn Header="合同编号" Binding="{Binding f_hetongnum}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户姓名" Binding="{Binding f_username}"/>
							<sdk:DataGridTextColumn Header="小区名称" Binding="{Binding f_districtname}"/>
							<sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/>
							
							<sdk:DataGridTextColumn Header="受理日期" Binding="{Binding f_shoulidate,StringFormat=yyyy-MM-dd}"/>
						</sdk:DataGrid.Columns>
					</sdk:DataGrid>
					
					<!--提交按钮-->
				</toolkit:DockPanel >
			</toolkit:BusyIndicator>
			<toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy or SaveAction.IsBusy}" Margin="5,0,0,0" Grid.Column="1" >
				<toolkit:DockPanel>
					<!--按钮-->
					<toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Center" VerticalAlignment="Center" >
						<Button x:Name="save" Content="保  存" Margin="5" 
							IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors }" 
							Click="{m:Exp Str='SaveAction.Invoke()'}" Width="50" />
						<Button Content="撤  销" Margin="5" Click="{m:Exp Str=userfile.DataContext.New()}" Width="50"/>
						<Button Content="打  印" Margin="5" Click="{m:Exp Str=print.Print()}" Width="50"/>
					</toolkit:DockPanel>
					<toolkit:DockPanel   Margin="0"   Height="182" VerticalAlignment="Stretch"   Width="892"   >
						<toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="22">
							<TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{areafiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="0,0,0,1"/>
						</toolkit:DockPanel>
						<toolkit:DockPanel  x:Name="areafilesearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,203,5" Background="#FFEAF8FF"  >
							<a:Data.Context>
								<o:SearchObject />        	
							</a:Data.Context>
							<Grid Width="489" Margin="0,0,200,0">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="70"/>
									<ColumnDefinition Width="50*"/>
									<ColumnDefinition Width="50*"/>
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition Height="28"/>
									<RowDefinition Height="28"/>
								</Grid.RowDefinitions>
								<TextBlock TextWrapping="Wrap" Text="小区名称：" Margin="5,4,0,4" />
								<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_districtname}" Width="Auto" Margin="0" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_districtname"  Operator="f_districtname like 'this%'"/>          
									</a:Data.Prop>
								</TextBox>
								<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="小区地址：" Margin="5,4,0,4" />
								<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_address}" Width="Auto" Margin="0" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_address"  Operator="f_address like 'this%'"/>          
									</a:Data.Prop>
								</TextBox>
								<Button Content="查  询" x:Name="areasearchbutton" Grid.Column="2" Grid.Row="1" Click="{m:Exp Str=areafilesearch.DataContext.Search()}" Margin="0,0,85,-2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
							</Grid>
						</toolkit:DockPanel>
						<sdk:DataGrid x:Name="areafiles" Margin="2,0,2,-22"  
							AutoGenerateColumns="False" Background="{x:Null}" IsReadOnly="True"  >
							<a:Data.Items>
												<!--and f_gasmeterstyle \=\'卡表\' and f_userNum \> CAST(f_endNumber as int) \- CAST(f_usernumber as int) and CAST(f_endNumber as int) \- CAST(f_usernumber as int) \>\= 0  -->
								<o:ObjectList x:Name="areafileslist" WebClientInfo="{StaticResource dbclient}"
									Path="{m:Exp Str=$from t_area where \{areafilesearch.DataContext.Condition\} and f_filiale \=\'\{LoginUser.f_fengongsi\}\'}"
									IsOld="{m:Exp Str=SaveAction.State\=\=$End$ or userfile.DataContext.State\=\=$End$}" 
												/> 
							</a:Data.Items>
							<sdk:DataGrid.Columns>
								<!--sdk:DataGridTemplateColumn Header="已建档户数">
										<sdk:DataGridTemplateColumn.CellTemplate>
											<DataTemplate>
												<TextBox Text="{m:Bind Path=f_yijianhushu}" IsEnabled="False"> 
													<a:Data.Prop>
										                <o:PropertySetter PropertyName="f_yijianhushu"  
											             Value="{m:Exp Str=this.f_usernumber.ToInt() - this.f_startNumber.ToInt()}"/>
									                   </a:Data.Prop>
												</TextBox>
											</DataTemplate>
										</sdk:DataGridTemplateColumn.CellTemplate>
									</sdk:DataGridTemplateColumn-->
								<sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
								<sdk:DataGridTextColumn Header="小区名称" Binding="{Binding f_districtname}"/>
								<sdk:DataGridTextColumn Header="小区地址" Binding="{Binding f_address}"/>
								<sdk:DataGridTextColumn Header="户数" Binding="{Binding f_userNum}"/>
							</sdk:DataGrid.Columns>
						</sdk:DataGrid>
										
					</toolkit:DockPanel>
					<Grid x:Name="userfile" DataContext="{m:Exp Str=userfilego}" Margin="0,0,0,20" >
					<!--非民用主操作区-->
					<!--非民用主操作区-->
						<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" d:LayoutOverrides="Height">
							<Grid Margin="0" HorizontalAlignment="Stretch"  >
								<Grid.Background>
									<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
										<GradientStop Color="#FFF2F8FF" Offset="0"/>
										<GradientStop Color="#FFDDF2FD" Offset="1"/>
									</LinearGradientBrush>
								</Grid.Background>
								<Grid.RowDefinitions>
									<RowDefinition Height="8"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="120"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="10"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="8"/>
									<RowDefinition Height="30"/>
									<RowDefinition Height="30"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="100"/>
									<ColumnDefinition Width="0.1*"/>
									<ColumnDefinition Width="110"/>
									<ColumnDefinition Width="1*"/>
									<ColumnDefinition Width="100"/>
									<ColumnDefinition Width="0.8*"/>
								</Grid.ColumnDefinitions>
								<ComboBox x:Name="CoboxGas" DisplayMemberPath="name" SelectedValuePath="name" ItemsSource="{m:Exp Str=params[data.name\=\=$受理凭证$].First().f_paramvalues}" SelectedValue="{m:Bind Path=f_pingzheng}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="4" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_pingzheng"  />
									</a:Data.Prop>
								</ComboBox>
								<TextBox Text="{m:Bind Path=f_hetongnum}"   Grid.Column="5" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.RowSpan="1">
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_hetongnum" NotNull="True"  />
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_username}" Grid.Column="1" Margin="0,3,0,2" Grid.Row="6" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_username" NotNull="True"/>
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_phone}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="6" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" IsReadOnly="False" Grid.ColumnSpan="1" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_phone"/>
									</a:Data.Prop>
								</TextBox>
								<ComboBox DisplayMemberPath="name" SelectedValuePath="name" SelectedValue="{m:Bind Path=f_credentials}"  Grid.Column="5" Margin="0,3,0,2" Grid.Row="6" Height="25" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" > 
									<a:Data.Items>
										<o:ObjectList WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='证件类型'"/>
									</a:Data.Items>
								</ComboBox>
								<TextBox Text="{m:Bind Path=f_idnumber}"  Grid.Column="5" Margin="0,2,0,3" Grid.Row="7" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" IsReadOnly="False" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_idnumber"/>
									</a:Data.Prop>
								</TextBox>
								<ComboBox DisplayMemberPath="name" SelectedValuePath="name" SelectedValue="{m:Bind Path=f_shenqinglu}"  Grid.Column="3" Margin="0,2,-1,3" Grid.Row="7" Height="25" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" Grid.ColumnSpan="1" > 
									<a:Data.Items>
										<o:ObjectList WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='申请理由'"/>
									</a:Data.Items>
								</ComboBox>
								<TextBox Text="{m:Bind Path=f_cusDom}"  Grid.Column="1" Margin="0,3,-1,2" Grid.Row="13"  Grid.ColumnSpan="1"  TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_cusDom" />
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_cusDy}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="13"  Grid.ColumnSpan="1"  TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_cusDy" />
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_cusFloor}"  Grid.Column="5" Margin="0,3,0,2" Grid.Row="13"  Grid.ColumnSpan="1"  TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_cusFloor"/>
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_shoulibeizhu}"  Grid.Column="1" Margin="0,2,0,3" Grid.Row="14" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" Grid.ColumnSpan="3" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_shoulibeizhu" />
									</a:Data.Prop>
								</TextBox>
								<TextBox Text="{m:Bind Path=f_apartment}"  Grid.Column="5" Margin="0,2,0,3" Grid.Row="14" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Left" Width="201" Grid.RowSpan="1" Grid.ColumnSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_apartment"  NotNull="True" Value="{m:Exp Str=$\{userfilego.f_cusDom\}幢\{userfilego.f_cusDy\}单元\{userfilego.f_cusFloor\}室$}"/>
									</a:Data.Prop>
								</TextBox>	
								<TextBlock Text="用户姓名：" Margin="1,3,0,2" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
								<TextBlock Text="楼　　号：" Margin="1,4,0,1" Grid.Row="13" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
								<TextBlock Text="小区地址：" Margin="0,0,0,2" Grid.Row="12" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
								<TextBlock Text="证件类型：" Margin="0,3,0,2" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="4" Height="25"/>
								<TextBlock Text="门 牌 号：" Margin="0,2,0,3" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="14" Height="25" HorizontalAlignment="Right" Width="100" Grid.Column="4"/>
								<TextBlock Text="证件号码：" Grid.Column="4" Margin="0,1,0,4" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="7" Height="25"/>
								<TextBlock Text="电　　话：" Margin="0,3,0,2" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25" />
								<TextBlock Text="受理凭证：" Grid.Column="2" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
								<TextBlock Text="合同编号：" Grid.Column="4" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
									
						
									
						
								<TextBox Text="{m:Bind Path=f_address}"  Grid.Column="3" Margin="0,0,0,2" Grid.Row="12" TextWrapping="Wrap" Grid.ColumnSpan="3" Height="25" VerticalAlignment="Bottom" Grid.RowSpan="1" d:LayoutOverrides="Height" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_address"  Value="{m:Exp Str=areafiles.SelectedItem.f_address}" />
									</a:Data.Prop>
								</TextBox>
								
								
						
									
						
								
								
								
								<TextBlock Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" Text="受理编号：" TextAlignment="Right" Height="25"/>
								<TextBox Text="{m:Bind Path=f_shoulicode}"  Grid.Column="1" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" d:LayoutOverrides="HorizontalAlignment" Grid.RowSpan="1" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_shoulicode" Default="{m:Exp Str=$\{SysTime.Now.ToString($yyyy$)\}\{Seriasl.Value\}}"/>
									</a:Data.Prop>
								</TextBox>
								<TextBlock Margin="-201,4,0,1" Grid.Row="4" TextWrapping="Wrap" Text="是否发折：" TextAlignment="Right" Height="25" HorizontalAlignment="Left" Width="110"/>
								<TextBlock Grid.Row="16" TextWrapping="Wrap" Text="网       点：" TextAlignment="Right" Height="25" Margin="0,2,0,3"/>
								<TextBox Text="{m:Bind Path=f_yytdepa}" Grid.Column="1" Margin="0,2,0,3" Grid.Row="16" TextWrapping="Wrap"  VerticalAlignment="Stretch" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_yytdepa" NotNull="True" 
											Default="{m:Exp Str=LoginUser.f_parentname}"/>
									</a:Data.Prop>
								</TextBox>
								<TextBlock Grid.Column="2" Grid.Row="16" TextWrapping="Wrap" Text="受 理 人："  TextAlignment="Right" Height="25" Margin="0,2,0,3"/>
								<TextBox Text="{m:Bind Path=f_shouliren}" Grid.Column="3" Margin="0,2,0,3" Grid.Row="16" TextWrapping="Wrap"  VerticalAlignment="Stretch" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_shouliren" NotNull="True"  
											Default="{m:Exp Str=LoginUser.name}"/>
									</a:Data.Prop>
								</TextBox>
								<TextBlock Grid.Column="4" Margin="0,2,0,3" Grid.Row="16" TextWrapping="Wrap" Text="受理日期：" TextAlignment="Right" Height="25" />
								<sdk:DatePicker SelectedDate="{m:Bind Path=f_shoulidate}"  Grid.Column="5" Margin="0,2,0,3" Grid.Row="16"  IsTodayHighlighted="True" IsDropDownOpen="False" SelectedDateFormat="Short" VerticalAlignment="Stretch"  Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_shoulidate" NotNull="True" 
											Default="{m:Exp Str=SysTime.Today}"/>
									</a:Data.Prop>
								</sdk:DatePicker>
								
