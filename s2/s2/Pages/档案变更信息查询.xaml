                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_changeman" />
                                                </a:Data.Prop>
                                            </TextBox>
                                            <TextBlock Text="操作日期：" Grid.Column="2" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="1" Height="20" />
                                            <TextBox Text="{Binding Path=f_changedate,StringFormat=yyyy-MM-dd}" Grid.Column="3" Margin="3,2,3,2" Grid.Row="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_changedate" Operator="substring(f_changedate,1,10) >= substring('this',1,10)"/>
                                                </a:Data.Prop>
                                            </TextBox>
                                            <TextBlock Text="网点：" Grid.Column="0" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="2" Height="20"/>
                                            <TextBox Text="{m:Bind Path=f_changemannetwork}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_changemannetwork" />
                                                </a:Data.Prop>
                                            </TextBox>
                                            <TextBlock Text="分公司：" Grid.Column="2" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="2" Height="20" />
                                            <TextBox Text="{m:Bind Path=f_changemansubsidiary}" Grid.Column="3" Margin="3,2,3,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_changemansubsidiary" />
                                                </a:Data.Prop>
                                            </TextBox>
                                        </Grid>
                                    </Grid>
                                </ScrollViewer>
