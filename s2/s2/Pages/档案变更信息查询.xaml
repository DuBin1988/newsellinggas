                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="1.5*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="修改原因：" Grid.Column="0" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="0" Height="20"/>
                                            <TextBox Text="{m:Bind Path=f_changecause}" Grid.ColumnSpan="3" Grid.Column="1" Margin="3,2,3,2" Grid.Row="0" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
