                                        <TextBox Text="{m:Bind Path=f_stair3price}" Grid.Column="4" Margin="3,2,3,0" Grid.Row="31" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" Grid.RowSpan="1" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_stair3price" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_stair3pricechange}" Grid.Column="5" Margin="3,2,3,0" Grid.Row="31" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" Grid.RowSpan="1" >
