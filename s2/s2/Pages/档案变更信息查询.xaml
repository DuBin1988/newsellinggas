<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.档案变更信息查询">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-101,0">
            <!--档案修改记录列表-->
            <o:PagedObjectList Name="userfileschangelist" WebClientInfo="{StaticResource dbclient}" 
                Path="{m:Exp Str=$from t_userfileschange t where \{userfilechangesearch.DataContext.Condition\}$}"
				SumNames="," PageSize="{m:Exp Str=pager.PageSize}" PageIndex="{m:Exp Str=pager.PageIndex}"/>
            <!--档案修改对象-->
            <o:GeneralObject Name="userfileschangeobj" EntityType="t_userfileschange" WebClientInfo="{StaticResource dbclient}"
                Path="{m:Exp Str=$one/from t_userfileschange where id\=\'\{userfiles.SelectedItem.id\}\'}"/>
        </m:ResourceLoad>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.25*"/>
                <ColumnDefinition Width="0.75*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
                <Border BorderThickness="1" BorderBrush="#FF9AA2AB" Margin="0" Padding="2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" >
                    <toolkit:DockPanel Width="Auto"   >
                        <toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="22">
                            <TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="0,0,0,1"/>
                            <sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=userfiles}"  Margin="0,0,0,-2" PageSize="20"/>
                        </toolkit:DockPanel>
                        <toolkit:DockPanel  x:Name="userfilechangesearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5" Background="#FFEAF8FF"  >
                            <a:Data.Context>
                                <o:SearchObject />
                            </a:Data.Context>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70"/>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="50"/>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="60"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="28"/>
                                    <RowDefinition Height="28"/>
                                    <RowDefinition Height="28"/>
                                </Grid.RowDefinitions>
                                <TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,4,0,4" />
                                <TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" Width="Auto" Margin="0" Height="25" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_userid"  Operator="f_userid='this'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="0" Text="卡 号：" Margin="5,4,0,4" Grid.Column="2" />
                                <TextBox TextWrapping="Wrap" Grid.Row="0" Grid.Column="3" Text="{m:Bind Path=f_cardid}" Width="Auto" Margin="0" Height="25" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_cardid"  Operator="f_cardid='this'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" Margin="5,4,0,4" />
                                <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_username}" Width="Auto" Margin="0" Height="25" Grid.ColumnSpan="3" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_username"  Operator="f_username like '%this%'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="2" Margin="0,0,0,3" Height="25" TextAlignment="Right" Text="操作日期："/>
                                <sdk:DatePicker Name="tj4" SelectedDate="{m:Bind Path=f_yytdate1}" Grid.Row="2" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,0,0,3" Height="25" HorizontalAlignment="Stretch" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_yytdate1"  Value="{m:Exp Str=SysTime.Today}" Operator="f_changedate >= substring('this',1,10)"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <TextBlock TextWrapping="Wrap" Grid.Row="2" Margin="0,0,0,3" Height="25" TextAlignment="Right" Grid.Column="2" Text="到："/>
                                <sdk:DatePicker Name="tj8" SelectedDate="{m:Bind Path=f_yytdate2}" Grid.Row="2" Grid.Column="3" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,0,0,3" Height="25" Grid.ColumnSpan="1" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_yytdate2"   Value="{m:Exp Str=SysTime.Today}" Operator="f_changedate ^= substring('this',1,10)"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <Button Content="查  询" x:Name="usersearchbutton" Grid.Column="4" Grid.Row="2" Click="{m:Exp Str=userfilechangesearch.DataContext.Search()}" Margin="0,0,5,2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
                                <Button Content="清  除" x:Name="usersearchbutton1" Grid.Column="4" Grid.Row="1" Click="{m:Exp Str=userfilechangesearch.DataContext.f_userid\=null;userfilechangesearch.DataContext.f_cardid\=null;userfilechangesearch.DataContext.f_username\=null;userfilechangesearch.DataContext.f_yytdate1\=null;userfilechangesearch.DataContext.f_yytdate2\=null}" Margin="0,0,5,2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
                            </Grid>
                        </toolkit:DockPanel>
                        <sdk:DataGrid x:Name="userfiles" Margin="0" ItemsSource="{m:Exp Str=userfileschangelist}" AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
                            <sdk:DataGrid.Columns>
                                <sdk:DataGridTextColumn Header="序号" Binding="{Binding Index}"/>
                                <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}"/>
                                <sdk:DataGridTextColumn Header="用户姓名" Binding="{Binding f_usernamechange}"/>
                                <sdk:DataGridTextColumn Header="卡    号" Binding="{Binding f_cardid}"/>
                                <sdk:DataGridTextColumn Header="地    址" Binding="{Binding f_addresschange}"/>
                            </sdk:DataGrid.Columns>
                        </sdk:DataGrid>
                    </toolkit:DockPanel>
                </Border>
            </c:BusyIndicatorEx>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" Margin="5,0,0,0" Grid.Column="1" >
                <toolkit:DockPanel>
                    <Grid x:Name="userfileschange" DataContext="{m:Exp Str=userfileschangeobj}" Margin="0,0,0,20" >
                        <!--非民用主操作区-->
                        <sdk:TabControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <sdk:TabItem Header="基本信息">
                                <!--非民用主操作区-->
                                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Width="Auto">
                                    <Grid Margin="0" HorizontalAlignment="Stretch"  >
                                        <Grid.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                                <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                                <GradientStop Color="#FFDDF2FD" Offset="1"/>
                                            </LinearGradientBrush>
                                        </Grid.Background>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="10"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>

                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>

                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>

                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="35"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>


                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="120"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="80"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid Grid.RowSpan="1" Grid.ColumnSpan="6">
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="25"/>
                                            </Grid.RowDefinitions>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="1*"/>
                                                <ColumnDefinition Width="80"/>
                                                <ColumnDefinition Width="1*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Text="用户编号：" Grid.Column="0" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="0"/>
                                            <TextBox Text="{m:Bind Path=f_userid}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="0" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" Grid.RowSpan="1" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_userid" />
                                                </a:Data.Prop>
                                            </TextBox>
                                            <TextBlock Text="单位编号：" Grid.Column="2" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="0"/>
                                            <TextBox Text="{m:Bind Path=f_unitid}" Grid.Column="3" Margin="3,2,3,2" Grid.Row="0" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" Grid.RowSpan="1" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_unitid" />
                                                </a:Data.Prop>
                                            </TextBox>
                                            <TextBlock Text="卡号：" Grid.Column="4" Margin="3,2,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="0"/>
                                            <TextBox Text="{m:Bind Path=f_unitid}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="0" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" Grid.RowSpan="1" IsReadOnly="True" >
                                                <a:Data.Prop>
                                                    <o:PropertySetter PropertyName="f_unitid" />
                                                </a:Data.Prop>
                                            </TextBox>
                                        </Grid>
                                        <Border Margin="0,2,0,2" BorderThickness="0,0,0,0"  Height="6" Grid.ColumnSpan="6" Grid.RowSpan="1" Grid.Row="1">
                                            <Border.Background>
                                                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                                    <GradientStop Color="#FFC6E2FB" Offset="1"/>
                                                    <GradientStop Color="#FFE6F1FB"/>
                                                </LinearGradientBrush>
                                            </Border.Background>
                                        </Border>
                                        <TextBlock Text="用户档案修改前" Grid.Column="1" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Center" Grid.Row="2" Height="20" />
                                        <TextBlock Text="用户档案修改后" Grid.Column="2" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Center" Grid.Row="2" Height="20" />
                                        <TextBlock Text="用户档案修改前" Grid.Column="4" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Center" Grid.Row="2" Height="20" />
                                        <TextBlock Text="用户档案修改后" Grid.Column="5" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Center" Grid.Row="2" Height="20" />
                                        <TextBlock Text="联系人：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Height="20"/>
                                        <TextBox Text="{m:Bind Path=f_linkname}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_linkname" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_linknamechange}" Foreground="{m:Exp Str=userfileschangeobj.f_linkname\!\=userfileschangeobj.f_linknamechange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_linknamechange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="用户、单位名称：" Grid.Column="3" Margin="3,3,3,0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Height="20" VerticalAlignment="Top"/>
                                        <TextBox Text="{m:Bind Path=f_username}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_username" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_usernamechange}" Foreground="{m:Exp Str=userfileschangeobj.f_username\!\=userfileschangeobj.f_usernamechange:Red\,Black}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usernamechange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="证件类型：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Height="20"/>
                                        <TextBox Text="{m:Bind Path=f_credentials}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_credentials" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_credentialschange}" Foreground="{m:Exp Str=userfileschangeobj.f_credentials\!\=userfileschangeobj.f_credentialschange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_credentialschange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="证件号码：" Grid.Column="3" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Height="20" />
                                        <TextBox Text="{m:Bind Path=f_idnumber}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_idnumber" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_idnumberchange}" Foreground="{m:Exp Str=userfileschangeobj.f_idnumber\!\=userfileschangeobj.f_idnumberchange:Red\,Black}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_idnumberchange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="电话：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Height="20" />
                                        <TextBox Text="{m:Bind Path=f_phone}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_phone" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_phonechange}" Foreground="{m:Exp Str=userfileschangeobj.f_phone\!\=userfileschangeobj.f_phonechange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_phonechange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="地址：" Grid.Column="3" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Height="20" />
                                        <TextBox Text="{m:Bind Path=f_address}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_address" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_addresschange}" Foreground="{m:Exp Str=userfileschangeobj.f_address\!\=userfileschangeobj.f_addresschange:Red\,Black}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_addresschange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="用户状态：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Height="20"/>
                                        <TextBox Text="{m:Bind Path=f_userstate}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_userstate" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_userstatechange}" Foreground="{m:Exp Str=userfileschangeobj.f_userstate\!\=userfileschangeobj.f_userstatechange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_userstatechange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="安装地点：" Grid.Column="3" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Height="20" />
                                        <TextBox Text="{m:Bind Path=f_weizhi}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_weizhi" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_weizhichange}" Foreground="{m:Exp Str=userfileschangeobj.f_weizhi\!\=userfileschangeobj.f_weizhichange:Red\,Black}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_weizhichange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="用户类型：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="7" Height="20"/>
                                        <TextBox Text="{m:Bind Path=f_usertype}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usertype" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_usertypechange}" Foreground="{m:Exp Str=userfileschangeobj.f_usertype\!\=userfileschangeobj.f_usertypechange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usertypechange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="交费类型：" Grid.Column="3" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="7" Height="20" />
                                        <TextBox Text="{m:Bind Path=f_payment}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_payment" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_paymentchange}" Foreground="{m:Exp Str=userfileschangeobj.f_payment\!\=userfileschangeobj.f_paymentchange:Red\,Black}" Grid.Column="5" Margin="3,2,3,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_paymentchange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="抄表员：" Grid.Column="0" Margin="3,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="8" Height="20"/>
                                        <TextBox Text="{m:Bind Path=f_inputtor}" Grid.Column="1" Margin="3,2,3,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_inputtor" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_inputtorchange}" Foreground="{m:Exp Str=userfileschangeobj.f_inputtor\!\=userfileschangeobj.f_inputtorchange:Red\,Black}" Grid.Column="2" Margin="3,2,3,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_inputtorchange" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="最后抄表日期：" Grid.Column="3" Margin="0,3,3,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="8" Height="20" />
                                        <TextBox Text="{Binding Path=lastinputdate,StringFormat=yyyy-MM-dd}" Grid.Column="4" Margin="3,2,3,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" IsReadOnly="True" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="lastinputdate" Operator=" substring(lastinputdate,1,10) >= substring('this',1,10)"/>
