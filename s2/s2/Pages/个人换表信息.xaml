<c:CustomChildWindow 
           x:Class="s2.Pages.个人换表信息"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
		   xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
		   xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	       xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
           xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
		   xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
		   xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="800" Height="500"   FontSize="14"
           Title="个人换表信息" Name="child">
    <Grid x:Name="LayoutRoot2" Grid.Row="3" Grid.Column="3"  Margin="12,0,12,0"  DataContext="{m:Exp Str=child.ParamValue}" >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>

            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_userid}" IsReadOnly="True" Grid.Column="1" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_userid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="用户姓名：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_username" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="用户类型：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="5" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_usertype" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="小区名称：" Margin="0" TextWrapping="Wrap" Grid.Row="1"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_districtname}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_districtname" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="旧用户编号：" Margin="139,1" TextWrapping="Wrap"  Grid.Column="1" Grid.Row="1" TextAlignment="Right" Height="25" Grid.ColumnSpan="3"/>
        <TextBox Text="{m:Bind Path=f_olduserid}" IsReadOnly="True" Grid.Column="3" Grid.Row="1" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_olduserid" />
            </a:Data.Prop>
        </TextBox>
        

        <TextBlock Text="地址：" Margin="0" TextWrapping="Wrap" Grid.Row="2"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Grid.ColumnSpan="5" Margin="0" Grid.Row="2"   Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_address" />
            </a:Data.Prop>
        </TextBox>


        <TextBlock Text="超用气量：" Margin="0" TextWrapping="Wrap" Grid.Row="3"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cypregas}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="3"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cypregas" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="补气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cmaddgas}" IsReadOnly="True" Grid.Column="3" Grid.Row="3" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmaddgas" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="剩余补气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_nextaddgasamount}" IsReadOnly="True" Grid.Column="5" Grid.Row="3" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_nextaddgasamount" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="操作类型：" Margin="0" TextWrapping="Wrap" Grid.Row="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_watchtype}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="4"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_watchtype" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="换表类型：" Margin="0,5,0,0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_replacewatchtype}" IsReadOnly="True" Grid.Column="3" Grid.Row="4" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_replacewatchtype" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="换表费：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_watchfee}" IsReadOnly="True" Grid.Column="5" Grid.Row="4" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_watchfee" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="旧表品牌：" Margin="0" TextWrapping="Wrap" Grid.Row="5"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_gaswatchbrand}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="5"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_gaswatchbrand" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="新表品牌：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cmnewgaswatchbrand}" IsReadOnly="True" Grid.Column="3" Grid.Row="5" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmnewgaswatchbrand" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="旧表型号：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_metertype}" IsReadOnly="True" Grid.Column="5" Grid.Row="5" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_metertype" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="新表型号：" Margin="0" TextWrapping="Wrap" Grid.Row="6"  TextAlignment="Right" Height="25"/>

        <TextBox  Text="{m:Bind Path=f_cmnewmetertype}" IsReadOnly="True"  Grid.Column="1" Margin="0" Grid.Row="6"   Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmnewmetertype" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="旧卡号：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cardid}" IsReadOnly="True" Grid.Column="3" Grid.Row="6" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cardid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="新卡号：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_newcardid}" IsReadOnly="True" Grid.Column="5" Grid.Row="6" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_newcardid" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="换表日期：" Margin="0" TextWrapping="Wrap" Grid.Row="7"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_cmchangemeterdate, StringFormat=yyyy-MM-dd}"  IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="7"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmchangemeterdate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="分公司：" Margin="0" TextWrapping="Wrap" Grid.Column="2" Grid.Row="7"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_filiale}" IsReadOnly="True" Grid.Column="3" Margin="0" Grid.Row="7"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_filiale" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="换表登记人：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="8" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cmoperator}" IsReadOnly="True" Grid.Column="3" Grid.Row="8" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmoperator" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="登记日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="7" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_cmdate,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Grid.Row="7" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmdate" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="登记时间：" Margin="0" TextWrapping="Wrap" Grid.Row="8"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_cmtime,StringFormat=hh:mm:ss tt}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="8"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cmtime" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="卡厅审核人：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="9" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_rcoperator}" IsReadOnly="True" Grid.Column="1" Grid.Row="9" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_rcoperator" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="卡厅审核意见：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="9" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_yijian}" IsReadOnly="True" Grid.Column="3" Grid.Row="9" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_yijian" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="卡厅审核日期：" Margin="0" TextWrapping="Wrap" Grid.Row="9"  Grid.Column="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_date,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Margin="0" Grid.Row="9"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_date" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="客服经理：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="10" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_bmoper}" IsReadOnly="True" Grid.Column="1" Grid.Row="10" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_bmoper" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="客服经理意见：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="10" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_bmyijian}" IsReadOnly="True" Grid.Column="3" Grid.Row="10" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_bmyijian" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="客服经理审核日期：" Margin="0" TextWrapping="Wrap" Grid.Row="10" Grid.Column="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_bmdate,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Margin="0" Grid.Row="10"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_bmdate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="审计员：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="11" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_shejiyuan}" IsReadOnly="True" Grid.Column="1" Grid.Row="11" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_shejiyuan" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="审计员意见：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="11" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_shejiyuanyijian}" IsReadOnly="True" Grid.Column="3" Grid.Row="11" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_shejiyuanyijian" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="审计员审核日期：" Margin="0" TextWrapping="Wrap" Grid.Row="11" Grid.Column="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_shejiyuandate,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Margin="0" Grid.Row="11"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_shejiyuandate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="生产运营部经理：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_yyoper}" IsReadOnly="True" Grid.Column="1" Grid.Row="12" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_yyoper" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="生产运营部意见：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_yyyijian}" IsReadOnly="True" Grid.Column="3" Grid.Row="12" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_yyyijian" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="生产运营部审核日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_yydate,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Grid.Row="12" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_yydate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="维修确认人：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="13" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_wxoper}" IsReadOnly="True" Grid.Column="1" Grid.Row="13" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_wxoper" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="维修确认意见：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="13" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_wxyijian}" IsReadOnly="True" Grid.Column="3" Grid.Row="13" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_wxyijian" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="维修日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="13" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_wxdate,StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Grid.Row="13" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_wxdate" />
            </a:Data.Prop>
        </TextBox>

        <!--<Button x:Name="CancelButton" Content="取消" Click="CancelButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,17,55,-10" Grid.Row="12" Grid.Column="4" />-->
        <Button x:Name="OKButton" Content="确定" Click="OKButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,14,95,-7" Grid.Row="14" Grid.Column="2" Grid.ColumnSpan="2" />
    </Grid>
</c:CustomChildWindow>