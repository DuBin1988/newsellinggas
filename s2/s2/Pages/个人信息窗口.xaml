				 <a:Data.Prop>
					<o:PropertySetter PropertyName="f_districtname"    />
                 </a:Data.Prop>
            </TextBox>                       
			<TextBlock Text="小区地址：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="4" Height="25" Grid.Row="1"/>
