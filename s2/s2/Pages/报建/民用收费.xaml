<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="s2.民用收费" FontSize="13"
	d:DesignWidth="1024">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad d:IsHidden="True">
            <o:ObjectList Name="paytype" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='报建付款方式'"/>
            <o:ObjectList Name="mygastype" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproperties as gasproperties left join fetch gasproperties.qijiatype}" EntityType="t_gasproperties" />
            <o:ObjectList Name="mypricetype" Source="{m:Exp Str=CoboxPinps.SelectedItem.qijiatype}" EntityType="t_qijiatype" />
            <!--报装选中对象-->
            <o:GeneralObject Name="mypgshObj" WebClientInfo="{StaticResource dbclient}" EntityType="t_apply"
                Source="{m:Exp Str='applys.SelectedItem'}"  IsInit="{m:Exp Str='SaveAction.State==$End$'}">

               <!--实交金额-->

                <o:PropertySetter PropertyName="f_actualfee" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:fee.f_fee + mypgshObj.f_actualfee\,null}"/>

                <o:PropertySetter PropertyName="f_applycustomer" Default="{m:Exp Str=userlist_mysfbz}"/>
                <!--实交金额-->

            </o:GeneralObject>
            <o:ObjectList Name="paydetails" EntityType="t_paydetail" WebClientInfo="{StaticResource dbclient}" 
			Path="{m:Exp Str='applys.SelectedItem!=null:$from t_paydetail where applyid={applys.SelectedItem.id}$,null'}"/>

            <!--收费编辑对象-->
            <o:GeneralObject Name="fee" WebClientInfo="{StaticResource dbclient}" EntityType="t_paydetail"
                IsInit="{m:Exp Str='SaveAction.State==$End$'}">
                <!--对应报装id-->
                <o:PropertySetter PropertyName="applyid" Default="{m:Exp Str=mypgshObj.id}" NoDependValue="{m:Exp Str=mypgshObj.id}"/>
                <o:PropertySetter PropertyName="f_state" Default="有效"/>
            </o:GeneralObject>

         
            <o:ObjectList Name="costreductionlist_mypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='mypgshInfo.DataContext.State==$End$'}"
             Path="{m:Exp Str='mypgshInfo.DataContext.id!=null:$from t_costreduction where applyid={mypgshInfo.DataContext.id}$,null'}"      EntityType="t_costreduction" HasEmptyRow="False" />

            <!--保存-->
            <b:BatchExcuteAction Name="SaveAction" CanSave="{m:Exp Str=SaveButton\=>Click!\=null}" WebClientInfo="{StaticResource dbclient}">
                <!--报装对象保存-->
                <b:BatchInfo Source="{m:Exp Str=mypgshObj}" MethodName="SaveToJson"/>
                <!--收费对象保存-->
                <b:BatchInfo Source="{m:Exp Str=fee}" MethodName="SaveToJson"/>
            </b:BatchExcuteAction>
        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <toolkit:DockPanel   LastChildFill="True" Grid.ColumnSpan="1" Margin="0">
                <toolkit:WrapPanel x:Name="applysearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                    <a:Data.Context>
                        <o:SearchObject />
                    </a:Data.Context>
                    <TextBlock TextWrapping="Wrap" Text="申 请 人："/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" Width="Auto" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone = 'this'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <Button Content="  查 询  " x:Name="searchbutton" Click="{m:Exp Str=applysearch.DataContext.Search()}" Margin="5,0,0,0"/>
                </toolkit:WrapPanel>
                <!--提交按钮-->
                <toolkit:DockPanel  toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5"  >
                    <sdk:DataPager x:Name="pager" Source="{Binding Path=ItemsSource.Count,ElementName=applys,Converter={StaticResource DataPagerConverter}}" Margin="0" 
                                   PageSize="20" Height="24" toolkit:DockPanel.Dock="Top" />
                    <TextBlock Height="27" TextWrapping="Wrap" Text="{m:Exp Str=$共\{applys.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
                </toolkit:DockPanel>
                <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}">
                    <sdk:DataGrid x:Name="applys" Margin="0" MinHeight="250" AutoGenerateColumns="False" Background="{x:Null}" IsReadOnly="True" SelectionChanged="applys_SelectionChanged">
                        <a:Data.Items>
                            <o:PagedObjectList  x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
                               IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
		                       Path="{m:Exp Str=$from t_apply as apply
		                       where \{applysearch.DataContext.Condition\} and (f_usertype\=\'民用\' 
                               and f_totalfee is not null and f_totalfee>f_actualfee) or (f_usertype\=\'民用\' and   f_actualfee is null)}"  
                                PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"
                                />
                        </a:Data.Items>
                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                            <sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                            <sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                            <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date,StringFormat=yyyy-MM-dd}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </c:BusyIndicatorEx>
            </toolkit:DockPanel>

            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=mypgshObj.IsBusy}" Grid.Column="1">
                <Grid Background="{StaticResource LoginBgColorBrush1}" x:Name="mypgshInfo" DataContext="{m:Exp Str=mypgshObj}">


                    <sdk:TabControl  Margin="0" VerticalAlignment="Top" Height="600">
                        <sdk:TabItem Header="报建基本信息">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="0.196,0,-0.196,37" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
                                <ScrollViewer.RenderTransform>
                                    <CompositeTransform SkewX="-0.18" TranslateX="-0.997"/>
                                </ScrollViewer.RenderTransform>
                                <StackPanel>
                                    <Grid HorizontalAlignment="Stretch" Height="400">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="40"/>
                                            <RowDefinition Height="40"/>
                                            <RowDefinition Height="40"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>

                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="130"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="130"/>
                                            <ColumnDefinition Width="100"/>
                                            <ColumnDefinition Width="130"/>

                                        </Grid.ColumnDefinitions>
                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" Grid.Column="2"/>
                                        <TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="3" Margin="0,2" TextWrapping="Wrap" Height="Auto" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_code"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" TextWrapping="Wrap" Text="报建日期：" TextAlignment="Right"/>
                                        <sdk:DatePicker SelectedDate="{m:Bind Path=f_date}" IsEnabled="False" Grid.Column="5" Margin="0,3"  Height="Auto" Grid.Row="0" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_date"/>
                                            </a:Data.Prop>
                                        </sdk:DatePicker>
                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="申 请 人：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="1" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_username"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="电 话：" TextAlignment="Right" Grid.Column="4"/>
                                        <TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="5" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" HorizontalAlignment="Stretch" Width="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_phone"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="用气地址：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_address"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="片 区：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="5" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_area"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usertype"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="房屋类型：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_usertypepro}" Grid.Column="3" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usertypepro"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="4" TextWrapping="Wrap" Text="申请户数：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_applycount"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="4" TextWrapping="Wrap" Text="安装户数：" TextAlignment="Right"/>

                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="设计费：" TextAlignment="Right"/>
                                        <TextBlock Margin="0,4,0,2" Grid.Row="5" TextWrapping="Wrap" Text="工程款：" TextAlignment="Right"/>
                                        <TextBlock  Margin="0,3" Grid.Row="6" Grid.Column="4"  TextWrapping="Wrap" Text="总合计：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="1" Margin="0,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_installnum" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_sjfee}" Grid.Column="5" Margin="0,2,0,0" Grid.Row="5" TextWrapping="Wrap" Height="26" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Top">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_sjfee"  />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,3" Grid.Row="7" TextWrapping="Wrap" Text="定金：" TextAlignment="Right" Grid.Column="2"/>
                                        <TextBox Text="{m:Bind Path=f_earnest}" Grid.Column="3" Margin="0,29,0,0" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="2">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_earnest"   />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_gckfee}" Grid.Column="1" Margin="0,0,0,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gckfee" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="6" TextWrapping="Wrap" Text="小锅炉费：" TextAlignment="Right" Height="21" VerticalAlignment="Top"/>
                                        <TextBox Text="{m:Bind Path=f_xglfyfee}" Grid.Column="3" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_xglfyfee"  />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_totalfee}" Grid.Column="5" Margin="0,29,0,3" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="2">
                                        </TextBox>

                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" TextAlignment="Right"  Text="用户单位：" Grid.Row="1"/>
                                        <TextBox Text="{m:Bind Path=f_minyongdept}" IsReadOnly="True" Grid.Column="1" Margin="0,2"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="1" Grid.ColumnSpan="3"  Height="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_minyongdept"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="1" Grid.Column="4" TextWrapping="Wrap" Text="用气性质：" TextAlignment="Right"/>
                                        <ComboBox x:Name="CoboxPinps" ItemsSource="{m:Exp Str=mygastype}" DisplayMemberPath="f_gasproperties" SelectedValuePath="f_gasproperties" SelectedValue="{m:Bind Path=f_gasproperties}" 
										Grid.Column="5" Margin="0" Grid.Row="1" Height="Auto" IsEnabled="false" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gasproperties"
									Default="{m:Exp Str=mygastype[data.f_gasproperties\=\=$民用$].ToObjectList().Count &gt; 0 :$民用$\,null}" />
                                            </a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock Grid.Column="4" Grid.Row="7" TextWrapping="Wrap" Text="气价类型：" TextAlignment="Right" Margin="0,6,0,0"/>
                                        <ComboBox  x:Name="pricetype1" ItemsSource="{m:Exp Str=mypricetype}" DisplayMemberPath="f_gaspricetype"  SelectedValuePath="f_gaspricetype" SelectedValue="{m:Bind Path=f_gaspricetype}"  
										Grid.Column="5" Grid.Row="7" Height="Auto" IsEnabled="false" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gaspricetype"
									Default="{m:Exp Str=mypricetype[data.f_gaspricetype\=\=$民用气价$].ToObjectList().Count &gt; 0 :$民用气价$\,null}" />
                                            </a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock Grid.Column="4" Margin="0,5,0,2" Grid.Row="8" TextWrapping="Wrap" Text="气价：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_gasprice}"   IsReadOnly="True" Grid.Column="5" Margin="0,28,0,2"   Height="Auto" Grid.Row="7" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" RenderTransformOrigin="0.389,2.08" Grid.RowSpan="2">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gasprice"  Value="{m:Exp Str=pricetype1.SelectedItem.f_gasprice}" Default="1.55" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="合同约定：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="11" Grid.Column="4"/>

                                        <ComboBox DisplayMemberPath="name" SelectedValuePath="name" SelectedValue="{m:Bind Path=f_htyuding}" Grid.Column="5" Margin="0,4,0,7" Grid.Row="11" Height="Auto" Width="Auto">
                                            <a:Data.Items>
                                                <o:ObjectList WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='合同约定'" />
                                            </a:Data.Items>
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_htyuding"  NotNull="True" Default="{m:Exp Str=data.f_actualfee>\=data.f_totalfee:$费用交清$\,$费用未交清$}" Value="{m:Exp Str=data.f_actualfee>\=data.f_totalfee:$费用交清$\,$费用未交清$}"/>
                                            </a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="报建备注：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="9"/>
                                        <TextBox Text="{m:Bind Path=f_baojbeizhu}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="9" TextWrapping="Wrap" Grid.ColumnSpan="3" Height="Auto" HorizontalAlignment="Stretch" Width="Auto" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_baojbeizhu" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,11,0,6" Grid.Row="10" TextWrapping="Wrap" Text="勘查意见：" TextAlignment="Right" VerticalAlignment="Center" Height="23"/>
                                        <TextBox Grid.ColumnSpan="3" Text="{m:Bind Path=f_gcbkcopinion}" Grid.Column="1" Margin="0,2" Grid.Row="10" TextWrapping="Wrap" IsReadOnly="True" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" />
                                        <TextBlock Margin="0,7,0,6" Text="勘察审核意见：" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" VerticalAlignment="Center" Height="27"/>
                                        <TextBox Text="{m:Bind Path=f_opinion}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="11" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Width="Auto" HorizontalAlignment="Stretch" />
                                        <TextBlock  Grid.Row="8" TextWrapping="Wrap" Text="定金实交：" TextAlignment="Right" Margin="0,2,0,38" Grid.RowSpan="2"/>
                                        <TextBox Text="{m:Bind Path=f_actualfee}" Grid.Column="1" Margin="0,2,0,38" Grid.Row="8" TextWrapping="Wrap" Height="Auto" IsReadOnly="true" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="2" Grid.ColumnSpan="1">
                                        </TextBox>

                                        <TextBlock Margin="0,6,0,0" Grid.Row="6" TextWrapping="Wrap" Text="设备费：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_facilityfee}" Grid.Column="1" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">

                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_facilityfee"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,2" Grid.Column="2" Grid.Row="5" TextWrapping="Wrap" Text="管网建设费：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_gwjsfee}" Grid.Column="3" Margin="0.25,0,0,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch" Grid.ColumnSpan="1">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gwjsfee"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock  Text="欠款：" Margin="0,6,0,0" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_debt}" Grid.Column="1" Margin="0,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_debt" Default="0"/>
                                            </a:Data.Prop>
                                        </TextBox>

                                        <TextBlock Margin="0,6,0,0" Grid.Row="8" Grid.Column="2" TextWrapping="Wrap" Text="减免金额合计：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_jmhj}" Grid.Column="3" Margin="0,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" HorizontalAlignment="Stretch">

                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_jmhj"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                    </Grid>
                                    <Grid HorizontalAlignment="Stretch" DataContext="{m:Exp Str=fee}">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="30"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>

                                        <TextBlock Margin="0" Grid.Row="0" Grid.Column="2" TextWrapping="Wrap" Text="付款方式 :" TextAlignment="Right"/>
                                        <ComboBox Grid.Row="0" Grid.Column="3" Height="Auto" Width="Auto" ItemsSource="{m:Exp Str=paytype}" 
									DisplayMemberPath="name" SelectedValuePath="name" SelectedValue="{m:Bind Path=f_paytype}" Margin="0,0,0,0">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_paytype"
									Default="现金" />
                                            </a:Data.Prop>
                                        </ComboBox>
