<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="Com.Aote.Pages.非民用总信息查看" FontSize="13">

	<Grid x:Name="LayoutRoot">
	<m:ResourceLoad Margin="0,0,-12,0">
	<!--附件信息-->
		<o:ObjectList Name="gjlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='报建状态'"/>
		<o:ObjectList Name="paydetaillist" EntityType="t_paydetail" HasEmptyRow="True"></o:ObjectList>				
		<o:ObjectList Name="annexlist" EntityType="t_applyannex" HasEmptyRow="True"></o:ObjectList>	
		<o:ObjectList Name="userlist" EntityType="t_applycustomer" HasEmptyRow="True"></o:ObjectList>
		<o:ObjectList Name="paylist" EntityType="t_paydetail" HasEmptyRow="False"></o:ObjectList>
		<o:ObjectList Name="costreductionlist" EntityType="t_costreduction" HasEmptyRow="False"></o:ObjectList>
		<o:ObjectList Name="devicelist" EntityType="t_device" HasEmptyRow="False"></o:ObjectList>
		<o:ObjectList Name="image" EntityType="t_applyimage" HasEmptyRow="False"></o:ObjectList>
		<!--批量删除语句-->
		<o:ObjectList Name="projectlist" WebClientInfo="{StaticResource dbclient}"  
		Path="{m:Exp Str='apply.DataContext.id!=null:$from t_projectplan where applyid={apply.DataContext.id}$,null'}" EntityType="t_projectplan"   />
	
        <b:HQLAction Name="DeleteAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str='$delete from t_apply where id in ({applys.SelectedItems.ToString(id)=>SelectionChanged})'}">
		</b:HQLAction>
	</m:ResourceLoad>
    <Grid>
    	<Grid.ColumnDefinitions>
			<ColumnDefinition Width="0.3*"/>
			<ColumnDefinition Width="0.7*"/>
		</Grid.ColumnDefinitions>
        <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}" Grid.ColumnSpan="1" Margin="0,0,5,45">
            <toolkit:DockPanel LastChildFill="True" MaxWidth="380">
                <toolkit:WrapPanel x:Name="applysearch" toolkit:DockPanel.Dock="Top"  >
                   <a:Data.Context>
            		  	<o:SearchObject ></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申请人"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22" Margin="2,0,0,0">
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like '%this%'"/>               
					  </a:Data.Prop>
					</TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电话" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" Width="Auto" Height="22" Margin="2,0,0,0" >
                    	 <a:Data.Prop>
                      	  <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone like 'this%'"/>          
					   </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="单位" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_applydept}" Width="Auto" Height="22" Margin="2,0,0,0">
                    	<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_applydept"  Operator="apply.f_applydept like '%this%'"/>          
                    	</a:Data.Prop>
                    </TextBox>
					 <TextBlock TextWrapping="Wrap" Text="地 址：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_address}" Width="Auto" Height="22">
                    	 <a:Data.Prop>
                      	  <o:PropertySetter PropertyName="f_address"  Operator="apply.f_address like '%this%'"/>          
					   </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="报建状态" Margin="5,0,0,0"/>
                    <!--TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_state}" Width="Auto" Height="22" Margin="2,0,0,0">
                    	<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_state"  Operator="apply.f_state like 'this%'"/>          
                    	</a:Data.Prop>
                    </TextBox-->
					<ComboBox Name="ComboxZht" SelectedValue="{m:Bind Path=f_state}" ItemsSource="{m:Exp Str=gjlist}"  Grid.Column="1" Margin="0,3,0,2" DisplayMemberPath="name" SelectedValuePath="name" Grid.Row="2"  Height="25">
						<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_state"  Operator="apply.f_state like 'this%'"/>          
                    	</a:Data.Prop>
					</ComboBox>
                    <TextBlock TextWrapping="Wrap" Text="片区：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_area}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_area" Operator="apply.f_area = 'this'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="报建编号：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_code}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_code" Operator="apply.f_code = 'this'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="日期类型：" Margin="5,0,0,0" Height="25"/>
                    <ComboBox SelectedValue="{m:Bind Path=dataType}" Margin="2,0,0,0" DisplayMemberPath="name" SelectedValuePath="code" Height="22">
                        <a:Data.Items>
                            <o:ObjectList WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='日期类型'"/>
                        </a:Data.Items>
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="dataType" Operator="1=1" />
                        </a:Data.Prop>
                    </ComboBox>
                    <sdk:DatePicker SelectedDate="{m:Bind Path=startDate}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="startDate" Operator="{m:Exp Str=this.dataType!\=null:$\{this.dataType\} >\= \'\{this.startDate\}\'$\,$1\=1$}" />
                        </a:Data.Prop>
                    </sdk:DatePicker>
                    <TextBlock TextWrapping="Wrap" Text="至" Margin="5,0,0,0" Height="25"/>
                    <sdk:DatePicker SelectedDate="{m:Bind Path=endDate}" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="endDate" Operator="{m:Exp Str=this.dataType!\=null:$\{this.dataType\} ^\= \'\{this.endDate\}\'$\,$1\=1$}" />
                        </a:Data.Prop>
                    </sdk:DatePicker>
                    <Button Content=" 查 询 " x:Name="searchbutton" Click="{m:Exp Str=applysearch.DataContext.Search();ComboxZht.SelectedValue\=null}" Margin="2,0,0,0"/>
                </toolkit:WrapPanel>
				<toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Left">
					 <TextBlock Height="27" TextWrapping="Wrap" Text="{m:Exp Str='$共{applys.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
				</toolkit:DockPanel>
                <sdk:DataGrid x:Name="applys" Margin="0"  
                	AutoGenerateColumns="False" Width="Auto" IsReadOnly="True">
                	<a:Data.Items>
                		<o:ObjectList x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
						Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_applycustomer
						 left join fetch apply.f_costreduction left join fetch apply.f_device left join fetch apply.f_applyannex
						 left join fetch apply.f_paydetail left join fetch apply.f_applyimage  where \{applysearch.DataContext.Condition\} and f_usertype\=\'非民用\'}"
                		IsOld="{m:Exp Str=DeleteAction.State\=\=$End$ or apply.DataContext.State\=\=$End$}"/>
                	</a:Data.Items>
                	<sdk:DataGrid.Columns>
                		<sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                		<sdk:DataGridTextColumn Header="报建编号" Binding="{Binding f_code}"/>
						<sdk:DataGridTextColumn Header="报建状态" Binding="{Binding f_state}"/>
                		<sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                		<sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
						<sdk:DataGridTextColumn Header="申请单位" Binding="{Binding f_applydept}"/>
                		<sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                		<sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
						<sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                		<sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
                	</sdk:DataGrid.Columns>
                </sdk:DataGrid>
			</toolkit:DockPanel>
        </c:BusyIndicatorEx>
        <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=apply.DataContext.IsBusy}" Grid.ColumnSpan="1" Grid.Column="1" Grid.RowSpan="1" Margin="0,0,0,45" HorizontalAlignment="Left">
       <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" FontSize="13" HorizontalAlignment="Left">
            <toolkit:DockPanel x:Name="apply" Margin="0,0,0,0" HorizontalAlignment="Left" Width="830" >
            	<a:Data.Context>
            		<o:GeneralObject  Name="applygo" WebClientInfo="{StaticResource dbclient}" 
		               EntityType="t_apply" Source="{m:Exp Str=applys.SelectedItem}"
		               IsInit="{m:Exp Str='this.State==$End'}">
            			 <o:PropertySetter PropertyName="f_applycustomer" Default="{m:Exp Str=userlist}"></o:PropertySetter>  
						 <o:PropertySetter PropertyName="f_costreduction" Default="{m:Exp Str=costreductionlist}"></o:PropertySetter>
						 <o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=devicelist}"></o:PropertySetter>
						 <o:PropertySetter PropertyName="f_paydetail" Default="{m:Exp Str=paylist}"></o:PropertySetter> 
						 <o:PropertySetter PropertyName="f_applyannex" Default="{m:Exp Str=annexlist}"/> 
						 <o:PropertySetter PropertyName="f_paydetail" Default="{m:Exp Str=paydetaillist}"/>
						 <o:PropertySetter PropertyName="f_applyimage" Default="{m:Exp Str=image}"/>      		
					   </o:GeneralObject>
				</a:Data.Context>
		<Grid HorizontalAlignment="Left" >
            <sdk:TabControl TabStripPlacement="Right" Margin="0,0,0,8" HorizontalAlignment="Left" Width="830" Background="{x:Null}" >
			<sdk:TabItem Header="报建登记　  　" Height="Auto" HorizontalAlignment="Left" >
				  <Grid Width="700">
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="0.149*"/>
						<ColumnDefinition Width="0.253*"/>
						<ColumnDefinition Width="84"/>
						<ColumnDefinition Width="0.251*"/>
						<ColumnDefinition Width="85.666"/>
						<ColumnDefinition Width="0.347*" />
					</Grid.ColumnDefinitions>
					<TextBlock Margin="0" TextWrapping="Wrap" Text="申请人：" TextAlignment="Right" Height="22"/>
                    <TextBox Text="{m:Bind Path=f_username}" Grid.Column="1" Margin="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True" Height="22">
                    	
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电话：" TextAlignment="Right" Margin="0" Grid.Column="4" Height="22" />
                    <TextBlock Margin="0"  Text="操作日期" TextAlignment="Right" Grid.Row="3" Height="22" />
                    <sdk:DatePicker  SelectedDateFormat="long"  IsEnabled="False" SelectedDate="{m:Bind Path=f_date}"  Grid.Column="1" Margin="0,3,0,0" Grid.Row="3"  Width="Auto" Height="22" IsTodayHighlighted="False" d:IsHidden="True">
						
                    </sdk:DatePicker>
                    <TextBlock Margin="0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" Grid.Column="2" Height="22" />
                    <TextBox Text="{m:Bind Path=f_code}" Grid.Column="3" Margin="0"  Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True" Height="22">
                    	
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="地址：" TextAlignment="Right" Margin="0"  Grid.Row="1" Height="22"/>
                    <TextBox Text="{m:Bind Path=f_phone}" Grid.Column="5" Margin="0,3,0,0"  Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Left" Width="150" Grid.RowSpan="1" Grid.ColumnSpan="1" Height="22" IsReadOnly="True">
                    	
                    </TextBox>
                    <TextBox Text="{m:Bind Path=f_address}" Grid.Column="1" Margin="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="1" IsReadOnly="True" Height="22">
                    	
                    </TextBox>
                    <TextBox IsReadOnly="True" Text="{m:Bind Path=f_usertype}"  Grid.Column="3" Margin="0" Grid.Row="1" TextWrapping="Wrap" Height="22">
                    	               
					</TextBox>
                    <TextBlock Margin="0" TextWrapping="Wrap" Text="片区选择：" TextAlignment="Right"  Grid.Column="4" Height="22" Grid.Row="1"/>
                    <TextBlock Margin="0" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right"  Grid.Column="2" Height="22" Grid.Row="1"/>
                    <TextBox IsReadOnly="True" Text="{m:Bind Path=f_area}"  Grid.Column="5" Margin="0" Grid.Row="1" TextWrapping="Wrap" Height="22" Width="150" HorizontalAlignment="Left">
                    	              
					</TextBox>
                    <TextBlock  Text="申请用气点数：" TextAlignment="Right" Margin="0"  Grid.Row="2" Height="22" />
                    <TextBox Text="{m:Bind Path=f_applycount}" Grid.Column="1" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="2" IsReadOnly="True">
                    	
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="热水器台数：" TextAlignment="Right" Margin="0,3,0,0"  Grid.Row="2"  Grid.Column="2" Height="22" VerticalAlignment="Top"/>
				    <TextBox Text="{m:Bind Path=f_geyser}" Grid.Column="3" Margin="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="2" IsReadOnly="True" Height="22">
                    	
                    </TextBox>
				    <TextBlock TextWrapping="Wrap" Text="壁挂炉台数：" TextAlignment="Right" Margin="0" Grid.Row="2" Grid.Column="4" Height="22"/>
				    <TextBox Text="{m:Bind Path=f_bglnum}" Grid.Column="5" Margin="0" VerticalAlignment="Stretch" HorizontalAlignment="Left" Width="150" Grid.Row="2" IsReadOnly="True" Height="22">
				    	
				    </TextBox>
				    <TextBlock Margin="0" TextWrapping="Wrap" Text="部门：" TextAlignment="Right" Grid.Row="3" Height="22" Grid.Column="2"/>
				    <TextBox Text="{m:Bind Path=f_department}" IsReadOnly="True" Grid.Column="3" Margin="0" Height="22" Grid.Row="3" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">
				    	
				    </TextBox>
				    <TextBlock Margin="0" TextWrapping="Wrap" Text="操作人：" TextAlignment="Right" Grid.Row="3" Height="22" Grid.Column="4"/>
				    <TextBox Text="{m:Bind Path=f_opertor}" IsReadOnly="True" Grid.Column="5" Margin="0" Height="22" Grid.Row="3" VerticalAlignment="Stretch" HorizontalAlignment="Left" Width="150">
				    	
				    </TextBox>
					</Grid>	
			</sdk:TabItem>
			<sdk:TabItem Header="现场勘查　　  " HorizontalAlignment="Left">
				 <Grid Margin="0" HorizontalAlignment="Stretch" Width="700" >
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30" />
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="60"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="80"/>
						<ColumnDefinition Width="0.177*"/>
						<ColumnDefinition Width="80"/>
						<ColumnDefinition Width="0.176*"/>
						<ColumnDefinition Width="80"/>
						<ColumnDefinition Width="0.19*"/>
					</Grid.ColumnDefinitions>
						<TextBlock TextAlignment="Right" Margin="0" TextWrapping="Wrap" Text="勘查意见：" Height="22" Grid.Row="6" />
						<TextBox Text="{m:Bind Path=f_gcbkcopinion}" Grid.Column="1" Margin="0,1" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Grid.ColumnSpan="4" HorizontalAlignment="Left" Width="400">
							
						</TextBox>
						<TextBlock TextAlignment="Right" Margin="0" Grid.Row="7" TextWrapping="Wrap" Text="部门：" Height="22"/>
						<TextBox Text="{m:Bind Path=f_gcbkcdepartment}" Grid.Column="1" Margin="0" Grid.Row="7" TextWrapping="Wrap" Height="22" IsReadOnly="True">
							
						</TextBox>
						<TextBlock TextAlignment="Right" Grid.Column="2" Margin="0" Grid.Row="7" TextWrapping="Wrap" Text="操作人：" Height="22"/>
						<TextBox Text="{m:Bind Path=f_gcbkcopertor}" Grid.Column="3" Margin="0" Grid.Row="7" TextWrapping="Wrap" Height="22" IsReadOnly="True">
							
						</TextBox>
						<TextBlock TextAlignment="Right" Grid.Column="4" Margin="0" Grid.Row="7" TextWrapping="Wrap" Text="日期：" Height="22"/>
						<sdk:DatePicker Text="{m:Bind Path=f_gcbkcoperdate}" Grid.Column="5" Margin="0" Grid.Row="7"  Height="22" IsEnabled="False">
							
						</sdk:DatePicker>
						<TextBlock Margin="0" TextWrapping="Wrap" Text="报建编号：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0" TextWrapping="Wrap" Height="22" Grid.Row="0" VerticalAlignment="Stretch">
							
						</TextBox>
						<TextBlock Grid.Column="2" Margin="0" TextWrapping="Wrap" Text="报建日期：" Height="22" TextAlignment="Right"/>
						<sdk:DatePicker SelectedDate="{m:Bind Path=f_date}"  Grid.Column="3" Margin="0" IsEnabled="False" Height="22" Grid.Row="0" VerticalAlignment="Stretch">
							
						</sdk:DatePicker>
						<TextBlock Grid.Column="4" Margin="0" TextWrapping="Wrap" Text="申 请 人：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="5" Margin="0" TextWrapping="Wrap" Height="22" Grid.Row="0" VerticalAlignment="Stretch">
							
						</TextBox>
						<TextBlock Margin="-1,4,1,1" Grid.Row="3" TextWrapping="Wrap" Text="电 话：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="1" Margin="0,2,0,1" Grid.Row="3" TextWrapping="Wrap" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">
							
						</TextBox>
						<TextBlock Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="用气地址：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="22" VerticalAlignment="Stretch" Grid.ColumnSpan="3">
							
						</TextBox>
						<TextBlock Grid.Column="2" Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="片 区：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="3" Margin="0" Grid.Row="3" TextWrapping="Wrap" Height="22" VerticalAlignment="Stretch">
							
						</TextBox>
						<TextBlock Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="用户类型：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="22" VerticalAlignment="Stretch">
							
						</TextBox>
						<TextBlock Margin="0" Grid.Row="1" TextWrapping="Wrap" Text="申请单位：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_applydept}" Grid.Column="1" Margin="1,4,0,4" Grid.Row="1" TextWrapping="Wrap" Height="22" IsReadOnly="True" VerticalAlignment="Stretch" Grid.ColumnSpan="3">
							
						</TextBox>
						<TextBlock Margin="0" Grid.Row="5" TextWrapping="Wrap" Text="申请用气点数：" Height="22" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="1" Margin="0" Grid.Row="5" TextWrapping="Wrap" Height="22" IsReadOnly="True" VerticalAlignment="Stretch">
							
						</TextBox>
						<TextBlock  Margin="0" Grid.Row="5" TextWrapping="Wrap" Text="安装用气点数：" Height="22" TextAlignment="Right" Grid.Column="2"/>
						<TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="3" Margin="0" Grid.Row="5" TextWrapping="Wrap" Height="22" IsReadOnly="True" VerticalAlignment="Stretch">
							
						</TextBox>
				  </Grid>
			</sdk:TabItem>
			<sdk:TabItem Header="设计审核　  　" HorizontalAlignment="Left">
				 <Grid HorizontalAlignment="Stretch" Width="700">
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="97"/>
						<ColumnDefinition Width="0.58*"/>
						<ColumnDefinition Width="84.25"/>
						<ColumnDefinition Width="0.42*"/>
						<ColumnDefinition Width="73.75"/>
						<ColumnDefinition Width="120.75"/>
					</Grid.ColumnDefinitions>
					<Grid.RowDefinitions>
						<RowDefinition Height="30"/>
						<RowDefinition Height="1"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="1"/>
						<RowDefinition Height="30"/>
						<RowDefinition Height="30" />
						<RowDefinition Height="60"/>
						<RowDefinition Height="1" />
						<RowDefinition Height="30"/>
						<RowDefinition Height="30" />
						<RowDefinition Height="30"/>
						<RowDefinition />
					</Grid.RowDefinitions>
						<TextBlock Margin="0,22,0,0" TextWrapping="Wrap" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="7" Grid.Column="2" Height="22" VerticalAlignment="Top"/>
						<TextBlock TextWrapping="Wrap" Text="备注：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="6" Height="22"/>
						<TextBox Text="{m:Bind Path=f_sjexambz}" Grid.Column="1" Margin="0,1" TextWrapping="Wrap" Grid.Row="6" Grid.ColumnSpan="4" HorizontalAlignment="Left" IsReadOnly="True" Height="Auto" Width="400" >
							
						</TextBox>
						<TextBlock TextWrapping="Wrap" Text="部门：" Width="Auto" Margin="0" Grid.Row="8" Height="22" d:LayoutOverrides="Height" TextAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_sjexamdepartment}" Margin="0,1,0,0" TextWrapping="Wrap" Width="Auto" Height="22" Grid.Column="1" Grid.Row="8" IsReadOnly="True" VerticalAlignment="Top" d:LayoutOverrides="Height">
							
