<UserControl x:Class="Com.Aote.Pages.涉县待办非民用收费标准"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
    xmlns:ec="http://schemas.microsoft.com/expression/2010/controls"
    mc:Ignorable="d">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad>

            <!--流程启动对象-->
            <o:GeneralObject  Name="startprocess" WebClientInfo="{StaticResource server}" 
		        EntityType="t_test" 
	 	        IsInit="{m:Exp Str='this.State==$End$'}" CustomePath="{m:Exp Str=$/workflow/perfomactivity/\{fmypgshList.SelectedItem.actid\}/\{LoginUser.name\}/\{LoginUser.id\}}"
                 >
            </o:GeneralObject>

            <o:PagedList Name="fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='fmypgshInfo.DataContext.State==$End$ |or| SubmitAction.State==$End$ |or| fmypgshPager.DataContext.State==$End$ |or| fmypgshBack=>Click!=null'}"
                Names="id,f_substate,f_code,f_username,f_phone,f_minyongdept,f_address,f_applycount,f_date,actid,f_processid" 
                HQL="{m:Exp Str=$select apply.id\,apply.f_substate\,apply.f_code\,apply.f_username\,apply.f_phone\,apply.f_minyongdept\,apply.f_address\,apply.f_applycount\,CONVERT(varchar\, apply.f_date\,23)f_date\,act.id actid\,apply.f_processid
                from activityins act 
                join t_actorperson p 
                on act.actorexpression \= p.actorid 
                join t_apply apply 
                on apply.f_processid\=act.processid 
                where \{fmypgshSearch.DataContext.Condition\} and act.state\=\'开始活动\' and act.processName\=\'涉县非民用流程\' and act.defname\=\'收费标准\' and p.userid\=\'\{LoginUser.ename\}\'}"
                Path="sql" PageIndex="{m:Exp Str=fmypgshPager.PageIndex}" PageSize="{m:Exp Str=fmypgshPager.PageSize}" SumNames="," />

            <o:GeneralObject Name="fmypgshObj" EntityType="t_apply" WebClientInfo="{StaticResource dbclient}" 
			    Path="{m:Exp Str=fmypgshList.SelectedItem \!\=null:$one/from t_apply where id\=\{fmypgshList.SelectedItem.id\}$\,$null$}"  
                IsInit="{m:Exp Str='this.State==$End$ or fmypgshBack=>Click!=null'}">
                <o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=devicelist_fmypgsh}"/>
                <o:PropertySetter PropertyName="f_applyimage" Default="{m:Exp Str=imagelist_fmypgsh}" />
                <o:PropertySetter PropertyName="f_applyannex" Default="{m:Exp Str=annexlist_fmypgsh}" />
            </o:GeneralObject>

            <o:ObjectList Name="imagelist_fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='fmypgshInfo.DataContext.State==$End$ or fmypgshBack=>Click!=null'}"
            Path="{m:Exp Str='fmypgshInfo.DataContext.id!=null:$from t_applyimage where applyid={fmypgshInfo.DataContext.id}$,null'}" EntityType="t_applyimage" HasEmptyRow="True" />
            <o:ObjectList Name="annexlist_fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='fmypgshInfo.DataContext.State==$End$ or SubmitAction.State==$End$ or fmypgshPager.DataContext.State==$End$ or  fmypgshBack=>Click!=null'}"
            Path="{m:Exp Str='fmypgshInfo.DataContext.id!=null:$from t_applyannex where applyid={fmypgshInfo.DataContext.id}$,null'}" EntityType="t_applyannex"   HasEmptyRow="True"/>

            <o:ObjectList Name="dwlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='设备单位'"/>
            <o:ObjectList Name="fmygastype" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproperties as gasproperties left join fetch gasproperties.qijiatype}" EntityType="t_gasproperties" />
            <o:ObjectList Name="fmypricetype" Source="{m:Exp Str=CoboxPinpl.SelectedItem.qijiatype}" EntityType="t_qijiatype" />
            <!--非民用收费标准-->
            <o:ObjectList Name="devicelist_fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='fmypgshInfo.DataContext.State==$End$ or fmypgshBack=>Click!=null'}"
            Path="{m:Exp Str='fmypgshInfo.DataContext.id!=null:$from t_device where applyid={fmypgshInfo.DataContext.id}$,null'}" EntityType="t_device" HasEmptyRow="False"  >

                <o:PropertySetter PropertyName="f_count"  NoDependValue="{m:Exp Str='this.f_count!\=null :this.f_count\,1'}"/>
                <o:PropertySetter PropertyName="f_price"  NoDependValue="{m:Exp Str=devicename[data.name\=\=this.f_name].First().code.ToDouble() }"/>
                <o:PropertySetter PropertyName="f_money"  NoDependValue="{m:Exp Str=this.f_name!\=$模块炉$ and this.f_name!\=$茶水炉$ and this.f_name!\=$采暖炉$ :(this.f_count * this.f_price.ToDouble()).MathRound(2)\,(dwlist[data.name\=\=this.f_unit].First().code.ToDouble() * this.f_usegasnum *this.f_count * this.f_price.ToDouble()).MathRound(2)}"/>
            </o:ObjectList>
            <o:ObjectList Name="costreductionlist_fmypgsh" EntityType="t_costreduction" HasEmptyRow="True" />

            <b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_apply set f_sfbzzt\=\'已完成\' \,f_substate\=null where id in (\{fmypgshList.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
        </m:ResourceLoad>
        <!--非民用收费标准-->
        <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=fmypgsh.IsBusy}">
            <StackPanel Margin="2,0,2,8">
                <toolkit:WrapPanel x:Name="fmypgshSearch" Width="488">
                    <a:Data.Context>
                        <o:SearchObject CanSearch="{m:Exp Str='LayoutRoot=>Loaded!=null'}" />
                    </a:Data.Context>
                    <TextBlock TextWrapping="Wrap" Text="非民用收费标准" Margin="5,0,20,0"/>
                    <TextBlock TextWrapping="Wrap" Text="申请人：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_username" Operator="f_username like 'this%'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="单位：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_minyongdept}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_minyongdept"  Operator="f_minyongdept like 'this%'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_phone"  Operator="f_phone = 'this'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="地址：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_address}" MinWidth="30" Margin="5,0,0,0" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_address"  Operator="f_address like '%this%'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <Button Content="查 询" Click="{m:Exp Str=fmypgshSearch.DataContext.Search()}" Margin="0" HorizontalAlignment="Left" Width="60" />
                </toolkit:WrapPanel>
                <sdk:DataGrid Name="fmypgshList" AutoGenerateColumns="False" AlternatingRowBackground="{x:Null}" Background="{x:Null}" IsReadOnly="True" Height="300" Width="488"
								ItemsSource="{m:Res Key=fmypgsh}">
                    <sdk:DataGrid.Columns >
                        <sdk:DataGridTemplateColumn d:IsLocked="True" Header="序号" >
                            <sdk:DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <Button Content="  ->>  " Click="{m:Exp Str='fmypgshPopup.BringToUp(waitGrid);fmypgshPopup.IsOpen=True'}" />
                                        <TextBlock Text="{Binding Index}" />
                                    </StackPanel>
                                </DataTemplate>
                            </sdk:DataGridTemplateColumn.CellTemplate>
                        </sdk:DataGridTemplateColumn>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                        <sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
                        <sdk:DataGridTextColumn Header="申请单位" Binding="{Binding f_minyongdept}"/>
                        <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                        <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date,StringFormat=yyyy-MM-dd}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
                <StackPanel Orientation="Horizontal" Width="488" Height="25">
                    <Button Content="提交承包合同" Margin="5,0" Click="{m:Exp Str=submittip.Show()}" IsEnabled="{m:Exp Str='fmypgshList.SelectedItem!=null and fmypgshList.SelectedItem.f_substate!=null'}" Width="100" />
                    <c:MessageTipObj Name="submittip" Tip="您确认要提交到承包合同？" OK="{m:Exp Str=startprocess.id\=fmypgshList.SelectedItem.f_processid;SubmitAction.Invoke();startprocess.Save()}" />
                    <sdk:DataPager Name="fmypgshPager" Source="{Binding Path=ItemsSource.Count,ElementName=fmypgshList,Converter={StaticResource DataPagerConverter}}"
		                    HorizontalAlignment="Left" PageSize="20" Width="378" Height="24" VerticalAlignment="Bottom" d:LayoutOverrides="GridBox"/>
                    <TextBlock Text="总待办数:" Margin="-300,5,260,0" />
                    <TextBlock Text="{m:Exp Str='fmypgsh.Count'}"  Margin="-250,5,210,0" />
                </StackPanel>
            </StackPanel>
        </c:BusyIndicatorEx>
        <!--非民用收费标准-->
        <!-- -->
        <Popup x:Name="fmypgshPopup">
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=fmypgshObj.IsBusy or devicelist_fmypgsh.IsBusy or costreductionlist_fmypgsh.IsBusy}">
                <Grid Background="{StaticResource LoginBgColorBrush1}" x:Name="fmypgshInfo" DataContext="{m:Exp Str=fmypgshObj}" Width="1000" Height="640">
                    <sdk:TabControl  Margin="0,0,0,55" VerticalAlignment="Stretch" d:LayoutOverrides="Height" >
                        <sdk:TabItem Header="报建基本信息">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="0.273,0,-0.273,86" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
                                <ScrollViewer.RenderTransform>
                                    <CompositeTransform SkewX="-0.18" TranslateX="-0.997"/>
                                </ScrollViewer.RenderTransform>
                                <Grid Margin="0" HorizontalAlignment="Stretch" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>

                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="29"/>
                                        <RowDefinition Height="50"/>
                                        <RowDefinition Height="50"/>
                                        <RowDefinition Height="50"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210"/>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210"/>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" Grid.Column="2"/>
                                    <TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="3" Margin="0,2" TextWrapping="Wrap" Height="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_code"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="4" Margin="0,6,0,0" TextWrapping="Wrap" Text="报建日期：" TextAlignment="Right"/>
                                    <sdk:DatePicker SelectedDate="{m:Bind Path=f_date}"  Grid.Column="5" Margin="0,3" IsEnabled="False" Height="Auto" Grid.Row="0">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_date"/>
                                        </a:Data.Prop>
                                    </sdk:DatePicker>
                                    <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="申 请 人：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Height="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_username"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" Text="电　　话：" TextAlignment="Right" Grid.Column="4"/>
                                    <TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="5" Margin="0,2" Grid.Row="1" TextWrapping="Wrap" Height="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_phone"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" Text="用气地址：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="1" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_address"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="片　　区：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="5" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_area"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_usertype"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="申请单位：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_minyongdept}" Grid.Column="3" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_minyongdept"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="申请用气点数：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_applycount"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="安装用气点数：" TextAlignment="Right"/>
                                    <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="交费用气点数：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="1" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_installnum" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBox Text="{m:Bind Path=f_payfeenum}" Grid.Column="3" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_payfeenum" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,6,0,0" Grid.Row="6"  Grid.Column="4" TextWrapping="Wrap" Text="部 门：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_feedepartment}" Grid.Column="5" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_feedepartment" NotNull="True" 
									Default="{m:Exp Str=LoginUser.f_parentname}"  Value="{m:Exp Str='(this.IsModified or devicelist_fmypgsh.IsModified):LoginUser.f_parentname,this.f_feedepartment'}"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="7" TextWrapping="Wrap" Text="操 作 人：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_feeopertor}" Grid.Column="5" Margin="0,2" Grid.Row="7" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_feeopertor" NotNull="True"  
									 Default="{m:Exp Str='LoginUser.name'}" Value="{m:Exp Str='this.IsModified:LoginUser.name,this.f_feeopertor'}"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Grid.Column="4" Margin="0,14,0,0" Grid.Row="8" TextWrapping="Wrap" Text="日 期：" TextAlignment="Right"/>
                                    <sdk:DatePicker SelectedDate="{m:Bind Path=f_feedate}" IsEnabled="False" Grid.Column="5" Margin="0,10" Grid.Row="8"  Height="Auto" IsTodayHighlighted="True" IsDropDownOpen="False" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_feedate" NotNull="True" 
									Default="{m:Exp Str=SysTime.Today}"/>
                                        </a:Data.Prop>
                                    </sdk:DatePicker>
                                    <TextBlock Grid.Column="0" Margin="0,14,0,0" Grid.Row="8" TextWrapping="Wrap" Text="收费明细：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_sfbzbeizhu}" Grid.Column="1" Margin="2,2,0,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Width="Auto" HorizontalAlignment="Stretch">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_sfbzbeizhu" />
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBlock Grid.Column="4" Margin="0,9,0,27" Grid.Row="4" TextWrapping="Wrap" Text="设计费：" TextAlignment="Right" Grid.RowSpan="2"/>
                                    <TextBox Text="{m:Bind Path=f_sjfee}" Grid.Column="5" Margin="0,5,0,0" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="1">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_sjfee"   />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,9,0,27" Grid.Column="2" Grid.Row="4" TextWrapping="Wrap" Text="工程款：" TextAlignment="Right" Grid.RowSpan="2"/>
                                    <TextBox Text="{m:Bind Path=f_gckfee}" Grid.Column="3" Margin="0,5,0,0" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="1">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gckfee"  Default="0" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,9,0,27" Grid.Row="4" TextWrapping="Wrap" Text="管网建设费：" TextAlignment="Right" Grid.RowSpan="2"/>
                                    <TextBox Text="{m:Bind Path=f_gwjsfee}" Grid.Column="1" Margin="0,5,0,0" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="1">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gwjsfee" Default="0" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="0,7,0,0" Grid.Row="5" TextWrapping="Wrap" Text="小锅炉费：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_xglfyfee}" Grid.Column="1" Margin="0,4,0,0" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto" HorizontalAlignment="Stretch" Grid.RowSpan="1">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_xglfyfee" Default="0" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock  Margin="0,8,0,0" Grid.Column="2" Grid.Row="5" TextWrapping="Wrap" Text="费用合计：" TextAlignment="Right"/>
                                    <TextBox Text="{m:Bind Path=f_totalfee}" Grid.Column="3" Margin="0,4,0,0" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="false" Width="210" HorizontalAlignment="Right" Grid.RowSpan="1">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_totalfee" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>


                                    <!--
                                <StackPanel Grid.ColumnSpan="6" Grid.Row="4">
                                    <StackPanel Orientation="Horizontal" Width="800" >
                                        <TextBlock Text="大采暖炉模块炉" HorizontalAlignment="Left" Margin="0,3,0,0" TextWrapping="Wrap" Width="100" />
                                        <TextBox Text="{m:Bind Path=f_cnnum}" Margin="0,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Width="108" Height="25">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_cnnum" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="台" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap" />
                                        <TextBox Text="{m:Bind Path=f_cnm}" Margin="5,1,0,1" TextWrapping="Wrap" IsReadOnly="True" Width="72" Height="25">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_cnm" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Text="KM" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap"/>
                                        <TextBlock Text="壁挂炉" HorizontalAlignment="Left" Margin="8,10,0,0" TextWrapping="Wrap" />
                                        <TextBox Text="{m:Bind Path=f_bglnum}" Margin="8,1,0,1" TextWrapping="Wrap" IsReadOnly="True" Width="86" Height="25">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_bglnum" />
