<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="Com.Aote.Pages.非民用档案导入" FontSize="13">

    <Grid x:Name="LayoutRoot" >
        <m:ResourceLoad Margin="0,0,-101,0">
            <o:ObjectList Name="stairlist" EntityType="t_stairprice" WebClientInfo="{StaticResource dbclient}" Path="from t_stairprice"  HasEmptyRow="True" />


            <o:PagedObjectList Name="userlist"  WebClientInfo="{StaticResource dbclient}"
	      Path="{m:Exp Str=$from t_device  where applyid\=\{main.DataContext.id\}and (f_isimport is null or  f_isimport\=0 )$}" EntityType="t_device" 
         IsOld="{m:Exp Str=(main.DataContext.State\=\=$Free$ or main.DataContext.State\=\=$End$)}"
							  PageIndex="0" PageSize="10000"
							  SumNames="f_isimport"
		>
                
            </o:PagedObjectList>
          
           
            <o:ObjectList  Name="dangans"  
									   WebClientInfo="{StaticResource dbclient}"  TempObj="{m:Res Key=tempobj}" 
									  SelectObject="{m:Exp Str='userlist[True].ToObjectList()'}" 
									   TempletObject="{m:Res Key=tobj}" />

            <!--导库完成按钮-->
            <!--<b:HQLAction Name="DaokuoverAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_daokuover\=\'1\' \,f_ercifandan\=null  where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_daokustate\=\'完成\'}"/>-->
            <!--不导档案-->
            <b:HQLAction Name="NoDaokuoverAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_daokuover\=\'1\'\,f_nodaodevices\=\'1\'\, f_ercifandan\=null  where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\})}"/>

          <b:HQLAction Name="UpdateAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_daokustate\=\'完成\' where id \= \{main.DataContext.id\}$\,null}"/>
            <b:BatchExcuteAction Name="SubmitAction" Completed="AllImportSubmitAction_Completed" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=UpdateAction}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>
            <!--用气设备信息-->
            
           

            <o:ObjectList Name="qibiaolist" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from qibiao}" ></o:ObjectList>
            <o:GeneralObject Name="tempobj" EntityType="t_userfiles"></o:GeneralObject>
            <o:GeneralObject Name="tobj" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}">


                <!-- 报建从表id -->
                <!--<o:PropertySetter PropertyName="cbid" Value="{m:Exp Str=tempobj.id}"/> -->
                <!-- 客户签名 -->
                <!--<o:PropertySetter PropertyName="USER_SIGN" Value="{m:Exp Str=tempobj.USER_SIGN}"/>-->
                <!--客户评价-->
                <!--<o:PropertySetter PropertyName="f_kehupingjia" Value="{m:Exp Str=tempobj.f_kehupingjia}"/>-->
                <!-- 照片1 -->
                <!--<o:PropertySetter PropertyName="PHOTO_FIRST" Value="{m:Exp Str=tempobj.PHOTO_FIRST}"/>-->
                <!-- 照片2 -->
                <!--<o:PropertySetter PropertyName="PHOTO_SECOND" Value="{m:Exp Str=tempobj.PHOTO_SECOND}"/>-->
                <!-- 照片3 -->
                <!--<o:PropertySetter PropertyName="PHOTO_THIRD" Value="{m:Exp Str=tempobj.PHOTO_THIRD}"/>-->
                <!-- 照片4 -->
                <!--<o:PropertySetter PropertyName="PHOTO_FOUTH" Value="{m:Exp Str=tempobj.PHOTO_FOUTH}"/>-->
                <!-- 照片5 -->
                <!--<o:PropertySetter PropertyName="PHOTO_FIFTH" Value="{m:Exp Str=tempobj.PHOTO_FIFTH}"/>-->
                <!--基表读书-->
                <!--<o:PropertySetter PropertyName="F_METERDUSHU" Value="{m:Exp Str=tempobj.F_METERDUSHU}"/>-->
                <!--通气情况-->
                <!--<o:PropertySetter PropertyName="F_TQQK" Value="{m:Exp Str=tempobj.F_TQQK}"/>-->
                <!--通气状态-->
                <!--<o:PropertySetter PropertyName="F_TQSTATUS" Value="{m:Exp Str=tempobj.F_TQSTATUS}"/>-->
                <!-- 安检日期-->
                <!--<o:PropertySetter PropertyName="f_anjianriqi" Value="{m:Exp Str=tempobj.f_anjianriqi}"/>-->
                <!-- 到达时间 -->
                <!--<o:PropertySetter PropertyName="ARRIVAL_TIME" Value="{m:Exp Str=tempobj.ARRIVAL_TIME}"/>-->
                <!-- 离开时间 -->
                <!--<o:PropertySetter PropertyName="DEPARTURE_TIME" Value="{m:Exp Str=tempobj.DEPARTURE_TIME}"/>-->
                <!--验收状态-->
                <!--<o:PropertySetter PropertyName="F_YSSTATUS" Value="{m:Exp Str=tempobj.F_YSSTATUS}"/>-->
                <!-- 验收情况-->
                <!--<o:PropertySetter PropertyName="F_YSQK" Value="{m:Exp Str=tempobj.F_YSQK}"/>-->
                <!-- 验收人员 -->
                <!-- <o:PropertySetter PropertyName="F_YSY" Value="{m:Exp Str=tempobj.F_YSY}"/>-->
                <!-- 通气人员 -->
                <!--<o:PropertySetter PropertyName="F_TQY" Value="{m:Exp Str=tempobj.F_TQY}"/>-->

                <!--用户姓名-->
                <o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=main.DataContext.f_username}" Default="{m:Exp Str=main.DataContext.f_username}"/>
                <!--联系人或用户自己本人f_linkname-->
                <o:PropertySetter PropertyName="f_linkname" Default="{m:Exp Str=main.DataContext.f_username}" Value="{m:Exp Str=main.DataContext.f_username}"/>
                <!--身份证f_idnumber-->
                <o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=main.DataContext.f_code}" Default="{m:Exp Str=main.DataContext.f_code}"/>
                <!--地址f_address-->
                <o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=main.DataContext.f_address}" Default="{m:Exp Str=main.DataContext.f_address}"/>
                <!--小区名称f_districtname-->
                <o:PropertySetter PropertyName="f_districtname" Value="{m:Exp Str=main.DataContext.f_district}" Default="{m:Exp Str=main.DataContext.f_district}"/>
                <!--单元f_cusDy-->
                <o:PropertySetter PropertyName="f_cusDy" Value="{m:Exp Str=main.DataContext.f_cusDy}" Default="{m:Exp Str=main.DataContext.f_cusDy}"/>
                <!--楼号f_cusDom-->
                <o:PropertySetter PropertyName="f_cusDom" Value="{m:Exp Str=main.DataContext.f_cusDom}" Default="{m:Exp Str=main.DataContext.f_cusDom}"/>
                <!--楼层f_cusFloor-->
                <o:PropertySetter PropertyName="f_cusFloor" Value="{m:Exp Str=main.DataContext.f_cusFloor}" Default="{m:Exp Str=main.DataContext.f_cusFloor}"/>
                <!--门牌号f_apartment-->
                <o:PropertySetter PropertyName="f_apartment" Value="{m:Exp Str=main.DataContext.f_doorplate}" Default="{m:Exp Str=main.DataContext.f_doorplate}"/>
                <!--电话f_phone-->
                <o:PropertySetter PropertyName="f_phone" Value="{m:Exp Str=main.DataContext.f_phone}" Default="{m:Exp Str=main.DataContext.f_phone}"/>
                <!--片区-->
                <o:PropertySetter PropertyName="f_area" Default="{m:Exp Str=main.DataContext.f_area}" Value="{m:Exp Str=main.DataContext.f_area}"/>
                <!--用户类型f_usertype * -->
                <o:PropertySetter PropertyName="f_usertype" Default="民用" Value="民用"/>
                <!-- 用户状态f_userstate正常 * -->
                <o:PropertySetter PropertyName="f_userstate" Default="正常" Value="正常"/>
                <!--营业厅建档状态f_yytjdstate未完成 *-->
                <o:PropertySetter PropertyName="f_yytjdstate" Default="未完成" Value="未完成"/>
                <!--当前表累计购气量 f_metergasnums0 *-->
                <o:PropertySetter PropertyName="f_metergasnums" Default="0" Value="0"/>
                <!--用户总累计购气量f_cumulativepurchase0 * -->
                <o:PropertySetter PropertyName="f_cumulativepurchase" Default="0" Value="0"/>
                <!--是否发卡f_whethergivecard未发* -->
                <o:PropertySetter PropertyName="f_whethergivecard" Default="未发" Value="未发"/>
                <!--是否发折f_whethergivepassbook未发  *-->
                <o:PropertySetter PropertyName="f_whethergivepassbook" Default="未发" Value="未发"/>
                <!--最后购气量finallybought0 *-->
                <o:PropertySetter PropertyName="finallybought" Default="0" Value="0"/>
                <!--购气次数f_times1 * -->
                <o:PropertySetter PropertyName="f_times" Default="0" Value="0"/>
                <!--应交气量oughtamount0 *-->
                <o:PropertySetter PropertyName="oughtamount" Default="0" Value="0"/>
                <!--应交金额oughtfee0 *-->
                <o:PropertySetter PropertyName="oughtfee" Default="0" Value="0"/>
                <!--最后抄表气量lastinputgasnum0 *-->
                <o:PropertySetter PropertyName="lastinputgasnum" Default="0" Value="0"/>
                <!--最后一次抄表底数lastrecord0 * -->
                <o:PropertySetter PropertyName="lastrecord" Default="0" Value="0"/>
                <!--机表预购气量pregasnum0-->
                <o:PropertySetter PropertyName="pregasnum" Default="0" Value="0"/>
                <!--身份类型-->
                <o:PropertySetter PropertyName="f_credentials" Default="身份证" Value="身份证"/>
                <!--用户是否刚补卡f_buka否-->
                <o:PropertySetter PropertyName="f_buka" Default="否" Value="否"/>
                <!--公司类型-->
                <o:PropertySetter PropertyName="f_comtype" Default="天然气公司"/>
                <!--限购-->
                <o:PropertySetter PropertyName="f_xiangou" Default="0"/>
                <!--结余-->
                <o:PropertySetter PropertyName="f_zhye" Default="0"/>
                <!--账户实际结余-->
                <o:PropertySetter PropertyName="f_accountzhye" Default="0"/>
                <!--是否有壁挂锅炉-->
                <o:PropertySetter PropertyName="f_wallhangboiler" Default="{m:Exp Str=False}"/>
                <!--是否有灶具-->
                <o:PropertySetter PropertyName="f_kitchen" Default="{m:Exp Str=False}"/>
                <!--是否有热水器-->
                <o:PropertySetter PropertyName="f_waterheater" Default="{m:Exp Str=False}"/>
                <!--表底数-->
                <o:PropertySetter PropertyName="f_gasmeteraccomodations" Default="0"/>
                <!--额定流量-->
                <o:PropertySetter PropertyName="f_flownum" Default="0" Value="0"/>
                <!--脉冲(表系数)-->
                <o:PropertySetter PropertyName="f_maichong" Default="0" Value="0"/>
                <!--海力表型编码-->
                <o:PropertySetter PropertyName="f_watchnum" Default="0" Value="0"/>

                <!--用气性质f_gasproperties-->
                <o:PropertySetter PropertyName="f_gasproperties" Default="{m:Exp Str=main.DataContext.f_gasproperties}" Value="{m:Exp Str=main.DataContext.f_gasproperties}"/>
                <!--气价类型f_gaspricetype -->
                <o:PropertySetter PropertyName="f_gaspricetype" Default="{m:Exp Str=main.DataContext.f_gaspricetype}" Value="{m:Exp Str=main.DataContext.f_gaspricetype}"/>
                <!--气价f_gasprice   -->
                <o:PropertySetter PropertyName="f_gasprice" Default="{m:Exp Str=main.DataContext.f_gasprice}" Value="{m:Exp Str=main.DataContext.f_gasprice}"/>
                <!--阶梯气价类型f_stairtype  <o:PropertySetter PropertyName="f_stairtype" Default="民用阶梯气价" Value="民用阶梯气价"/> -->
                <o:PropertySetter PropertyName="f_stairtype" Default="{m:Exp Str=main.DataContext.f_stairtype}" Value="{m:Exp Str=main.DataContext.f_stairtype}"/>
                <!--合同编号f_metertitles   -->
                <o:PropertySetter PropertyName="f_metertitles" Default="{m:Exp Str=main.DataContext.f_htnum}" Value="{m:Exp Str=main.DataContext.f_htnum}"/>
                <!--气表品牌f_gaswatchbrand   -->
                <o:PropertySetter PropertyName="f_gaswatchbrand" Default="{m:Exp Str=main.DataContext.f_gaswatchbrand}" Value="{m:Exp Str=main.DataContext.f_gaswatchbrand}"/>
                <!--气表型号f_metertype   -->
                <o:PropertySetter PropertyName="f_metertype" Default="{m:Exp Str=main.DataContext.f_metertype}" Value="{m:Exp Str=main.DataContext.f_metertype}"/>
                <!--气表类型f_gasmeterstyle   -->
                <o:PropertySetter PropertyName="f_gasmeterstyle" Default="{m:Exp Str=main.DataContext.f_type}" Value="{m:Exp Str=main.DataContext.f_type}"/>
                <!--别名f_aliasname   -->
                <o:PropertySetter PropertyName="f_aliasname" Default="{m:Exp Str=main.DataContext.f_aliasname}" Value="{m:Exp Str=main.DataContext.f_aliasname}"/>
                <!--气表厂家f_gasmetermanufacturers   -->
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" Default="{m:Exp Str=main.DataContext.f_changjia}" Value="{m:Exp Str=main.DataContext.f_changjia}"/>
                <!--对应报建编号-->
                <o:PropertySetter PropertyName="applyid"  Default="{m:Exp Str=main.DataContext.id}"  Value="{m:Exp Str=main.DataContext.id}" />
            </o:GeneralObject>
        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.2*"/>
                <ColumnDefinition Width="0.8*"/>
            </Grid.ColumnDefinitions>
            <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=Allmain.DataContext.IsBusy}" Grid.ColumnSpan="1">
                <Grid x:Name="Allmain">
                    <sdk:TabControl Margin="0,0,0,55">
                        <!--待办-->
                        <sdk:TabItem Header=" 导非民用档案 ">
                            <toolkit:DockPanel LastChildFill="True">
                                <toolkit:WrapPanel x:Name="personsearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,5,0,5" Orientation="Horizontal" >
                                    <a:Data.Context>
                                        <o:SearchObject></o:SearchObject>
                                    </a:Data.Context>
                                    <TextBlock TextWrapping="Wrap" Text="申请人：" Margin="3,0,0,0"/>
                                    <TextBox TextWrapping="Wrap" MinWidth="30" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Margin="3,0,0,0">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Text="单位：" Margin="3,0,0,0"/>
                                    <TextBox TextWrapping="Wrap" MinWidth="30" Text="{m:Bind Path=f_minyongdept}" a:ControlAttach.DefaultButton="searchbutton" Margin="3,0,0,0" Height="25">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_minyongdept" Operator="f_minyongdept like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <Button Content=" 查  询 " x:Name="searchbutton" Click="{m:Exp Str=personsearch.DataContext.Search()}" Margin="10,0,0,0"/>
                                </toolkit:WrapPanel>
                                <!--提交按钮-->
                                <toolkit:DockPanel Margin="0,5" toolkit:DockPanel.Dock="Bottom" >
                                    <TextBlock Height="23" TextWrapping="Wrap" Text="{m:Exp Str='$共{persons.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right"/>
                                </toolkit:DockPanel>
                                <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=persons.ItemsSource.IsBusy}">
                                    <!--DataGrid-->
                                    <sdk:DataGrid Name="persons" Margin="0"  AutoGenerateColumns="False"  
				         IsReadOnly="True" >
                                        <a:Data.Items>
                                            <o:ObjectList x:Name="personslist" WebClientInfo="{StaticResource dbclient}"
                             Path="{m:Exp Str=$select distinct apply from t_apply as apply  where \{personsearch.DataContext.Condition\} and f_usertype\=\'非民用\'  and f_sendopertor is not null and f_huneiover \=\'1\' and f_daokustate is null}" 
					         IsOld="{m:Exp Str=(SubmitAction.State\=\=$Free$ or SubmitAction.State\=\=$End$)
							 and(main.DataContext.State\=\=$Free$ or main.DataContext.State\=\=$End$) 
							 and(NoDaokuoverAction.State\=\=$Free$ or NoDaokuoverAction.State\=\=$End$) }">
                                            </o:ObjectList>
                                        </a:Data.Items>
                                        <sdk:DataGrid.Columns>
                                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                                            <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_daokustate}"/>
                                            <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                                            <sdk:DataGridTextColumn Header="编号" Binding="{Binding f_code}"/>
                                            <sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
                                            <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
                                            <sdk:DataGridTextColumn Header="申请几处" Binding="{Binding f_dijichu}"/>
                                            <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
                                            <sdk:DataGridTextColumn Header="单位" Binding="{Binding f_minyongdept}"/>
                                        </sdk:DataGrid.Columns>
                                    </sdk:DataGrid>
                                </toolkit:BusyIndicator>
                            </toolkit:DockPanel>
                        </sdk:TabItem>
                        <!--已办-->
                    </sdk:TabControl>
                    <!--<Button Content="导数据完成"  Click="{m:Exp Str=daoshujutip.Show()}"
            		 Height="25" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,25,26" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment, Height"/>-->

                    <Button Content="不导档案"  Click="{m:Exp Str=nodaoshujutip.Show()}"
            		 IsEnabled="{m:Exp Str='persons.SelectedItems.Count=>SelectionChanged!=0'}" Height="25" VerticalAlignment="Bottom" Margin="0,0,25,26" d:LayoutOverrides="VerticalAlignment, Height" HorizontalAlignment="Center"/>
                </Grid>
            </toolkit:BusyIndicator>

            <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=SubmitAction.IsBusy or userlist.IsBusy}" Margin="10,0,0,0" Grid.ColumnSpan="1" Grid.Column="1" >
                <Grid Background="{x:Null}"  x:Name="main" Margin="0">
                    <a:Data.Context>
                        <!--编辑对象-->
                        <o:GeneralObject  Name="persongo" WebClientInfo="{StaticResource dbclient}" 
		EntityType="t_apply" 
		Source="{m:Exp Str='persons.SelectedItem'}"
		IsInit="{m:Exp Str='this.State==$End$'}">
                            <o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=userlist}"/>
                        </o:GeneralObject>
                        
                    </a:Data.Context>
                    <sdk:TabControl  Margin="0,0,0,55" VerticalAlignment="Stretch" d:LayoutOverrides="Height" >
                        <!--民用主操作区-->
                        <sdk:TabItem Header="报建基本信息">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                <Grid  VerticalAlignment="Top" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210" />
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210"/>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="210"/>
                                    </Grid.ColumnDefinitions>
                                    <!---->
            				<TextBlock Margin="0" TextWrapping="Wrap" Text="申请人：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625"   Height="25"/>
            				<TextBox Text="{m:Bind Path=f_username}" Grid.Column="1" Margin="0"   Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_username" />
            					</a:Data.Prop>
            				</TextBox>
            				<TextBlock TextWrapping="Wrap" Text="电 话：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.538,0.364" Grid.Column="2"   Height="25"/>
            				<TextBlock Margin="0,0,0,5" TextWrapping="Wrap" Text="操作日期：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="6"   Height="25"/>
            				<sdk:DatePicker  SelectedDateFormat="Short"  IsEnabled="False" SelectedDate="{m:Bind Path=f_date}"     Grid.Column="1" Margin="0,0,0,5"   Height="25" Grid.Row="6" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_date"
            							Default="{m:Exp Str=SysTime.Today}"/>
            					</a:Data.Prop>		 
            				</sdk:DatePicker>
            				<TextBlock Margin="0,0,0,5" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625"   Height="25" Grid.Row="5"/>
            				<TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0,0,0,5"   Height="25" Grid.Row="5" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_code" />
            					</a:Data.Prop>
            				</TextBox>
            				<TextBlock TextWrapping="Wrap" Text="地 址：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.827,0.333" Grid.Row="1"   Height="25"/>
            				<TextBox Text="{m:Bind Path=f_phone}" Grid.Column="3" Margin="0"  Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" Grid.ColumnSpan="1"   Height="25" IsReadOnly="True">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_phone" />
            					</a:Data.Prop>
            				</TextBox>
            				<TextBox Text="{m:Bind Path=f_address}" Grid.Column="1" Margin="0"   Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="1" Grid.ColumnSpan="3" IsReadOnly="True">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_address" NotNull="True" />
            					</a:Data.Prop>
            				</TextBox>
            				<TextBox IsReadOnly="True" Text="{m:Bind Path=f_usertype}"  Grid.Column="5" Margin="0" Grid.Row="0" TextWrapping="Wrap"   Height="25">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_usertype" NotNull="True" Default="民用"/>
            					</a:Data.Prop>               
            				</TextBox>
            				<TextBlock Margin="0" TextWrapping="Wrap" Text="片区选择：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Column="4"   Height="25" Grid.Row="1"/>
            				<TextBlock Margin="0" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Column="4"   Height="25"/>
