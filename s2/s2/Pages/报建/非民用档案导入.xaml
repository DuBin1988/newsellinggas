<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="Com.Aote.Pages.非民用档案导入" FontSize="13">

	<Grid x:Name="LayoutRoot" >
		<m:ResourceLoad Margin="0,0,-101,0">
		
		<o:ObjectList   Name="personslist2"  
									   WebClientInfo="{StaticResource dbclient}"  TempObj="{m:Res Key=tempobj}" 
									   SelectObject="{m:Exp Str='applyuser.SelectedItems=>SelectionChanged!=null:applyuser.SelectedItems,null'}" 
									   TempletObject="{m:Res Key=tobj}"/>	
		
		<o:ObjectList   Name="rightlist"  
									   WebClientInfo="{StaticResource dbclient}" 
									   Source="{m:Exp Str=personslist2[data.f_gaswatchbrand \!\= null and data.f_metertype \!\= null and data.f_meternumber \!\=null].ToObjectList()}"/>	
		
		
		<!--是否有采暖炉-->
		<o:ObjectList Name="heatingboiler" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有采暖炉'"/>
		<!--是否有茶水炉-->
		<o:ObjectList Name="teaboiler" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有茶水炉'"/>
		<!--是否有模块炉-->
		<o:ObjectList Name="moduleboiler" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有模块炉'"/>
		<!--是否有壁挂炉-->
		<o:ObjectList Name="wallhungboiler" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有壁挂炉'"/>
		<!--是否有热水器-->
		<o:ObjectList Name="waterheater" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有热水器'"/>
		<!--是否有家用灶-->
		<o:ObjectList Name="homekitchen" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有家用灶'"/>
		<!--是否有火眼-->
		<o:ObjectList Name="fiereye" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有火眼'"/>
		<!--是否有小火眼-->
		<o:ObjectList Name="smallfireeye" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有小火眼'"/>
		<!--是否有蒸车-->
		<o:ObjectList Name="steamcars" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue  where param.name='是否有蒸车'"/>
		
		
		<!--导库完成按钮-->
        <b:HQLAction Name="DaokuoverAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_daokuover\=\'1\' \,f_ercifandan\=null  where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_daokustate\=\'完成\'}"/>
		 <!--不导档案-->
        <b:HQLAction Name="NoDaokuoverAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_daokuover\=\'1\'\,f_nodaodevices\=\'1\'\, f_ercifandan\=null  where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\})}"/>
		 
		 <b:HQLAction Name="ImportAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str='$update t_device set f_isimport=true where id in  ({applyuser.SelectedItems.ToString(id)=>SelectionChanged})   '}"/>
		<b:HQLAction Name="UpdateAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=rightlist.Count\=\=applyuser.ItemsSource.ToObjectList().Count:$update t_apply set f_daokustate\=\'完成\' where id \= \{main.DataContext.id\}$\,null}"/>
	   <b:BatchExcuteAction Name="SubmitAction" WebClientInfo="{StaticResource dbclient}">
				<b:BatchInfo Source="{m:Exp Str=right.ItemsSource}" MethodName="SaveToJson"/>
				<b:BatchInfo Source="{m:Exp Str=ImportAction}" MethodName="InvokeToJson"/>
			    <b:BatchInfo Source="{m:Exp Str=UpdateAction}" MethodName="InvokeToJson"/>
			</b:BatchExcuteAction>
	    <!--用气设备信息-->
		<o:ObjectList Name="userlist"      EntityType="t_device" HasEmptyRow="False"></o:ObjectList>
		<o:ObjectList Name="qibiaolist" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from qibiao}" ></o:ObjectList>
    <o:GeneralObject Name="tempobj" EntityType="t_userfiles"></o:GeneralObject>
	<o:GeneralObject Name="tobj" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}">
	        
		   
				<!-- 报建主表ID关联从表id，对应applyid -->
	        <o:PropertySetter PropertyName="applyid" Value="{m:Exp Str=tempobj.applyid}"/> 
			<!-- 报建从表id -->
			<o:PropertySetter PropertyName="cbid" Value="{m:Exp Str=tempobj.id}"/> 
			<!-- 客户签名 -->
			<o:PropertySetter PropertyName="USER_SIGN" Value="{m:Exp Str=tempobj.USER_SIGN}"/> 
			<!--客户评价-->
			<o:PropertySetter PropertyName="f_kehupingjia" Value="{m:Exp Str=tempobj.f_kehupingjia}"/> 
			<!-- 照片1 -->
			<o:PropertySetter PropertyName="PHOTO_FIRST" Value="{m:Exp Str=tempobj.PHOTO_FIRST}"/> 
			<!-- 照片2 -->
			<o:PropertySetter PropertyName="PHOTO_SECOND" Value="{m:Exp Str=tempobj.PHOTO_SECOND}"/> 
			<!-- 照片3 -->
			<o:PropertySetter PropertyName="PHOTO_THIRD" Value="{m:Exp Str=tempobj.PHOTO_THIRD}"/> 
			<!-- 照片4 -->
			<o:PropertySetter PropertyName="PHOTO_FOUTH" Value="{m:Exp Str=tempobj.PHOTO_FOUTH}"/> 
			<!-- 照片5 -->
			<o:PropertySetter PropertyName="PHOTO_FIFTH" Value="{m:Exp Str=tempobj.PHOTO_FIFTH}"/> 
			<!--基表读书-->
			<o:PropertySetter PropertyName="F_METERDUSHU" Value="{m:Exp Str=tempobj.F_METERDUSHU}"/> 
			<!--通气情况-->
			<o:PropertySetter PropertyName="F_TQQK" Value="{m:Exp Str=tempobj.F_TQQK}"/>
			<!--通气状态-->
			<o:PropertySetter PropertyName="F_TQSTATUS" Value="{m:Exp Str=tempobj.F_TQSTATUS}"/>
			<!-- 安检日期-->
			<o:PropertySetter PropertyName="f_anjianriqi" Value="{m:Exp Str=tempobj.f_anjianriqi}"/>
			<!-- 到达时间 -->
			<o:PropertySetter PropertyName="ARRIVAL_TIME" Value="{m:Exp Str=tempobj.ARRIVAL_TIME}"/>
			<!-- 离开时间 -->
			<o:PropertySetter PropertyName="DEPARTURE_TIME" Value="{m:Exp Str=tempobj.DEPARTURE_TIME}"/>

                <!--验收状态-->
                <o:PropertySetter PropertyName="F_YSSTATUS" Value="{m:Exp Str=tempobj.F_YSSTATUS}"/>
                <!-- 验收情况-->
                <o:PropertySetter PropertyName="F_YSQK" Value="{m:Exp Str=tempobj.F_YSQK}"/>
                <!-- 验收人员 -->
                <o:PropertySetter PropertyName="F_YSY" Value="{m:Exp Str=tempobj.F_YSY}"/>
                <!-- 通气人员 -->
                <o:PropertySetter PropertyName="F_TQY" Value="{m:Exp Str=tempobj.F_TQY}"/>








                <!--收费系统中的申请人f_petitionuser-->
			<o:PropertySetter PropertyName="f_petitionuser" Value="{m:Exp Str=main.DataContext.f_username}"/>
			<!--用户姓名-->
	        <o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=persons.SelectedItem.f_minyongdept}"/>
			<!--联系人或用户自己本人f_linkname-->
			<o:PropertySetter PropertyName="f_linkname" Value="{m:Exp Str=main.DataContext.f_username}"/>
			<!--身份证f_idnumber-->
			<o:PropertySetter PropertyName="f_idnumber" Default="{m:Exp Str=main.DataContext.f_feicard}" Value="{m:Exp Str=main.DataContext.f_feicard}"/>
			<!--地址f_address-->
			<o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=main.DataContext.f_address}"/>
			<!--小区名称f_districtname-->
			<o:PropertySetter PropertyName="f_districtname" Value="{m:Exp Str=tempobj.f_district}"/>
			<!--门牌号f_apartment-->
			<o:PropertySetter PropertyName="f_apartment" Value="{m:Exp Str=tempobj.f_doorplate}"/>
			<!--电话f_phone-->
			<o:PropertySetter PropertyName="f_phone" Value="{m:Exp Str=main.DataContext.f_phone}"/>
			<!--气表品牌f_gaswatchbrand-->
			<o:PropertySetter PropertyName="f_gaswatchbrand" Value="{m:Exp Str=tempobj.f_gaswatchbrand}"/>
			<!--气表型号f_metertype-->
			<o:PropertySetter PropertyName="f_metertype" Value="{m:Exp Str=tempobj.f_metertype}"/>
			<!--气表厂家f_gasmetermanufacturers-->
			<o:PropertySetter PropertyName="f_changjia"  Default="{m:Exp Str=tempobj.f_changjia}" Value="{m:Exp Str=tempobj.f_changjia}"/>
			<!--气表类型f_gasmeterstyle 卡表-->
			<o:PropertySetter PropertyName="f_type" Default="{m:Exp Str=tempobj.f_type}" Value="{m:Exp Str=tempobj.f_qbtype}"/>
			<!--片区-->
			<o:PropertySetter PropertyName="f_area" Default="{m:Exp Str=main.DataContext.f_area}" Value="{m:Exp Str=main.DataContext.f_area}"/>
	     	<!--安装户数-->
			<o:PropertySetter PropertyName="f_installnum" Default="{m:Exp Str=main.DataContext.f_installnum}" Value="{m:Exp Str=main.DataContext.f_installnum}"/>
	     	<!--别名f_aliasname-->
			<o:PropertySetter PropertyName="f_aliasname" Default="{m:Exp Str=tempobj.f_aliasname}" Value="{m:Exp Str=tempobj.f_aliasname}"/>
			<!--左右表f_aroundmeter-->
			<o:PropertySetter PropertyName="f_aroundmeter" Value="{m:Exp Str=tempobj.f_aroundmeter}"/>
	
			<!--表号-->
			<o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=tempobj.f_deviceno}"/>
			<!--表封-->
			<o:PropertySetter PropertyName="f_metertitles" Value="{m:Exp Str=tempobj.f_devicetitle}"/>
			<!--用户类型f_usertype-->
			<o:PropertySetter PropertyName="f_usertype" Default="非民用" Value="非民用"/>
			<!--市场部是否提交f_scbtijiao否-->
			<o:PropertySetter PropertyName="f_scbtijiao" Default="是" Value="是"/>
			<!-- 用户状态f_userstate正常 -->
			<o:PropertySetter PropertyName="f_userstate" Default="正常" Value="正常"/>
			<!--营业厅建档状态f_yytjdstate未完成-->
			<o:PropertySetter PropertyName="f_yytjdstate" Default="未完成" Value="未完成"/>
			<!--当前表累计购气量f_metergasnums0-->
			<o:PropertySetter PropertyName="f_metergasnums" Default="0" Value="0"/>
			<!--用户总累计购气量f_cumulativepurchase0-->
			<o:PropertySetter PropertyName="f_cumulativepurchase" Default="0" Value="0"/>
			<!--是否发卡f_whethergivecard未发-->
			<o:PropertySetter PropertyName="f_whethergivecard" Default="未发" Value="未发"/>
			<!--是否发折f_whethergivepassbook未发-->
			<o:PropertySetter PropertyName="f_whethergivepassbook" Default="未发" Value="未发"/>
			<!--最后购气量finallybought0-->
			<o:PropertySetter PropertyName="finallybought" Default="0" Value="0"/>
			<!--购气次数f_times1-->
			<o:PropertySetter PropertyName="f_times" Default="1" Value="1"/>
			<!--应交气量oughtamount0-->
			<o:PropertySetter PropertyName="oughtamount" Default="0" Value="0"/>
			<!--应交金额oughtfee0-->
			<o:PropertySetter PropertyName="oughtfee" Default="0" Value="0"/>
			<!--最后抄表日期lastinputdate-->
			<!--最后抄表气量lastinputgasnum0-->
			<o:PropertySetter PropertyName="lastinputgasnum" Default="0" Value="0"/>
			<!--最后一次抄表底数lastrecord0-->
			<o:PropertySetter PropertyName="lastrecord" Default="0" Value="0"/>
			<!--机表预购气量pregasnum0-->
			<o:PropertySetter PropertyName="pregasnum" Default="0" Value="0"/>
			<!--工程部建档状态f_gcbjiandang未完成-->
			<o:PropertySetter PropertyName="f_gcbjiandang" Default="完成" Value="完成"/>
			<!--是否提交到营业厅f_gcbtijiao否-->
			<o:PropertySetter PropertyName="f_gcbtijiao" Default="是" Value="是"/>
			<!--停用日期f_stopdate-->
			<!--停用底数f_gasmeteraccomodations0-->
			<o:PropertySetter PropertyName="f_gasmeteraccomodations" Default="0" Value="0"/>
			<!--特殊限购气量f_customgas-->
			<!--用户是否刚补卡f_buka否-->
			<o:PropertySetter PropertyName="f_buka" Default="否" Value="否"/>
			<!--部门f_departments市场部-->
			<o:PropertySetter PropertyName="f_departments" Default="市场部" Value="市场部"/>	
			<!--操作人f_operator市场部操作员01-->
			<o:PropertySetter PropertyName="f_operator" Default="市场部操作员02" Value="市场部操作员02"/>
			<!--市场部录入日期f_date-->
			 <o:PropertySetter PropertyName="f_date" Default="{m:Exp Str=SysTime.Today.ToString()}" Value="{m:Exp Str=SysTime.Today.ToString()}"/>
			<!--表名tablenamet_userfiles-->
			<o:PropertySetter PropertyName="tablename" Default="t_userfiles" Value="t_userfiles"/>
			<!--额定流量-->
			<o:PropertySetter PropertyName="f_flownum" Default="{m:Exp Str=tempobj.f_flownum}"  Value="{m:Exp Str=tempobj.f_flownum}"/>
			<!--用气性质f_gasproperties-->
		     <o:PropertySetter PropertyName="f_gasproperties" Default="{m:Exp Str=main.DataContext.f_gasproperties}" Value="{m:Exp Str=main.DataContext.f_gasproperties}"/>
		    <!--气价类型f_gaspricetype-->
		    <o:PropertySetter PropertyName="f_gaspricetype" Default="{m:Exp Str=main.DataContext.f_gaspricetype}" Value="{m:Exp Str=main.DataContext.f_gaspricetype}"/>
		    <!--气价f_gasprice-->
		    <o:PropertySetter PropertyName="f_gasprice" Default="{m:Exp Str=main.DataContext.f_gasprice}" Value="{m:Exp Str=main.DataContext.f_gasprice}"/>
			<!--工程部完成信息-->
			<!--工程部门名称f_departments2-->
			<o:PropertySetter PropertyName="f_departments2" Value="{m:Exp Str=main.DataContext.f_finishdepartment}"/>
			<!--操作人f_operat0r2-->
			<o:PropertySetter PropertyName="f_operat0r2" Value="{m:Exp Str=main.DataContext.f_finishopertor}"/>
			<!--日期f_date2-->
			<o:PropertySetter PropertyName="f_date2" Default="{m:Exp Str=SysTime.Today.ToString()}" Value="{m:Exp Str=SysTime.Today.ToString()}"/>
			<!--是否有壁挂炉-->
			<!--o:PropertySetter PropertyName="f_heatingboiler"  Value="{m:Exp Str=data.f_heatingboiler \=\=\'1\':壁挂炉\,null}"/-->
		</o:GeneralObject>
</m:ResourceLoad>		
    <Grid>
    	<Grid.ColumnDefinitions>
    		<ColumnDefinition Width="0.2*"/>
    		<ColumnDefinition Width="0.8*"/>
    	</Grid.ColumnDefinitions>
    	  <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=Allmain.DataContext.IsBusy}" Grid.ColumnSpan="1">
            <Grid x:Name="Allmain">
            	<sdk:TabControl Margin="0,0,0,55">
            		<!--待办-->
            		<sdk:TabItem Header=" 导非民用档案 ">
            			 <toolkit:DockPanel LastChildFill="True">
            			 	 <toolkit:WrapPanel x:Name="personsearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,5,0,5" Orientation="Horizontal" >
                   <a:Data.Context>
            		  	<o:SearchObject></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申请人：" Margin="3,0,0,0"/>
                    <TextBox TextWrapping="Wrap" MinWidth="30" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Margin="3,0,0,0">
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>               
					  </a:Data.Prop>
					</TextBox>
												<TextBlock TextWrapping="Wrap" Text="单位：" Margin="3,0,0,0"/>
		                    <TextBox TextWrapping="Wrap" MinWidth="30" Text="{m:Bind Path=f_minyongdept}" a:ControlAttach.DefaultButton="searchbutton" Margin="3,0,0,0" Height="25">
		                      <a:Data.Prop>
		                      	   <o:PropertySetter PropertyName="f_minyongdept" Operator="f_minyongdept like '%this%'"/>               
							  </a:Data.Prop>
							</TextBox>
                    <Button Content=" 查  询 " x:Name="searchbutton" Click="{m:Exp Str=personsearch.DataContext.Search()}" Margin="10,0,0,0"/>
                </toolkit:WrapPanel>
                <!--提交按钮-->
				<toolkit:DockPanel Margin="0,5" toolkit:DockPanel.Dock="Bottom" >
					 <TextBlock Height="23" TextWrapping="Wrap" Text="{m:Exp Str='$共{persons.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
				     <StackPanel Orientation="Horizontal" HorizontalAlignment="Right"/>
                 </toolkit:DockPanel>
				 			<toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=persons.ItemsSource.IsBusy}">
				<!--DataGrid-->
                <sdk:DataGrid Name="persons" Margin="0"  AutoGenerateColumns="False"  
				         IsReadOnly="True" >
                	<a:Data.Items>
                		<o:ObjectList x:Name="personslist" WebClientInfo="{StaticResource dbclient}"
                             Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_device   where \{personsearch.DataContext.Condition\} and f_usertype\=\'非民用\' and f_state \!\=\'报建登记\' and f_sendopertor is not null and f_huneiover \=\'1\' and (f_daokuover is null or f_ercifandan \=\'1\')}" 
					         IsOld="{m:Exp Str=(SubmitAction.State\=\=$Free$ or SubmitAction.State\=\=$End$)
							 and(main.DataContext.State\=\=$Free$ or main.DataContext.State\=\=$End$) 
							 and(DaokuoverAction.State\=\=$Free$ or DaokuoverAction.State\=\=$End$) 
							 and(NoDaokuoverAction.State\=\=$Free$ or NoDaokuoverAction.State\=\=$End$) }">
                		</o:ObjectList>
					</a:Data.Items>
                       <sdk:DataGrid.Columns>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
						<sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_daokustate}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
						<sdk:DataGridTextColumn Header="编号" Binding="{Binding f_code}"/>
                        <sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
                        <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}"/>
						<sdk:DataGridTextColumn Header="申请几处" Binding="{Binding f_dijichu}"/>
						<sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
						<sdk:DataGridTextColumn Header="单位" Binding="{Binding f_minyongdept}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
                  </toolkit:BusyIndicator>
						 </toolkit:DockPanel>      		
					</sdk:TabItem>    
					<!--已办-->        	
				</sdk:TabControl>
            	<Button Content="导数据完成"  Click="{m:Exp Str=daoshujutip.Show()}"
            		 Height="25" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0,0,25,26" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment, Height"/>
				
				<Button Content="不导档案"  Click="{m:Exp Str=nodaoshujutip.Show()}"
            		 IsEnabled="{m:Exp Str='persons.SelectedItems.Count=>SelectionChanged!=0'}" Height="25" VerticalAlignment="Bottom" Margin="0,0,25,26" d:LayoutOverrides="VerticalAlignment, Height" HorizontalAlignment="Center"/>
		    </Grid>
		</toolkit:BusyIndicator>
      
        <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=right.DataContext.IsBusy}" Margin="10,0,0,0" Grid.ColumnSpan="1" Grid.Column="1" >
            <Grid Background="{x:Null}"  x:Name="main" Margin="0">
            	<a:Data.Context>
            		<!--编辑对象-->
            <o:GeneralObject  Name="persongo" WebClientInfo="{StaticResource dbclient}" 
		EntityType="t_apply" 
		Source="{m:Exp Str='persons.SelectedItem'}"
		IsInit="{m:Exp Str='this.State==$End or BatchSave.State==$End$'}">    		  	
				<o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=userlist}"/>  
			</o:GeneralObject>
        	
				</a:Data.Context>
                <sdk:TabControl  Margin="0,0,0,55" VerticalAlignment="Stretch" d:LayoutOverrides="Height" >
             <!--民用主操作区-->
            		<sdk:TabItem Header="报建基本信息">
            			<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            			<Grid  VerticalAlignment="Top" Background="{x:Null}" Margin="0" >
            				<Grid.RowDefinitions>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            					<RowDefinition Height="40"/>
            				</Grid.RowDefinitions>
            				<Grid.ColumnDefinitions>
            					<ColumnDefinition Width="110"/>
            					<ColumnDefinition Width="1*" />
            					<ColumnDefinition Width="110"/>
            					<ColumnDefinition Width="1*"/>
            					<ColumnDefinition Width="110"/>
            					<ColumnDefinition Width="1*"/>
            				</Grid.ColumnDefinitions>
                  <!---->
            				<TextBlock Margin="0,8,0,7" TextWrapping="Wrap" Text="申请人：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625"   Height="25"/>
            				<TextBox Text="{m:Bind Path=f_username}" Grid.Column="1" Margin="0,8,0,7"   Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True">
            					<a:Data.Prop>
            						<o:PropertySetter PropertyName="f_username" NotNull="True" />
            					</a:Data.Prop>
            				</TextBox>
            				<TextBlock TextWrapping="Wrap" Text="电 话：" TextAlignment="Right" Margin="0,8,0,7" RenderTransformOrigin="0.538,0.364" Grid.Column="2"   Height="25"/>
            				<TextBlock Margin="0" TextWrapping="Wrap" Text="操作日期：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="5"   Height="25"/>
