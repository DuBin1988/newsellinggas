<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="Com.Aote.Pages.民用总信息查看" FontSize="13">
    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-12,0" Grid.ColumnSpan="3" d:LayoutOverrides="GridBox">
            <o:GeneralObject Name="impusernum" EntityType="t1" WebClientInfo="{StaticResource dbclient}" 
		  Path="{m:Exp Str='$one/select new Map(count(*)  as num) from t_applycustomer where applyid\=\{apply.DataContext.id\} and f_isimport\=1'}"   ></o:GeneralObject>
		  <o:ObjectList Name="projectlist" WebClientInfo="{StaticResource dbclient}"  
		Path="{m:Exp Str='apply.DataContext.id!=null:$from t_projectplan where applyid={apply.DataContext.id}$,null'}" EntityType="t_projectplan"   />
	
      <o:ObjectList Name="gjlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='报建状态'"/>
		<!--图片信息-->
		<o:ObjectList Name="userimagelist" Source="{m:Exp Str=nopeople.SelectedItem.f_customerimage}" EntityType="t_customerimage"></o:ObjectList>			
		<!--附件信息-->
		<o:ObjectList Name="annexlist" EntityType="t_applyannex" HasEmptyRow="True"></o:ObjectList>	
		<o:ObjectList Name="userlist" EntityType="t_applycustomer" ></o:ObjectList>
		<!--交费信息-->
		<o:ObjectList Name="paylist" EntityType="t_paydetail" ></o:ObjectList>
		<!--减免信息-->
		<o:ObjectList Name="costreductionlist" EntityType="t_costreduction" HasEmptyRow="False"></o:ObjectList>
		<!--图片信息-->
		<o:ObjectList Name="image" EntityType="t_applyimage" ></o:ObjectList>
		<!--批量删除语句-->
        <b:HQLAction Name="DeleteAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str='$delete from t_apply where id in ({applys.SelectedItems.ToString(id)=>SelectionChanged})'}">
		</b:HQLAction>
        </m:ResourceLoad>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.3*"/>
                <ColumnDefinition Width="0.7*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}" Grid.ColumnSpan="1">

                <toolkit:DockPanel MaxWidth="380">
                    <a:Data.Context>
                        <o:SearchObject></o:SearchObject>
                    </a:Data.Context>
                    <toolkit:WrapPanel x:Name="applysearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >

                        <TextBlock TextWrapping="Wrap" Text="申 请 人："/>
                        <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like '%this%'"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                        <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone like 'this%'"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock TextWrapping="Wrap" Text="地 址：" Margin="5,0,0,0"/>
                        <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_address}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_address"  Operator="apply.f_address like '%this%'"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock TextWrapping="Wrap" Text="报建状态" Margin="5,0,0,0"/>
                        <ComboBox Name="ComboxZht" SelectedValue="{m:Bind Path=f_state}" a:ControlAttach.DefaultButton="searchbutton" ItemsSource="{m:Exp Str=gjlist}"  Grid.Column="1" Margin="0,3,0,2" DisplayMemberPath="name" SelectedValuePath="name" Grid.Row="2"  Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_state"  Operator="apply.f_state like 'this%'"/>
                            </a:Data.Prop>
                        </ComboBox>
                        <!--TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_state}" Width="Auto" Height="22" Margin="2,0,0,0">
                    	<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_state"  Operator="apply.f_state like 'this%'"/>          
                    	</a:Data.Prop>
                    </TextBox-->
                        <TextBlock TextWrapping="Wrap" Text="片区：" Margin="5,0,0,0"/>
                        <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_area}" a:ControlAttach.DefaultButton="searchbutton" MinWidth="30" Margin="5,0,0,0" Height="22">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_area" Operator="apply.f_area = 'this'"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock TextWrapping="Wrap" Text="报建编号：" Margin="5,0,0,0"/>
                        <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_code}" a:ControlAttach.DefaultButton="searchbutton" MinWidth="30" Margin="5,0,0,0" Height="22">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_code" Operator="apply.f_code = 'this'"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock TextWrapping="Wrap" Text="日期类型：" Margin="5,0,0,0" Height="25"/>
                        <ComboBox SelectedValue="{m:Bind Path=dataType}" a:ControlAttach.DefaultButton="searchbutton" Margin="2,0,0,0" DisplayMemberPath="name" SelectedValuePath="code" Height="22">
                            <a:Data.Items>
                                <o:ObjectList WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='日期类型'"/>
                            </a:Data.Items>
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="dataType" Operator="1=1" />
                            </a:Data.Prop>
                        </ComboBox>
                        <sdk:DatePicker SelectedDate="{m:Bind Path=startDate}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="startDate" Operator="{m:Exp Str=this.dataType!\=null:$\{this.dataType\} >\= \'\{this.startDate\}\'$\,$1\=1$}" />
                            </a:Data.Prop>
                        </sdk:DatePicker>
                        <TextBlock TextWrapping="Wrap" Text="至" Margin="5,0,0,0" Height="25"/>
                        <sdk:DatePicker SelectedDate="{m:Bind Path=endDate}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,6"  Height="25" VerticalAlignment="Stretch"  >
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="endDate" Operator="{m:Exp Str=this.dataType!\=null:$\{this.dataType\} ^\= \'\{this.endDate\}\'$\,$1\=1$}" />
                            </a:Data.Prop>
                        </sdk:DatePicker>
                        <Button Content="查 询" Name="searchbutton" Click="{m:Exp Str=applysearch.DataContext.Search()}"  Margin="2,0,0,0" />
                    </toolkit:WrapPanel>
                    <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Left">
                        <TextBlock Height="37" TextWrapping="Wrap" Text="{m:Exp Str='$共{applys.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
                    </toolkit:DockPanel>
                    <sdk:DataGrid x:Name="applys" Margin="0"  
                	AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
                        <a:Data.Items>
                            <o:ObjectList x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
                			Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_costreduction left join fetch apply.f_paydetail left join fetch apply.f_applyannex where \{applysearch.DataContext.Condition\} and f_usertype\=\'民用\'}"
                			IsOld="{m:Exp Str=DeleteAction.State\=\=$End$ or apply.DataContext.State\=\=$End$}"/>
                        </a:Data.Items>
                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn Header="报建编号" Binding="{Binding f_code}"/>
                            <sdk:DataGridTextColumn Header="报建状态" Binding="{Binding f_state}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                            <sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                            <sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                            <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date,StringFormat=yyyy-MM-dd}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                    <TextBlock Height="27" TextWrapping="Wrap" Text="{m:Exp Str='$共{applys.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
                </toolkit:DockPanel>
            </c:BusyIndicatorEx>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=apply.DataContext.IsBusy}" Grid.ColumnSpan="1" Grid.Column="1" Grid.RowSpan="1" >
                <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" FontSize="13">
                    <toolkit:DockPanel Background="{x:Null}" x:Name="apply" Margin="0,0,0,0" >
                        <a:Data.Context>
                            <o:GeneralObject  Name="applygo" WebClientInfo="{StaticResource dbclient}" 
		               EntityType="t_apply" Source="{m:Exp Str=applys.SelectedItem}"
		               IsInit="{m:Exp Str='this.State==$End'}">
                                <o:PropertySetter PropertyName="f_costreduction" Default="{m:Exp Str=costreductionlist}"></o:PropertySetter>
                                <o:PropertySetter PropertyName="f_paydetail" Default="{m:Exp Str=paylist}" ></o:PropertySetter>
                                <o:PropertySetter PropertyName="f_applyannex" Default="{m:Exp Str=annexlist}"/>
                            </o:GeneralObject>
                        </a:Data.Context>
                        <Grid>
                            <sdk:TabControl TabStripPlacement="Right" Margin="0" Background="{x:Null}" Width="850" >
                                <sdk:TabItem Header="报建登记   　　" Background="{x:Null}" Width="auto" Height="auto" HorizontalAlignment="Left" VerticalAlignment="Stretch">
                                    <Grid  Width="700">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                            <RowDefinition Height="25"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="80"/>
                                            <ColumnDefinition Width="0.332*"/>
                                            <ColumnDefinition Width="75"/>
                                            <ColumnDefinition Width="0.337*"/>
                                            <ColumnDefinition Width="85"/>
                                            <ColumnDefinition Width="0.332*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="申请人：" TextAlignment="Right" LineHeight="8" Height="22"/>
                                        <TextBox Text="{m:Bind Path=f_username}" Grid.Column="1" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_username" NotNull="{x:Null}" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="电 话：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.538,0.364" Grid.Column="4" Height="22"/>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="操作日期：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="4" Height="22"/>
                                        <sdk:DatePicker  SelectedDateFormat="Short"  IsEnabled="False" Text="{m:Bind Path=f_date}"     Grid.Column="1" Margin="0" Height="22" Grid.Row="4" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">

                                        </sdk:DatePicker>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="编 号：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Column="2" Height="22"/>
                                        <TextBox Text="{m:Bind Path=f_code}" Grid.Column="3" Margin="0" Height="22" Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" IsReadOnly="True">

                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="地 址：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.827,0.333" Grid.Row="1" Height="22"/>
                                        <TextBox Text="{m:Bind Path=f_phone}" Grid.Column="5" Margin="0"  Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="1" Grid.ColumnSpan="1" Height="Auto" IsReadOnly="True">

                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_address}" Grid.Column="1" Grid.ColumnSpan="3" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="1" IsReadOnly="True">

                                        </TextBox>
                                        <TextBox IsReadOnly="True" Text="{m:Bind Path=f_usertype}"  Grid.Column="3" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="22">

                                        </TextBox>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="片区选择：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Column="4" Height="22" Grid.Row="1"/>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Column="2" Height="22" Grid.Row="2"/>
                                        <TextBox  Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="5" Margin="0" Grid.Row="1" Height="22" Width="170">

                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="申请户数：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.827,0.333" Grid.Row="6" Height="22"/>
                                        <TextBox Text="{m:Bind Path=f_applycount}" Grid.Column="1" Margin="0,2,0,1" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="6" IsReadOnly="True">

                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="热水器台数：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.827,0.333" Grid.Row="2" Height="22" Grid.Column="0"/>
                                        <TextBox Text="{m:Bind Path=f_geyser}" Grid.Column="1" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="2" IsReadOnly="True">

                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="壁挂炉台数：" TextAlignment="Right" Margin="0" RenderTransformOrigin="0.827,0.333" Grid.Row="2" Height="22" Grid.Column="4"/>
                                        <TextBox Text="{m:Bind Path=f_boiler}" Grid.Column="5" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="2" IsReadOnly="True">

                                        </TextBox>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="部 门：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="4" Height="22" Grid.Column="2"/>
                                        <TextBox Text="{m:Bind Path=f_department}" IsReadOnly="True" Grid.Column="3" Margin="0" Height="22" Grid.Row="4" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">

                                        </TextBox>
                                        <TextBlock Margin="0" TextWrapping="Wrap" Text="操 作 人：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="4" Height="22" Grid.Column="4"/>
                                        <TextBox Text="{m:Bind Path=f_opertor}" IsReadOnly="True" Grid.Column="5" Margin="0" Height="22" Grid.Row="4" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto">

                                        </TextBox>
                                        <TextBlock Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="房屋类型：" Height="22" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_usertypepro}" Grid.Column="1" Margin="0,3,0,0" Grid.Row="3" Height="22" Width="170" IsReadOnly="True">

                                        </TextBox>
                                        <sdk:Label Margin="0,1,0,2" Style="{StaticResource LabelTitleSplit}"  Content="用户花名单统计信息:"  RenderTransformOrigin="0.393,2.625" Grid.Row="5" Height="22" Grid.ColumnSpan="6"/>
                                        <TextBlock Margin="0,0,-2,0" TextWrapping="Wrap" Text="安装户数：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="6" Height="22" Grid.Column="2"/>
                                        <TextBlock Margin="5,3,0,0" TextWrapping="Wrap" Text="已导入：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="7" Height="22"/>
                                        <TextBlock Margin="0,0,0,3" TextWrapping="Wrap" Text="未导入：" TextAlignment="Right" RenderTransformOrigin="0.393,2.625" Grid.Row="7" Height="22" Grid.Column="2"/>
                                        <TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="3" Margin="2,2,0,1" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="6" IsReadOnly="True"/>
                                        <TextBox Text="{m:Exp Str='impusernum.num !\=null:impusernum.num\,0'}" Grid.Column="1" Margin="0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="7" IsReadOnly="True"/>
                                        <TextBox Text="{m:Exp Str=apply.DataContext.f_installnum-impusernum.num}" Grid.Column="3" Margin="0,0,2,0" Height="22" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="7" IsReadOnly="True"/>
                                    </Grid>
                                </sdk:TabItem>
                                <!--sdk:TabItem Header="报建登记核对" Margin="0" Width="120" Height="23" HorizontalAlignment="Left">
				<Grid Width="600" >
					<Grid.RowDefinitions>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition Height="40"/>
						<RowDefinition />
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="82"/>
						<ColumnDefinition Width="0.294*"/>
						<ColumnDefinition Width="88"/>
						<ColumnDefinition Width="0.353*"/>
						<ColumnDefinition Width="110"/>
						<ColumnDefinition Width="0.353*"/>
					</Grid.ColumnDefinitions>
					<TextBlock Margin="0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" FontSize="12" Height="25"/>
					<TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0" TextWrapping="Wrap" Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_code"/>
						</a:Data.Prop>
					</TextBox>	
					<TextBlock Grid.Column="2" Margin="0" TextWrapping="Wrap" Text="报建日期：" Height="25" TextAlignment="Right"/>
					<sdk:DatePicker Text="{m:Bind Path=f_date}" IsEnabled="False" Grid.Column="3" Margin="0"  Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_date"/>
						</a:Data.Prop>
					</sdk:DatePicker>	
					<TextBlock Grid.Column="4" Margin="0" TextWrapping="Wrap" Text="申 请 人：" Height="25" TextAlignment="Right"/>
					<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="5" Margin="0" TextWrapping="Wrap" Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_username"/>
						</a:Data.Prop>
					</TextBox>	
					<TextBlock Margin="0" Grid.Row="1" TextWrapping="Wrap" Text="电 话：" Height="25" TextAlignment="Right"/>
					<TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1" TextWrapping="Wrap" Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_phone"/>
						</a:Data.Prop>
					</TextBox>	
					<TextBlock Grid.Column="2" Margin="0" Grid.Row="1" TextWrapping="Wrap" Text="用气地址：" Height="25" TextAlignment="Right"/>
					<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="3" Margin="0" Grid.Row="1"  Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_address" NotNull="{x:Null}"/>
						</a:Data.Prop>
					</TextBox>	
					<TextBlock Grid.Column="4" Margin="0" Grid.Row="1" TextWrapping="Wrap" Text="片 区：" Height="25" TextAlignment="Right"/>	
					<ComboBox DisplayMemberPath="name" IsEnabled="False" SelectedValuePath="name" SelectedValue="{m:Bind Path=f_area}"  Grid.Column="5" Margin="0" Grid.Row="1" Height="25">
						<a:Data.Items>
							<o:ObjectList WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='片区'"/>
						</a:Data.Items>
					</ComboBox>
					<TextBlock Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="用户类型：" Height="25" TextAlignment="Right"/>
					<TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="25">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_usertype"/>
						</a:Data.Prop>
					</TextBox>		 
					<TextBlock Grid.Column="2" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="房屋类型：" Height="25" TextAlignment="Right"/>
					<ComboBox DisplayMemberPath="name" SelectedValuePath="name" IsEnabled="False" SelectedValue="{m:Bind Path=f_usertypepro}" Grid.Column="3" Margin="0" Grid.Row="2" Height="25">
						<a:Data.Items>
							<o:ObjectList WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='房屋类型'"/>
						</a:Data.Items>
					</ComboBox>
					<TextBlock Grid.Column="4" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="申请户数：" Height="25" TextAlignment="Right"/>
					<TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="25" IsReadOnly="true">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_applycount"/>
						</a:Data.Prop>
					</TextBox>
					<TextBlock Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="热水器台数：" Height="25" TextAlignment="Right"/>
					<TextBlock Grid.Column="2" Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="壁挂炉台数：" Height="25" TextAlignment="Right"/>
					<TextBlock Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="25" TextAlignment="Right" Text="核对意见："/>
					<TextBox Text="{m:Bind Path=f_geyser}" Grid.Column="1" Margin="0" Grid.Row="3" TextWrapping="Wrap" Height="25" IsReadOnly="True">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_geyser" />
						</a:Data.Prop>
					</TextBox>
					<TextBox Text="{m:Bind Path=f_boiler}" Grid.Column="3" Margin="0" Grid.Row="3" TextWrapping="Wrap" Height="25" IsReadOnly="True">
						<a:Data.Prop>
							<o:PropertySetter PropertyName="f_boiler" />
