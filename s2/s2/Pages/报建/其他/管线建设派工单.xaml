				<TextBox Text="{m:Bind Path=f_projecttext}" Grid.Column="1" Margin="0" TextWrapping="Wrap" Grid.Row="4" IsReadOnly="False" Height="Auto" VerticalAlignment="Stretch" Grid.ColumnSpan="4" Width="480" HorizontalAlignment="Left">
