				</TextBox>
				<TextBox Text="{m:Bind Path=f_constructionunit}" Grid.Column="5" Margin="0" TextWrapping="Wrap" Grid.Row="10" IsReadOnly="True" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="2">
