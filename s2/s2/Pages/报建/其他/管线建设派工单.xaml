						<o:PropertySetter PropertyName="f_jsunit" NotNull="True"/>
					</a:Data.Prop>
				</TextBox>

				<TextBlock TextWrapping="Wrap" Text="部门：" Width="Auto" Margin="0,0,0,11" Grid.Row="13" Height="25" TextAlignment="Right"/>
