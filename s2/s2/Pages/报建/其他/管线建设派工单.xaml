				<sdk:Label  Content="线路明细"   Style="{StaticResource LabelTitleSplit}"    RenderTransformOrigin="0.393,2.625" Grid.Row="7" Height="25" Grid.ColumnSpan="7" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
