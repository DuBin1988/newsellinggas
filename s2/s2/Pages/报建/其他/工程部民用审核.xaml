<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
    x:Class="Com.Aote.Pages.工程部民用审核" FontSize="13"
    mc:Ignorable="d">
	<Grid x:Name="LayoutRoot"> 
	<m:ResourceLoad Margin="0">
        <!--用户信息-->
		<o:ObjectList Name="userlist" EntityType="t_applycustomer"></o:ObjectList>
		<!--图片信息-->
		<o:ObjectList Name="userimagelist" Source="{m:Exp Str=nopeople.SelectedItem.f_customerimage}" EntityType="t_customerimage"></o:ObjectList>	
		  <!--图片信息-->
		<o:ObjectList Name="imagelist" EntityType="t_applyimage"></o:ObjectList>			
		<!--批量删除语句-->
        <b:HQLAction Name="DeleteAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str='$delete from t_apply where id in ({applys.SelectedItems.ToString(id)=>SelectionChanged})'}">
		</b:HQLAction>
		<!--终止报建-->
        <b:HQLAction Name="EndAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'终止报建\' \,f_substate\=null where id in (\{applys.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
		<!--提交市场部民用转商用MzsreturnAction-->
		<b:HQLAction Name="MzsreturnAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'民用转商用\' \,f_substate\=null where id in (\{applys.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
		<!--返回勘查人ReturnAction-->
		<b:HQLAction Name="ReturnAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'现场勘查\' \,f_substate\=null where id in (\{applys.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
		<!--提交-->
        <b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'收费标准\' \,f_substate\=null where id in (\{applys.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
	</m:ResourceLoad>
    <Grid>
       <Grid.ColumnDefinitions>
       	<ColumnDefinition Width="0.3*"></ColumnDefinition>   
		<ColumnDefinition Width="0.7*"></ColumnDefinition>
	   </Grid.ColumnDefinitions>
            <Grid x:Name="Allmain">
            	<sdk:TabControl Margin="0,0,0,45">
            		<!--待办-->
            		<sdk:TabItem Header=" 待  办 ">
            			<toolkit:DockPanel>
                <toolkit:WrapPanel x:Name="applysearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                   <a:Data.Context>
            		  	<o:SearchObject ></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申 请 人："/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>               
					  </a:Data.Prop>
					</TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" Width="Auto" Height="22">
                    	 <a:Data.Prop>
                      	  <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone = 'this'"/>          
					   </a:Data.Prop>
                    </TextBox>
                    <Button Content="  查 询  " Name="searchbutton" Click="{m:Exp Str=applysearch.DataContext.Search()}" Margin="2,0,0,0">
                    </Button>
                </toolkit:WrapPanel>
				 
             <!--提交按钮-->
			 <StackPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Orientation="Horizontal" >
                     <TextBlock Height="27" TextWrapping="Wrap" Text="{m:Exp Str='$共{applys.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
					<Button Content="提交收费标准" Click="{m:Exp Str=submittip.Show()}"
                        IsEnabled="{m:Exp Str='False and applys.SelectedItems.Count=>SelectionChanged!=0'}" HorizontalAlignment="Stretch" Margin="58,0,1,0" Height="25">            
					</Button>
					<Button Content="民转商" Click="{m:Exp Str=mzssubmittip.Show()}"
                        IsEnabled="{m:Exp Str='False and applys.SelectedItems.Count=>SelectionChanged!=0'}" HorizontalAlignment="Stretch" Margin="1,0" Height="25">            
					</Button>
					<Button Content="返回放线人" Click="{m:Exp Str=returnsubmittip.Show()}"
                        IsEnabled="{m:Exp Str='False and applys.SelectedItems.Count=>SelectionChanged!=0'}" HorizontalAlignment="Stretch" Margin="1,0" Height="25">            
					</Button>
					<Button Content="终止报建"  Margin="1,5" Click="{m:Exp Str=endsubmittip.Show()}"
                        IsEnabled="{m:Exp Str='False and applys.SelectedItems.Count=>SelectionChanged!=0'}" 
						 HorizontalAlignment="Stretch" Width="70">            
					</Button>
             </StackPanel>
			 
			 <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}">
                <sdk:DataGrid x:Name="applys" Margin="0"  
				AutoGenerateColumns="False" Width="Auto" IsReadOnly="True"
				SelectedItem="{m:Exp Str='apply.DataContext.IsInit or yijing.SelectedItem!=null:null,applys.SelectedItem'}" >
                	<a:Data.Items>
                		<o:ObjectList x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
                      Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_applycustomer left join fetch apply.f_applyimage  where \{applysearch.DataContext.Condition\} and f_usertype\=\'民用\' and f_state\=\'工程部审核\'}"
					  IsOld="{m:Exp Str=(SubmitAction.State\=\=$Free$ or SubmitAction.State\=\=$End$)and
					       (DeleteAction.State\=\=$Free$ or DeleteAction.State\=\=$End$)and 
						   (ReturnAction.State\=\=$Free$ or ReturnAction.State\=\=$End$)and 
						   (EndAction.State\=\=$Free$ or EndAction.State\=\=$End$)and 
						   (MzsreturnAction.State\=\=$Free$ or MzsreturnAction.State\=\=$End$)and 
						   (apply.DataContext.State\=\=$Free$ or apply.DataContext.State\=\=$End$)}"/>
					</a:Data.Items>
		    <!--
			<a:TabControlAttach.ContentTempalte>
                <DataTemplate>
                	<ContentControl a:ControlAttach.Source="m:Exp Str='selectItem[attr]=myxianchangkancha?民用pagey:feimyxianchangkancha?非民用'{Binding navigate}"/>
                </DataTemplate>
            </a:TabControlAttach.ContentTempalte>
			-->
                    <sdk:DataGrid.Columns>
                    	<sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
                       	<sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                       	<sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                       	<sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                       	<sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                       	<sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
                 </toolkit:BusyIndicator>
			 </toolkit:DockPanel>
			 </sdk:TabItem>
			 <!--已办-->
			 <sdk:TabItem Header="已  办">
			 	<toolkit:DockPanel LastChildFill="True">
			  <StackPanel x:Name="applylist"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                   <a:Data.Context>
            		  	<o:SearchObject ></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申 请 人："/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchButton" Width="Auto" Height="22">
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>               
					  </a:Data.Prop>
					</TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" Width="Auto" Height="22">
                    	 <a:Data.Prop>
                      	  <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone = 'this'"/>          
					   </a:Data.Prop>
                    </TextBox>
                    <Button Content=" 查 询 " Name="searchButton" Click="{m:Exp Str=applylist.DataContext.Search()}" Margin="2,0,0,0">
                    </Button>
                </StackPanel>
				 <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=yijing.ItemsSource.IsBusy}">
                <sdk:DataGrid x:Name="yijing" Margin="0"  
				AutoGenerateColumns="False" Width="Auto" IsReadOnly="True"
			>
                	<a:Data.Items>
                		<o:ObjectList x:Name="yijingslist" WebClientInfo="{StaticResource dbclient}"
                      Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_applycustomer left join fetch apply.f_applyimage  where \{applylist.DataContext.Condition\} and f_usertype\=\'民用\'  and f_gcbshopertor is not null and f_state\!\=\'工程部审核\'}"
					  IsOld="{m:Exp Str=SubmitAction.State\=\=$End$ or DeleteAction.State\=\=$End$ or apply.DataContext.State\=\=$End$}"/>
					</a:Data.Items>
		    <!--
			<a:TabControlAttach.ContentTempalte>
                <DataTemplate>
                	<ContentControl a:ControlAttach.Source="m:Exp Str='selectItem[attr]=myxianchangkancha?民用pagey:feimyxianchangkancha?非民用'{Binding navigate}"/>
                </DataTemplate>
            </a:TabControlAttach.ContentTempalte>
			-->
                    <sdk:DataGrid.Columns>
                    	<sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                        
                       	<sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                       	<sdk:DataGridTextColumn d:IsLocked="True" Header="报建状态" Binding="{Binding f_state}"/>
						
					   	<sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                       	<sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                       	<sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                       	<sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
                 </toolkit:BusyIndicator>
				 </toolkit:DockPanel>
				</sdk:TabItem>
				 </sdk:TabControl>
			</Grid>
			<!--结束-->
      
        <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=apply.DataContext.IsBusy}" Margin="5,0,0,0" Grid.Column="1">
        	 <Grid  x:Name="apply" Margin="0" >
        		<a:Data.Context>
        			<o:GeneralObject Name="applygo" WebClientInfo="{StaticResource dbclient}" 
        				EntityType="t_apply" Source="{m:Exp Str=applys.SelectedItem !\=null:applys.SelectedItem\,yijing.SelectedItem !\=null:yijing.SelectedItem\,null}"
        				IsInit="{m:Exp Str=this.State\=\=$End}" >
        				<o:PropertySetter  PropertyName="f_substate" Default="{m:Exp Str=applys.SelectedItem !\= null:$完成$\,null}"/>
        				<o:PropertySetter PropertyName="f_applycustomer" Default="{m:Exp Str=userlist}"/>
        				<o:PropertySetter PropertyName="f_applyimage" Default="{m:Exp Str=imagelist}"/>       		
        			</o:GeneralObject>
        		</a:Data.Context>
        		<!--<ContentControl a:ControlAttach.Source=
		"{m:Exp Str='$/Pages/右边空$'}"
				></ContentControl>-->
        <!--按钮-->
        		<toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Right" VerticalAlignment="Bottom" >
        			<Button x:Name="save" Content="  保 存  " Margin="5" 
        				IsEnabled="{m:Exp Str=False and (apply.DataContext.IsModified or userlist.IsModified) and data.f_state\=\=$工程部审核$ and !apply.DataContext.HasErrors}"
        				Click="{m:Exp Str=apply.DataContext.Save();apply.DataContext.New()}"/>
        			<Button Content=" 打印确认单 " Margin="5" Click="{m:Exp Str=print.Print()}"/>
        		</toolkit:DockPanel>
			
        		<sdk:TabControl  Margin="0,0,0,45" VerticalAlignment="Stretch">
             <!--民用主操作区-->
        			<sdk:TabItem Header="报建基本信息">
        				<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" >
        				<Grid Margin="0" HorizontalAlignment="Stretch" Height="Auto" >
        					<Grid.RowDefinitions>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="32"/>
        						<RowDefinition Height="48"/>
        						<RowDefinition Height="48"/>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="40"/>
        						<RowDefinition Height="60"/>
        						<RowDefinition Height="60"/>
        					</Grid.RowDefinitions>
        					<Grid.ColumnDefinitions>
        						<ColumnDefinition Width="110"/>
        						<ColumnDefinition Width="1*"/>
        						<ColumnDefinition Width="110"/>
        						<ColumnDefinition Width="1*"/>
        						<ColumnDefinition Width="110"/>
        						<ColumnDefinition Width="1*"/>
        					</Grid.ColumnDefinitions>
        					<TextBlock Margin="0,9" TextWrapping="Wrap" Text="报建编号：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0,9" TextWrapping="Wrap" Height="22" Grid.ColumnSpan="1" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_code"/>
        						</a:Data.Prop>
        					</TextBox>	
        					<TextBlock Grid.Column="2" Margin="0,9" TextWrapping="Wrap" Text="报建日期：" Height="22" TextAlignment="Right"/>
        					<sdk:DatePicker SelectedDate="{m:Bind Path=f_date}" IsEnabled="false" Grid.Column="3" Margin="0,9"  Height="22" Grid.ColumnSpan="1" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_date"/>
        						</a:Data.Prop>
        					</sdk:DatePicker>	
        					<TextBlock Grid.Column="4" Margin="0,9" TextWrapping="Wrap" Text="申 请 人：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_username}"  Grid.Column="5" Margin="0,9" TextWrapping="Wrap" Height="22" Grid.ColumnSpan="1" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_username"/>
        						</a:Data.Prop>
        					</TextBox>	
        					<TextBlock Margin="0,9" Grid.Row="2" TextWrapping="Wrap" Text="电 话：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_phone}"  Grid.Column="1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="25" Grid.ColumnSpan="1" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_phone"/>
        						</a:Data.Prop>
        					</TextBox>	
        					<TextBlock Grid.Column="2" Margin="0,9" Grid.Row="2" TextWrapping="Wrap" Text="用气地址：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_address}"  Grid.Column="3" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="25" Grid.ColumnSpan="3" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_address"/>
        						</a:Data.Prop>
        					</TextBox>	
        					<TextBlock Margin="0" Grid.Row="6" TextWrapping="Wrap" Text="片 区：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_area}"  Grid.Column="1" Margin="0,0,4,0" Grid.Row="6" TextWrapping="Wrap" IsReadOnly="True" Height="22" Grid.ColumnSpan="1" Grid.RowSpan="1">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_area"/>
        						</a:Data.Prop>
        					</TextBox>	
        					<TextBlock Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Text="用户类型：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Height="22">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_usertype"/>
        						</a:Data.Prop>
        					</TextBox>		 
        					<TextBlock Grid.Column="2" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Text="房屋类型：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_usertypepro}"  Grid.Column="3" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Height="22" IsReadOnly="True"/>
        					<TextBlock Grid.Column="4" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Text="申请户数：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_applycount"/>
        						</a:Data.Prop>
        					</TextBox>
        					<TextBlock Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="放 线 人：" Height="22" TextAlignment="Right"/>
        					<TextBlock Grid.Column="2" Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="放线日期：" Height="22" TextAlignment="Right"/>
        					<TextBlock Grid.Column="4" Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="安装户数：" Height="22" TextAlignment="Right"/>
        					<TextBlock Grid.Column="2" Margin="0,5" Grid.Row="5" TextWrapping="Wrap" Text="设 计 人：" Height="22" TextAlignment="Right"/>
        					<TextBlock Grid.Column="4" Margin="0,5" Grid.Row="5" TextWrapping="Wrap" Text="设计审核日期：" TextAlignment="Right" Height="22"/>
        					<TextBlock Grid.Column="2" Margin="0" Grid.Row="8" TextWrapping="Wrap" Text="工程部意见：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_survey}" Grid.Column="1" Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_survey" />
        						</a:Data.Prop>
        					</TextBox>
        					<sdk:DatePicker SelectedDate="{m:Bind Path=f_surveydate}" Grid.Column="3" Margin="0" Grid.Row="4" IsEnabled="False" Height="22" >
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_surveydate" />
        						</a:Data.Prop>
        					</sdk:DatePicker>
        					<TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="5" Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="22" >
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_installnum"  NotNull="True" />
        						</a:Data.Prop>
        					</TextBox>
        					<TextBlock  Margin="0" Grid.Row="5" TextWrapping="Wrap" Text="交费户数：" Height="25" TextAlignment="Right" />
        					<TextBox Text="{m:Bind Path=f_payfeenum}" Grid.Column="1" Margin="0" Grid.Row="5" TextWrapping="Wrap" IsReadOnly="True" Height="22">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_payfeenum" Value="{m:Exp Str=this.f_installnum}" Default="{m:Exp Str=this.f_installnum}"/>
        						</a:Data.Prop>
        					</TextBox>
        					<CheckBox Content="可以进行设计" IsChecked="{m:Bind Path=f_designe}" Grid.Column="0" Margin="0,6,0,12" Grid.Row="7" IsEnabled="False" Grid.ColumnSpan="1" Height="22" HorizontalAlignment="Left" d:LayoutOverrides="GridBox" />	
        					<TextBox Text="{m:Bind Path=f_designers}" Grid.Column="3" Margin="0,5" Grid.Row="5" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_designers" />
        						</a:Data.Prop>
        					</TextBox>	
        					<sdk:DatePicker SelectedDate="{m:Bind Path=f_sjexamdate}" Grid.Column="5" Margin="0,5" Grid.Row="5" IsEnabled="False" Height="22" >
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_sjexamdate" />
        						</a:Data.Prop>
        					</sdk:DatePicker>	
        					<CheckBox IsChecked="{m:Bind Path=f_havepay}" Grid.Column="0" Margin="0" Grid.Row="8"  Height="22" Content="具备安装交款" Grid.ColumnSpan="2" VerticalAlignment="Stretch">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_havepay"  Default="{m:Exp Str=False}"/>
        						</a:Data.Prop>
        					</CheckBox>
        					<CheckBox Content="民用转商用" IsChecked="{m:Bind Path=f_isminzhsha}" IsEnabled="False" Grid.Column="1" Margin="0,6,0,12" Grid.Row="7" Height="22">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_isminzhsha" />
        						</a:Data.Prop>
        					</CheckBox>
        					<TextBox Text="{m:Bind Path=f_opinion}" Grid.Column="3" Margin="0,1,1,1" Grid.Row="8" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Grid.RowSpan="1" HorizontalAlignment="Stretch" d:LayoutOverrides="GridBox">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_opinion" />
        						</a:Data.Prop>
        					</TextBox>
        					<TextBlock Margin="0,1" Grid.Row="9" TextWrapping="Wrap" Text="部 门：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_gcbshdepartment}" Grid.Column="1" Margin="0,1" Grid.Row="9" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        						<a:Data.Prop>
        							<o:PropertySetter PropertyName="f_gcbshdepartment" NotNull="True" 
        								Default="{m:Exp Str=LoginUser.f_parentname}"/>
        						</a:Data.Prop>
        					</TextBox>
        					<TextBlock Grid.Column="2" Margin="0,1" Grid.Row="9" TextWrapping="Wrap" Text="操 作 人：" Height="22" TextAlignment="Right"/>
        					<TextBox Text="{m:Bind Path=f_gcbshopertor}" Grid.Column="3" Margin="0,1" Grid.Row="9" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        						<a:Data.Prop>
