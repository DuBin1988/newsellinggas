<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
    x:Class="Com.Aote.Pages.民用现场勘查" FontSize="13"
    mc:Ignorable="d">
	<Grid x:Name="LayoutRoot">
	
		<m:ResourceLoad Margin="0">
			<!--民用用气类型-->
			<o:ObjectList  Name="usegastypelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='民用用气类型'" />
			<o:ObjectList  Name="shjrlist" WebClientInfo="{StaticResource dbclient}" Path="from t_user where roles like '5253539%'" />
			<!--用户信息-->
		    <o:ObjectList Name="userlist" IsOld="{m:Exp Str='surveyAction.State\=\=$End$'}" 
			WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str='$select  applycus  from t_applycustomer as applycus  where applyid\=\{main.DataContext.id\}  and (f_isok is  null or f_isok = true) '}"
			EntityType="t_applycustomer"  HasEmptyRow="True"></o:ObjectList>
			
			
			
			<!--用户图片-->
		    <o:ObjectList Name="userimagelist" Source="{m:Exp Str=nopeople.SelectedItem.f_customerimage}" EntityType="t_customerimage" HasEmptyRow="True"></o:ObjectList>	
			<!--批量删除语句-->
	        <b:HQLAction Name="DeleteAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str='$delete from t_apply where id in ({persons.SelectedItems.ToString(id)=>SelectionChanged})'}">
			</b:HQLAction>
			 
		    <!--图片信息-->
		    <o:ObjectList Name="imagelist" EntityType="t_applyimage"></o:ObjectList>	

		   <!--终止报建-->
            <b:HQLAction Name="EndAction"  WebClientInfo="{StaticResource dbclient}" 
		      HQL="{m:Exp Str=$update t_apply set f_state\=\'终止报建\' \,f_substate\=null where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
			<!-- 核对户数-->
			<b:HQLAction Name="surveyAction"  WebClientInfo="{StaticResource dbclient}" 
		 HQL="{m:Exp Str=$update t_applycustomer set f_isok\=1  where id in (\{yespeople.SelectedItems.ToString(id)\=>SelectionChanged\})}"/>
	        <!-- 重新核对 -->
			<b:HQLAction Name="unsurveyAction"  WebClientInfo="{StaticResource dbclient}" 
		 HQL="{m:Exp Str=$update t_applycustomer set f_isok\=null  where id in (\{nopeople.SelectedItems.ToString(id)\=>SelectionChanged\})}"/>       
	  <!--清除列表中的垃圾数据-->
      <b:HQLAction Name="DeleteDirty" 
		HQL="{m:Exp Str='userlist.Dirty.Count!=0:$delete from t_applycustomer where id in ({userlist.Dirty[True].ToString(id)})$,null'}">
		</b:HQLAction>
		 <!--
		<b:HQLAction Name="DeleteDirtytwo" 
		HQL="{m:Exp Str='$delete from t_applycustomer where id in ({userstwo.ItemsSource.Dirty[True].ToString(id)})'}">
		</b:HQLAction>
		-->
		<!--保存，保存时要删除列表里的垃圾数据-->
		<b:BatchExcuteAction Name="BatchSave"  WebClientInfo="{StaticResource dbclient}">
			<b:BatchInfo Source="{m:Exp Str=DeleteDirty}" MethodName="InvokeToJson"></b:BatchInfo>
			<b:BatchInfo Source="{m:Exp Str=main.DataContext}" MethodName="SaveToJson"></b:BatchInfo>
		</b:BatchExcuteAction>
		<!--直接提交工程部审核SubmitgcbAction-->
		<b:HQLAction Name="SubmitgcbAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'工程部审核\' \,f_substate\=null where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
       <!--提交-->
        <b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_apply set f_state\=\'设计审核\' \,f_substate\=null where id in (\{persons.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
			<!--提交语句
	        <b:HQLAction Name="OkAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str='$update t_apply set f_state=\'提交\' where id in ({persons.SelectedItems.ToString(id)=>SelectionChanged})'}">
			</b:HQLAction>-->
			<!-- 收费系统对象 -->
		<o:GeneralObject Name="tempobj" EntityType="t_applycustomer"></o:GeneralObject>
		<o:GeneralObject Name="tobj" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}">
			<o:PropertySetter PropertyName="id" Value="{m:Exp Str=tempobj.id}"/>
			<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=tempobj.f_username}"/>
			<o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=tempobj.f_address}"/>
			<o:PropertySetter PropertyName="f_districtname" Value="{m:Exp Str=tempobj.f_district}"/>
			<o:PropertySetter PropertyName="f_apartment" Value="{m:Exp Str=tempobj.f_doorplate}"/>
			<o:PropertySetter PropertyName="f_phone" Value="{m:Exp Str=tempobj.f_phone}"/>
			<o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=tempobj.f_code}"/>
			<o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=tempobj.f_meternumber}"/>
			<o:PropertySetter PropertyName="f_metertitles" Value="{m:Exp Str=tempobj.f_metertitles}"/>
		</o:GeneralObject>	
			
		</m:ResourceLoad>
    <Grid>
           	<Grid.ColumnDefinitions>
    		<ColumnDefinition Width="0.3*"/>
    		<ColumnDefinition Width="0.7*"/>
    	</Grid.ColumnDefinitions>
			<Grid x:Name="pname">
            <sdk:TabControl  Margin="0,0,0,45">
            	<!--待办-->
				<sdk:TabItem Header="待  办">
					<toolkit:DockPanel LastChildFill="True">
                <toolkit:WrapPanel x:Name="personsearch" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                   <a:Data.Context>
            		  	<o:SearchObject  ></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申请人："/>
                    <TextBox TextWrapping="Wrap" MinWidth="60" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Height="22" >
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>               
					  </a:Data.Prop>
					</TextBox>
					<TextBlock TextWrapping="Wrap" Text="地址："/>
                    <TextBox TextWrapping="Wrap" MinWidth="60" Text="{m:Bind Path=f_address}" a:ControlAttach.DefaultButton="searchbutton" Height="22" >
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_address" Operator="apply.f_address like '%this%'"/>               
					  </a:Data.Prop>
					</TextBox>
                    <Button Content="  查 询  " x:Name="searchbutton" Click="{m:Exp Str=personsearch.DataContext.Search()}" Margin="2,0,0,0"/>
                </toolkit:WrapPanel>
				
                 <!--提交按钮-->
                 <StackPanel   Margin="0,5" toolkit:DockPanel.Dock="Bottom" Orientation="Horizontal" >
				 	<TextBlock TextWrapping="Wrap" Text="{m:Exp Str='$共{persons.ItemsSource.Count}项'}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
				<Button Content=" 提交设计审核 "  HorizontalAlignment="Stretch"  Click="{m:Exp Str=submittip.Show()}"
					IsEnabled="{m:Exp Str=False and persons.SelectedItems.Count\=&gt;SelectionChanged!\=0}" Margin="60,5,0,5" Height="25" />
					<Button Content=" 工程部审核 "  Margin="2,5,1,5" Click="{m:Exp Str=gcbsubmittip.Show()}"
                        IsEnabled="{m:Exp Str='False and persons.SelectedItems.Count=>SelectionChanged!=0'}" 
						 HorizontalAlignment="Stretch">            
					</Button>
					<Button Content="打印列表" Click="{m:Exp Str=print.Print()}" Height="25" Margin="2,0,1,0"/>
					<Button Content=" 终止报建 "  Margin="2,5,5,5" Click="{m:Exp Str=endsubmittip.Show()}"
						IsEnabled="{m:Exp Str=False and persons.SelectedItems.Count\=&gt;SelectionChanged!\=0}" 
						HorizontalAlignment="Stretch"/>
                 </StackPanel>
				   
				  <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=persons.ItemsSource.IsBusy}" >
                <sdk:DataGrid x:Name="persons" Margin="0" MinHeight="250" IsReadOnly="True"  
				 AutoGenerateColumns="False" >
                	<a:Data.Items>
                		<o:ObjectList Name="personslist" WebClientInfo="{StaticResource dbclient}"
                            Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_applycustomer as applycustomer left join  applycustomer.f_customerimage left  join fetch apply.f_applyimage  where \{personsearch.DataContext.Condition\}  and f_survey \=\'\{LoginUser.name\}\'  and f_usertype \=\'民用\' and f_state \=\'现场勘查\'}"
							 IsOld="{m:Exp Str=(SubmitAction.State\=\=$Free$ or SubmitAction.State\=\=$End$)and
					       (DeleteAction.State\=\=$Free$ or DeleteAction.State\=\=$End$)and 
						   (SubmitgcbAction.State\=\=$Free$ or SubmitgcbAction.State\=\=$End$)and 
						   (main.DataContext.State\=\=$Free$ or main.DataContext.State\=\=$End$)and
						   (BatchSave.State\=\=$Free$ or BatchSave.State\=\=$End$)and
						   (EndAction.State\=\=$Free$ or EndAction.State\=\=$End$)
						   }"
							 >
                		</o:ObjectList>
					</a:Data.Items>
                       <sdk:DataGrid.Columns>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
	                		<sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
	                		<sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
	                		<sdk:DataGridTextColumn Header="用气地址" Binding="{Binding f_address}"/>
	                		<sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
	                		<sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
				</toolkit:BusyIndicator>
				 </toolkit:DockPanel>
				 </sdk:TabItem>
			<!--已办-->
			<!--sdk:TabItem Header="已  办">
				<toolkit:DockPanel LastChildFill="True">
				 
				  <StackPanel x:Name="newsearch" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                   <a:Data.Context>
            		  	<o:SearchObject  ></o:SearchObject>        	
				   </a:Data.Context>
					<TextBlock TextWrapping="Wrap" Text="申请人："/>
                    <TextBox TextWrapping="Wrap" MinWidth="60" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchButton" Height="22" >
                      <a:Data.Prop>
                      	   <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>               
					  </a:Data.Prop>
					</TextBox>
                    <Button Content="  查 询  " x:Name="searchButton" Click="{m:Exp Str=newsearch.DataContext.Search()}" Margin="50,0,0,0"/>
                </StackPanel>
				 <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=yijing.ItemsSource.IsBusy}" >
                <sdk:DataGrid x:Name="yijing" Margin="0" MinHeight="250" IsReadOnly="True"  
				 AutoGenerateColumns="False" >
                	<a:Data.Items>
                		<o:ObjectList Name="yijinglist" WebClientInfo="{StaticResource dbclient}"
  Path="{m:Exp Str=$select distinct apply from t_apply as apply left join fetch apply.f_applycustomer as applycustomer  left join  applycustomer.f_customerimage left join fetch apply.f_applyimage   where \{newsearch.DataContext.Condition\} and f_survey \=\'\{LoginUser.name\}\'  and f_usertype \=\'民用\' and  f_gcbkcopertor is not null and f_state \!\=\'现场勘查\'}"					         IsOld="{m:Exp Str=(SubmitAction.State\=\=$Free$ or SubmitAction.State\=\=$End$)and
					       (DeleteAction.State\=\=$Free$ or DeleteAction.State\=\=$End$)and
						   (main.DataContext.State\=\=$Free$ or main.DataContext.State\=\=$End$)and
						   (BatchSave.State\=\=$Free$ or BatchSave.State\=\=$End$)
						   }">
                		</o:ObjectList>
					</a:Data.Items>
                       <sdk:DataGrid.Columns>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
	                			<sdk:DataGridTextColumn d:IsLocked="True" Header="报建状态" Binding="{Binding f_state}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
	                		<sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
	                		<sdk:DataGridTextColumn Header="用气地址" Binding="{Binding f_address}"/>
	                		<sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
	                		<sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
				</toolkit:BusyIndicator>
				</toolkit:DockPanel>
				</sdk:TabItem-->
				</sdk:TabControl>
				
				
			</Grid>
			<!--结束-->
        <toolkit:BusyIndicator BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=main.DataContext.IsBusy}" Margin="5,0,0,0" Grid.Column="1">
        	<Grid x:Name="main" Margin="0" >
        		<a:Data.Context>
        			<o:GeneralObject  Name="persongo" WebClientInfo="{StaticResource dbclient}" 
        				EntityType="t_apply"  Source="{m:Exp Str=persons.SelectedItem}"
        				IsInit="{m:Exp Str=this.State\=\=$End}">
        				<!--o:PropertySetter PropertyName="f_state" Default="报建登记"></o:PropertySetter-->
        				<o:PropertySetter  PropertyName="f_substate" Default="完成"/>
        				<o:PropertySetter PropertyName="f_applycustomer" Default="{m:Exp Str=userlist}"/>
        				<o:PropertySetter PropertyName="f_customerimage" Default="{m:Exp Str=userimagelist}"/>
        				<o:PropertySetter PropertyName="f_applyimage" Default="{m:Exp Str=imagelist}"/>  
        			</o:GeneralObject>
        		</a:Data.Context>
        		<StackPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Right" Orientation="Horizontal" VerticalAlignment="Bottom">
                   
        			<Button x:Name="save" Content="  保 存  " Margin="5,5,0,5" 
        				IsEnabled="{m:Exp Str=False and main.DataContext.IsModified}"
        				Click="{m:Exp Str=userlist.MakeDirty();BatchSave.Invoke();main.DataContext.New()}"/>
        			<Button Content="不能安装" Visibility="Collapsed" Margin="5" IsEnabled="{m:Exp Str='yespeople.SelectedItems.Count\=&gt;SelectionChanged!\=0 '}" 
        				Click="{m:Exp Str=surveyAction.Invoke()}" Width="50" FontWeight="Bold"/>
        			<Button Content="重新核对" Margin="5" Visibility="Collapsed" IsEnabled="{m:Exp Str='nopeople.SelectedItems.Count\=&gt;SelectionChanged!\=0 '}" 
        				Click="{m:Exp Str=unsurveyAction.Invoke()}" Width="50" />
					
        			<Button Visibility="Collapsed" Content=" 导入收费系统 " IsEnabled="{m:Exp Str='yespeople.SelectedItems.Count\=&gt;SelectionChanged!\=0 '}" 
        				Click="{m:Exp Str=right.SelectedItem.Save()}" Height="22" HorizontalAlignment="Left" Margin="10,0,0,0"/>
        		</StackPanel>
        		<Grid Height="Auto">
        			<sdk:TabControl  Margin="0,0,0,45" >
        				<sdk:TabItem Header="报建基本信息">
        					<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" >
        					<Grid Margin="0" HorizontalAlignment="Stretch"  >
        						<Grid.RowDefinitions>
        							<RowDefinition Height="40"/>
        							<RowDefinition Height="40"/>
        							<RowDefinition Height="40"/>
        							<RowDefinition Height="40"/>
        							<RowDefinition Height="40"/>
        							<RowDefinition Height="60"/>
        							<RowDefinition Height="60"/>
        							<RowDefinition Height="60"/>
        							<RowDefinition Height="60"/>
        							<RowDefinition Height="60"/>
        						</Grid.RowDefinitions>
        						<Grid.ColumnDefinitions>
        							<ColumnDefinition Width="110"/>
        							<ColumnDefinition Width="1*"/>
        							<ColumnDefinition Width="110"/>
        							<ColumnDefinition Width="1*"/>
        							<ColumnDefinition Width="110"/>
        							<ColumnDefinition Width="1*" />
        						</Grid.ColumnDefinitions>
        						<TextBlock Margin="0" TextWrapping="Wrap" Text="报建编号：" Height="22" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0,9" TextWrapping="Wrap" Height="22">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_code"/>
        							</a:Data.Prop>
        						</TextBox>
        						<TextBlock Margin="0,7.5" TextWrapping="Wrap" TextAlignment="Right"  Text="用户单位：" Grid.Row="1"  Height="25"/>
        						<TextBox Text="{m:Bind Path=f_minyongdept}" IsReadOnly="True" Grid.Column="1" Margin="0,8,0,7"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Grid.Row="1" Grid.ColumnSpan="4"  Height="25">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_minyongdept"/>
        							</a:Data.Prop>
        						</TextBox>	
        						<TextBlock Grid.Column="2" Margin="0" TextWrapping="Wrap" Text="报建日期：" Height="22" TextAlignment="Right"/>
        						<sdk:DatePicker Text="{m:Bind Path=f_date}"  Grid.Column="3" Margin="0,9"  Height="22" SelectedDateFormat="Short" >
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_date"  />
        							</a:Data.Prop>
        						</sdk:DatePicker>	
        						<TextBlock Grid.Column="4" Margin="0" TextWrapping="Wrap" Text="申 请 人：" Height="22" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="5" Margin="0,9" TextWrapping="Wrap" Height="22">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_username"/>
        							</a:Data.Prop>
        						</TextBox>	
        						<TextBlock Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="电 话：" Height="22" TextAlignment="Right" />
        						<TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="1" Margin="0,9" Grid.Row="2" TextWrapping="Wrap" Height="22">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_phone"/>
        							</a:Data.Prop>
        						</TextBox>	
        						<TextBlock Grid.Column="2" Margin="0" Grid.Row="2" TextWrapping="Wrap" Text="用气地址：" Height="22" TextAlignment="Right" />
        						<TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="3" Margin="0,9" Grid.Row="2" TextWrapping="Wrap" Height="22" Grid.ColumnSpan="3">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_address"/>
        							</a:Data.Prop>
        						</TextBox>	
        						<TextBlock Grid.Column="4" Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="片 区：" Height="22" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="5" Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="22">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_area"/>
        							</a:Data.Prop>
        						</TextBox>	
        						<TextBlock Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="用户类型：" Height="22" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="3" TextWrapping="Wrap" Height="22" >
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_usertype"/>
        							</a:Data.Prop>
        						</TextBox>		 
        						<TextBlock Grid.Column="2" Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="房屋类型：" Height="22" TextAlignment="Right" />
        						<TextBox  Text="{m:Bind Path=f_usertypepro}" Grid.Column="3" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Height="22" IsReadOnly="True"/>
        						<TextBlock Grid.Column="4" Margin="0" Grid.Row="3" TextWrapping="Wrap" Text="申请户数：" Height="22" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0,9" Grid.Row="3" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_applycount"/>
        							</a:Data.Prop>
        						</TextBox>
        						<TextBlock  Margin="0" Grid.Row="4" TextWrapping="Wrap" Text="安装户数：" Height="22" TextAlignment="Right" />
        						<TextBlock Grid.Column="0" Margin="0" Grid.Row="6" TextWrapping="Wrap" Text="勘查意见：" Height="25" TextAlignment="Right" />	
        						<TextBox Text="{m:Bind Path=f_installnum}" Grid.Column="1" Margin="0,8,0,7" Grid.Row="4" TextWrapping="Wrap" IsReadOnly="False" Height="22">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_installnum" />
        							</a:Data.Prop>
        						</TextBox>
        						<TextBox Text="{m:Bind Path=f_gcbkcopinion}" Grid.Column="1" Margin="1" Grid.Row="6" TextWrapping="Wrap" Grid.ColumnSpan="4" Height="58" HorizontalAlignment="Left" Width="350" >
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_gcbkcopinion" />
        							</a:Data.Prop>
        						</TextBox>
        						<TextBlock Margin="0" Grid.Row="9" TextWrapping="Wrap" Text="部 门：" Height="25" TextAlignment="Right"/>
        						<TextBox Text="{m:Bind Path=f_gcbkcdepartment}" Grid.Column="1" Margin="0" Grid.Row="9" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_gcbkcdepartment" NotNull="True" 
        									Default="{m:Exp Str=LoginUser.f_parentname}"/>
        							</a:Data.Prop>
        						</TextBox>
        						<TextBlock Grid.Column="2" Margin="0" Grid.Row="9" TextAlignment="Right" TextWrapping="Wrap" Text="操 作 人：" Height="25"/>
        						<TextBox Text="{m:Bind Path=f_gcbkcopertor}" Grid.Column="3" Margin="0" Grid.Row="9" TextWrapping="Wrap" Height="22" IsReadOnly="True">
        							<a:Data.Prop>
        								<o:PropertySetter PropertyName="f_gcbkcopertor" NotNull="True"  
        									Default="{m:Exp Str=LoginUser.name}"/>
