<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
	mc:Ignorable="d"
	x:Class="s2.非民用费用变更" FontSize="13"
	d:DesignWidth="1200" Height="624">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad d:IsHidden="True">
            <o:ObjectList Name="fmygastype" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproperties as gasproperties left join fetch gasproperties.qijiatype}" EntityType="t_gasproperties" />
            <o:ObjectList Name="paytype" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='报建付款方式'"/>
            <o:ObjectList Name="fmypricetype" Source="{m:Exp Str=CoboxPinpl.SelectedItem.qijiatype}" EntityType="t_qijiatype" />
            <o:ObjectList Name="devicelist_fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='mypgshInfo.DataContext.State==$End$'}"
            Path="{m:Exp Str='mypgshInfo.DataContext.id!=null:$from t_device where applyid={mypgshInfo.DataContext.id}$,null'}" EntityType="t_device" HasEmptyRow="False" />

            <!--收费列表 -->
            <o:ObjectList Name="paydetails" EntityType="t_paydetail" WebClientInfo="{StaticResource dbclient}"
			Path="{m:Exp Str='applys.SelectedItem!=null:$from t_paydetail where applyid={applys.SelectedItem.id}$,null'}"/>

            <!--<o:ObjectList Name="mygastype" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproperties as gasproperties left join fetch gasproperties.qijiatype}" EntityType="t_gasproperties" />
            <o:ObjectList Name="mypricetype" Source="{m:Exp Str=CoboxPinps.SelectedItem.qijiatype}" EntityType="t_qijiatype" /> -->
            <!--报装选中对象-->
            <o:GeneralObject Name="mypgshObj" WebClientInfo="{StaticResource dbclient}" EntityType="t_apply"
                Source="{m:Exp Str='applys.SelectedItem'}"
                             IsInit="{m:Exp Str='SaveAction.State==$End$'}" >
                <!--实交金额-->
                <o:PropertySetter PropertyName="f_actualfee" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:fee.f_fee + mypgshObj.f_actualfee\,null}"/>
                <o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=devicelist_fmypgsh}"/>
                <o:PropertySetter PropertyName="f_costreduction" Default="{m:Exp Str=costreductionlist_fmypgsh}"/>
            </o:GeneralObject>

            <!--收费编辑对象-->
            <o:GeneralObject Name="fee" WebClientInfo="{StaticResource dbclient}" EntityType="t_paydetail"
                IsInit="{m:Exp Str='SaveAction.State==$End$'}">
                <!-- 收费对象是否以保存信息完成为依据来初始化-->
                <!--对应报装id-->
                <o:PropertySetter PropertyName="applyid" Default="{m:Exp Str=mypgshObj.id}" NoDependValue="{m:Exp Str=mypgshObj.id}"/>
                <o:PropertySetter PropertyName="f_state" Default="有效"/>
            </o:GeneralObject>

            <o:ObjectList Name="userlist_mypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='mypgshInfo.DataContext.State==$End$'}"
            Path="{m:Exp Str='mypgshInfo.DataContext.id!=null:$from t_applycustomer where applyid={mypgshInfo.DataContext.id}$,null'}" EntityType="t_applycustomer" HasEmptyRow="False" />

            <o:ObjectList Name="costreductionlist_fmypgsh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='mypgshInfo.DataContext.State==$End$'}"
             Path="{m:Exp Str='mypgshInfo.DataContext.id!=null:$from t_costreduction where applyid={mypgshInfo.DataContext.id}$,null'}"   EntityType="t_costreduction" HasEmptyRow="False" />


            <!--保存-->
            <b:BatchExcuteAction Name="SaveAction" CanSave="{m:Exp Str=SaveButton\=>Click!\=null}" WebClientInfo="{StaticResource dbclient}">
                <!--报装对象保存-->
                <b:BatchInfo Source="{m:Exp Str=userfile}" MethodName="SaveToJson"/>
                <b:BatchInfo Source="{m:Exp Str=userfileschangeset}" MethodName="SaveToJson"/>
            </b:BatchExcuteAction>



            <o:GeneralObject Name="userfile" EntityType="t_apply" WebClientInfo="{StaticResource dbclient}"
                Path="{m:Exp Str=applys.SelectedItem!\=null:$one/from t_apply where id\=\'\{applys.SelectedItem.id\}\'$\,$aaa$}">

                <o:PropertySetter PropertyName="f_emptyingfee" Value="{m:Exp Str=userfileschangeset.f_emptyingfeechange}"/>
                <o:PropertySetter PropertyName="f_materialfee" Value="{m:Exp Str=userfileschangeset.f_materialfeechange}"/>
                <o:PropertySetter PropertyName="f_totalfee" Value="{m:Exp Str=userfileschangeset.f_totalfeechange}"/>
                <o:PropertySetter PropertyName="f_htnum" Value="{m:Exp Str=userfileschangeset.f_htnumchange}"/>
                <!--  <o:PropertySetter PropertyName="f_selfclosfee" Value="{m:Exp Str=userfileschangeset.f_selfclosfeechange}"/>
                <o:PropertySetter PropertyName="f_alarmfee" Value="{m:Exp Str=userfileschangeset.f_alarmfeechange}"/>
                <o:PropertySetter PropertyName="f_bgglf" Value="{m:Exp Str=userfileschangeset.f_bgglfchange}"/>
                <o:PropertySetter PropertyName="f_rsqf" Value="{m:Exp Str=userfileschangeset.f_rsqfchange}"/>
                <o:PropertySetter PropertyName="f_cailiaofei" Value="{m:Exp Str=userfileschangeset.f_cailiaofeichange}"/>
                <o:PropertySetter PropertyName="f_rengongfei" Value="{m:Exp Str=userfileschangeset.f_rengongfeichange}"/>
                <o:PropertySetter PropertyName="f_jiebofei" Value="{m:Exp Str=userfileschangeset.f_jiebofeichange}"/>-->


            </o:GeneralObject>


            <o:GeneralObject Name="userfileschangeset" WebClientInfo="{StaticResource dbclient}" EntityType="t_feechange" IsInit="{m:Exp Str=SaveAction.State\=\=$End$}">
                <o:PropertySetter PropertyName="f_emptyingfee" Value="{m:Exp Str=applys.SelectedItem.f_emptyingfee}"/>
                <o:PropertySetter PropertyName="f_materialfee" Value="{m:Exp Str=applys.SelectedItem.f_materialfee}"/>
                <o:PropertySetter PropertyName="f_totalfee" Value="{m:Exp Str=applys.SelectedItem.f_totalfee}"/>
                <o:PropertySetter PropertyName="f_htnum" Value="{m:Exp Str=applys.SelectedItem.f_htnum}"/>
                <!-- <o:PropertySetter PropertyName="f_selfclosfee" Value="{m:Exp Str=applys.SelectedItem.f_selfclosfee}"/>
                <o:PropertySetter PropertyName="f_alarmfee" Value="{m:Exp Str=applys.SelectedItem.f_alarmfee}"/>
                <o:PropertySetter PropertyName="f_bgglf" Value="{m:Exp Str=applys.SelectedItem.f_bgglf}"/>
                <o:PropertySetter PropertyName="f_rsqf" Value="{m:Exp Str=applys.SelectedItem.f_rsqf}"/>
                <o:PropertySetter PropertyName="f_cailiaofei" Value="{m:Exp Str=applys.SelectedItem.f_cailiaofei}"/>
                <o:PropertySetter PropertyName="f_rengongfei" Value="{m:Exp Str=applys.SelectedItem.f_rengongfei}"/>
                <o:PropertySetter PropertyName="f_jiebofei" Value="{m:Exp Str=applys.SelectedItem.f_jiebofei}"/>-->


            </o:GeneralObject>




        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <toolkit:DockPanel  LastChildFill="True" Grid.ColumnSpan="1" Margin="0,0,0,27">
                <toolkit:WrapPanel x:Name="applysearch"  Background="{x:Null}" toolkit:DockPanel.Dock="Top" Margin="5,0,0,5" Orientation="Horizontal" >
                    <a:Data.Context>
                        <o:SearchObject />
                    </a:Data.Context>
                    <TextBlock TextWrapping="Wrap" Text="申 请 人："/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_username" Operator="apply.f_username like 'this%'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" Width="Auto" Height="22">
                        <a:Data.Prop>
                            <o:PropertySetter PropertyName="f_phone"  Operator="apply.f_phone = 'this'"/>
                        </a:Data.Prop>
                    </TextBox>
                    <Button Content="  查 询  " x:Name="searchbutton" Click="{m:Exp Str=applysearch.DataContext.Search()}" Margin="5,0,0,0"/>
                </toolkit:WrapPanel>
                <!--提交按钮-->
                <toolkit:DockPanel  toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="55"  >
                    <sdk:DataPager x:Name="pager" Source="{Binding Path=ItemsSource.Count,ElementName=applys,Converter={StaticResource DataPagerConverter}}" 
                                   PageSize="20" Height="25" toolkit:DockPanel.Dock="Top" />
                    <TextBlock  TextWrapping="Wrap" Text="{m:Exp Str=$共\{applys.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>
                </toolkit:DockPanel>
                <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}" Margin="0,0,0,19">
                    <sdk:DataGrid x:Name="applys" Margin="0,0,0,-16" MinHeight="250" AutoGenerateColumns="False" Background="{x:Null}" IsReadOnly="True" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
                        <sdk:DataGrid.RenderTransform>
                            <CompositeTransform SkewX="-0.102" TranslateX="-0.498"/>
                        </sdk:DataGrid.RenderTransform>
                        <a:Data.Items>
                            <o:PagedObjectList  x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
                               IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
		                       Path="{m:Exp Str=$from t_apply as apply
		                       where \{applysearch.DataContext.Condition\} and (f_usertype\=\'非民用\' 
                               and f_totalfee is not null and f_totalfee>f_actualfee) or (f_usertype\=\'非民用\' and   f_actualfee is null)}"  
                                PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"
                                />
                        </a:Data.Items>
                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                            <sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                            <sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                            <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </c:BusyIndicatorEx>
            </toolkit:DockPanel>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfileschangeset.IsBusy or userlist_mypgsh.IsBusy}" Grid.Column="1">
                <Grid Background="{StaticResource LoginBgColorBrush1}" x:Name="mypgshInfo" DataContext="{m:Exp Str=userfileschangeset}" Margin="0,0,0,17">

                    <sdk:TabControl  Margin="0" VerticalAlignment="Top" Height="607" >
                        <sdk:TabItem Header="报建基本信息" Margin="0,0,1,3">
                            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Margin="6.196,6,-6.196,31" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
                                <ScrollViewer.RenderTransform>
                                    <CompositeTransform SkewX="-0.18" TranslateX="-0.997"/>
                                </ScrollViewer.RenderTransform>
                                <StackPanel>
                                    <Grid Margin="0" HorizontalAlignment="Stretch" Height="auto" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>

                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="29"/>
                                            <RowDefinition Height="50"/>
                                            <RowDefinition Height="50"/>
                                            <RowDefinition Height="50"/>
                                            <RowDefinition Height="50"/>
                                            <RowDefinition Height="29"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                            <ColumnDefinition Width="110"/>
                                            <ColumnDefinition Width="1*"/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right" Grid.Column="2"/>
                                        <TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="3" Margin="0,2" TextWrapping="Wrap" Height="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_code" Value="{m:Exp Str=applys.SelectedItem.f_code}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" TextWrapping="Wrap" Text="报建日期：" TextAlignment="Right"/>
                                        <sdk:DatePicker SelectedDate="{m:Bind Path=f_date}"  Grid.Column="5" Margin="0,3" IsEnabled="False" Height="Auto" Grid.Row="0">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_date" Value="{m:Exp Str=applys.SelectedItem.f_date}"/>
                                            </a:Data.Prop>
                                        </sdk:DatePicker>
                                        <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="申 请 人：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Height="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=applys.SelectedItem.f_username}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" Text="电　　话：" TextAlignment="Right" Grid.Column="4"/>
                                        <TextBox Text="{m:Bind Path=f_phone}" IsReadOnly="True" Grid.Column="5" Margin="0,2" Grid.Row="1" TextWrapping="Wrap" Height="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_phone"  Value="{m:Exp Str=applys.SelectedItem.f_phone}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" Text="用气地址：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="1" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_address"  Value="{m:Exp Str=applys.SelectedItem.f_address}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,4,0,2" Grid.Row="3" TextWrapping="Wrap" Text="片　　区：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_area}" IsReadOnly="True" Grid.Column="1" Margin="0,0,148.643,4" Grid.Row="3" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="2" Grid.RowSpan="1">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_area"  Value="{m:Exp Str=applys.SelectedItem.f_area}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_usertype"  Value="{m:Exp Str=applys.SelectedItem.f_usertype}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="申请单位：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_minyongdept}" Grid.Column="3" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_minyongdept"  Value="{m:Exp Str=applys.SelectedItem.f_minyongdept}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="交费用气点数：" TextAlignment="Right"/>
                                        <TextBlock Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="工程材料费：" TextAlignment="Right"/>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="排 空 费：" TextAlignment="Right"/>
                                        <TextBlock  Margin="0,6,0,0" Grid.Row="6" TextWrapping="Wrap" Text="费用合计：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_payfeenum}" Grid.Column="3" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" IsReadOnly="True">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_payfeenum"  Value="{m:Exp Str=applys.SelectedItem.f_payfeenum}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_materialfeechange}" Grid.Column="1" Margin="0,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_materialfeechange"  Value="{m:Exp Str=applys.SelectedItem.f_materialfee}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_emptyingfeechange}" Grid.Column="3" Margin="0,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_emptyingfeechange"   Value="{m:Exp Str=applys.SelectedItem.f_emptyingfee}" />
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBox Text="{m:Bind Path=f_totalfeechange}" Grid.Column="1" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" IsReadOnly="False" Width="Auto" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_totalfeechange"   Default="{m:Exp Str=applys.SelectedItem.f_totalfeechange}" 
                                                                  NoDependValue="{m:Exp Str=this.f_materialfeechange  + this.f_emptyingfeechange}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="0,4,0,2" Grid.Row="12"  TextWrapping="Wrap" Text="部 门：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_fybgdepartment}" Grid.Column="1" Margin="0,4,148.643,0" Grid.Row="12" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" Grid.ColumnSpan="2">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_fybgdepartment"  Value="{m:Exp Str=applys.SelectedItem.f_fybgdepartment}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="2" Margin="0.333,4,162.333,2" Grid.Row="12" TextWrapping="Wrap" Text="操 作 人：" TextAlignment="Right" Grid.ColumnSpan="2"/>
                                        <TextBox Text="{m:Bind Path=f_chargeopertor}" Grid.Column="3" Margin="0.357,4,0.322,0" Grid.Row="12" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto" Grid.ColumnSpan="1">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_chargeopertor" Value="{m:Exp Str=applys.SelectedItem.f_chargeopertor}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Grid.Column="4" Margin="9.678,4,140,1" Grid.Row="12" TextWrapping="Wrap" Text="日 期：" TextAlignment="Right" Grid.ColumnSpan="2"/>
                                        <sdk:DatePicker SelectedDate="{m:Bind Path=f_chargeopertdate}" IsEnabled="False" Grid.Column="5" Margin="9.678,4,0,1" Grid.Row="12"  Height="Auto" IsTodayHighlighted="True" IsDropDownOpen="False" >
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_chargeopertdate"  Value="{m:Exp Str=applys.SelectedItem.f_chargeopertdate}"/>
                                            </a:Data.Prop>
                                        </sdk:DatePicker>
                                        <TextBlock Grid.Column="0" Margin="0,6,0,0" Grid.Row="8" TextWrapping="Wrap" Text="预算明细：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_budget}" Grid.Column="1" Margin="0,2,0.333,2" Grid.Row="8" IsReadOnly="True" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Width="Auto" HorizontalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_budget"  Value="{m:Exp Str=applys.SelectedItem.f_budget}"/>
                                            </a:Data.Prop>
                                        </TextBox>
                                        <TextBlock Margin="4.678,3,0.322,3" Grid.Row="2" TextWrapping="Wrap" Text="用气性质：" TextAlignment="Right" Grid.Column="4"/>
                                        <ComboBox x:Name="CoboxPinpl" ItemsSource="{m:Exp Str=fmygastype}" DisplayMemberPath="f_gasproperties" SelectedValuePath="f_gasproperties" SelectedValue="{m:Bind Path=f_gasproperties}" 
										Grid.Column="4" Margin="109.678,3,0,3" Grid.Row="2" Height="Auto" IsEnabled="False" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="Auto" RenderTransformOrigin="1.139,4.36" Grid.ColumnSpan="2">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gasproperties" Value="{m:Exp Str=applys.SelectedItem.f_gasproperties}"/>
                                            </a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="6" TextWrapping="Wrap" Text="气价类型：" TextAlignment="Right"/>
                                        <ComboBox x:Name="pricetype" ItemsSource="{m:Exp Str=fmypricetype}" DisplayMemberPath="f_gaspricetype"  SelectedValuePath="f_gaspricetype" IsEnabled="False" SelectedValue="{m:Bind Path=f_gaspricetype}"  
										Grid.Column="3" Margin="0,3" Grid.Row="6" Height="Auto"  Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto" VerticalAlignment="Stretch">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gaspricetype" Value="{m:Exp Str=applys.SelectedItem.f_gaspricetype}"/>
                                            </a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="气价：" TextAlignment="Right"/>
                                        <TextBox Text="{m:Bind Path=f_gasprice}"   IsReadOnly="True" Grid.Column="5" Margin="0,2"   Height="Auto" Grid.Row="5" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" RenderTransformOrigin="0.389,2.08">
                                            <a:Data.Prop>
                                                <o:PropertySetter PropertyName="f_gasprice"   Value="{m:Exp Str=applys.SelectedItem.f_gasprice}"/>
                                            </a:Data.Prop>
