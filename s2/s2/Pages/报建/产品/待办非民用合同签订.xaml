                   			<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
							<Grid Width="Auto" Height="Auto">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="63"/>
                                    <ColumnDefinition Width="83"/>
                                    <ColumnDefinition Width="Auto" MinWidth="184"/>
                                    <ColumnDefinition Width="Auto" MinWidth="87"/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="120"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="139"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="29"/>
                                </Grid.RowDefinitions>
                                <TextBlock Margin="0,2,0,1" TextWrapping="Wrap" Text="供 气 人" TextAlignment="Center" Grid.Row="1" Height="27" />
                                <TextBlock Margin="0,2,0,1" TextWrapping="Wrap" Text="用 气 人" TextAlignment="Center" Grid.Row="2" Height="27"/>
                                <TextBlock Margin="0,2,0,1" TextWrapping="Wrap" Text="联系人电话" TextAlignment="Right" Grid.Column="3" Grid.Row="3" Grid.ColumnSpan="1" Height="27"/>
                                <TextBlock Margin="0,2,0,1" TextWrapping="Wrap" Text="用气地址" TextAlignment="Center" Grid.Row="3" Height="27"/>
                                <TextBlock Margin="0,47,0,46" TextWrapping="Wrap" Text="用气类型" TextAlignment="Center" Grid.Row="4" Height="27" />
                                <TextBlock Margin="0" TextWrapping="Wrap" Text="最大用气量" TextAlignment="Center"  Grid.Row="5" />
                                <TextBox Text="{m:Bind Path=f_gtren}"  Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Grid.Row="1" IsReadOnly="True" Grid.ColumnSpan="2" Height="25">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_gtren" 
            								Default="{m:Exp Str=$阿拉善盟昆仑燃气有限责任公司$}"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBox Text="{m:Bind Path=f_username}"  Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Grid.Row="2" IsReadOnly="True" Grid.ColumnSpan="2" Height="25">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_username" />
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBox Text="{m:Bind Path=f_phone}" Grid.Column="4" Margin="3.5,2,0,2" TextWrapping="Wrap" Grid.Row="3" IsReadOnly="True" Height="25" HorizontalAlignment="Left" Width="146" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_phone" />
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBox Text="{m:Bind Path=f_address}" Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Grid.Row="3" IsReadOnly="True" Grid.ColumnSpan="2" Height="25">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_address" />
                                    </a:Data.Prop>
                                </TextBox>
                                <StackPanel Margin="0" Orientation="Horizontal" Grid.ColumnSpan="5">
                                    <TextBlock HorizontalAlignment="Left"  Margin="10,0,0,0" TextWrapping="Wrap" Text="合同编号" Height="27" VerticalAlignment="Bottom"/>
                                    <TextBox Text="{m:Bind Path=f_htnum}" Margin="9,2,0,2" TextWrapping="Wrap" IsReadOnly="False" Height="25" Width="260" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_htnum" NotNull="True" 
												/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Margin="20,0,0,0" Text="签订日期" Height="27"/>
                                    <sdk:DatePicker SelectedDate="{m:Bind Path=f_signdate}" Margin="5,2" Width="137" Height="25" Grid.Row="4" Grid.Column="5" SelectedDateFormat="Short" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_signdate"
            									Default="{m:Exp Str=SysTime.Today}"/>
                                        </a:Data.Prop>
                                    </sdk:DatePicker>
                                </StackPanel>
                                <StackPanel Grid.ColumnSpan="4" Margin="0,0,20,0" Grid.Row="5" Orientation="Horizontal" Grid.Column="1" d:LayoutOverrides="GridBox">
                                    <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="小时用气量" Margin="8,0,0,0" Height="27"  />
                                    <TextBox Text="{m:Bind Path=f_xsgasnum}" TextWrapping="Wrap" Width="108" Margin="5,1,0,1" Height="25">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_xsgasnum" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="m2" Margin="5,0,0,0" Height="27" />
                                    <TextBlock Margin="49,0,0,0" TextWrapping="Wrap" TextAlignment="Center" Text="日用气量" Height="27" />
                                    <TextBox Text="{m:Bind Path=f_datenum}" Margin="8,0,0,0" TextWrapping="Wrap" Width="82" IsReadOnly="False" Height="25" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_datenum" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="m2" Margin="5,0,0,0" Height="27" />
                                    <TextBlock Margin="16,0,0,0" TextAlignment="Center" TextWrapping="Wrap" Text="月用气量" Height="27" />
                                    <TextBox Text="{m:Bind Path=f_monthenum}" Margin="5,0,0,0" TextWrapping="Wrap" Width="97" IsReadOnly="False" Height="25">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_monthenum" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock Margin="5,0,0,0" TextWrapping="Wrap" Text="m2" Height="27" />
                                </StackPanel>
                                <StackPanel Grid.ColumnSpan="5" Margin="0" Grid.Row="7" Orientation="Horizontal" Height="30">
                                    <TextBlock TextAlignment="Right" Margin="0,2,0,1" TextWrapping="Wrap" Text="供气方式" Width="133" Height="27" />
                                    <TextBox Text="{m:Bind Path=f_givegasfund}" Margin="11,3,0,2" TextWrapping="Wrap" Width="108" IsReadOnly="True" Height="25">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_givegasfund" 
            									Default="{m:Exp Str=$管道输送$}"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock HorizontalAlignment="Left" TextAlignment="Center" Margin="74,0,0,0" TextWrapping="Wrap" Text="供气单价" Height="27" />
                                    <TextBox Text="{m:Bind Path=f_givegasprice}" Margin="9,0,0,0" TextWrapping="Wrap"  Width="258" Height="25">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_givegasprice" Default="{m:Exp Str=0}"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock HorizontalAlignment="Left" Margin="5,0,0,0" TextWrapping="Wrap" Text="元/m2" Height="27" />
                                </StackPanel>
                                <TextBlock TextAlignment="Right" TextWrapping="Wrap" Text="供气质量及方式" Grid.Row="8" Grid.ColumnSpan="2" Margin="0,0,16,0" Height="25"/>
                                <TextBox Text="{m:Bind Path=f_givegaszl}" Grid.Column="2" Margin="1,2,0,2" Grid.Row="8" TextWrapping="Wrap" Grid.ColumnSpan="2" HorizontalAlignment="Left" Width="449" IsReadOnly="True" Height="25" d:LayoutOverrides="GridBox" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_givegaszl"
            								Default="{m:Exp Str=$符合天然气———SG7514标准$}" />
                                    </a:Data.Prop>
                                </TextBox>
                                <StackPanel Grid.ColumnSpan="4" Grid.Column="1" Margin="0" Grid.Row="4">
                                    <StackPanel Margin="0" VerticalAlignment="Top" Orientation="Horizontal">
                                        <TextBlock HorizontalAlignment="Left" Margin="0,0,10,0" TextWrapping="Wrap" Text="锅炉" TextAlignment="Center" VerticalAlignment="Center" />
                                        <StackPanel Margin="8,0,0,0" >
                                            <StackPanel Orientation="Horizontal" Width="700" >
                                                <TextBlock Text="       采暖炉" HorizontalAlignment="Left" Margin="0,5,0,0" TextWrapping="Wrap" Width="100" />
                                                <TextBox Text="{m:Bind Path=f_cnnum}" Margin="3,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="108" Height="25">
                                                    <a:Data.Prop>
                                                        <o:PropertySetter PropertyName="f_cnnum" />
                                                    </a:Data.Prop>
                                                </TextBox>
                                                <TextBlock Text="台" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap" />
                                                <TextBox Text="{m:Bind Path=f_cnm}" Margin="5,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="72" Height="25">
                                                    <a:Data.Prop>
                                                        <o:PropertySetter PropertyName="f_cnm" />
                                                    </a:Data.Prop>
                                                </TextBox>
                                                <TextBlock Text="MW" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap"/>
                                                <TextBlock Text="壁挂炉" HorizontalAlignment="Left" Margin="8,10,0,0" TextWrapping="Wrap" />
                                                <TextBox Text="{m:Bind Path=f_bglnum}" Margin="8,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="86" Height="25">
                                                    <a:Data.Prop>
                                                        <o:PropertySetter PropertyName="f_bglnum" />
                                                    </a:Data.Prop>
                                                </TextBox>
                                                <TextBlock HorizontalAlignment="Left" Height="19" Margin="5,0,0,0" TextWrapping="Wrap" Width="14"><Run Text="台"/><LineBreak/><Run/></TextBlock>
                                                <TextBox Text="{m:Bind Path=f_bglk}" Margin="5,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="97" Height="25"/>
                                                <TextBlock Text="MW" HorizontalAlignment="Left" Height="19" Margin="5,0,0,0" TextWrapping="Wrap" />
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" Width="700" >
                                                <TextBlock Text="       模块炉" HorizontalAlignment="Left" Margin="0,5,0,0" TextWrapping="Wrap" Width="100" />
                                                <TextBox Text="{m:Bind Path=f_mklm}" Margin="3,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="108" Height="25">
