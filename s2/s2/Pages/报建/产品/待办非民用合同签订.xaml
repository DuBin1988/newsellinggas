                                                        <o:PropertySetter PropertyName="f_mklm" />
                                                    </a:Data.Prop>
                                                </TextBox>
                                                <TextBlock Text="台" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap" />
                                                <TextBox Text="{m:Bind Path=f_mkm}" Margin="5,1,0,1" TextWrapping="Wrap" IsReadOnly="False" Width="72" Height="25">
                                                    <a:Data.Prop>
                                                        <o:PropertySetter PropertyName="f_mkm" />
                                                    </a:Data.Prop>
                                                </TextBox>
                                                <TextBlock Text="MW" HorizontalAlignment="Left" Margin="5,10,0,0" TextWrapping="Wrap"/>
                                            </StackPanel>
                                            <StackPanel Orientation="Horizontal" Margin="0" >
