<UserControl x:Class="Com.Aote.Pages.待办工程部民用审核"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit" 
    xmlns:ec="http://schemas.microsoft.com/expression/2010/controls"
    mc:Ignorable="d">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad>

            <!--流程启动对象-->
            <o:GeneralObject  Name="startprocess" WebClientInfo="{StaticResource server}" 
		        EntityType="t_test" 
	 	        IsInit="{m:Exp Str='this.State==$End$'}" CustomePath="{m:Exp Str=$/workflow/perfomactivity/\{gcbmyshList.SelectedItem.actid\}/\{LoginUser.name\}/\{LoginUser.id\}}"
                 >
            </o:GeneralObject>

            <o:PagedList Name="gcbmysh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='gcbmyshInfo.DataContext.State==$End$ |or| SubmitAction.State==$End$ |or| gcbmyshPager.DataContext.State==$End$ |or| gcbmyshBack=>Click!=null'}"
                Names="id,f_substate,f_code,f_username,f_phone,f_applydept,f_address,f_applycount,f_date,actid,f_processid" 
                HQL="{m:Exp Str=$select apply.id\,apply.f_substate\,apply.f_code\,apply.f_username\,apply.f_phone\,apply.f_applydept\,apply.f_address\,apply.f_applycount\,CONVERT(varchar\, apply.f_date\,23)f_date\,act.id actid\,apply.f_processid
                from activityins act 
                join t_actorperson p 
                on act.actorexpression \= p.actorid 
                join t_apply apply 
                on apply.f_processid\=act.processid 
                where \{gcbmyshSearch.DataContext.Condition\} and act.state\=\'开始活动\' and act.processName\=\'报装流程\' and act.defname\=\'工程部审核\' and p.userid\=\'\{LoginUser.ename\}\'}"
                Path="sql" PageIndex="{m:Exp Str=gcbmyshPager.PageIndex}" PageSize="{m:Exp Str=gcbmyshPager.PageSize}" SumNames="," />

            <o:GeneralObject Name="gcbmyshObj" EntityType="t_apply" WebClientInfo="{StaticResource dbclient}" 
			    Path="{m:Exp Str=gcbmyshList.SelectedItem \!\=null:$one/from t_apply where id\=\{gcbmyshList.SelectedItem.id\}$\,$null$}"  
                IsInit="{m:Exp Str='this.State==$End$ or gcbmyshBack=>Click!=null'}">
                <o:PropertySetter PropertyName="f_applycustomer" Default="{m:Exp Str=userlist_gcbmysh}" />
                <o:PropertySetter PropertyName="f_applyimage" Default="{m:Exp Str=imagelist_gcbmysh}" />

            </o:GeneralObject>








            <o:ObjectList Name="fwlxlist" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='房屋类型'"/>
            <!--工程部民用审核-->
           <o:ObjectList Name="userlist_gcbmysh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='gcbmyshInfo.DataContext.State==$End$ or gcbmyshBack=>Click!=null'}"
            Path="{m:Exp Str='gcbmyshInfo.DataContext.id!=null:$from t_applycustomer where applyid={gcbmyshInfo.DataContext.id}$,null'}" EntityType="t_applycustomer" HasEmptyRow="True" />
            <o:ObjectList Name="userimagelist_gcbmysh" Source="{m:Exp Str=nopeople_gcbmysh.SelectedItem.f_customerimage}" EntityType="t_customerimage" />
            <o:ObjectList Name="imagelist_gcbmysh" WebClientInfo="{StaticResource dbclient}" IsOld="{m:Exp Str='gcbmyshInfo.DataContext.State==$End$ or gcbmyshBack=>Click!=null'}"
            Path="{m:Exp Str='gcbmyshInfo.DataContext.id!=null:$from t_applyimage where applyid={gcbmyshInfo.DataContext.id}$,null'}" EntityType="t_applyimage" />
			
                    
			<b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_apply set f_gcbshzt\=\'已完成\' \,f_substate\=null where id in (\{gcbmyshList.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/>
        </m:ResourceLoad>
        <!--工程部民用审核-->
        <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=gcbmysh.IsBusy}">
            <StackPanel Margin="2,0,2,8">
                <StackPanel HorizontalAlignment="Left" Orientation="Horizontal" Width="488">
                    <toolkit:WrapPanel x:Name="gcbmyshSearch" Width="405">
	                	<a:Data.Context>
	                        <o:SearchObject CanSearch="{m:Exp Str='LayoutRoot=>Loaded!=null'}" />
	                    </a:Data.Context>
	                    <TextBlock TextWrapping="Wrap" Text="工程部民用审核" Margin="5,0,20,0"/>
	                    <TextBlock TextWrapping="Wrap" Text="申请人：" Margin="5,0,0,0"/>
	                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" MinWidth="30" Margin="5,0,0,0" Height="22">
	                        <a:Data.Prop>
	                            <o:PropertySetter PropertyName="f_username" Operator="f_username like 'this%'"/>
	                        </a:Data.Prop>
	                    </TextBox>
	                    <TextBlock TextWrapping="Wrap" Text="电 话：" Margin="5,0,0,0"/>
	                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_phone}" MinWidth="30" Margin="5,0,0,0" Height="22">
	                        <a:Data.Prop>
	                            <o:PropertySetter PropertyName="f_phone"  Operator="f_phone = 'this'"/>
	                        </a:Data.Prop>
	                    </TextBox>
	                    <TextBlock TextWrapping="Wrap" Text="地址：" Margin="5,0,0,0"/>
	                    <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_address}" MinWidth="30" Margin="5,0,0,0" Height="22">
	                        <a:Data.Prop>
	                            <o:PropertySetter PropertyName="f_address"  Operator="f_address like '%this%'"/>
	                        </a:Data.Prop>
	                    </TextBox>
	                    <TextBlock TextWrapping="Wrap" Text="状态：" Margin="5,0,0,0"/>
	                    <CheckBox IsChecked="{m:Bind Path=isOver}" IsThreeState="True" Content="完成" MinWidth="30" Margin="5,0,0,0" Height="22">
	                        <a:Data.Prop>
	                            <o:PropertySetter PropertyName="isOver" Operator="1=1"/>
	                        </a:Data.Prop>
	                    </CheckBox>
						<TextBox TextWrapping="Wrap" Text="{m:Bind Path=isOvered}" Visibility="Collapsed" MinWidth="30" Margin="5,0,0,0" Height="22">
	                        <a:Data.Prop>
	                            <o:PropertySetter PropertyName="isOvered" Value="{m:Exp Str=this.isOver\=\=null:$1\=1$\,this.isOver:$f_substate\=\'完成\'$\,!this.isOver:$f_substate is null$\,$1\=1$}" Operator="this" />
	                        </a:Data.Prop>
	                    </TextBox>
	                    <Button Content="查 询" Click="{m:Exp Str=gcbmyshSearch.DataContext.Search()}" Margin="0" HorizontalAlignment="Left" Width="60" />
	                </toolkit:WrapPanel>
					<Button Content="打印选中列表" Click="{m:Exp Str=print_gcbmysh1.Print()}" Height="25" Width="80" Margin="4,0,4,0"/>
				</StackPanel>
                <sdk:DataGrid Name="gcbmyshList" AutoGenerateColumns="False" AlternatingRowBackground="{x:Null}" Background="{x:Null}" IsReadOnly="True" Height="300" Width="488"
								ItemsSource="{m:Res Key=gcbmysh}">
                    <sdk:DataGrid.Columns >
                        <sdk:DataGridTemplateColumn d:IsLocked="True" Header="序号" >
                            <sdk:DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                        <Button Content="  ->>  " Click="{m:Exp Str='gcbmyshPopup.BringToUp(waitGrid);gcbmyshPopup.IsOpen=True'}" />
                                        <TextBlock Text="{Binding Index}" />
                                    </StackPanel>
                                </DataTemplate>
                            </sdk:DataGridTemplateColumn.CellTemplate>
                        </sdk:DataGridTemplateColumn>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="状态" Binding="{Binding f_substate}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                        <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                        <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                        <sdk:DataGridTextColumn Header="用户类型" Binding="{Binding f_usertype}"/>
                        <sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>

                        <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date,StringFormat=yyyy-MM-dd}"/>
                    </sdk:DataGrid.Columns>
                </sdk:DataGrid>
                <StackPanel Orientation="Horizontal" Width="488" Height="25">
                    <Button Content="提交收费标准" Margin="5,0" Click="{m:Exp Str=submittip.Show()}" IsEnabled="{m:Exp Str='gcbmyshList.SelectedItem!=null and gcbmyshList.SelectedItem.f_substate!\=null'}" Width="100" />
                    <c:MessageTipObj Name="submittip" Tip="您确认要提交收费标准吗？" OK="{m:Exp Str=startprocess.id\=gcbmyshList.SelectedItem.f_processid;SubmitAction.Invoke();startprocess.Save()}" />
					<sdk:DataPager Name="gcbmyshPager" Source="{Binding Path=ItemsSource.Count,ElementName=gcbmyshList,Converter={StaticResource DataPagerConverter}}"
		                    HorizontalAlignment="Left" PageSize="20" Width="378" Height="24" VerticalAlignment="Bottom" d:LayoutOverrides="GridBox"/>
                    <TextBlock Text="总待办数:" Margin="-300,5,260,0" />
                    <TextBlock Text="{m:Exp Str='gcbmysh.Count'}"  Margin="-250,5,210,0" />
					<c:PrintObj x:Name="print_gcbmysh1" UseDefaultPrinter="False" Area="{m:Res Key=personsprint_gcbmysh1}"/>
                    <sdk:DataGrid x:Name="personsprint_gcbmysh1" Margin="0,0,0,-2525" ItemsSource="{m:Exp Str='gcbmyshList.SelectedItems.ToObjectList()=>SelectionChanged'}"		   
                	                AutoGenerateColumns="False" Width="Auto" IsReadOnly="True" Height="Auto" VerticalAlignment="Bottom" Background="{x:Null}" RowBackground="{x:Null}"
                     Style="{StaticResource PrintDataGrid}" ColumnHeaderStyle="{StaticResource PrintDataGridColumnHeader}" CellStyle="{StaticResource PrintDataGridCell}" RowHeaderStyle="{StaticResource PrintDataGridRowHeader}" RowStyle="{StaticResource PrintDataGridRow}">
                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="报建编号" Binding="{Binding f_code}"/>
                            <sdk:DataGridTextColumn Header="类型" Binding="{Binding f_usertype}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="申请人" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="电话" Binding="{Binding f_phone}"/>
                            <sdk:DataGridTextColumn Header="片区" Binding="{Binding f_area}"/>
                            <sdk:DataGridTextColumn Header="用气地址" Binding="{Binding f_address}"/>
                            <sdk:DataGridTextColumn Header="申请户数" Binding="{Binding f_applycount}"/>
                            <sdk:DataGridTextColumn Header="报建日期" Binding="{Binding f_date,StringFormat=yyyy-MM-dd}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </StackPanel>
            </StackPanel>
        </c:BusyIndicatorEx>
        <!--工程部民用审核-->
        <Popup x:Name="gcbmyshPopup">
		    <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=gcbmyshObj.IsBusy or userlist_gcbmysh.IsBusy or imagelist_gcbmysh.IsBusy}">
                <Grid Background="{StaticResource LoginBgColorBrush1}" x:Name="gcbmyshInfo" DataContext="{m:Exp Str=gcbmyshObj}" Width="1000" Height="640">
                    <sdk:TabControl  Margin="0,0,0,55" VerticalAlignment="Stretch" d:LayoutOverrides="Height" >
                        <sdk:TabItem Header="报建基本信息">
                        	<ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                     		<Grid HorizontalAlignment="Stretch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="50"/>
                                    <RowDefinition Height="29"/>
                                    <RowDefinition Height="50"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="110"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="110"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="110"/>
                                    <ColumnDefinition Width="1*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Margin="0,6,0,0" TextWrapping="Wrap" Text="报建编号：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_code}" IsReadOnly="True" Grid.Column="1" Margin="0,2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="1" Grid.RowSpan="1" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_code"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" TextWrapping="Wrap" Text="报建日期：" TextAlignment="Right"/>
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_date}" IsEnabled="false" Grid.Column="3" Margin="0,3"  Height="Auto" Grid.ColumnSpan="1" Grid.RowSpan="1" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_date"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <TextBlock Grid.Column="4" Margin="0,6,0,0" TextWrapping="Wrap" Text="申 请 人：" TextAlignment="Right"/>
                                <TextBox IsReadOnly="True" Text="{m:Bind Path=f_username}"  Grid.Column="5" Margin="0,2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="1" Grid.RowSpan="1" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_username"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="电 话：" TextAlignment="Right"/>
                                <TextBox IsReadOnly="True" Text="{m:Bind Path=f_phone}"  Grid.Column="1" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="1" Grid.RowSpan="1" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_phone"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="2" TextWrapping="Wrap" Text="用气地址：" TextAlignment="Right"/>
                                <TextBox IsReadOnly="True" Text="{m:Bind Path=f_address}"  Grid.Column="3" Margin="0,2" Grid.Row="2" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Grid.RowSpan="1" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_address"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Margin="0,6,0,0" Grid.Row="6" TextWrapping="Wrap" Text="片 区：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_area}"  Grid.Column="1" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" IsReadOnly="True" Height="Auto" Grid.ColumnSpan="1" Grid.RowSpan="1" Width="Auto" VerticalAlignment="Top">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_area"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="用户类型：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="1" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_usertype"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="房屋类型：" TextAlignment="Right"/>
                                <ComboBox IsEnabled="False" DisplayMemberPath="name" SelectedValuePath="name" ItemsSource="{m:Exp Str=fwlxlist}" SelectedValue="{m:Bind Path=f_usertypepro}"   Grid.Column="3" Margin="0,2" Grid.Row="3"   Height="Auto"  Width="Auto">
                                <a:Data.Prop>
									<o:PropertySetter PropertyName="f_usertypepro" NotNull="True" ></o:PropertySetter>
								</a:Data.Prop>
								</ComboBox>
								<TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="3" TextWrapping="Wrap" Text="申请户数：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_applycount}"  Grid.Column="5" Margin="0,2" Grid.Row="3" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_applycount"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Margin="0,6,0,0" Grid.Row="4" TextWrapping="Wrap" Text="放 线 人：" TextAlignment="Right"/>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="4" TextWrapping="Wrap" Text="放线日期：" TextAlignment="Right"/>
                                <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="4" TextWrapping="Wrap" Text="安装户数：" TextAlignment="Right"/>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="设 计 人：" TextAlignment="Right"/>
                                <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="设计审核日期：" TextAlignment="Right"/>
                                <TextBlock Grid.Column="2" Margin="0" Grid.Row="8" TextWrapping="Wrap" Text="工程部意见：" TextAlignment="Right" VerticalAlignment="Center"/>
                                <TextBox Text="{m:Bind Path=f_survey}" Grid.Column="1" Margin="0,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_survey" />
                                    </a:Data.Prop>
                                </TextBox>
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_surveydate}" Grid.Column="3" Margin="0,3" Grid.Row="4" IsEnabled="False" Height="Auto" Width="Auto" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_surveydate" />
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <TextBox IsReadOnly="True" Text="{m:Bind Path=f_installnum}" Grid.Column="5" Margin="0,2" Grid.Row="4" TextWrapping="Wrap" Height="Auto" Width="Auto" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_installnum"  NotNull="True" />
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock  Margin="0,6,0,0" Grid.Row="5" TextWrapping="Wrap" Text="交费户数：" TextAlignment="Right" />
                                <TextBox Text="{m:Bind Path=f_payfeenum}" Grid.Column="1" Margin="0,2" Grid.Row="5" TextWrapping="Wrap" IsReadOnly="True" Height="Auto" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_payfeenum" Value="{m:Exp Str=this.f_installnum}" Default="{m:Exp Str=this.f_installnum}"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <CheckBox Content="可以进行设计" IsChecked="{m:Bind Path=f_designe}" Grid.Column="0" Margin="0,6,0,0" Grid.Row="7" IsEnabled="False" Grid.ColumnSpan="1" Height="Auto" HorizontalAlignment="Left" d:LayoutOverrides="GridBox" />
                                <TextBox Text="{m:Bind Path=f_designers}" Grid.Column="3" Margin="0,2" Grid.Row="5" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_designers" />
                                    </a:Data.Prop>
                                </TextBox>
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_sjexamdate}" Grid.Column="5" Margin="0,3" Grid.Row="5" IsEnabled="False" Height="Auto" Width="Auto" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_sjexamdate" />
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <CheckBox IsChecked="{m:Bind Path=f_havepay}" Grid.Column="0" Margin="0,6,0,0" Grid.Row="8"  Height="Auto" Content="具备安装交款" Grid.ColumnSpan="2" VerticalAlignment="Stretch">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_havepay"  Default="{m:Exp Str=False}"/>
                                    </a:Data.Prop>
                                </CheckBox>
                             <!--   <CheckBox Content="民用转商用" IsChecked="{m:Bind Path=f_isminzhsha}" IsEnabled="False" Grid.Column="1" Margin="0,6,0,0" Grid.Row="7" Height="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_isminzhsha" />
                                    </a:Data.Prop>
                                </CheckBox>-->
                                <TextBox Text="{m:Bind Path=f_opinion}" Grid.Column="3" Margin="0,2" Grid.Row="8" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Grid.RowSpan="1" HorizontalAlignment="Stretch" d:LayoutOverrides="GridBox" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_opinion" />
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Margin="0,6,0,0" Grid.Row="9" TextWrapping="Wrap" Text="部 门：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_gcbshdepartment}" Grid.Column="1" Margin="0,2" Grid.Row="9" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_gcbshdepartment" NotNull="True" 
        								Default="{m:Exp Str=LoginUser.f_parentname}"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Grid.Column="2" Margin="0,6,0,0" Grid.Row="9" TextWrapping="Wrap" Text="操 作 人：" TextAlignment="Right"/>
                                <TextBox Text="{m:Bind Path=f_gcbshopertor}" Grid.Column="3" Margin="0,2" Grid.Row="9" TextWrapping="Wrap" Height="Auto" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_gcbshopertor" NotNull="True"  
        								Default="{m:Exp Str=LoginUser.name}"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock Grid.Column="4" Margin="0,6,0,0" Grid.Row="9" TextWrapping="Wrap" Text="日 期：" TextAlignment="Right"/>
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_gcbshoperdate}" Grid.Column="5" Margin="0,3" Grid.Row="9" IsEnabled="False" Height="Auto" VerticalAlignment="Stretch" Width="Auto" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_gcbshoperdate" NotNull="True" 
        								Default="{m:Exp Str=SysTime.Today}"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <TextBlock Grid.Column="2" Margin="0" Grid.Row="6" TextWrapping="Wrap" Text="勘查意见：" TextAlignment="Right" VerticalAlignment="Center"/>
                                <TextBox Text="{m:Bind Path=f_gcbkcopinion}" Grid.Column="3" Margin="0,2" Grid.Row="6" TextWrapping="Wrap" Height="Auto" Grid.ColumnSpan="3" Grid.RowSpan="1" VerticalAlignment="Stretch" IsReadOnly="True" Width="Auto">
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_gcbkcopinion" />
