<c:CustomChildWindow 
           x:Class="s2.Pages.个人抄表信息界面"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
		   xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
		   xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	       xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
           xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
		   xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
		   xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="800" Height="500"   FontSize="14"
           Title="个人抄表信息界面" Name="child">
    <Grid x:Name="LayoutRoot2" Grid.Row="3" Grid.Column="3"   Margin="12,0,12,0"  DataContext="{m:Exp Str=child.ParamValue}" >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>

            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"  />
        <TextBox Text="{m:Bind Path=f_userid}" IsReadOnly="True" Grid.Column="1" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_userid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="责任部门：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_zerenbumen}" IsReadOnly="True" Grid.Column="3" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_zerenbumen" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="计划年月：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_handdate, StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_handdate" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="表状态：" Margin="0" TextWrapping="Wrap" Grid.Row="1"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_meterstate}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_meterstate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="旧用户编号：" Margin="139,1" TextWrapping="Wrap"  Grid.Column="1" Grid.Row="1" TextAlignment="Right" Height="25" Grid.ColumnSpan="3"/>
        <TextBox Text="{m:Bind Path=f_olduserid}" IsReadOnly="True" Grid.Column="3" Grid.Row="1" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_olduserid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="用户姓名：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="1" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="5" Grid.Row="1" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_username" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="地址：" Margin="0" TextWrapping="Wrap" Grid.Row="2"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="True" Grid.Column="1" Grid.ColumnSpan="5" Margin="0" Grid.Row="2"   Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_address" />
            </a:Data.Prop>
        </TextBox>


        <TextBlock Text="上次抄表底数：" Margin="0" TextWrapping="Wrap" Grid.Row="3"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=lastinputgasnum}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="3"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="lastinputgasnum" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="本次抄表底数：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=lastrecord}" IsReadOnly="True" Grid.Column="3" Grid.Row="3" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="lastrecord" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="本月用气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=oughtamount}" IsReadOnly="True" Grid.Column="5" Grid.Row="3" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="oughtamount" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="气  价：" Margin="0" TextWrapping="Wrap" Grid.Row="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_gasprice}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="4"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_gasprice" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="本月用气量金额：" Margin="0,5,0,0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=oughtfee}" IsReadOnly="True" Grid.Column="3" Grid.Row="4" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="oughtfee" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="抄表状态：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_state}" IsReadOnly="True" Grid.Column="5" Grid.Row="4" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_state" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="是否下账：" Margin="0" TextWrapping="Wrap" Grid.Row="5"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_xiazhang}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="5"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_xiazhang" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="是否缴费：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=shifoujiaofei}" IsReadOnly="True" Grid.Column="3" Grid.Row="5" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="shifoujiaofei" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="抄 表 员：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_inputtor}" IsReadOnly="True" Grid.Column="5" Grid.Row="5" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_inputtor" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="抄表日期：" Margin="0" TextWrapping="Wrap" Grid.Row="6"  TextAlignment="Right" Height="25"/>

        <TextBox  Text="{Binding lastinputdate, StringFormat=yyyy-MM-dd}" IsReadOnly="True"  Grid.Column="1" Margin="0" Grid.Row="6"   Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="lastinputdate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="网　　点：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_network}" IsReadOnly="True" Grid.Column="3" Grid.Row="6" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_network" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="录 入 员：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_operator}" IsReadOnly="True" Grid.Column="5" Grid.Row="6" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_operator" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="录入日期：" Margin="0" TextWrapping="Wrap" Grid.Row="7"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_inputdate, StringFormat=yyyy-MM-dd}"  IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="7"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_inputdate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="录入时间：" Margin="0" TextWrapping="Wrap" Grid.Column="2" Grid.Row="7"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stime}" IsReadOnly="True" Grid.Column="3" Margin="0" Grid.Row="7"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stime" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="交费截止日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="8" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_endjfdate, StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="3" Grid.Row="8" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_endjfdate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶段开始日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="7" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stardate}" IsReadOnly="True" Grid.Column="5" Grid.Row="7" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stardate" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="阶段结束日期：" Margin="0" TextWrapping="Wrap" Grid.Row="8"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_enddate}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="8"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_enddate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯一气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="9" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair1amount}" IsReadOnly="True" Grid.Column="3" Grid.Row="9" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair1amount" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯一金额：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="9" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair1fee}" IsReadOnly="True" Grid.Column="5" Grid.Row="9" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair1fee" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="阶梯一单价：" Margin="0" TextWrapping="Wrap" Grid.Row="9"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair1price}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="9"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair1price" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯二气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="10" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair2amount}" IsReadOnly="True" Grid.Column="3" Grid.Row="10" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair2amount" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯二金额：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="10" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair2fee}" IsReadOnly="True" Grid.Column="5" Grid.Row="10" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair2fee" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯二单价：" Margin="0" TextWrapping="Wrap" Grid.Row="10"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair2price}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="10"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair2price" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯三气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="11" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair3amount}" IsReadOnly="True" Grid.Column="3" Grid.Row="11" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair3amount" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯三金额：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="11" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair3fee}" IsReadOnly="True" Grid.Column="5" Grid.Row="11" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair3fee" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯三单价：" Margin="0" TextWrapping="Wrap" Grid.Row="11"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair3price}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="11"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair3price" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯四气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair4amount}" IsReadOnly="True" Grid.Column="3" Grid.Row="12" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair4amount" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯四金额：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair4fee}" IsReadOnly="True" Grid.Column="5" Grid.Row="12" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair4fee" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="阶梯四单价：" Margin="0" TextWrapping="Wrap"  Grid.Column="0" Grid.Row="12" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_stair4price}" IsReadOnly="True" Grid.Column="1" Grid.Row="12" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_stair4price" />
            </a:Data.Prop>
        </TextBox>

        <!--<Button x:Name="CancelButton" Content="取消" Click="CancelButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,17,55,-10" Grid.Row="12" Grid.Column="4" />-->
        <Button x:Name="OKButton" Content="确定" Click="OKButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,30,92,-23" Grid.Row="13" Grid.Column="2" Grid.ColumnSpan="2" />
    </Grid>
</c:CustomChildWindow>