<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.非民用报建登记">

    <Grid x:Name="LayoutRoot">

        <m:ResourceLoad Margin="0,0,-101,0">
            <!--气表品牌列表-->
            <o:ObjectList Name="pinpailist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from qibiao as qibiao left join fetch qibiao.qbnumber}"/>
            <!--用气性质列表-->
            <o:ObjectList Name="usegaslist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproper as gasproper left join fetch gasproper.pricetype where f_gasproperties\=\'非民用\' or f_gasproperties\=\'混合型\'}"/>

            <!--单值用户编号对象ksnumobj.value-->
            <o:GeneralObject Name="ksnumobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				Path="{m:Exp Str=$one/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\用\户\编\号\'}" 
				IsInit="{m:Exp Str=this.State\=\=$End\=>Completed!\=null and userfilego.button\=\=$temp_feisave$}" 	>
            </o:GeneralObject>
            <o:ObjectList Name="zerenbumenlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='责任部门'"/>
            <o:ObjectList Name="chaobiaoyuanlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='抄表员'"/>
            <!--单值用户编号对象,用于保存时获取用户编号-->
            <o:GeneralObject Name="ksnumobj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				LoadOnPathChanged="False"  Path="{m:Exp Str=$sernum/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\用\户\编\号\' /value$}" 
				  >
            </o:GeneralObject>

            <o:ObjectList Name="yhxzlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用户性质'">
            </o:ObjectList>


            <!--打开子窗口对象-->
            <o:ChildWindowObj Name="openkafei" CWName="/Pages/高级管理员登录"/>
            <!--单值对象-->
            <o:GeneralObject Name="whfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='维护费推后月数'"   ></o:GeneralObject>
            <o:GeneralObject Name="dkfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='大卡发卡费' "   ></o:GeneralObject>
            <o:GeneralObject Name="xkfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='小卡发卡费' "   ></o:GeneralObject>
            <o:GeneralObject Name="mzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用单次最大购气量'"   ></o:GeneralObject>
            <o:GeneralObject Name="fzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用单次最大购气量'"   ></o:GeneralObject>
            <!--混合气价对象 -->
            <o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=$one/from t_hunhe }">

            </o:GeneralObject>
            <!--用气设备信息-->
            <o:ObjectList Name="devicelist" EntityType="t_devices" HasEmptyRow="True"></o:ObjectList>
            <!--批量删除语句-->
            <b:BatchExcuteAction Name="DeleteAction" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=DeleteDevice}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=DeleteApply}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>
            <b:HQLAction Name="DeleteApply"  WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str='$delete from t_userfiles where id in ({userfiles.SelectedItems.ToString(id)=>SelectionChanged})'}">
            </b:HQLAction>
            <b:HQLAction Name="DeleteDevice"  WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str='$delete from t_device where userfileid in ({userfiles.SelectedItems.ToString(id)=>SelectionChanged})'}">
            </b:HQLAction>
            <!--清除列表中的垃圾数据-->
            <b:HQLAction Name="DeleteDirty" 
			HQL="{m:Exp Str='devicelist.Dirty.Count!=0:$delete from t_devices where id in ({devicelist.Dirty[True].ToString(id)})$,null'}">
            </b:HQLAction>
            <!--保存，保存时要删除列表里的垃圾数据-->
            <b:BatchExcuteAction Name="BatchSave"  WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=DeleteDirty}" MethodName="InvokeToJson"></b:BatchInfo>
                <b:BatchInfo Source="{m:Exp Str=userfile.DataContext}" MethodName="SaveToJson"></b:BatchInfo>
            </b:BatchExcuteAction>
            <!--提交-->
            <b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_userfiles set f_state\=\'建档完成\' \,f_substate\=null\,f_whethergivecard\=\'已发\' where id in (\{userfiles.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\' znd f_pregas is not null}"/>
            <b:SyncActionFactory Name="SaveAction"   >
                <!--获取编号-->
                <b:CallMethodAction    Name="getbianhao" TargetObject="{m:Exp Str=ksnumobj1}"    MethodName="Load"/>
                <b:CallMethodAction    Name="saveu" TargetObject="{m:Exp Str=userfile.DataContext}"    MethodName="Save"/>
            </b:SyncActionFactory>
            <o:GeneralObject  Name="userfilego" WebClientInfo="{StaticResource dbclient}" EntityType="t_userfiles" 
			Source="{m:Exp Str=userfiles.SelectedItem}" IsInit="{m:Exp Str=SaveAction.State\=\=$End}">
                <o:PropertySetter  PropertyName="f_substate" Default="完成"/>
                <!--流转状态-->
                <o:PropertySetter  PropertyName="f_lzstate" Default="预备"/>
                <o:PropertySetter PropertyName="f_device" Default="{m:Exp Str=devicelist}"></o:PropertySetter>
                <!--基表品牌-->
                <o:PropertySetter PropertyName="f_jbfactory"/>
                <!--机表型号-->
                <o:PropertySetter PropertyName="f_jbbrand"/>
                <!--机表厂家-->
                <o:PropertySetter PropertyName="f_jbtype"/>
                <!--表底数-->
                <o:PropertySetter PropertyName="f_gasmeteraccomodations" Default="0"  NotNull="True"/>
                <!--海力表型编码-->
                <o:PropertySetter PropertyName="f_watchnum" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_watchnum}" />
                <!--脉冲/表系数-->
                <o:PropertySetter PropertyName="f_maichong" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_maichong}" />
                <!--门站-->
                <o:PropertySetter PropertyName="f_menzhan" Value="{m:Exp Str=userfiles.SelectedItem.f_menzhan}" Default="{m:Exp Str=userfilego.f_menzhan}" />
                <!--责任部门-->
                <o:PropertySetter PropertyName="f_zerenbumen" Value="{m:Exp Str=userfiles.SelectedItem.f_zerenbumen}" Default="{m:Exp Str=userfilego.f_zerenbumen}" />
                <!--抄表人-->
                <o:PropertySetter PropertyName="f_inputtor" Value="{m:Exp Str=userfiles.SelectedItem.f_inputtor}" Default="{m:Exp Str=userfilego.f_inputtor}" />
            </o:GeneralObject>
        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.25*"/>
                <ColumnDefinition Width="0.75*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
                <toolkit:DockPanel >
                    <toolkit:DockPanel  Height="45"  Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0">
                        <TextBlock TextWrapping="Wrap" Margin="30,0,5,2"  Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="237" HorizontalAlignment="Stretch" Foreground="#FF140000" VerticalAlignment="Bottom" Height="19" />
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" RenderTransformOrigin="0.5,0.5" Width="18" Margin="0,0,0,21">
                            <StackPanel.RenderTransform>
                                <CompositeTransform ScaleX="-1"/>
                            </StackPanel.RenderTransform>
                        </StackPanel>
                    </toolkit:DockPanel>
                    <Grid x:Name="userfilesearch" toolkit:DockPanel.Dock="Top"  Background="#FFCFEFFF" Margin="0,0,0,1" Height="Auto">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                        </Grid.RowDefinitions>
                        <a:Data.Context>
                            <o:SearchObject />
                        </a:Data.Context>
                        <StackPanel Orientation="Horizontal" Margin="5,0,0,0">
                            <TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="0,4" />
                            <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_userid}" Width="176" Margin="0,3,0,2" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid"  Operator="f_userid = 'this'"/>
                                </a:Data.Prop>
                            </TextBox>
                        </StackPanel>
                        <StackPanel Grid.Row="2" Margin="5,0,0,0" Orientation="Horizontal" Grid.RowSpan="1" >
                            <TextBlock TextWrapping="Wrap" Text="建档日期：" Margin="0,4" />

                            <sdk:DatePicker SelectedDate="{m:Bind Path=f_yytdate}" Margin="0,3,0,2"  Height="25" VerticalAlignment="Stretch" Width="117"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_yytdate" 
												Operator=" (f_yytdate, StringFormat=yyyy-MM-dd)  = cast('this' as date)"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>
                        </StackPanel>
                        <toolkit:DockPanel  Grid.Row="2">
                            <Button Content="查  询" x:Name="searchbutton" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,-2,5,2" HorizontalAlignment="Right" Width="50" d:LayoutOverrides="Height"/>
                        </toolkit:DockPanel>
                        <StackPanel Orientation="Horizontal" Margin="5,-1,0,1" Grid.RowSpan="1" Grid.Row="1">
                            <TextBlock TextWrapping="Wrap" Text="单位名称：" Margin="0,4" />
                            <TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_username}" Width="176" Margin="0,3,0,2" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_username"  Operator="f_username like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                        </StackPanel>
                    </Grid>
                    <sdk:DataGrid x:Name="userfiles" Margin="0,1"  
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True">
                        <a:Data.Items>
                            <o:PagedObjectList x:Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
								Path="{m:Exp Str=$from t_userfiles where \{userfilesearch.DataContext.Condition\} and f_usertype\=\'非民用\'  and f_userstate\=\'预备\' and f_filiale \=\'\{LoginUser.f_fengongsi\}\' }"
								IsOld="{m:Exp Str='SaveAction.State\=\=$End$ or userfile.DataContext.State\=\=$End$ or BatchSave.State==$End$'}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="f_times"/>
                        </a:Data.Items>
                        <sdk:DataGrid.Columns>

                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}"/>
                            <sdk:DataGridTextColumn Header="单位名称" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="操作日期" Binding="{Binding f_yytdate,StringFormat=yyyy-MM-dd}"/>
                            <sdk:DataGridTextColumn Header="联系人" Binding="{Binding f_linkname}"/>
                            <sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                    <!--提交按钮-->
                </toolkit:DockPanel >
            </c:BusyIndicatorEx>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy or SaveAction.IsBusy}" Margin="5,0,0,0" Grid.Column="1">
                <Grid  x:Name="userfile" DataContext="{m:Exp Str=userfilego}">

                    <toolkit:DockPanel>
                        <!--按钮-->
                        <StackPanel  HorizontalAlignment="Center" toolkit:DockPanel.Dock="Bottom" Orientation="Horizontal" Width="293" >
                            <!--IsEnabled="{m:Exp Str='(data.IsModified or devicelist.IsModified) and !data.HasErrors and !devicelist.HasErrors'}"-->
                            <Button x:Name="save" Content="保  存" Margin="5" 
								IsEnabled="{m:Exp Str=userfiles.SelectedItem\=\=null and userfilego.id\=\=null}" 
								Click="{m:Exp Str=userfilego.button\=$temp_feisave$;SaveAction.Invoke()}" Width="50" />
                            <Button x:Name="edite" Content="修  改" Margin="5" 
							IsEnabled="{m:Exp Str=userfiles.SelectedItem!\=null}" 
							Click="{m:Exp Str=userfilego.button\=$temp_feiedit$;userfile.DataContext.Save()}" Width="50" />
                            <Button Content="撤  销" Margin="5" Click="{m:Exp Str=userfile.DataContext.New()}" Width="50"/>
                            <Button Content="打印流程表" Margin="5" Click="{m:Exp Str='
							userfilego.f_lccode\=userfilego.f_lccode!=$$:userfilego.f_lccode,$\{SysTime.Now.ToString($yyyy$)\}AD\{Seriaadyb.Value\}$;userfilego.button\=$temp_feilc$;
							userfilego.Save()'}" Width="80" />
                        </StackPanel>
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" toolkit:DockPanel.Dock="Top">
                            <toolkit:DockPanel>
                                <Grid toolkit:DockPanel.Dock="Top" Margin="0" HorizontalAlignment="Stretch" >
                                    <Grid.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                            <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                            <GradientStop Color="#FFDDF2FD" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Grid.Background>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="1"/>
                                        <RowDefinition Height="1"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="1"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>

                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="0"/>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="14"/>
                                        <RowDefinition Height="0"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="130"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="130"/>
                                        <ColumnDefinition Width="1*"/>
                                        <ColumnDefinition Width="110"/>
                                        <ColumnDefinition Width="1*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Text="单位名称：" Grid.Column="2" Margin="0,5,0,0" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="2"/>
                                    <TextBlock Text="联 系 人：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Height="25"/>
                                    <TextBlock Text="地　　址：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="2" Grid.Column="4"/>
                                    <TextBlock Text="证件类型：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Row="4"/>
                                    <TextBlock Text="电　　话：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Grid.Row="3" Height="25"/>
                                    <TextBlock Text="用气设备：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="9" Height="25"/>
                                    <TextBlock Text="协议号：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="9" Height="25" Grid.Column="2"/>
                                    <TextBlock Text="用气性质：" Margin="0,2,0,3" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" d:LayoutOverrides="VerticalAlignment"/>
                                    <TextBlock Text="气价类型：" Grid.Column="2" Margin="0,2,0,3" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" d:LayoutOverrides="VerticalAlignment"/>
                                    <TextBlock Text="气　　价：" Grid.Column="4" Margin="0,2,0,3" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" d:LayoutOverrides="VerticalAlignment"/>
                                    <Border  Grid.Row="13" Margin="5,0,0,0" BorderThickness="0,0,0,3"  Grid.ColumnSpan="6" >
                                        <Border.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFC6E2FB" Offset="1"/>
                                                <GradientStop Color="#FFE6F1FB"/>
                                            </LinearGradientBrush>
                                        </Border.Background>
                                    </Border>
                                    <Border  Grid.Row="23" Margin="0,0,0,2" BorderThickness="0,0,0,3"  Grid.ColumnSpan="6" Height="14" VerticalAlignment="Bottom" >
                                        <Border.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFC6E2FB" Offset="1"/>
                                                <GradientStop Color="#FFE6F1FB"/>
                                            </LinearGradientBrush>
                                        </Border.Background>
                                    </Border>
                                    <Border  Grid.Row="28" Margin="5,0,0,0" BorderThickness="0,0,0,3"  Grid.ColumnSpan="6" >
                                        <Border.Background>
                                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                                <GradientStop Color="#FFC6E2FB" Offset="1"/>
                                                <GradientStop Color="#FFE6F1FB"/>
                                            </LinearGradientBrush>
                                        </Border.Background>
                                    </Border>
                                    <TextBox Text="{m:Bind Path=f_username}" Grid.Column="3" LostFocus="{m:Exp Str=ksnumobj.Load()}" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" IsReadOnly="False" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_username" />
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBox Text="{m:Bind Path=f_linkname}"  Grid.Column="1" Margin="0,3,0,2" Grid.Row="3" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="-1.313,0.4" Grid.RowSpan="1"  >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_linkname" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBox Text="{m:Bind Path=f_address}" IsReadOnly="False" Grid.Column="5" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="0.694,0.52" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_address" />
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBox Text="{m:Bind Path=f_idnumber}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="0.368,-0.68" HorizontalAlignment="Stretch" Width="Auto"  >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_idnumber" />
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBox Text="{m:Bind Path=f_phone}"  Grid.Column="3" Margin="0,3,0,2" Grid.Row="3" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" RenderTransformOrigin="-1.435,0.56" Grid.RowSpan="1" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_phone" NotNull="True"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBox Text="{m:Bind Path=f_shebei}"  Grid.Column="1" Margin="0,3,0,2" Grid.Row="9" TextWrapping="Wrap" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_shebei"  />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBox Text="{m:Bind Path=f_xieyihao}" Grid.Column="3" Margin="0,2,0,3" Grid.Row="9" IsReadOnly="False" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_xieyihao" />
                                        </a:Data.Prop>
                                    </TextBox>
                                    <ComboBox x:Name="CoboxGasPro" ItemsSource="{m:Exp Str=usegaslist}" DisplayMemberPath="f_gasproperties" SelectedValuePath="f_gasproperties" SelectedValue="{m:Bind Path=f_gasproperties}"  Grid.Column="1" Margin="0,2,0,0" Grid.Row="11" Height="25" VerticalAlignment="Top" d:LayoutOverrides="Height" >
                                        <!--a:Data.Items>
										<o:ObjectList WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproper as gasproper left join fetch gasproper.pricetype where f_gasproperties\=\'非民用\' or f_gasproperties\=\'混合型\'}"/>
									</a:Data.Items-->
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gasproperties" NotNull="True" Value="{m:Exp Str=CoboxGasPro.SelectedItem.f_gasproperties}" />
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <ComboBox x:Name="CoboxGas" DisplayMemberPath="f_gaspricetype" SelectedValuePath="f_gaspricetype" ItemsSource="{m:Exp Str=CoboxGasPro.SelectedItem.pricetype}" SelectedValue="{m:Bind Path=f_gaspricetype}"  Grid.Column="3" Margin="0,2,0,3" Grid.Row="11" Height="25" VerticalAlignment="Stretch" d:LayoutOverrides="VerticalAlignment, VerticalMargin" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gaspricetype" NotNull="True" Value="{m:Exp Str=CoboxGasPro.SelectedItem.f_gaspricetype}" ></o:PropertySetter>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBox Text="{m:Bind Path=f_gasprice}" Grid.Column="5" Margin="0,1,0,4" Grid.Row="11" TextWrapping="Wrap" IsReadOnly="True" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" d:LayoutOverrides="VerticalAlignment">
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gasprice" Value="{m:Exp Str=CoboxGas.SelectedItem.f_gasprice}" NotNull="True" />
                                        </a:Data.Prop>
                                    </TextBox>

