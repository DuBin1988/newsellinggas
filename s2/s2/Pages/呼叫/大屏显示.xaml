    <UserControl.Background>
        <ImageBrush Stretch="Fill" ImageSource="System.Windows.Media.Imaging.BitmapImage"/>
    </UserControl.Background>


    <Viewbox HorizontalAlignment="Left" Height="768" Margin="0" VerticalAlignment="Top" Width="1024">


        <Grid x:Name="LayoutRoot" Height="768" Width="1024" d:LayoutOverrides="Width, Height" >
            <m:ResourceLoad>
                <!--<o:GeneralObject Name="persongo1" WebClientInfo="{StaticResource dbclient}" 
						EntityType="t_screenset" Path="{m:Exp Str='$one/from t_screenset where rownum \= 1 order by id desc'}"
						IsOld="{m:Exp Str='Timer.State==$Loaded'}"/>-->
                <!--大屏查询信息-->


                <!--来电总数-->
                <o:ObjectList Name="ldcount" WebClientInfo="{StaticResource dbclient}"
				
                              Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_comingrecord where islink\=\'yes\' and comingtime like \'\{SysTime.Now.ToString($yyyyMMdd$)\}\%\'$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--累计来电总数-->

                <o:ObjectList Name="ljldcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_comingrecord where islink\=\'yes\'$}"
                          
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--f_cucode like \'\{SysTime.Now.ToString($yyyyMMdd$)\}\%\'-->
                <!--在线处理-->

                <o:ObjectList Name="zxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_dealtype\=\'电话处理\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />


                <!--累计在线处理-->

                <o:ObjectList Name="ljzxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_dealtype\=\'电话处理\'$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--上门维修-->

                <o:ObjectList Name="smwxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_dealtype\=\'转交维修\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--累计上门维修-->

                <o:ObjectList Name="ljsmwxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_dealtype\=\'转交维修\'$}"
                IsOld="{m:Exp Str='Timer.State==$Loaded'}" />


                <!--抢险zhushil-->
                <!--f_repairtype
            <o:ObjectList Name="qxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$from t_repairsys where f_dealtype\=\'抢险\' and f_cucode like \'\{SysTime.Now.ToString($yyyyMMdd$)\}\%\'}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />-->
                <!--抢险1-->
                <o:ObjectList Name="qxcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_repairtype\=\'抢险\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--用户咨询-->

                <o:ObjectList Name="yhzxcount" WebClientInfo="{StaticResource dbclient}"
                       
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_repairtype in (\'业务咨询\'\,\'客服类\') and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--停气漏气-->

                <o:ObjectList Name="tqlqcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) a\,count(id) b from t_repairsys where f_repairtype\=\'停气漏气类\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--投诉类-->

                <o:ObjectList Name="tscount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_repairtype\=\'投诉类\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--表具故障-->

                <o:ObjectList Name="bjgzcount" WebClientInfo="{StaticResource dbclient}"
                          
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_repairtype\=\'灶具类\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />


                <!--已处理-->

                <o:ObjectList Name="yclcount" WebClientInfo="{StaticResource dbclient}"
                         
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_havacomplete\=\'已完成\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--累计已处理-->

                <o:ObjectList Name="ljyclcount" WebClientInfo="{StaticResource dbclient}"
                         
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_havacomplete\=\'已完成\' $}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />

                <!--待处理-->

                <o:ObjectList Name="dclcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_havacomplete!\=\'已完成\' and substring(f_cucode\,1\,8)\=convert(char(8)\,getdate()\,112)$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}"/>

                <!--累计待处理-->

                <o:ObjectList Name="ljdclcount" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select count(id) total\,count(id) from t_repairsys where f_havacomplete!\=\'已完成\' $}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}"/>
                <!--客服系统开始运行时间-->
                <o:ObjectList Name="rundate1" WebClientInfo="{StaticResource dbclient}"
					Path="{m:Exp Str=$sql/select  datediff( day\,\'2015-12-31\'\, convert(char(10)\,getdate()\,120) ) diffday\,year(getdate()) year\,month(getdate()) month\,day(getdate()) day$}"
					IsOld="{m:Exp Str='Timer.State==$Loaded'}" />


                <o:UtilAction Name="cha" Page="/Pages/呼叫/大屏显示.xaml" LargeTime="{m:Exp Str='${SysTime.Now.ToString($yyyy-MM-dd$)}'}"  Calculate="{m:Exp Str='Timer.State==$Loaded'}"/>


