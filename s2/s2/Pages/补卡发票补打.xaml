<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.补卡发票补打" FontFamily="Microsoft YaHei">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-101,0">
            <!--批量删除语句>
	        <b:HQLAction Name="DeleteAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str='$delete from t_chongzheng where id in ({userfiles.SelectedItems.ToString(id)=>SelectionChanged})'}">
			</b:HQLAction-->
            <!--提交>
	        <b:HQLAction Name="SubmitAction"  WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_chongzheng set \f_substate\=null where id in (\{userfiles.SelectedItems.ToString(id)\=>SelectionChanged\}) and f_substate \=\'完成\'}"/-->

            <!--单值对象-->
            <o:GeneralObject Name="fapiaogeshi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  			Path="one/from t_singlevalue where name='发票格式'"   ></o:GeneralObject>
            <!--补卡对象-->
            <o:GeneralObject Name="buda" EntityType="t_renewalcard" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$one/from t_renewalcard where id\=(select max(id) from t_renewalcard where f_userid\=\'\{userfile.DataContext.f_userid\}\')}" 
				IsInit="{m:Exp Str=print.State\=\=$End$}"  >
            </o:GeneralObject>

            <!--补卡发票补打对象Source="{m:Exp Str=userfiles.SelectedItem}"-->
            <o:GeneralObject  Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
			EntityType="t_bucardfapiao" 
			IsInit="{m:Exp Str=print.State\=\=$End$}"     >
            </o:GeneralObject>

            <!--保存对象 -->
            <b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=userfile.DataContext}" MethodName="SaveToJson"/>
            </b:BatchExcuteAction>
        </m:ResourceLoad>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
                <toolkit:DockPanel >
                    <toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" toolkit:DockPanel.Dock="Top" Margin="0,0,0,3" >
                        <a:Data.Context>
                            <o:SearchObject />
                        </a:Data.Context>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28"></RowDefinition>
                                <RowDefinition Height="28"></RowDefinition>
                                <RowDefinition Height="28"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="70"/>
                                <ColumnDefinition Width="150"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock TextWrapping="Wrap"  Text="用户编号：" TextAlignment="Right"/>
                            <TextBox TextWrapping="Wrap"  Text="{m:Bind Path=f_userid}" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Height="25" Width="150" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid" Operator="f_userid like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap"  Text="补打日期从：" Grid.Row="1" Margin="5,6,0,6"/>
                            <sdk:DatePicker Name="tj4"  SelectedDate="{m:Bind Path=f_yytdate1}" Grid.Row="1" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" HorizontalAlignment="Stretch" >
                                <a:Data.Prop>
                                    <!-- Value="{m:Exp Str=SysTime.Today}"-->
                                    <o:PropertySetter PropertyName="f_yytdate1" 
											Operator="f_reversaldate >= substring('this',1,10)" />
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock TextWrapping="Wrap"  Grid.Row="2" Text="到" Margin="5,6,0,6" TextAlignment="Center"/>
                            <sdk:DatePicker Name="tj8" SelectedDate="{m:Bind Path=f_yytdate2}" Grid.Row="2" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" Grid.ColumnSpan="1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_yytdate2"  
											Operator="f_reversaldate ^= substring('this',1,10) + ' 23:59:59'" />
                                </a:Data.Prop>
                            </sdk:DatePicker>
                        </Grid>
                        <Button Content="查  询" x:Name="searchbutton" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,5,5,0" HorizontalAlignment="Right" Width="50" />
                    </toolkit:DockPanel>
                    <toolkit:DockPanel   Background="#FFC3E4FD" toolkit:DockPanel.Dock="Bottom" Height="33">
                        <TextBlock TextWrapping="Wrap" Margin="5" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000" Height="33"/>

                    </toolkit:DockPanel>
                    <sdk:DataGrid x:Name="userfiles" Margin="0,0,0,26"  
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
                        <a:Data.Items>
                            <o:PagedObjectList Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
								Path="{m:Exp Str=$from t_bucardfapiao where \{userfilesearch.DataContext.Condition\} and f_filiale1\=\'\{LoginUser.f_fengongsi\}\'}"
    						    IsOld="{m:Exp Str=SaveAction.State\=\=$End$ or userfile.DataContext.State\=\=$End$ }"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="id"/>
                        </a:Data.Items>
                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="用户编号" Binding="{Binding f_userid}"/>
                            <sdk:DataGridTextColumn d:IsLocked="True" Header="用户姓名" Binding="{Binding f_username}"/>
                            <sdk:DataGridTextColumn Header="卡费" Binding="{Binding f_cardfees}"/>
                            <sdk:DataGridTextColumn Header="身份证号" Binding="{Binding f_idnumber}"/>
                            <sdk:DataGridTextColumn Header="卡编号" Binding="{Binding f_cardnumber}"/>
                            <sdk:DataGridTextColumn Header="新卡编号" Binding="{Binding f_newcardnumber}"/>
                            <sdk:DataGridTextColumn Header="补打日期" Binding="{Binding f_reversaldate, StringFormat=yyyy-MM-dd}"/>
                            <sdk:DataGridTextColumn Header="补打时间" Binding="{Binding f_reversaltime, StringFormat=hh:mm:ss tt}"/>
                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                    <!--提交按钮-->
                </toolkit:DockPanel>
            </c:BusyIndicatorEx>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy}" Margin="5,0,0,0" BorderThickness="0" BorderBrush="{x:Null}" Background="{x:Null}" Grid.Column="1" >
                <Grid  x:Name="userfile"  DataContext="{m:Exp Str=userfilego}">

                    <!--按钮-->
                    <!--非民用主操作区-->
                    <toolkit:DockPanel >
                        <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom"  Background="#FFC3E4FD" >
                            <StackPanel Orientation="Horizontal">
                                <Button x:Name="save" Content="保  存" Margin="5,0,5,5" 
									IsEnabled="{m:Exp Str=userfile.DataContext.f_userid!\=null and userfile.DataContext.IsModified and !userfile.DataContext.HasErrors}"
									Click="{m:Exp Str=SaveAction.Invoke()}" Width="50" Height="28" VerticalAlignment="Bottom" />
                                <Button Content="撤  销" Margin="5,0,5,5" FontSize="12" Click="{m:Exp Str=userfile.DataContext.New()}" Width="50" Height="28" VerticalAlignment="Bottom"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right"/>
                        </toolkit:DockPanel>
                        <Grid toolkit:DockPanel.Dock="Top" Height="Auto">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                    <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                    <GradientStop Color="#FFDDF2FD" Offset="1"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="90"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="78"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="1.333*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="1.333*"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="1*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="用户编号：" Margin="0,6" TextWrapping="Wrap"  TextAlignment="Right"/>
                            <TextBlock Text="用户姓名：" Margin="0,3,0,2" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
                            <TextBlock Text="身份证号：" Margin="0,6" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right"/>
                            <TextBlock Text="用户类型：" Grid.Column="2" Margin="0,3,0,2" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="表    号：" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="气表品牌：" Grid.Column="2" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="气表型号：" Margin="0,3,0,2" Grid.Row="2" Grid.Column="4" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Margin="0,3,0,2" Grid.Row="3" TextWrapping="Wrap" Text="气表厂家：" TextAlignment="Right"  Height="25"/>
                            <TextBlock Text="气    价：" Grid.Column="2"  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Margin="0,3,0,2" Height="25"/>
                            <TextBlock Text="卡费："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Margin="0,3,0,2" Height="25"/>
                            <TextBlock Text="卡号："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Grid.Column="2" Margin="0,3,0,2" Height="25"/>
                            <TextBlock Text="补卡原因：" Margin="0,3,0,0" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Height="25" VerticalAlignment="Center" d:LayoutOverrides="Height"/>
                            <TextBlock Text="部    门：" Margin="0,3,0,2" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Height="25"/>
                            <TextBlock Text="操作人：" Grid.Column="2" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Height="25"/>
                            <TextBox Text="{m:Bind Path=f_rcoperator}"  Grid.Column="3" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Grid.Row="6" IsReadOnly="True" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_rcoperator"   Value="{m:Exp Str=buda.f_rcoperator\=&gt;Completed}" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Text="操作人：" Grid.Column="2" Margin="0" Grid.Row="10" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="分公司编号：" Visibility="Collapsed" Margin="0,0,0,1" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="分公司：" Margin="0,4,0,0" Grid.Row="8" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBox x:Name="f_userid" Text="{m:Bind Path=f_userid}"  Grid.Column="1" Margin="0,3,70,2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=buda.f_userid}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <!--Button Content="读卡" Margin="0,3,5,2" FontSize="12" Width="50" Grid.Column="1" HorizontalAlignment="Right" Height="25"/-->
                            <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Margin="0,3,0,2" TextWrapping="Wrap" Grid.ColumnSpan="3" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=buda.f_username\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_idnumber}" IsReadOnly="True" Grid.Column="1" Margin="0,3,0,2" Grid.Row="1" TextWrapping="Wrap" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=buda.f_idnumber\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="3" Margin="0,3,0,2" Grid.Row="1" TextWrapping="Wrap" Grid.ColumnSpan="3" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_usertype" Value="{m:Exp Str=buda.f_usertype\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>

                            <TextBox Text="{m:Bind Path=f_meternumber}" IsReadOnly="True" Grid.Column="1" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=buda.f_meternumber\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_gaswatchbrand}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="2" IsReadOnly="True" Height="25"    >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_gaswatchbrand" Value="{m:Exp Str=buda.f_gaswatchbrand\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_metertype}" Grid.Column="5" Margin="0,3,0,2" Grid.Row="2" IsReadOnly="True" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_metertype" Value="{m:Exp Str=buda.f_metertype\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_gasmetermanufacturers}"  Grid.Column="1" Margin="0,3,0,2" Grid.Row="3" IsReadOnly="True" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_gasmetermanufacturers" Value="{m:Exp Str=buda.f_gasmetermanufacturers\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_gasprice}" Grid.Column="3"  Grid.Row="3" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Width="Auto" Margin="0,3,0,2" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_gasprice" Value="{m:Exp Str=buda.f_gasprice}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_cardfees}" Grid.Column="1"  Grid.Row="4" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Width="Auto" Margin="0,3,0,2" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cardfees" Value="{m:Exp Str=buda.f_cardfees}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_cardid}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str=buda.f_cardid\=&gt;Completed}" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_rcreason}" Grid.Column="1" Margin="0" Grid.Row="5" IsReadOnly="True" HorizontalAlignment="Stretch" Height="Auto" VerticalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="5" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_rcreason" Value="{m:Exp Str=buda.f_rcreason\=&gt;Completed}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_parentname}"  Grid.Column="1" Margin="0,3,0,2" TextWrapping="Wrap" Grid.ColumnSpan="1" IsReadOnly="True" Grid.Row="6" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_parentname" Value="{m:Exp Str=buda.f_parentname\=&gt;Completed}" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_rcoperator1}" Grid.Column="3" Margin="0" Grid.Row="10" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_rcoperator1" Default="{m:Exp Str=LoginUser.name}"  />
                                </a:Data.Prop>
                            </TextBox>

                            <TextBox Text="{m:Bind Path=f_fengongsinum}" Visibility="Collapsed" IsReadOnly="True" Grid.Column="1" Margin="0,0,0,1" Grid.Row="8" TextWrapping="Wrap" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_fengongsinum" Value="{m:Exp Str=buda.f_fengongsinum\=&gt;Completed}"   />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBox Text="{m:Bind Path=f_filiale}"  Grid.Column="1" Margin="0,3,0,0" TextWrapping="Wrap" Grid.ColumnSpan="2" IsReadOnly="True" Grid.Row="8"  HorizontalAlignment="Stretch" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=buda.f_filiale\=&gt;Completed}"  />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Grid.Row="12" TextWrapping="Wrap" Text="分公司：" TextAlignment="Right" Height="25" Margin="0" Grid.Column="2"/>
                            <TextBox Text="{m:Bind Path=f_filiale1}" Grid.Column="3" Margin="0" Grid.Row="12" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch"  Grid.ColumnSpan="2" Height="25">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_filiale1" Default="{m:Exp Str=LoginUser.f_fengongsi}" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Grid.Row="12" TextWrapping="Wrap" Visibility="Collapsed" Text="分公司编号："  TextAlignment="Right" Height="25" Margin="0"/>
                            <TextBox Text="{m:Bind Path=f_fengongsinum1}" Visibility="Collapsed" Grid.Column="1" Margin="0" Grid.Row="12" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_fengongsinum1"  
										Default="{m:Exp Str=LoginUser.f_fengongsinum}"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Grid.Column="2" Margin="0" Grid.Row="11" TextWrapping="Wrap" Text="补打日期：" TextAlignment="Right" Height="25"/>
                            <sdk:DatePicker SelectedDate="{m:Bind Path=f_reversaldate}" IsEnabled="False" Grid.Column="3" Margin="0" Grid.Row="11"  IsTodayHighlighted="True" IsDropDownOpen="False" SelectedDateFormat="Short" VerticalAlignment="Stretch" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_reversaldate" 
										Default="{m:Exp Str=SysTime.Today}"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock  Margin="0" Grid.Row="10" TextWrapping="Wrap" Text="部    门："  TextAlignment="Right" Height="25"/>
                            <TextBox Text="{m:Bind Path=f_parentname1}" Grid.Column="1" Margin="0" Grid.Row="10" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_parentname1"  Default="{m:Exp Str=LoginUser.f_parentname}" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Grid.Column="2" Margin="0" Grid.Row="7" TextWrapping="Wrap" Text="补卡日期：" TextAlignment="Right"  />
                            <sdk:DatePicker SelectedDate="{m:Bind Path=f_recoverycarddate}" IsEnabled="false" Grid.Column="3" Margin="0" Grid.Row="7"  IsTodayHighlighted="True" IsDropDownOpen="False" SelectedDateFormat="Short" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="25" Grid.ColumnSpan="1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_recoverycarddate" Value="{m:Exp Str=buda.f_recoverycarddate\=&gt;Completed}"   />
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock Margin="0" Grid.Row="9" TextWrapping="Wrap" Text="补打原因：" Height="25" TextAlignment="Right"/>
                            <TextBox Text="{m:Bind Path=f_budason}" Grid.ColumnSpan="5" Grid.Column="1" Margin="0" Grid.Row="9" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="Auto" Grid.RowSpan="1">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_budason" />
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock Margin="0" Grid.Row="7" Text="补卡时间：" TextWrapping="Wrap" TextAlignment="Right" Height="25" d:LayoutOverrides="HorizontalAlignment"/>
                            <toolkit:TimePicker Value="{m:Bind Path=f_recoverycardtime}" IsEnabled="False" Grid.Column="1" Margin="0" Grid.Row="7"   IsDropDownOpen="False"  VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" Grid.ColumnSpan="1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_recoverycardtime" Value="{m:Exp Str=buda.f_recoverycardtime\=&gt;Completed}" />
                                </a:Data.Prop>
                            </toolkit:TimePicker>
                            <TextBlock Margin="0" Grid.Row="11" Text="补打时间：" TextWrapping="Wrap" TextAlignment="Right" Height="25" d:LayoutOverrides="HorizontalAlignment"/>
                            <toolkit:TimePicker Value="{m:Bind Path=f_reversaltime}" IsEnabled="False" Grid.Column="1" Margin="0" Grid.Row="11"   IsDropDownOpen="False"  VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" Grid.ColumnSpan="1"  >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_reversaltime" Value="{m:Exp Str=SecRefresh\=&gt;TimeEvent!\=null:SysTime.Now\,$$}" />
                                </a:Data.Prop>
                            </toolkit:TimePicker>

                        </Grid>
                    </toolkit:DockPanel>
