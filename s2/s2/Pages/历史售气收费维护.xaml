<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.历史售气收费维护">

	<Grid x:Name="LayoutRoot">
		<m:ResourceLoad x:Name="PageResourceLoad" Margin="0,0,-101,0">
			<!--总公司名称-->
		<o:GeneralObject Name="zonggongsi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="{m:Exp Str=$one/from t_singlevalue where name \= \'总公司名称\'}"   >
        </o:GeneralObject>
			 
			<!--付款方式列表-->
			<o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
			<o:ObjectList Name="youxiaolist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='是否有效查询用' order by id"/>
					<o:ObjectList Name="fgslist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='分公司'"/>
			<!--卡表收费对象 -->
        	<o:GeneralObject Name="kbfee" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
              IsInit="{m:Exp Str=SaveAction.State\=\=$End$}" 
			  Source="{m:Exp Str=userfiles.SelectedItem !\=null:userfiles.SelectedItem\,null}">
			</o:GeneralObject>
			<!--卡表保存对象 -->
			 <b:BatchExcuteAction Name="SaveAction"  WebClientInfo="{StaticResource dbclient}">
				<!--b:BatchInfo Source="{m:Exp Str=kbusers}" MethodName="SaveToJson"/-->
				<b:BatchInfo Source="{m:Exp Str=kbfee1.DataContext}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
		     
		</m:ResourceLoad>
		
        <Grid>  
        	<c:BusyIndicatorEx BusyContent="正在工作，请稍等" x:Name="changebusy"  Grid.Column="1" Margin="8,0,0,0">  
			<Grid>
			
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="0.25*"/>
				<ColumnDefinition Width="0.75*"/>
			</Grid.ColumnDefinitions >
              <toolkit:DockPanel >
					<toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" toolkit:DockPanel.Dock="Top" Margin="0,0,0,3" >
						<a:Data.Context>
							<o:SearchObject />        	
						</a:Data.Context>
						<Grid>
							<Grid.RowDefinitions>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="100"/>
								<ColumnDefinition Width="50*"/>
								<ColumnDefinition Width="50*"/>
							</Grid.ColumnDefinitions>
						<TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="0" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_userid" Operator="f_userid like 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock TextWrapping="Wrap"  Grid.Row="1" Text="卡　　号：" Margin="0" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Column="1"  Grid.Row="1" Text="{m:Bind Path=f_cardid}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0" Height="25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_cardid" Operator="f_cardid like '%this%'"/>               
							</a:Data.Prop>
						</TextBox>
						<TextBlock Text="地　　址：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" HorizontalAlignment="Right"/>
						<TextBox Text="{m:Bind Path=f_address}" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_address" Operator="f_address like 'this%'"/>
									
								</a:Data.Prop>
							</TextBox>
							<TextBlock TextWrapping="Wrap" Grid.Row="3" Text="姓名/单位：" Margin="5,4,0,4" HorizontalAlignment="Right" />
							<TextBox TextWrapping="Wrap" Grid.Row="3" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Text="{m:Bind Path=f_username}" Width="Auto" Margin="0" Height="25" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username"  Operator="f_username like 'this%'"/>          
								</a:Data.Prop>
							</TextBox>
								<ComboBox Name="Comboxfx"   Visibility="{m:Exp Str='LoginUser.f_fengongsi \=\= zonggongsi.value:$Visible$\,$Collapsed$'}" SelectedValue="{m:Bind Path=f_filiale}" Grid.Row="0" Grid.Column="3" ItemsSource="{m:Exp Str=fgslist}"  Margin="0" DisplayMemberPath="name" SelectedValuePath="name"  Height="25">
						<a:Data.Prop>
                    		<o:PropertySetter PropertyName="f_filiale"  Operator="f_filiale like 'this%'"/>          
                    	</a:Data.Prop>
						</ComboBox>
						<Button Content="查  询" x:Name="searchbutton"  Grid.Row="3" Grid.Column="2" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,-5,5,0" HorizontalAlignment="Right" Width="50" />
					</Grid></toolkit:DockPanel>
					
					<toolkit:DockPanel   Background="#FFC3E4FD" toolkit:DockPanel.Dock="Bottom">
						<sdk:DataPager x:Name="pager" Source="{Binding Path=ItemsSource.Count,ElementName=userfiles,Converter={StaticResource DataPagerConverter}}"    Margin="0" PageSize="18" Height="24" toolkit:DockPanel.Dock="Top"/>
						<TextBlock TextWrapping="Wrap" Margin="5" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000" toolkit:DockPanel.Dock="Bottom" Height="27"/>
						
					</toolkit:DockPanel>
					
					<sdk:DataGrid x:Name="userfiles" Margin="0,0,0,2"  
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
						<a:Data.Items>
							<o:PagedObjectList x:Name="userfileslist" WebClientInfo="{StaticResource dbclient}"		
								Path="{m:Exp Str=LoginUser.f_fengongsi \=\= zonggongsi.value:$from t_sellinggas where \{userfilesearch.DataContext.Condition\}$\, LoginUser.f_fengongsi \!\= zonggongsi.value:$from t_sellinggas where \{userfilesearch.DataContext.Condition\} and f_filiale \=\'\{LoginUser.f_fengongsi\}\'$\,$abc$}"
								IsOld="{m:Exp Str='SaveAction.State\=\=$End$'}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="f_times"/>
						</a:Data.Items>
						<sdk:DataGrid.Columns>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="名称" Binding="{Binding f_username}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户编号" Binding="{Binding f_userid}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="卡号" Binding="{Binding f_cardid}"/>
							<sdk:DataGridTextColumn Header="购气量" Binding="{Binding f_pregas}"/>
							<sdk:DataGridTextColumn Header="金额" Binding="{Binding f_preamount}"/>
							<!--sdk:DataGridTextColumn Header="地  址" Binding="{Binding f_address}"/>
							<sdk:DataGridTextColumn Header="电  话" Binding="{Binding f_phone}"/-->
							<sdk:DataGridTextColumn Header="交费日期" Binding="{Binding f_deliverydate,StringFormat=yyyy-MM-dd}"/>
							<sdk:DataGridTextColumn Header="交费时间" Binding="{Binding f_deliverytime,StringFormat=hh:mm:ss tt}"/>
							<sdk:DataGridTextColumn Header="操作员" Binding="{Binding f_sgoperator}"/>
						</sdk:DataGrid.Columns>
					</sdk:DataGrid>
					
					
					<!--提交按钮-->
				</toolkit:DockPanel>
			
			<Grid Grid.Column="1" Margin="5,0,0,0">
				<toolkit:DockPanel>
					<StackPanel toolkit:DockPanel.Dock="Top">
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
							<Grid.Background>
						<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
							<GradientStop Color="#FFF2F8FF" Offset="0"/>
							<GradientStop Color="#FFDDF2FD" Offset="1"/>
						</LinearGradientBrush>
					</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
								<RowDefinition Height="30"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="用户/单位名称：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="地      址：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" RenderTransformOrigin="1.638,0.44"/>
							<TextBlock Text="小区名称：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表品牌：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表型号：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Margin="0" Grid.Row="5" TextWrapping="Wrap" Text="气价类型：" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="气      价：" Grid.Column="4"  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="5" Margin="0" Height="25"/>
							<TextBlock Text="上次总累计购气量："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="7" Margin="0" Height="25"/>
							<TextBlock Text="限购气量：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Grid.Column="4" Height="25"/>
							<TextBlock Text="混合气价：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4"  Height="25"/>
							<StackPanel Grid.ColumnSpan="4" Grid.Column="1" Visibility="{m:Exp Str=kbfee1.DataContext.f_gasproperties\=\=$混合型$:$Visible$\,$Collapsed$}" Margin="0,2,-11,2" Grid.Row="4" Orientation="Horizontal" Height="25"  >
								<TextBlock Text="民用气价" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="0,2" />
                                    <TextBox Text="{m:Bind Path=f_myprice}" x:Name="f_myprice" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_myprice"  />
									</a:Data.Prop>
								</TextBox>
								<TextBlock Text="所占比率" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="10,2,0,2" />
                                    <TextBox Text="{m:Bind Path=f_myfenzhi}" x:Name="f_myfenzhi" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True"  Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_myfenzhi"  />
									</a:Data.Prop>
								</TextBox>
								<TextBlock Text="非民用气价" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="10,2,0,2" />
                                    <TextBox Text="{m:Bind Path=f_gsprice}" x:Name="f_gsprice" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gsprice"  />
									</a:Data.Prop>
								</TextBox>
								<TextBlock Text="所占比率" HorizontalAlignment="Left" TextWrapping="Wrap" Margin="10,2,0,2" />
                                    <TextBox Text="{m:Bind Path=f_gsfenzhi}" x:Name="f_gsfenzhi" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Height="25" >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_gsfenzhi"   />
									</a:Data.Prop>
								</TextBox>
                                    <TextBox Text="{m:Bind Path=f_myqiliang}" x:Name="f_myqiliang" Grid.Column="1" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Grid.ColumnSpan="1" Grid.Row="9" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="36"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_myqiliang" />
									</a:Data.Prop>
								</TextBox>
                                    <TextBox Text="{m:Bind Path=f_myqiliangjine}" x:Name="f_myqiliangjine"  Grid.Column="3" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Grid.ColumnSpan="1" Grid.Row="9" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="41"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_myqiliangjine" />
									</a:Data.Prop>
								</TextBox>
                                    <TextBox Text="{m:Bind Path=f_feimyqiliang}" x:Name="f_feimyqiliang" Grid.Column="5" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Grid.ColumnSpan="1" Grid.Row="9" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="39"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_feimyqiliang" />
									</a:Data.Prop>
								</TextBox>
                                    <TextBox Text="{m:Bind Path=f_feimyqiliangjine}" x:Name="f_feimyqiliangjine" Grid.Column="1" Margin="5,0,0,0" TextWrapping="Wrap" IsReadOnly="True" Grid.ColumnSpan="1" Grid.Row="10" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="35"  >
									<a:Data.Prop>
										<o:PropertySetter PropertyName="f_feimyqiliangjine" />
									</a:Data.Prop>
								</TextBox>
							</StackPanel>
							<TextBlock Text="用户类型：" Margin="0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="用气性质：" Margin="0" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="维管费起始月份：" Margin="0" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="维管费截止月份：" Margin="0" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBlock Text="预购气量：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="10" Width="130" Height="25"/>
							<TextBlock Text="预购金额：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="10" Grid.Column="2" Height="25"/>
							<TextBlock Text="付款方式：" Margin="0" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="11" Height="25"/>
							<TextBlock Text="收      款：" Grid.Column="2" Margin="0" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="应交金额：" Margin="0" Grid.Row="10" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4"/>
							<TextBlock Text="本次余额：" Margin="63.583,3,0.167,2" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4"/>
							<TextBox x:Name="f_userid" Text="{m:Bind Path=f_userid}" IsReadOnly="True" Grid.Column="1" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_userid" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_username}" x:Name="f_username" IsReadOnly="True" Grid.Column="3" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username" />
								</a:Data.Prop>
							</TextBox>
                                <CheckBox IsChecked="{m:Bind Path=f_dibaohu}" x:Name="f_dibaohu"  Content="低保户" Margin="-0.167,0,0,0" Grid.Row="0" Grid.Column="5" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Left" IsEnabled="False" Width="62">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_dibaohu" />
								</a:Data.Prop>
							</CheckBox>
                                <TextBox Text="{m:Bind Path=f_idnumber}" x:Name="f_idnumber" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Visibility="Collapsed" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_idnumber" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_address}" x:Name="f_address" Grid.Column="3" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_address"/>
									<!--o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=$\{gsxiangou.f_gscustomgas\=>Completed\}}"/-->
								</a:Data.Prop>
							</TextBox>

                                <TextBox x:Name="f_districtname" Text="{m:Bind Path=f_districtname}" Grid.Column="1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_districtname" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_gaswatchbrand}" IsReadOnly="True" x:Name="f_gaswatchbrand" Grid.Column="1" Margin="0" Grid.Row="3" Height="25" VerticalAlignment="Stretch"   >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gaswatchbrand" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_metertype}" x:Name="f_metertype" Grid.Column="3" Margin="0" Grid.Row="3" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_metertype" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_gaspricetype}" IsReadOnly="True" x:Name="f_gaspricetype" Grid.Column="3" Margin="0" Grid.Row="5" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gaspricetype" />
								</a:Data.Prop>	
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_gasprice}" IsReadOnly="True" x:Name="f_gasprice" Grid.Column="5"  Grid.Row="5" TextWrapping="Wrap" HorizontalAlignment="Stretch" Width="Auto" Margin="0" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gasprice" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_unitfee}" x:Name="f_unitfee" Grid.Column="5"  Grid.Row="2" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Right" Width="0" Margin="0,3,0,2" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_unitfee" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_upbuynum}" x:Name="f_upbuynum" Grid.Column="1" Margin="0" Grid.Row="7" TextWrapping="Wrap" HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_upbuynum" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_limitbuygas}" x:Name="f_limitbuygas"  Grid.Column="5" Margin="0" TextWrapping="Wrap" Grid.ColumnSpan="1" Grid.Row="3" Height="25"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_limitbuygas"   
										/>
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" x:Name="f_usertype" Grid.Column="3" Margin="0" Grid.Row="1"  HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_usertype" />
								</a:Data.Prop>	
							</TextBox>
                                <TextBox Text="{m:Bind Path=f_gasproperties}" IsReadOnly="True" x:Name="f_gasproperties" Grid.Column="1" Margin="0" Grid.Row="5" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gasproperties" />
								</a:Data.Prop>
							</TextBox>
                                <TextBox Text="{Binding f_beginfee, StringFormat=yyyy-MM-dd}" x:Name="f_beginfee" Grid.Column="1" Margin="0" Grid.Row="6" IsReadOnly="False" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_beginfee" />
								</a:Data.Prop>	
							</TextBox>
							<!--从档案中取购气次数>
										<TextBox Text="{m:Bind Path=f_times}"  Grid.Column="4" Margin="0,8,5,4" Grid.Row="11" IsReadOnly="True" HorizontalAlignment="Right" Width="40" RenderTransformOrigin="0.143,2.167" Height="Auto" VerticalAlignment="Stretch" >
											<a:Data.Prop>
												<o:PropertySetter PropertyName="f_times" Value="{m:Exp Str=kbusers.f_times.ToInt()+1}"/>
											</a:Data.Prop>
										</TextBox-->
					<!--TextBox Text="{Binding Path=f_endfee, StringFormat=yyyy-MM}"  Grid.Column="1" Margin="0,3,41,2" Grid.Row="6" Height="25"  >
											<a:Data.Prop>
