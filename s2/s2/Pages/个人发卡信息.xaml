<c:CustomChildWindow 
           x:Class="s2.Pages.个人发卡信息"
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
		   xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
		   xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	       xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
           xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
		   xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
		   xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           Width="800" Height="330"   FontSize="14"
           Title="个人发卡信息" Name="child">
    <Grid x:Name="LayoutRoot2" Grid.Row="3" Grid.Column="3"   Margin="12,0,12,0"  DataContext="{m:Exp Str=child.ParamValue}" >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="30"/>
         
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>

            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="140"/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"  />
        <TextBox Text="{m:Bind Path=f_userid}" IsReadOnly="True" Grid.Column="1" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_userid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="姓   名：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_username" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="发卡日期：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="0" TextAlignment="Right" Height="25"/>
        <TextBox Text="{Binding f_givecarddate, StringFormat=yyyy-MM-dd}" IsReadOnly="True" Grid.Column="5" Grid.Row="0" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_givecarddate" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="气表品牌：" Margin="0" TextWrapping="Wrap" Grid.Row="1"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_gaswatchbrand}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_gaswatchbrand" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="气表厂家：" Margin="139,1" TextWrapping="Wrap"  Grid.Column="1" Grid.Row="1" TextAlignment="Right" Height="25" Grid.ColumnSpan="3"/>
        <TextBox Text="{m:Bind Path=f_gasmetermanufacturers}" IsReadOnly="True" Grid.Column="3" Grid.Row="1" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="用户类型：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="1" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_usertype}" IsReadOnly="True" Grid.Column="5" Grid.Row="1" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_usertype" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="小区名称：" Margin="0" TextWrapping="Wrap" Grid.Row="2"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_districtname}" IsReadOnly="True" Grid.Column="1" Grid.ColumnSpan="5" Margin="0" Grid.Row="2"   Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_districtname" />
            </a:Data.Prop>
        </TextBox>


        <TextBlock Text="卡    号：" Margin="0" TextWrapping="Wrap" Grid.Row="3"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cardid}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="3"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cardid" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="卡    费：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cardfees}" IsReadOnly="True" Grid.Column="3" Grid.Row="3" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cardfees" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="气    价：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="3" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_gasprice}" IsReadOnly="True" Grid.Column="5" Grid.Row="3" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_gasprice" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="预购金额：" Margin="0" TextWrapping="Wrap" Grid.Row="4"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_preamount}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="4"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_preamount" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="本次总累计购气量：" Margin="0,5,0,0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_cumulativepurchase}" IsReadOnly="True" Grid.Column="3" Grid.Row="4" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_cumulativepurchase" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="当前表累计购气量：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="4" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_metergasnums}" IsReadOnly="True" Grid.Column="5" Grid.Row="4" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_metergasnums" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="收    款：" Margin="0" TextWrapping="Wrap" Grid.Row="5"  TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_grossproceeds}" IsReadOnly="True" Grid.Column="1" Margin="0" Grid.Row="5"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_grossproceeds" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="应交金额：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_totalcost}" IsReadOnly="True" Grid.Column="3" Grid.Row="5" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_totalcost" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="操  作  员：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="5" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_sgoperator}" IsReadOnly="True" Grid.Column="5" Grid.Row="5" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_sgoperator" />
            </a:Data.Prop>
        </TextBox>

        <TextBlock Text="购气日期：" Margin="0" TextWrapping="Wrap" Grid.Row="6"  TextAlignment="Right" Height="25"/>

        <TextBox  Text="{Binding f_deliverydate, StringFormat=yyyy-MM-dd}" IsReadOnly="True"  Grid.Column="1" Margin="0" Grid.Row="6"   Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_deliverydate" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="分  公  司：" Margin="0" TextWrapping="Wrap"  Grid.Column="2" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_filiale}" IsReadOnly="True" Grid.Column="3" Grid.Row="6" Margin="1"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_filiale" />
            </a:Data.Prop>
        </TextBox>
        <TextBlock Text="网  点：" Margin="0" TextWrapping="Wrap"  Grid.Column="4" Grid.Row="6" TextAlignment="Right" Height="25"/>
        <TextBox Text="{m:Bind Path=f_sgnetwork}" IsReadOnly="True" Grid.Column="5" Grid.Row="6" Margin="0"  Grid.ColumnSpan="1" Height="25"  >
            <a:Data.Prop>
                <o:PropertySetter PropertyName="f_sgnetwork" />
            </a:Data.Prop>
        </TextBox>

        <!--<Button x:Name="CancelButton" Content="取消" Click="CancelButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,17,55,-10" Grid.Row="12" Grid.Column="4" />-->
        <Button x:Name="OKButton" Content="确定" Click="OKButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,30,92,-23" Grid.Row="13" Grid.Column="2" Grid.ColumnSpan="2" />
    </Grid>
</c:CustomChildWindow>