<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    x:Class="Com.Aote.Pages.换表管理" FontSize="13"
    mc:Ignorable="d"  FontFamily="Microsoft YaHei">
    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-12,0" x:Name="loader">
              <o:ObjectList Name="meterstatelistnull"  EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='表状态' order by id" HasEmptyRow="True"/>
			<!--发卡费-->
            <o:GeneralObject Name="fakafei" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		        IsInit="{m:Exp Str=print.State\=\=$End$ or zzprint.State\=\=$End$ or print.State\=\=$Cancle$ or zzprint.State\=\=$Cancle$}" 
		  		Path="{m:Exp Str=$one/from t_singlevalue where name \= \'\{LoginUser.f_fengongsi\}\{userfiles.f_usertype\}\卡\费\'}"   >
            </o:GeneralObject>

            <!--换表操作类型-->
            <o:ObjectList Name="watchtype" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='换表操作类型' order by id"/>
            <!--换表类型-->
            <o:ObjectList Name="replacewatchtype" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='换表类型' order by id"/>

            <!--卡号获取-->
            <o:GeneralObject Name="cardid1" EntityType="t_cardreset" WebClientInfo="{StaticResource dbclient}"
           	LoadOnPathChanged="False"   
		   	Path="{m:Exp Str=userfiles.f_usertype\=\=$民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfiles.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_minyong\=\'民用\' and parentid is not null)/f_qishicard$\,userfiles.f_usertype\=\=$非民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfiles.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_feiminyong\=\'非民用\' and parentid is not null)/f_qishicard$\,null}" 
			>
            </o:GeneralObject>

            <!--换表对象-->
            <o:GeneralObject Name="pipelinego" EntityType="t_changmeter" WebClientInfo="{StaticResource dbclient}" 
		    IsInit="{m:Exp Str=print.State\=\=$End$ or zzprint.State\=\=$End$ or print.State\=\=$Cancle$ or zzprint.State\=\=$Cancle$}">
                <o:PropertySetter PropertyName="f_zherownum" Value="{m:Exp Str=userfiles !\= null and userfiles.f_zherownum !\= null and userfiles.f_zherownum.ToInt()^24:userfiles.f_zherownum+1\,1}"/>
                <o:PropertySetter PropertyName="f_state" Default="换表登记"></o:PropertySetter>
                <o:PropertySetter PropertyName="f_gasproperties" NoDependValue="{m:Exp Str=userfiles.f_gasproperties}"/>
				<o:PropertySetter  PropertyName="f_userinfoid" NoDependValue="{m:Exp Str=userfiles.f_userinfoid}"/>
            </o:GeneralObject>

            <!-- 获取标记信息 -->
            <o:GeneralObject Name="tabletel" EntityType="t_table_tel" WebClientInfo="{StaticResource dbclient}"
                             IsInit="{m:Exp Str=print.State\=\=$End$ or zzprint.State\=\=$End$ or print.State\=\=$Cancle$ or zzprint.State\=\=$Cancle$}"
                             Path="{m:Exp Str=$one/select new Map(meter_phone as meter_phone\,terminal_name as terminal_name) from t_table_tel  where f_meternumber\=\'\{pipeline.DataContext.f_cmnewmeternumber\}\' $}">
            </o:GeneralObject>

            <!--用户列表-->
            <o:PagedObjectList Name="pipelineslist" WebClientInfo="{StaticResource dbclient}"
            Path="{m:Exp Str=$from t_userfiles where \{pipelinesearch.DataContext.Condition\} and f_userstate\=\'正常\' and f_userstate !\=\'销户\' and f_filiale \=\'\{LoginUser.f_fengongsi\}\'}"
			PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames=","/>
            <!--用户对象-->
            <o:GeneralObject Name="userfiles" NotEmpty="true" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
			IsInit="{m:Exp Str=print.State\=\=$End$ or zzprint.State\=\=$End$ or print.State\=\=$Cancle$ or zzprint.State\=\=$Cancle$}"
            Path="{m:Exp Str=
			card.CardId\=>ReadCompleted!\=null and card.Factory!\=null:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,pipelines.SelectedItem.f_userid\=>SelectionChanged\!\=null:$one/from t_userfiles where f_userid\=\'\{pipelines.SelectedItem.f_userid\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,$null$}">
                <!--换表回写-->
                <o:PropertySetter PropertyName="f_buka" Value="{m:Exp Str=pipeline.DataContext.f_buka}"/>
                <o:PropertySetter PropertyName="f_gaswatchbrand" Value="{m:Exp Str=pipeline.DataContext.f_cmnewgaswatchbrand}"/>
                <o:PropertySetter PropertyName="f_metertype" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmetertype}"/>
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" Value="{m:Exp Str=pipeline.DataContext.f_newgasmetermanufacturers}"/>
                <o:PropertySetter PropertyName="f_gasmeterstyle" Value="{m:Exp Str=pipeline.DataContext.f_newgasmeterstyle}"/>
                <o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str='cardid1.State \=\= $Loaded$:cardid1.f_qishicard,null'}" />
                <o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmetertitles}"/>
                <o:PropertySetter PropertyName="f_metertitles" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmeternumber}"/>
                <o:PropertySetter PropertyName="f_meternumber" Value="{m:Exp Str=pipeline.DataContext.f_cmnewmeternumber}"/>
                <o:PropertySetter PropertyName="f_metergasnums" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:0\,0}"/>
                <o:PropertySetter PropertyName="f_cypregas" Value="{m:Exp Str=pipeline.DataContext.f_cypregas}"/>
                <o:PropertySetter PropertyName="f_replacewatchtype" Value="{m:Exp Str=pipeline.DataContext.f_replacewatchtype}"/>
                <o:PropertySetter PropertyName="f_watchfee" Value="{m:Exp Str=pipeline.DataContext.f_watchfee}"/>
                <o:PropertySetter PropertyName="terminal_name" Value="{m:Exp Str=pipeline.DataContext.terminal_name}"/>
                <o:PropertySetter PropertyName="meter_phone" Value="{m:Exp Str=pipeline.DataContext.meter_phone}"/>
                <o:PropertySetter PropertyName="f_jllx" Value="{m:Exp Str=pipeline.DataContext.f_jllx}"/>
                <!--卡信息-->
                <o:PropertySetter PropertyName="klx" Value="{m:Exp Str=pipeline.DataContext.klx}"/>
                <o:PropertySetter PropertyName="dqdm" Value="{m:Exp Str=pipeline.DataContext.dqdm}"/>
                <o:PropertySetter PropertyName="yhh" Value="{m:Exp Str=pipeline.DataContext.yhh}"/>
                <o:PropertySetter PropertyName="bjql" Value="{m:Exp Str=pipeline.DataContext.bjql}"/>
                <o:PropertySetter PropertyName="czsx" Value="{m:Exp Str=pipeline.DataContext.czsx}"/>
                <o:PropertySetter PropertyName="tzed" Value="{m:Exp Str=pipeline.DataContext.tzed}"/>
                <o:PropertySetter PropertyName="kmm" Value="{m:Exp Str=pipeline.DataContext.kmm}"/>
                <!-- 
                <o:PropertySetter PropertyName="meter_phone" Value="{m:Exp Str=pipeline.DataContext.meter_phone}"/>
                <o:PropertySetter PropertyName="terminal_name" Value="{m:Exp Str=pipeline.DataContext.terminal_name}"/> -->
                <o:PropertySetter PropertyName="f_channel_type" Value="{m:Exp Str=pipeline.DataContext.f_channel_type}"/>
                <o:PropertySetter PropertyName="switch_op" Value="{m:Exp Str=pipeline.DataContext.switch_op}"/>
                <o:PropertySetter PropertyName="old_val" Value="{m:Exp Str=pipeline.DataContext.f_oldcurrentbase}"/>
                <o:PropertySetter PropertyName="old_steal_no" Value="{m:Exp Str=pipeline.DataContext.f_meternumber}"/>
                <o:PropertySetter PropertyName="lastinputdate" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:SysTime.Today\,SysTime.Today}"/>

                <o:PropertySetter PropertyName="f_danganstatus" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$换表$\,$换表$}"/>
                <o:PropertySetter PropertyName="f_danganreturnvalue" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$1$\,$1$}"/>
                <!-- <o:PropertySetter PropertyName="refreshCache" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null and pipeline.DataContext.f_newgasmeterstyle!\=$物联网表$:$0$\,$1$}"/> -->
                <o:PropertySetter PropertyName="refreshCache" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$1$\,$1$}"/>

                <!--换机表时，抄表指数为新表底数-->
                <o:PropertySetter PropertyName="lastinputgasnum" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:pipelinego.f_newgasmeteraccomodations\,null}"/>
                <o:PropertySetter PropertyName="lastrecord" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:pipelinego.f_newgasmeteraccomodations\,null}"/>
                <!--别名-->
                <o:PropertySetter PropertyName="f_aliasname" Value="{m:Exp Str=pipeline.DataContext.f_aliasname}"/>
                <!--额定流量-->
                <o:PropertySetter PropertyName="f_flownum" Value="{m:Exp Str=pipeline.DataContext.f_flownum}"/>
                <!--气表型号对应类型-->
                <o:PropertySetter PropertyName="f_type" Value="{m:Exp Str=pipeline.DataContext.f_newtype}"/>
                <!--脉冲(表系数)-->
                <o:PropertySetter PropertyName="f_maichong" Value="{m:Exp Str=pipeline.DataContext.f_newmaichong}"/>
                <!--海力表型编码-->
                <o:PropertySetter PropertyName="f_watchnum" Value="{m:Exp Str=pipeline.DataContext.f_newwatchnum}"/>
                <o:PropertySetter PropertyName="f_zherownum" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:pipelinego.f_zherownum.ToInt()\,0}"/>
                <!--初始化卡标记-->
                <o:PropertySetter PropertyName="f_initcardsellgas" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$初始化卡购气标记$\,null}"/>
            </o:GeneralObject>
            <o:GeneralObject Name="userfiles1" NotEmpty="true" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
			IsInit="{m:Exp Str=print.State\=\=$End$ or zzprint.State\=\=$End$ or print.State\=\=$Cancle$ or zzprint.State\=\=$Cancle$}"
            Path="{m:Exp Str=
			card.CardId\=>ReadCompleted!\=null and card.Factory!\=null:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,pipelines.SelectedItem.f_userid\=>SelectionChanged\!\=null:$one/from t_userfiles where f_userid\=\'\{pipelines.SelectedItem.f_userid\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,$null$}">
            </o:GeneralObject>

            <!--卡对象-->
            <oo:NewGeneralICCard Name="card"    
		     CanInitCard="{m:Exp Str='cardid1.State\=>DataLoaded\=\=$Loaded$'}"
		     Gas="0"
		     CardId="{m:Exp Str=pipelinego.f_newcardid}"
		     Factory="{m:Exp Str=pipelinego.f_aliasname}"
		     BuyTimes="1" 
		     Klx="{m:Exp Str='pipelinego != null\:pipelinego.klx.ToInt()\,null'}"		
		     Kzt="{m:Exp Str=cardid1\=>DataLoaded!\=null:0\,0}"
		     Dqdm="{m:Exp Str='pipelinego != null \:pipelinego.dqdm\,null'}"
		     Yhh="{m:Exp Str='pipelinego != null \:pipelinego.yhh\,null'}"
             Tm="0001"
		     Ljgql="{m:Exp Str='pipelinego != null \:pipelinego.f_cumulativepurchase\,null'}"
		     Bkcs="0"
		     Bjql="{m:Exp Str='pipelinego != null \:pipelinego.bjql.ToInt()\,null'}"
		     Czsx="{m:Exp Str='pipelinego != null \:pipelinego.czsx.ToInt()\,null'}"
		     Tzed="{m:Exp Str='pipelinego != null \:pipelinego.tzed.ToInt()\,null'}"	
		     Kmm="{m:Exp Str='pipelinego != null \:pipelinego.kmm\,null'}"
		     Sqrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		     OldPrice="{m:Exp Str='pipelinego != null\:pipelinego.f_gasprice*100\,0'}"
		     NewPrice="{m:Exp Str='pipelinego != null\:pipelinego.f_gasprice*100\,0'}"
		     Sxrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		     Sxbj="{m:Exp Str=card.Factory \=\= $ZhenLan$ \:null\,$1$}"
             Stairprice1="{m:Exp Str='pipelinego != null \:pipelinego.f_stair1price\,null'}"
             Stairgas1="{m:Exp Str='pipelinego != null \:pipelinego.f_stair1amount\,null'}" 
             Stairprice2="{m:Exp Str='pipelinego != null \:pipelinego.f_stair2price\,null'}"
             Stairgas2="{m:Exp Str='pipelinego != null \:pipelinego.f_stair2amount\,null'}"                         
             Stairprice3="{m:Exp Str='pipelinego != null \:pipelinego.f_stair3price\,null'}"
             Stairgas3="{m:Exp Str='pipelinego != null \:pipelinego.f_stair3amount\,null'}" 
             Money="{m:Exp Str=pipeline.DataContext.f_preamount}"      
             TotalMoney="{m:Exp Str=pipeline.DataContext.f_totalcost}"
		    >
            </oo:NewGeneralICCard>

            <!--根据用户档案的计划月份产生抄表单-->
            <!--删除抄表单-->
            <b:HQLAction Name="DeleteHandplan" WebClientInfo="{StaticResource dbclient}" Type="sql"
			HQL="{m:Exp Str=$delete from t_handplan where f_state \=\'未抄表\' and shifoujiaofei \=\'否\' and f_userid\=\'\{userfiles1.f_userid\}\'}" />
            <b:HQLAction Name="InsertHandplan" WebClientInfo="{StaticResource dbclient}" Type="sql" 
			HQL="{m:Exp Str=$insert into t_handplan(f_userid\, f_username\, lastinputgasnum\, f_gaswatchbrand\, f_metertype\,
				f_address\, f_districtname\, f_usertype\, f_gasprice\, f_gaspricetype\, f_dibaohu\, f_apartment\,
				f_phone\, scinputdate\, f_inputtor\, f_yhxz\, f_weizhi\, f_menzhan\,
				f_zerenbumen\, f_state\, shifoujiaofei\, users\, f_cusDom\, f_cusDy\,f_gasmeterstyle )  
                select f_userid\, f_username\, lastinputgasnum\, f_gaswatchbrand\, f_metertype\,
				f_address\, f_districtname\, f_usertype\, f_gasprice\, f_gaspricetype\, f_dibaohu\, f_apartment\,
				f_phone\, lastinputdate\, f_inputtor\, f_yhxz\, f_weizhi\, f_menzhan\,
				f_zerenbumen\, \'未抄表\'\, \'否\'\, id\, f_cusDom\, f_cusDy\,f_gasmeterstyle from t_userfiles where f_gasmeterstyle \!\=\'卡表\' and f_userid\=\'\{userfiles1.f_userid\}\'}"/>
            <!-- 最新指数插入记录 -->
            <b:HQLAction Name="Insertcbnum" WebClientInfo="{StaticResource dbclient}" Type="sql" 
			HQL="{m:Exp Str=$insert into t_cbnum(f_userid\, f_gasdate\, f_gasnum\, f_meternumber\, f_gaswatchbrand\,jval ) values (
                \'\{pipeline.DataContext.f_userid\}\'\,\'\{pipeline.DataContext.f_cmdate\}\'\,\'\{pipeline.DataContext.f_newgasmeteraccomodations\}\'\,\'\{pipeline.DataContext.f_cmnewmeternumber\}\'\,\'\{pipeline.DataContext.f_cmnewgaswatchbrand\}\'\,\'-0.0001\') }"/>
            <!--生成抄表单-->
            <b:BatchExcuteAction Name="CreateHandplan" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=DeleteHandplan}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=InsertHandplan}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=Insertcbnum}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>

            <!--保存-->
            <b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}"  CanSave="{m:Exp Str=card.State\=\=$End$}" >
                <!-- CanSave="{m:Exp Str=card.State\=\=$End$ |or| onlySave\=>Click!\=null}"
                <b:BatchInfo Source="{m:Exp Str=DeleteHandplan}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=InsertHandplan}" MethodName="InvokeToJson"/>  -->
                <b:BatchInfo Source="{m:Exp Str=pipelinego}" MethodName="SaveToJson"/>
                <b:BatchInfo Source="{m:Exp Str=userfiles}" MethodName="SaveToJson"/>

            </b:BatchExcuteAction>
        </m:ResourceLoad>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=pipelines.ItemsSource.IsBusy}">
                <toolkit:DockPanel >
                    <!--信息列表按钮-->
                    <toolkit:DockPanel  toolkit:DockPanel.Dock="Bottom" Margin="0" Height="45">
                        <!--信息列表翻页-->
                        <TextBlock  TextWrapping="Wrap" Text="{m:Exp Str=$共\{pipelines.ItemsSource.Count\}项}" Width="197" HorizontalAlignment="Stretch" Foreground="#FF140000" VerticalAlignment="Stretch" Margin="5"/>
                    </toolkit:DockPanel>
                    <toolkit:DockPanel x:Name="pipelinesearch" toolkit:DockPanel.Dock="Top" Margin="0,0,0,5" Background="#FFCFEFFF" >
                        <a:Data.Context>
                            <o:SearchObject  />
                        </a:Data.Context>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="50*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                            </Grid.RowDefinitions>
                            <TextBlock TextWrapping="Wrap" Text="用户编号：" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="1" Height="25" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,1" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid" Operator="f_userid like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" HorizontalAlignment="Right" Height="28" VerticalAlignment="Top"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="1" Height="25" Grid.Column="1" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,0" VerticalAlignment="Top" d:LayoutOverrides="Height">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_username" Operator="f_username like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <Button Content="查 询" x:Name="searchbutton" Grid.Column="2" Grid.Row="2" Click="{m:Exp Str=pipelinesearch.DataContext.Search()}" Margin="0,-1,5,0" HorizontalAlignment="Right" Width="50" Height="28" VerticalAlignment="Top"/>
                            <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="地　　址：" HorizontalAlignment="Right" Height="28" VerticalAlignment="Top" Margin="0,2,0,0"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Height="25" Grid.Column="1" Text="{m:Bind Path=f_address}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,2,0,0" VerticalAlignment="Top" d:LayoutOverrides="Height">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_address" Operator="f_address like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                        </Grid>
                    </toolkit:DockPanel>
                    <sdk:DataGrid x:Name="pipelines" Margin="0,0,0,23" ItemsSource="{m:Exp Str=pipelineslist}"
				SelectedItem="{m:Exp Str=pipelinego.f_userid\=\=null:null\,pipelines.SelectedItem |or| pipelineslist\=>DataLoaded!\=null:null\,null}" 
            		AutoGenerateColumns="False"  IsReadOnly="True">

                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn Header="序号" Binding="{Binding Index}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户姓名" Binding="{Binding f_username}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}" d:IsLocked="True"/>

                            <sdk:DataGridTextColumn Header="卡号" Binding="{Binding f_cardid}" d:IsLocked="True"/>





                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </toolkit:DockPanel >
            </c:BusyIndicatorEx>
            <!--操作区按钮-->
            <c:BusyIndicatorEx x:Name="ui_userfiles" BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=pipeline.DataContext.IsBusy or userfiles.IsBusy or SaveAction.IsBusy or tabletel.IsBusy}" Grid.Column="1" Margin="8,0,0,0">
                <Grid Background="{x:Null}" x:Name="pipeline" DataContext="{m:Exp Str=pipelinego}" >
                    <StackPanel HorizontalAlignment="Right" Orientation="Horizontal" VerticalAlignment="Bottom">
                        <Button x:Name="save" Content="保存并发卡" Margin="0" 
            			IsEnabled="{m:Exp Str=data.f_newgasmeterstyle\=\=$卡表$ and !pipeline.DataContext.HasErrors}" 
            			Click="{m:Exp Str=userfiles.f_surplusgas\=pipeline.DataContext.f_cmaddgas;userfiles.f_bugas\=pipeline.DataContext.f_cmaddgas;userfiles.f_metergasnums\=pipeline.DataContext.f_zhi;userfiles.f_times\=0;card.Gas\=0;card.Sqrq\=pipelinego.f_cmdate.ToString($yyyyMMdd$);submit.IsShow\=True}"/>
                        <Button x:Name="onlySave" Content=" 保 存 " Margin="20,0,0,0" Width="50" Click="{m:Exp Str=SaveAction.Invoke()}" 
            			IsEnabled="{m:Exp Str=data.f_newgasmeterstyle\!\=$卡表$ and !pipeline.DataContext.HasErrors}"/>
                        <Button Content="撤 销" Margin="20,0,5,0" Click="{m:Exp Str=pipeline.DataContext.New();userfiles.New()}" Width="50"/>
                    </StackPanel>
                    <Grid Margin="0,0,0,45" Grid.Column="0" >
                        <Grid.Background>
                            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                <GradientStop Color="#FFDDF2FD" Offset="1"/>
                            </LinearGradientBrush>
                        </Grid.Background>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="1" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="1" />
                            <RowDefinition Height="60" />
                            <RowDefinition Height="30" />
