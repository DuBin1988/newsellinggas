<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:local="clr-namespace:Com.Aote.Pages"
    xmlns:beh="clr-namespace:Com.Aote.Behavior"
	mc:Ignorable="d" FontSize="12"
	x:Class="Com.Aote.Pages.安检结果小区统计"   >

    <UserControl.Resources>
        <!--也可以在这里引用自定义转换器，推荐在全局APP.xaml引用 -->
        <local:youwu x:Key="booltoyou" />
        <local:yesno x:Key="booltoyes" />
    </UserControl.Resources>

    <Grid x:Name="LayoutRoot">

        <m:ResourceLoad Margin="0,0,0,0">
            <!--供暖方式-->
            <o:ObjectList Name="gongnuanlist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='供暖方式'"/>
            <!--租住-->
            <o:ObjectList Name="zuzhulist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='租住户'"/>
            <!--重点用户-->
            <o:ObjectList Name="iszhongdianlist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='重点用户'"/>
            <!--小区性质-->
            <o:ObjectList Name="propertylist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='小区性质'"/>
            <!--漏气失效-->
            <o:ObjectList Name="lqsx" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='漏气失效'"/>
            <!--用气设备-->
            <o:ObjectList Name="deviceslist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用气设备'"/>

            <!--安检列表-->
            <o:PagedList Name="areafileslist1" WebClientInfo="{StaticResource dbclient}" Path="hql" 
                         SumHQL="{m:Exp Str=$from T_INSPECTION t where \{areafilesearch.DataContext.Condition\}$}" 
                         HQL="{m:Exp Str=$select distinct t from T_INSPECTION t  where \{areafilesearch.DataContext.Condition\}$}"
                         PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="," />

            <!--安检状态-->
            <o:ObjectList Name="conditionlist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='安检状态'"/>
            <o:ObjectList Name="f_checktypeList" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='安检类型'"/>

        </m:ResourceLoad>
        <ScrollViewer  VerticalScrollBarVisibility="Auto">
            <Grid >
                <Grid.RowDefinitions>
                    <RowDefinition />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <!-- <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=areafiles.ItemsSource.IsBusy}" >
                <Border BorderThickness="1" BorderBrush="#FF9AA2AB" Margin="0" Padding="2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" >  -->
                <toolkit:DockPanel Width="Auto"  Height="Auto"  >
                    <toolkit:DockPanel   Background="#94a8cd" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="25" >
                        <TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{areafiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Margin="0,0,0,1" Foreground="#FF140000" />
                        <sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=areafiles}"  Margin="0,0,0,-2" PageSize="20"/>
                    </toolkit:DockPanel>
                    <toolkit:DockPanel  x:Name="areafilesearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5"   >
                        <a:Data.Context>
                            <o:SearchObject />
                        </a:Data.Context>
                        <Grid Background="#FF94A8CD">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="100"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                            </Grid.RowDefinitions>
                            <TextBlock TextWrapping="Wrap" Text="基础条件" Margin="16,4,0,4" Grid.Column="0" HorizontalAlignment="Left" />
                            <TextBlock TextWrapping="Wrap" Text="街道：" Margin="5,4,0,4" Grid.Column="1" HorizontalAlignment="Right" />
                            <TextBox TextWrapping="Wrap" Grid.Column="2" Text="{m:Bind Path=ROAD}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="ROAD"  Operator="ROAD like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Text="小区名称：" Margin="5,4,0,4" Grid.Column="3" HorizontalAlignment="Right" />
                            <TextBox TextWrapping="Wrap" Grid.Column="4" Text="{m:Bind Path=UNIT_NAME}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="UNIT_NAME"  Operator="UNIT_NAME like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>

                            <TextBlock TextWrapping="Wrap" Text="楼号：" Grid.Column="5" Grid.Row="0" Margin="5,4,0,4" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="6" Grid.Row="0" Text="{m:Bind Path=CUS_DOM}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="CUS_DOM"  Operator="CUS_DOM like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Text="单元：" Grid.Row="0" Grid.Column="7" Margin="5,4,0,4"  HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="8" Grid.Row="0" Text="{m:Bind Path=CUS_DY}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="CUS_DY"  Operator="CUS_DY like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="0" Grid.Column="9" Text="楼层：" Margin="5,4,0,4"  HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="0" Grid.Column="10" Text="{m:Bind Path=CUS_FLOOR}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="CUS_FLOOR"  Operator="CUS_FLOOR like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="0" Text="房号："  Grid.Column="11" Margin="0,7,0,1"  HorizontalAlignment="Right" />
                            <TextBox TextWrapping="Wrap" Grid.Row="0" Grid.Column="12" Text="{m:Bind Path=CUS_ROOM}" Width="Auto" Margin="0,4,0,27" Height="25" Grid.RowSpan="2" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="CUS_ROOM"  Operator="CUS_ROOM like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="姓名："  Grid.Column="1" Margin="0,7,0,1"  HorizontalAlignment="Right" />
                            <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="2" Text="{m:Bind Path=f_consumername}" Width="Auto" Margin="0,4,0,27" Height="25" Grid.RowSpan="2" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_consumername"  Operator="f_consumername like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="卡号："  Grid.Column="3" Margin="0,7,0,1"  HorizontalAlignment="Right" />
                            <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="4" Text="{m:Bind Path=f_kahao}" Width="Auto" Margin="0,4,0,27" Height="25" Grid.RowSpan="2" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_kahao"  Operator="f_kahao like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Text="安检情况：" Margin="5,4,0,4" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Right"/>
                            <ComboBox Name="condition"   SelectedValue="{m:Bind Path=CONDITION}" Grid.Row="2" Grid.Column="2" ItemsSource="{m:Exp Str=conditionlist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="CONDITION"  Operator="CONDITION like '%this%'"/>
                                </a:Data.Prop>
                            </ComboBox>
                            <TextBlock TextWrapping="Wrap" Text="安检员：" Margin="5,4,0,4" Grid.Row="2" Grid.Column="3" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Grid.Column="4" Text="{m:Bind Path=f_anjianren}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_anjianren"  Operator="f_anjianren like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Text="维修员：" Margin="5,4,0,4" Grid.Row="2" Grid.Column="5" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Grid.Column="6" Text="{m:Bind Path=REPAIRMAN}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="REPAIRMAN"  Operator="REPAIRMAN like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Text="安检告知单号：" Margin="5,4,0,4" Grid.Row="2" Grid.Column="7" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Grid.Column="8" Text="{m:Bind Path=f_renow_id}" Width="Auto" Margin="0" Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_renow_id"  Operator="f_renow_id like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="安检开始日期：" Margin="0,7,0,1"  HorizontalAlignment="Right" Grid.Column="9"/>
                            <sdk:DatePicker SelectedDate="{m:Bind Path=f_anjianriqi}" Margin="0"  Height="25" VerticalAlignment="Stretch" Grid.Row="2" Grid.Column="10" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_anjianriqi" Value="{m:Exp Str=SysTime.MonthsBeforeToday(1)}"
								Operator=" substring(ARRIVAL_TIME,1,10) >= substring('this',1,10)"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>
                            <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="安检截至日期：" Margin="0,7,0,1"  HorizontalAlignment="Right" Grid.Column="11"/>
                            <sdk:DatePicker SelectedDate="{m:Bind Path=f_anjianriqi2}" Margin="0"  Height="25" VerticalAlignment="Stretch" Grid.Row="2" Grid.Column="12" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_anjianriqi2" Value="{m:Exp Str=SysTime.Today}"
								Operator=" substring(ARRIVAL_TIME,1,10) ^= substring('this',1,10)"/>
                                </a:Data.Prop>
                            </sdk:DatePicker>

                            <TextBlock TextWrapping="Wrap" Text="小区条件" Margin="16,4,0,4" Grid.Row="3" Grid.Column="0" HorizontalAlignment="Left" />
                            <TextBlock TextWrapping="Wrap" Text="小区性质：" Margin="5,4,0,4" Grid.Column="1" Grid.Row="3" HorizontalAlignment="Right"/>
                            <ComboBox  SelectedValue="{m:Bind Path=f_property}" Grid.Row="3" Grid.Column="2" ItemsSource="{m:Exp Str=propertylist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_property"  Operator="f_property like 'this%'"/>
                                </a:Data.Prop>
                            </ComboBox>

                            <TextBlock TextWrapping="Wrap" Text="供暖方式：" Margin="5,4,0,4" Grid.Column="3" Grid.Row="3" HorizontalAlignment="Right"/>
                            <ComboBox  SelectedValue="{m:Bind Path=f_gongnuan}" Grid.Row="3" Grid.Column="4" ItemsSource="{m:Exp Str=gongnuanlist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_gongnuan"  Operator="f_gongnuan like 'this%'"/>
                                </a:Data.Prop>
                            </ComboBox>
                            <TextBlock TextWrapping="Wrap" Text="租住：" Margin="5,4,0,4" Grid.Column="5" Grid.Row="3" HorizontalAlignment="Right"/>
                            <ComboBox  SelectedValue="{m:Bind Path=f_zuzhu}" Grid.Row="3" Grid.Column="6" ItemsSource="{m:Exp Str=zuzhulist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_zuzhu"  Operator="f_zuzhu like 'this%'"/>
                                </a:Data.Prop>
                            </ComboBox>
                            <TextBlock TextWrapping="Wrap" Text="是否重点用户：" Margin="5,4,0,4" Grid.Column="7" Grid.Row="3" HorizontalAlignment="Right"/>
                            <ComboBox  Name="ttttt" SelectedValue="{m:Bind Path=f_iszhongdian}" Grid.Row="3" Grid.Column="8" ItemsSource="{m:Exp Str=iszhongdianlist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_iszhongdian"  Operator="f_iszhongdian like 'this%'"/>
                                </a:Data.Prop>
                            </ComboBox>
                            <TextBlock TextWrapping="Wrap" Text="用气设备：" Margin="5,4,0,4" Grid.Column="9" Grid.Row="3" HorizontalAlignment="Right"/>
                            <ComboBox  SelectedValue="{m:Bind Path=f_devices}" Grid.Row="3" Grid.Column="10" ItemsSource="{m:Exp Str=deviceslist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_devices"  Operator="f_devices like 'this%'"/>
                                </a:Data.Prop>
                            </ComboBox>
                            <TextBlock TextWrapping="Wrap" Text="设备条件" Margin="16,4,0,4" Grid.Row="5" Grid.Column="0" HorizontalAlignment="Left" />
                            <CheckBox Content="报警器" Grid.Column="1" Grid.Row="5" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_baojingqi}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_baojingqi"  Operator="f_baojingqi = CASE WHEN 'True'= 'this' THEN '有' ELSE '无' END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="已到期" Grid.Column="2" Grid.Row="5" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_alarm_expire_time}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_alarm_expire_time"  Operator="f_alarm_expire_time is not null and substring(CONVERT(varchar(50),GETDATE(),120),1,7)- substring(CONVERT(varchar(50),f_alarm_expire_time,120),1,7)>0"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="热水器" Grid.Column="1" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_rshqshiyong}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_rshqshiyong"  Operator="f_rshqshiyong = CASE WHEN 'True'= 'this' THEN 0 ELSE 1 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="已到期" Grid.Column="2" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_expire_time}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_expire_time"  Operator="f_heater_expire_time is not null and substring(CONVERT(varchar(50),GETDATE(),120),1,7)- substring(CONVERT(varchar(50),f_heater_expire_time,120),1,7)>0"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="超期使用" Grid.Column="3" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_overdue}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_overdue"  Operator="f_heater_overdue = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="软管连接" Grid.Column="4" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_softconnector}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_softconnector"  Operator="f_heater_softconnector = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="未排室外" Grid.Column="5" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_trapped}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_trapped"  Operator="f_heater_trapped = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="直排" Grid.Column="6" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_emit}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_emit"  Operator="f_heater_emit = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="漏气" Grid.Column="7" Grid.Row="6" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_heater_leakage}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_heater_leakage"  Operator="f_heater_leakage = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="灶具" Grid.Column="1" Grid.Row="7" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_zjshiyong}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_zjshiyong"  Operator="f_zjshiyong = CASE WHEN 'True'= 'this' THEN 0 ELSE 1 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="已到期" Grid.Column="2" Grid.Row="7" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_cooker_expire_time}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cooker_expire_time"  Operator="f_cooker_expire_time is not null and substring(CONVERT(varchar(50),GETDATE(),120),1,7)- substring(CONVERT(varchar(50),f_cooker_expire_time,120),1,7)>0"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="超期使用" Grid.Column="3" Grid.Row="7" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_cooker_overdue}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cooker_overdue"  Operator="f_cooker_overdue = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="无熄火保护" Grid.Column="4" Grid.Row="7" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_cooker_nofireprotection}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cooker_nofireprotection"  Operator="f_cooker_nofireprotection = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="漏气" Grid.Column="5" Grid.Row="7" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_cooker_leakage}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_cooker_leakage"  Operator="f_cooker_leakage = CASE WHEN 'True'= 'this' THEN 1 ELSE 0 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="挂壁炉" Grid.Column="1" Grid.Row="8" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_bglshiyong}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_bglshiyong"  Operator="f_bglshiyong = CASE WHEN 'True'= 'this' THEN 0 ELSE 1 END"/>
                                </a:Data.Prop>
                            </CheckBox>
                            <CheckBox Content="已到期" Grid.Column="2" Grid.Row="8" IsThreeState="True" Click="CheckBox_Click" IsChecked="{m:Bind Path=f_furnace_expire_time}" HorizontalAlignment="Left" Margin="5,6,0,0"  VerticalAlignment="Top">
                                <a:Data.Prop>
