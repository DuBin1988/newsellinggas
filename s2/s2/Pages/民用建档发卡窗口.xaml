<c:CustomChildWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	mc:Ignorable="d" FontSize="13" Name="fakachild"
	x:Class="Com.Aote.Pages.民用建档发卡窗口" FontFamily="Microsoft YaHei" Title="发卡售气">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad x:Name="loader" Margin="0,0,-101,0">
            <!--发票对象-->
            <o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
         Path="{m:Exp Str=$one/from t_fapiaoinfos where f_invoicenum\=\'\{fakaobj.DataContext.f_invoicenum\}\'}" 
                             
			IsInit="{m:Exp Str=FaKaAction\=\=$Error$ |or| card.State\=\=$Error$ |or| print1.State\=\=$End$ |or| print.State\=\=$End$}">
                <o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_username}"/>
                <o:PropertySetter PropertyName="f_invoicenum" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_invoicenum}"/>
                <o:PropertySetter PropertyName="f_userid" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_userid}" />
                <o:PropertySetter PropertyName="f_cardid" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_cardid}"/>
                <o:PropertySetter PropertyName="f_gas" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_pregas}" />
                <o:PropertySetter PropertyName="f_money" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_preamount}" />
                <o:PropertySetter PropertyName="f_repairfee" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_amountmaintenance}" />
                <o:PropertySetter PropertyName="f_totalfee" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_totalcost}" />

                <o:PropertySetter PropertyName="f_sgoperator" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_sgoperator}" />
                <o:PropertySetter PropertyName="f_fapiaotime" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_deliverytime}" />

                <o:PropertySetter PropertyName="f_filiale" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_filiale}" />

            </o:GeneralObject>
            <!--重新获取发票号-->
            <b:ScriptAction Name="loadfapiao" CanRun="{m:Exp Str=print.State\=\=$End$ |or| print1.State\=\=$End$  }" Start="{m:Exp Str=LoginUserfapiao.IsOld\=True}"></b:ScriptAction>

            <!--发卡费-->
            <o:GeneralObject Name="fakafei" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}"
		    IsInit="{m:Exp Str='print.State==$End$'}"
		  	Path="{m:Exp Str=fakaobj.DataContext.f_userid \!\= null:$one/from t_singlevalue where name \= \'\{LoginUser.f_fengongsi\}\{fakaobj.DataContext.f_usertype\}\发\卡\费\'$\,$null$}">
            </o:GeneralObject>
            <!--开票类型-->
            <o:ObjectList Name="invoicelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='发票样式' order by id"/>
            <!--非民用限购参数对象-->
            <o:GeneralObject Name="gsxiangou" EntityType="t_gsxiangou" WebClientInfo="{StaticResource dbclient}"
            Path="{m:Exp Str=$one/from t_gsxiangou where f_fanwei &lt;\= \{userfile1.f_flownum\} and f_daofanwei &gt; \{userfile1.f_flownum\}}">
            </o:GeneralObject>

            <!--收费类型-->
            <o:ObjectList Name="payfeetypelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='发卡售气收费类型' order by id"/>
            <!--付款方式-->
            <o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
            <!--打开窗口对象-->
            <o:ChildWindowObj Name="openkafei"  CWName="/Pages/高级管理员登录" />
            <o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" />

            <!--混合气价对象 -->
            <o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
        	Path="{m:Exp Str=$one/from t_hunhe }">
            </o:GeneralObject>
            <o:GeneralObject Name="fbffobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用低保户月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="mwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="fwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="fapiaogeshi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  	Path="one/from t_singlevalue where name='发票格式'"   ></o:GeneralObject>

            <!--用户对象-->
            <o:GeneralObject Name="userfile1" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
        IsInit="{m:Exp Str='FaKaAction\=\=$Error$ |or| card.State\=\=$Error$ |or| print.State==$End$  |or| print1.State==$End$'}"
		Path="{m:Exp Str=userfile.DataContext.f_userid!\=null:$one/from t_userfiles where f_userid\=\'\{userfile.DataContext.f_userid\}\'$\,null}">
                <o:PropertySetter PropertyName="f_finallybought" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_pregas}" />
                <o:PropertySetter PropertyName="f_finallygas" NoDependValue="{m:Exp Str=FaKaAction\=>Saving!\=null:fakaobj.DataContext.f_pregas\,null}"/>
                <o:PropertySetter PropertyName="sxrq" NoDependValue="{m:Exp Str='SysTime.Today.ToString($yyyyMMdd$)'}" />
                <o:PropertySetter PropertyName="sxbj" NoDependValue="1" />
                <o:PropertySetter PropertyName="f_zherownum" NoDependValue="13" Default="13"/>
                <o:PropertySetter PropertyName="oldprice" Default="198" />
                <o:PropertySetter PropertyName="newprice" NoDependValue="198" />
                <o:PropertySetter PropertyName="f_zhye" 
				NoDependValue="{m:Exp Str=FaKaAction\=>Saving!\=null:fakaobj.DataContext.f_givechange\,null}"/>
                <!--维护费起始月份-->
                <o:PropertySetter PropertyName="f_beginfee" 
			NoDependValue="{m:Exp Str=FaKaAction\=>Saving!\=null:fakaobj.DataContext.f_beginfee\,null}" />
                <o:PropertySetter PropertyName="f_cardid" NoDependValue="{m:Exp Str='fakaobj.DataContext.f_cardid !\=null:fakaobj.DataContext.f_cardid\,null'}" />
                <o:PropertySetter PropertyName="f_times" 
			Value="{m:Exp Str='card.State==$Error$:0,1'}" />
            </o:GeneralObject>
            <!--收费对象-->
            <o:GeneralObject Name="sellgasobj" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
		IsInit="{m:Exp Str='userfile1.State\=\=$StartLoad$ |or| FaKaAction\=\=$Error$ |or| card.State\=\=$Error$ |or| print.State==$End$ |or| print1.State==$End$ |or| print1.State==$Cancle$ |or| print.State==$Cancle$'}">
                <o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_username}"/>
                <o:PropertySetter PropertyName="f_districtname" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_districtname}"/>
                <o:PropertySetter PropertyName="f_apartment" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_apartment}"/>
                <o:PropertySetter PropertyName="f_address" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_address}" />
                <o:PropertySetter PropertyName="f_gaswatchbrand" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_gaswatchbrand}" />
                <o:PropertySetter PropertyName="f_metertype" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_metertype}" />
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_gasmetermanufacturers}" />
                <o:PropertySetter PropertyName="f_meternumber" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_meternumber}" />
                <o:PropertySetter PropertyName="f_userid" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_userid}" />
                <o:PropertySetter PropertyName="f_singlegasnum" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_singlegasnum}" />
                <o:PropertySetter PropertyName="f_pregas" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_pregas}" />
                <o:PropertySetter PropertyName="f_preamount" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_preamount}" />
                <o:PropertySetter PropertyName="f_upbuynum" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_upbuynum}" />
                <o:PropertySetter PropertyName="f_premetergasnums" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_premetergasnums}" />
                <o:PropertySetter PropertyName="f_limitbuygas" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_limitbuygas}" />
                <o:PropertySetter PropertyName="f_grossproceeds" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_grossproceeds}" />
                <o:PropertySetter PropertyName="f_totalcost" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_totalcost}" />
                <o:PropertySetter PropertyName="f_givechange" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_givechange}" />
                <o:PropertySetter PropertyName="f_payment" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_payment}" />
                <o:PropertySetter PropertyName="f_buka" NoDependValue="否" />
                <!--公司类型-->
                <o:PropertySetter PropertyName="f_comtype" NoDependValue="天然气公司" />
                <!--是否有效-->
                <o:PropertySetter PropertyName="f_payfeevalid" NoDependValue="有效" />
                <!--别名f_aliasname-->
                <o:PropertySetter PropertyName="f_aliasname" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_aliasname}" />
                <!--卡号f_cardid-->
                <o:PropertySetter PropertyName="f_cardid" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_cardid}" />
                <!--收费类型f_payfeetype-->
                <o:PropertySetter PropertyName="f_payfeetype" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_payfeetype}" />
                <!--气价f_gasprice-->
                <o:PropertySetter PropertyName="f_gasprice" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_gasprice}" />
                <!--气价类型f_gaspricetype-->
                <o:PropertySetter PropertyName="f_gaspricetype" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_gaspricetype}" />
                <!--用气性质f_gasproperties-->
                <o:PropertySetter PropertyName="f_gasproperties" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_gasproperties}" />
                <!--用户类型f_usertype-->
                <o:PropertySetter PropertyName="f_usertype" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_usertype}" />
                <!--网点f_sgnetwork-->
                <o:PropertySetter PropertyName="f_sgnetwork" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_sgnetwork}" />
                <!--操作员f_sgoperator-->
                <o:PropertySetter PropertyName="f_sgoperator" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_sgoperator}" />
                <!--分公司名称f_filiale-->
                <o:PropertySetter PropertyName="f_filiale" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_filiale}" />
                <!--分公司编号f_fengongsinum-->
                <o:PropertySetter PropertyName="f_fengongsinum" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_fengongsinum}" />
                <!--卡费-->
                <o:PropertySetter PropertyName="f_cardfees" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_cardfees}" />
            </o:GeneralObject>
            <!--发卡对象-->
            <o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
		EntityType="t_myfaka" 
		IsInit="{m:Exp Str='userfile1.State\=\=$StartLoad$ |or| FaKaAction\=\=$Error$ |or| card.State\=\=$Error$ |or| print.State==$End$ |or| print.State==$Cancle$'}">
                <o:PropertySetter PropertyName="f_sgnetwork" NoDependValue="{m:Exp Str=fakaobj.DataContext.f_sgnetwork}"/>
                <o:PropertySetter PropertyName="f_comtype" NoDependValue="天然气公司"/>

            </o:GeneralObject>

            <!--卡号对象cardid.f_qishicard-->
            <o:GeneralObject Name="cardid1" EntityType="t_cardreset" WebClientInfo="{StaticResource dbclient}"
        LoadOnPathChanged="False"    
		Path="{m:Exp Str=userfile1.f_usertype\=\=$民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfile1.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_minyong\=\'民用\' and parentid is not null)/f_qishicard$\,userfile1.f_usertype\=\=$非民用$:$sernum/from t_cardreset where id \=(select parentid from cardresetwatch where f_gaswatchbrand\=\'\{userfile1.f_gaswatchbrand\}\' and f_fengongsi\=\'\{LoginUser.f_fengongsi\}\' and f_feiminyong\=\'非民用\' and parentid is not null)/f_qishicard$\,null}"
		>
            </o:GeneralObject>
            <!--卡对象GeneralICCard-->
            <oo:NewGeneralICCard Name="card"   
		 CanInitCard="{m:Exp Str='FaKaAction.State \=\= $End$'}"
		 Gas="{m:Exp Str=fakaobj.DataContext.f_pregas}"
		 CardId="{m:Exp Str=fakaobj.DataContext.f_cardid}"
		 Factory="{m:Exp Str='userfile1.State \=\= $Loaded$:userfile1.f_aliasname,null'}"
		 BuyTimes="{m:Exp Str=userfilego.f_times\=\=null:0\,userfilego.f_times.ToInt()}" 
		 Klx="{m:Exp Str='userfile1 != null\:userfile1.klx\,null'}"		
		 Kzt="0"
		 Dqdm="{m:Exp Str='userfile1 != null \:userfile1.dqdm\,null'}"
		 Yhh="{m:Exp Str='userfile1 != null \:userfile1.yhh\,null'}"
		 Tm="0001"
		 Ljgql="{m:Exp Str='userfile1 != null \:userfile1.f_cumulativepurchase\,null'}"
		 Bkcs="0"
		 Bjql="{m:Exp Str='userfile1 != null \:userfile1.bjql\,null'}"
		 Czsx="{m:Exp Str='userfile1 != null \:userfile1.czsx\,null'}"
		 Tzed="{m:Exp Str='userfile1 != null \:userfile1.tzed\,null'}"	
		 Kmm="{m:Exp Str='userfile1 != null \:userfile1.kmm\,null'}"
		 Sqrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		 OldPrice="{m:Exp Str='userfile1 != null\:userfile1.f_gasprice*100\,0'}"
		 NewPrice="{m:Exp Str='userfile1 != null\:userfile1.f_gasprice*100\,0'}"
		 Sxrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		 Sxbj="{m:Exp Str=card.Factory \=\= $ZhenLan$ \:null\,$1$}"
		>
            </oo:NewGeneralICCard>

            <!--更新发卡记录为无效-->
            <b:HQLAction Name="UpdateFaka" WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_myfaka set f_whethergivecard\=\'无效\' where id \= \{userfilego.id\}}"/>
            <!--更新售气记录为无效-->
            <b:HQLAction Name="UpdateSellgas" WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_sellinggas set f_payfeevalid\=\'无效\' where id \= \{sellgasobj.id\}}"/>
            <!--更新用户档案中相关信息-->
            <b:HQLAction Name="UpdateUserfile" WebClientInfo="{StaticResource dbclient}" 
		HQL="{m:Exp Str=$update t_userfiles  set f_whethergivecard\=\'未发\' where id \= \{userfile1.id\}}"/>
            <!--制卡失败后的自动撤销过程-->
            <b:BatchExcuteAction Name="RollbackAction" CanSave="{m:Exp Str=card.State\=\=$Error$}" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=UpdateFaka}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=UpdateUserfile}" MethodName="InvokeToJson"/>
                <b:BatchInfo Source="{m:Exp Str=UpdateSellgas}" MethodName="InvokeToJson"/>
            </b:BatchExcuteAction>

            <!--保存对象 -->
            <b:BatchExcuteAction Name="FaKaAction" WebClientInfo="{StaticResource dbclient}"
		CanSave="{m:Exp Str='cardid1.State \=\= $Loaded$ and sellgasobj.f_cardid !\= null and userfile1.f_cardid !\=null and userfilego.f_cardidjia !\=null'}">
                <!--售气对象-->
                <b:BatchInfo Source="{m:Exp Str=sellgasobj}" MethodName="SaveToJson"/>
                <!--用户对象-->
                <b:BatchInfo Source="{m:Exp Str=userfile1}" MethodName="SaveToJson"/>
                <!--发卡对象-->
                <b:BatchInfo Source="{m:Exp Str=userfilego}" MethodName="SaveToJson"/>
                <!--发票对象-->
                <b:BatchInfo Source="{m:Exp Str=fpinfosobj}" MethodName="SaveToJson"  CanSave="{m:Exp Str=fakaobj.DataContext.f_makeinvoicetype!\=$普通发票$}"/>
            	
            </b:BatchExcuteAction>
			 <!--关闭发卡界面-->
			<b:BatchExcuteAction Name="fakachild2" WebClientInfo="{StaticResource dbclient}"
			CanSave="{m:Exp Str='print.State==$End$  |or| print1.State==$End$'}">
	         <!--关闭发卡界面-->
                <b:BatchInfo Source="{m:Exp Str=fakachild}" MethodName="Close" />
            </b:BatchExcuteAction>
			
			
            <!--提交-->
            <b:HQLAction Name="SubmitAction" WebClientInfo="{StaticResource dbclient}" 
			HQL="{m:Exp Str=$update t_userfiles set f_substate\=null where id in (\{fakaobj.DataContext.id\}) }"/>
        </m:ResourceLoad>
        <Grid Margin="0,0,0,26" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx Name="ui_busy"
		BusyContent="{m:Exp Str=FaKaAction.IsBusy:$正在访问服务器，请稍等...$\,card.IsBusy:$正在操作卡，请稍等...$\,$正在工作，请稍等...$}" 
		IsBusy="{m:Exp Str=userfile1.IsBusy or card.IsBusy or FaKaAction.IsBusy}" Margin="10,0,0,0" Grid.Column="1" >
                <Grid Background="{x:Null}" x:Name="fakaobj" DataContext="{m:Exp Str=userfilego}" >
                    <toolkit:DockPanel>
                        <!--按钮-->
                        <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom"  >
                            <StackPanel Orientation="Horizontal" Width="170">
                                <Button x:Name="save" Content="写  卡" Margin="5" IsEnabled="{m:Exp Str=!userfilego.HasErrors}"
						Click="{m:Exp Str=userfile1.f_beginfee\=userfilego.f_beginfee;userfilego.f_times \= userfilego.f_times+1;userfile1.f_times\=userfilego.f_times.ToInt();sellgasobj.f_times\=userfilego.f_times.ToInt();sellgasobj.f_deliverydate\=userfilego.f_deliverydate;sellgasobj.f_deliverytime\=userfilego.f_deliverytime;sellgasobj.f_metergasnums\=userfilego.f_metergasnums;sellgasobj.f_cumulativepurchase\=userfilego.f_cumulativepurchase;userfile1.f_metergasnums\=userfilego.f_metergasnums;userfile1.f_cumulativepurchase\=userfilego.f_cumulativepurchase;userfile1.f_finabuygasdate\=userfilego.f_deliverydate;userfile1.f_dateofopening\=userfilego.f_givecarddate;userfile1.f_givecarddate\=userfilego.f_givecarddate;sellgasobj.f_payfeevalid\=$有效$;sellgasobj.f_comtype\=$天然气公司$;sellgasobj.f_payfeetype\=userfilego.f_payfeetype;userfile1.f_whethergivecard\=$已发$;userfile1.f_whethergivepassbook\=$已发$;sellgasobj.f_payment\=userfilego.f_payment;sellgasobj.f_sgoperator\=userfilego.f_sgoperator;sellgasobj.f_sgnetwork\=userfilego.f_sgnetwork;sellgasobj.f_filiale\=userfilego.f_filiale;userfile1.f_watchinstalldate\=userfilego.f_watchinstalldate;card.Sqrq\=userfilego.f_deliverydate.ToString($yyyyMMdd$);fpinfosobj.f_fapiaotype\=$发卡售气$;fpinfosobj.f_fapiaostatue\=$已用$;fpinfosobj.f_sgnetwork\=fakaobj.DataContext.f_sgnetwork;fpinfosobj.f_fapiaodate\=fakaobj.DataContext.f_deliverydate;cardid1.Load()}" Width="50" />
                                <Button Content="关 闭" Margin="5" Click="{m:Exp Str=fakachild.Close()}" Width="50">
                                </Button>
                            </StackPanel>
                            <StackPanel  Orientation="Horizontal" HorizontalAlignment="Right">
                            </StackPanel>
                        </toolkit:DockPanel>
                        <Grid  toolkit:DockPanel.Dock="Top" Margin="0" Height="Auto">
                            <Grid.Background>
                                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                    <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                    <GradientStop Color="#FFDDF2FD" Offset="1"/>
                                </LinearGradientBrush>
                            </Grid.Background>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="30"/>
                                <RowDefinition Height="2"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130"/>
                                <ColumnDefinition Width="0.323*"/>
                                <ColumnDefinition Width="150"/>
                                <ColumnDefinition Width="0.346*"/>
                                <ColumnDefinition Width="145"/>
                                <ColumnDefinition Width="0.331*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="用户编号：" Margin="0,6,0,0" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="姓      名：" Grid.Column="2" Margin="0,6,0,0" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="小区地址：" Grid.Column="4" Margin="0,6,0,0" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="小区名称：" Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="气表品牌：" Grid.Column="2" Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="气表型号：" Grid.Column="4" Margin="0,6,0,0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="气表厂家：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="用户类型：" Grid.Column="2" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
                            <TextBlock Text="用气性质：" Grid.Column="4" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
