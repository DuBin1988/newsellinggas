<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.民用户建档">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,-101,0">

            <!--抄表员列表-->
            <o:ObjectList Name="inputtorlist" EntityType="t_inputtor" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from t_inputtor where f_branch\=\'\{LoginUser.f_fengongsi\}\'}"  HasEmptyRow="False"/>

            <!--o:ObjectList Name="stairlist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from t_stairprice}"/-->

            <!--气表品牌列表-->
            <o:ObjectList Name="pinpailist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from qibiao as qibiao left join fetch qibiao.qbnumber}"/>
            <!--用气性质列表-->
            <o:ObjectList Name="usegaslist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from gasproper as gasproper left join fetch gasproper.pricetype where f_gasproperties\=\'民用\'}"/>
            <o:ObjectList Name="chaobiaoyuanlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='抄表员'"/>
            <o:ObjectList Name="jiaofeilist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='交费类型'"/>
            <o:ObjectList Name="usertypelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用户类型查询用'"/>
            <o:ObjectList Name="kaipiaolist" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='开票类型'"/>
            <o:ObjectList Name="qblxlist" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='气表类型'"/>
            <!--打开子窗口对象-->
            <o:ChildWindowObj Name="openkafei" CWName="/Pages/高级管理员登录" />
            <o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" />
            <!--机表对象
            <o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
					EntityType="t_userfiles"	
					IsInit="{m:Exp Str=SaveAction.State\=\=$End}">
                <o:PropertySetter PropertyName="f_comtype" Default="天然气公司"/>
                <o:PropertySetter PropertyName="f_times" Default="0"/>
                <o:PropertySetter PropertyName="f_xiangou" Default="0"/>
                <o:PropertySetter PropertyName="f_zhye" Default="0"/>
                
                <o:PropertySetter PropertyName="f_accountzhye" Default="0"/>
                <o:PropertySetter PropertyName="f_state" Default="正常"/>
                <o:PropertySetter PropertyName="f_usertype" Default="民用"/>
                <o:PropertySetter PropertyName="f_wallhangboiler" Default="{m:Exp Str=False}"/>
                <o:PropertySetter PropertyName="f_kitchen" Default="{m:Exp Str=False}"/>
                <o:PropertySetter PropertyName="f_waterheater" Default="{m:Exp Str=False}"/>
                <o:PropertySetter PropertyName="f_gasmeteraccomodations" Default="0"/>
                <o:PropertySetter PropertyName="f_gasmetermanufacturers" Value="{m:Exp Str=CoboxPinpai.SelectedItem.f_changjia}"/>
                <o:PropertySetter PropertyName="f_flownum" Default="0" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_flownum}"/>
                <o:PropertySetter PropertyName="f_type" Default="{m:Exp Str=userfile.DataContext.f_gasmeterstyle}" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_type}"/>
                <o:PropertySetter PropertyName="f_maichong" Default="0" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_maichong}"/>
                <o:PropertySetter PropertyName="f_watchnum" Default="0" Value="{m:Exp Str=DaXiaoKa.SelectedItem.f_watchnum}"/>
                <o:PropertySetter PropertyName="f_metergasnums" Default="0"  NoDependValue="0"/>
                <o:PropertySetter PropertyName="f_cumulativepurchase" Default="0"  NoDependValue="0"/>
                <o:PropertySetter PropertyName="f_dateofopening" Default="{m:Exp Str=SysTime.Today}"/>
                <o:PropertySetter PropertyName="f_kaihuql" Value="{m:Exp Str=areafiles.SelectedItem.f_accountVolume}"/>
                <o:PropertySetter PropertyName="f_whethergivecard" Default="未发"  />
                <o:PropertySetter PropertyName="f_PatchCard"  Default="0" />
            </o:GeneralObject> -->
            <!--用户ming-->
            <o:ObjectList Name="statelistnull" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}"  Path="from paramvalue where param.name='用户状态查询用' order by id"/>
            <!--楼号列表-->
            <o:ObjectList Name="louhaolist" EntityType="t_gasaddress" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$select new map(f_cusDom as f_cusDom) from t_gasaddress where f_districtname like \'%\{areafilesearch2.DataContext.f_districtname\}%\' group by f_cusdom}"/>

            <!--单元列表-->
            <o:ObjectList Name="danyuanlist" EntityType="t_gasaddress" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$select new map(f_cusDy as f_cusDy) from t_gasaddress where f_districtname like \'%\{areafilesearch2.DataContext.f_districtname\}%\' and f_cusDom \= \'\{CoboxGasPro2.SelectedItem.f_cusDom\}\' group by f_cusDy}"/>

            <!--楼层列表-->
            <o:ObjectList Name="loucenglist" EntityType="t_gasaddress" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$select new map(f_cusFloor as f_cusFloor) from t_gasaddress where f_districtname like \'%\{areafilesearch2.DataContext.f_districtname\}%\'  and f_cusDom \= \'\{CoboxGasPro2.SelectedItem.f_cusDom\}\'   and f_cusDy \= \'\{CoboxGasPro3.SelectedItem.f_cusDy\}\'  group by f_cusFloor}"/>


            <o:ObjectList Name="stairl" WebClientInfo="{StaticResource dbclient}"
             	Path="{m:Exp Str=CoboxGasPro.SelectedValue!\=null:$from t_stairprice where f_gasproperties\=\'\{CoboxGasPro.SelectedValue\}\' and f_branch\=\'\{LoginUser.f_fengongsi\}\'$\,$null$}"/>

            <!--判断用户是否存在-->
            <o:GeneralObject Name="userexist" EntityType="t_userinfo" WebClientInfo="{StaticResource dbclient}" 
		  	Path="{m:Exp Str=userfile.DataContext.f_userid \!\=null:$one/select new Map(count(*) as count) from t_userinfo where f_userid\=\'\{userfile.DataContext.f_userid\}\'$\,$aaa$}" 
			IsInit="{m:Exp Str=userfile.DataContext.State\=\=$End$ }">
            </o:GeneralObject>



            <!--用户对象-->
            <o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
					EntityType="t_userinfo"
					
					IsInit="{m:Exp Str=SaveAction.State\=\=$End}">

                <o:PropertySetter PropertyName="f_usertype" Default="民用"/>
                <o:PropertySetter PropertyName="f_zhye" Default="0"/>
                <o:PropertySetter PropertyName="f_metergasnums" Default="0"  NoDependValue="0"/>
                <o:PropertySetter PropertyName="f_cumulativepurchase" Default="0"  NoDependValue="0"/>

                <!--o:PropertySetter PropertyName="f_unitid" NoDependValue="{m:Exp Str=userfilego.f_unitid}"/-->
                <!--o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=userfilego.f_username}"/-->
                <!--o:PropertySetter PropertyName="f_districtname" NoDependValue="{m:Exp Str=userfilego.f_districtname}"/-->
                <!--o:PropertySetter PropertyName="f_address" NoDependValue="{m:Exp Str=userfilego.f_address}"/-->
                <!--o:PropertySetter PropertyName="f_apartment" NoDependValue="{m:Exp Str=userfilego.f_apartment}"/-->
                <!--o:PropertySetter PropertyName="f_linkname" NoDependValue="{m:Exp Str=userfilego.f_linkname}"/-->
                <!--o:PropertySetter PropertyName="f_city" NoDependValue="{m:Exp Str=userfilego.f_city}"/-->
                <!--o:PropertySetter PropertyName="f_area" NoDependValue="{m:Exp Str=userfilego.f_area}"/-->
                <!--o:PropertySetter PropertyName="f_road" NoDependValue="{m:Exp Str=userfilego.f_road}"/-->
                <!--o:PropertySetter PropertyName="f_cusDy" NoDependValue="{m:Exp Str=userfilego.f_cusDy}"/-->
                <!--o:PropertySetter PropertyName="f_cusDom" NoDependValue="{m:Exp Str=userfilego.f_cusDom}"/-->
                <!--o:PropertySetter PropertyName="f_cusFloor" NoDependValue="{m:Exp Str=userfilego.f_cusFloor}"/-->
                <!--o:PropertySetter PropertyName="f_credentials" NoDependValue="{m:Exp Str=userfilego.f_credentials}"/-->
                <!--o:PropertySetter PropertyName="f_idnumber" NoDependValue="{m:Exp Str=userfilego.f_idnumber}"/-->
                <!--o:PropertySetter PropertyName="f_phone" NoDependValue="{m:Exp Str=userfilego.f_phone}"/>
				<o:PropertySetter PropertyName="f_phone1" NoDependValue="{m:Exp Str=userfilego.f_phone1}"/-->
                <o:PropertySetter PropertyName="f_usertype" NoDependValue="民用"/>
                <!--o:PropertySetter PropertyName="f_gasproperties" NoDependValue="{m:Exp Str=userfilego.f_gasproperties}"/-->
                <!--o:PropertySetter PropertyName="f_yhxz" NoDependValue="{m:Exp Str=userfilego.f_yhxz}"/>
				<o:PropertySetter PropertyName="f_userstate" NoDependValue="{m:Exp Str=userfilego.f_userstate}"/-->
                <!--o:PropertySetter PropertyName="f_payment" NoDependValue="{m:Exp Str=userfilego.f_payment}"/-->
                <!--o:PropertySetter PropertyName="f_dateofopening" NoDependValue="{m:Exp Str=userfilego.f_dateofopening}"/-->
                <!--o:PropertySetter PropertyName="f_fireopening" NoDependValue="{m:Exp Str=userfilego.f_fireopening}"/-->
                <o:PropertySetter PropertyName="f_yytdepa" NoDependValue="{m:Exp Str=userfilego.f_yytdepa}"/>
                <!--<o:PropertySetter PropertyName="f_yytoper" NoDependValue="{m:Exp Str=userfilego.f_yytoper}"/>
                <o:PropertySetter PropertyName="f_yytdate" NoDependValue="{m:Exp Str=userfilego.f_yytdate}"/>
                <o:PropertySetter PropertyName="f_filiale" NoDependValue="{m:Exp Str=userfilego.f_filiale}"/>
                <o:PropertySetter PropertyName="f_fengongsinum" NoDependValue="{m:Exp Str=userfilego.f_fengongsinum}"/-->
                <o:PropertySetter PropertyName="f_orgstr" Default="{m:Exp Str=LoginUser.orgpathstr}"/>
                <o:PropertySetter PropertyName="f_stair1amount" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair1amount}"  />
                <o:PropertySetter PropertyName="f_stair1price" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair1price}"  />
                <o:PropertySetter PropertyName="f_stair2amount" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair2amount}"  />
                <o:PropertySetter PropertyName="f_stair2price" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair2price}"  />
                <o:PropertySetter PropertyName="f_stair3amount" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair3amount}"  />
                <o:PropertySetter PropertyName="f_stair3price" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair3price}"  />
                <o:PropertySetter PropertyName="f_stair4price" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stair4price}"  />
                <o:PropertySetter PropertyName="f_stairmonths" NoDependValue="{m:Exp Str=CoboxStair.SelectedItem.f_stairmonths}"  /> 
                <o:PropertySetter PropertyName="f_zherownum" Default="13" /> 

            </o:GeneralObject>
            <o:GeneralObject Name="temp" EntityType="" Source="{m:Exp Str=TabControl.SelectedIndex\=\=0:areafiles.SelectedItem\,areafiles2.SelectedItem}"/>

            <!--小区列表-->
            <o:PagedObjectList Name="areafileslist" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$from t_area where \{areafilesearch.DataContext.Condition\} and f_branch\=\'\{LoginUser.f_fengongsi\}\'}"
				 IsOld="{m:Exp Str=SaveAction.State\=\=$Free$ or SaveAction.State\=\=$End$}"
				SumNames=","
				PageSize="{m:Exp Str=pager.PageSize}" PageIndex="{m:Exp Str=pager.PageIndex}"/>


            <!--地址列表-->
            <o:PagedObjectList Name="areafileslist2" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$from t_gasaddress where \{areafilesearch2.DataContext.Condition\}}"
				IsOld="{m:Exp Str=SaveAction.State\=\=$End$}"
				SumNames=","
				PageSize="{m:Exp Str=pager2.PageSize}" PageIndex="{m:Exp Str=pager2.PageIndex}"/>
            <!--用户名列表>
			<o:ObjectList Name="usernamelist" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$from t_userinfo }"
				 IsOld="{m:Exp Str=SaveAction.State\=\=$Free$ or SaveAction.State\=\=$End$}"
				/-->
            <!--小区对象areas.f_usernumber-->
            <o:GeneralObject Name="areas" EntityType="t_area" WebClientInfo="{StaticResource dbclient}"
                  Path="{m:Exp Str=$one/from t_area where id\=\'\{areafiles.SelectedItem.id\}\'}" 
				  IsInit="{m:Exp Str='SaveAction.State==$End'}"/>
            <o:ObjectList Name="zhengjianlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='证件类型'"/>
            <!--更新用气地址表已建档案-->
            <b:HQLAction Name="updategasad"  WebClientInfo="{StaticResource dbclient}" 
				HQL="{m:Exp Str=$update t_gasaddress set f_state\=\'已建档\' where id in (\{areafiles.SelectedItems.ToString(id)\=>SelectionChanged\})}"/>

            <!--单值用户编号对象ksnumobj.value-->
            <o:GeneralObject Name="ksnumobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				Path="{m:Exp Str=$one/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\户\号\'}" 
				IsInit="{m:Exp Str='SaveAction.State==$End'}"/>

            <!--单值用户编号对象,用于保存时获取用户编号-->
            <o:GeneralObject Name="ksnumobj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
				LoadOnPathChanged="False"  Path="{m:Exp Str=$sernum/from t_singlevalue where name \= \'\{userfile.DataContext.f_filiale\}\户\号\' /value$}"/>

            <b:SyncActionFactory Name="SaveAction"   >
                <!--获取编号-->
                <b:CallMethodAction    Name="getbianhao" TargetObject="{m:Exp Str=ksnumobj1}"    MethodName="Load"/>
                <!--保存数据-->
                <b:CallMethodAction    Name="saveu" TargetObject="{m:Exp Str=userfile.DataContext}"    MethodName="Save"/>
                <!--b:CallMethodAction    Name="saveue" TargetObject="{m:Exp Str=userinfogo}"    MethodName="Save"/-->

            </b:SyncActionFactory>
        </m:ResourceLoad>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="0.25*"/>
                <ColumnDefinition Width="0.75*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=areafiles.ItemsSource.IsBusy}" >
                <sdk:TabControl Name="TabControl" BorderThickness="1" BorderBrush="#FF9AA2AB" Margin="0" Padding="2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" >
                    <sdk:TabItem Header="小区建档">
                        <toolkit:DockPanel Width="Auto"   >
                            <toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="22">
                                <TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{areafiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="0,0,0,1"/>
                                <sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=areafiles}"  Margin="0,0,0,-2" PageSize="20"/>
                            </toolkit:DockPanel>
                            <toolkit:DockPanel  x:Name="areafilesearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5" Background="#FFEAF8FF"  >
                                <a:Data.Context>
                                    <o:SearchObject />
                                </a:Data.Context>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="70"/>
                                        <ColumnDefinition Width="50*"/>
                                        <ColumnDefinition Width="50*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="小区名称：" Margin="5,4,0,4" />
                                    <TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_districtname}" Width="Auto" Margin="0" Height="25" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_districtname"  Operator="f_districtname like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="小区地址：" Margin="5,4,0,4" />
                                    <TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_address}" Width="Auto" Margin="0" Height="25" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_address"  Operator="f_address like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <Button Content="查  询" x:Name="areasearchbutton" Grid.Column="2" Grid.Row="1" Click="{m:Exp Str=areafilesearch.DataContext.Search()}" Margin="0,0,5,-2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
                                </Grid>
                            </toolkit:DockPanel>
                            <sdk:DataGrid x:Name="areafiles" Margin="0" ItemsSource="{m:Exp Str=areafileslist}"
							AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True"
							>
                                <sdk:DataGrid.Columns>
                                    <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                                    <sdk:DataGridTextColumn Header="小区名称" Binding="{Binding f_districtname}"/>
                                    <sdk:DataGridTextColumn Header="小区地址" Binding="{Binding f_address}"/>
                                    <sdk:DataGridTextColumn Header="街  道" Binding="{Binding f_road}"/>
                                </sdk:DataGrid.Columns>
                            </sdk:DataGrid>
                        </toolkit:DockPanel>
                    </sdk:TabItem>
                    <sdk:TabItem Header="地址建档" >
                        <toolkit:DockPanel Width="Auto"   >
                            <toolkit:DockPanel   Background="{x:Null}" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="22">
                                <TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{areafiles2.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="0,0,0,1"/>
                                <sdk:DataPager x:Name="pager2" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=areafiles2}"  Margin="0,0,0,-2" PageSize="20"/>
                            </toolkit:DockPanel>
                            <toolkit:DockPanel  x:Name="areafilesearch2" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5" Background="#FFEAF8FF"  >
                                <a:Data.Context>
                                    <o:SearchObject />
                                </a:Data.Context>
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="70"/>
                                        <ColumnDefinition Width="50*"/>
                                        <ColumnDefinition Width="50*"/>
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                    </Grid.RowDefinitions>
                                    <TextBlock TextWrapping="Wrap" Text="小区名称：" Margin="5,4,0,4" />
                                    <TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_districtname}" Width="Auto" Margin="0" Height="25" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_districtname"  Operator="f_districtname like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="楼号：" Margin="5,4,0,4" />
                                    <ComboBox x:Name="CoboxGasPro2" DisplayMemberPath="f_cusDom"  SelectedValuePath="f_cusDom" ItemsSource="{m:Exp Str=louhaolist}" SelectedValue="{m:Bind Path=f_cusDom}"  Grid.Column="1"  Grid.Row="1" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_cusDom" Operator="f_cusDom='this'" />
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="单元：" Margin="5,4,0,4" />
                                    <ComboBox x:Name="CoboxGasPro3" DisplayMemberPath="f_cusDy"  SelectedValuePath="f_cusDy" ItemsSource="{m:Exp Str=danyuanlist}" SelectedValue="{m:Bind Path=f_cusDy}"  Grid.Column="1"  Grid.Row="2" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_cusDy" Operator="f_cusDy='this'" />
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Grid.Row="3" Text="楼层：" Margin="5,4,0,4" />
                                    <ComboBox x:Name="CoboxGasPro4" DisplayMemberPath="f_cusFloor"  SelectedValuePath="f_cusFloor" ItemsSource="{m:Exp Str=loucenglist}" SelectedValue="{m:Bind Path=f_cusFloor}"  Grid.Column="1"  Grid.Row="3" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_cusFloor" Operator="f_cusFloor='this'" />
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <Button Content="查  询" x:Name="areasearchbutton2" Grid.Column="2" Grid.Row="3" Click="{m:Exp Str=areafilesearch2.DataContext.Search()}" Margin="0,0,5,-2" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="50"/>
                                </Grid>
                            </toolkit:DockPanel>
                            <sdk:DataGrid x:Name="areafiles2" Margin="0" ItemsSource="{m:Exp Str=areafileslist2}"
							AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True"
							>
                                <sdk:DataGrid.Columns>
                                    <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                                    <sdk:DataGridTextColumn Header="街  道" Binding="{Binding f_road}"/>
                                    <sdk:DataGridTextColumn Header="小  区" Binding="{Binding f_districtname}"/>
                                    <sdk:DataGridTextColumn Header="建档状态" Binding="{Binding f_state}"/>
                                    <sdk:DataGridTextColumn Header="楼  号" Binding="{Binding f_cusDom}"/>
                                    <sdk:DataGridTextColumn Header="单  元" Binding="{Binding f_cusDy}"/>
                                    <sdk:DataGridTextColumn Header="楼  层" Binding="{Binding f_cusFloor}"/>
                                    <sdk:DataGridTextColumn Header="房  号" Binding="{Binding f_apartment}"/>
                                </sdk:DataGrid.Columns>
                            </sdk:DataGrid>
                        </toolkit:DockPanel>
                    </sdk:TabItem>
                </sdk:TabControl>
            </c:BusyIndicatorEx>
            <c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy or SaveAction.IsBusy}" Margin="5,0,0,0" Grid.Column="1" >
                <toolkit:DockPanel>
                    <!--按钮-->
                    <toolkit:DockPanel toolkit:DockPanel.Dock="Bottom" HorizontalAlignment="Center" VerticalAlignment="Center" >
                        <Button x:Name="save" Content="保  存" Margin="5" 
							  IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors}"
							Click="{m:Exp Str='SaveAction.Invoke()'}" Width="88" />
