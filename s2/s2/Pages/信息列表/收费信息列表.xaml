<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
    
	mc:Ignorable="d" FontSize="13"  d:DesignWidth="1600" d:DesignHeight="1200"
	x:Class="Com.Aote.Pages.收费信息列表">
    <Grid>
        <m:ResourceLoad Margin="0,0,-12,0">
            <!--阶梯气价信息列表-->
            <o:ObjectList Name="stairlist" EntityType="t_stairprice" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=zonggongsi.value\=\=LoginUser.f_fengongsi:$from t_stairprice$\,$from t_stairprice where f_branch\=\'\{LoginUser.f_fengongsi\}\'}"  HasEmptyRow="True" />
            <o:ObjectList Name="jiaofeilistnull" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='是否' order by id"/>
			 <o:ExcelObj Name="excel" Bind="Index,f_userid,f_username,f_districtname,f_apartment,f_usertype,f_cardid,f_gaswatchbrand,f_gaspricetype,f_gasprice,f_pregas,f_preamount,f_beginfee,f_endfee,f_amountmaintenance,f_totalcost,f_comtype,f_invoicenum,f_payfeevalid,f_payfeetype,f_filiale,f_sgnetwork,f_sgoperator,f_deliverydate" Source="{m:Exp Str=daninfos}"/>
            <!--操作员列表对象-->
             <o:ObjectList Name="czylistnull" EntityType="t_user" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=zonggongsi.value\=\=LoginUser.f_fengongsi:$from t_user$\,$from t_user where f_fengongsi\=\'\{LoginUser.f_fengongsi\}\'}" HasEmptyRow="True"/>
              <!--公司类型列表对象-->
            <o:ObjectList Name="gongsitypelist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='公司类型' order by id"/>
            <o:ObjectList Name="gongsitypelistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='公司类型' " HasEmptyRow="True"/>
           <!--分公司列表对象-->
            <o:ObjectList Name="fgslist" EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='分公司' order by id"/>
            <!--用户类型查询用-->
            <o:ObjectList Name="usertypelist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用户类型查询用' order by id"/>
            <o:ObjectList Name="usertypelistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='用户类型查询用' " HasEmptyRow="True"/>
            <!--付款方式-->
            <o:ObjectList Name="fukuanfangshi"  EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
            <o:ObjectList Name="fukuanfangshinull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' " HasEmptyRow="True"/>
            <!--总公司名称-->
            <o:GeneralObject Name="zonggongsi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="{m:Exp Str=$one/from t_singlevalue where name \= \'总公司名称\'}"   >
            </o:GeneralObject>
            <!--气表品牌-->
            <o:ObjectList Name="pinpailist" WebClientInfo="{StaticResource dbclient}" Path="{m:Exp Str=$from qibiao as qibiao left join fetch qibiao.qbnumber}"/>
			<!--气价类型-->
            <o:ObjectList Name="qjlist"  EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='气价类型' order by id"/>
            <o:ObjectList Name="qjlistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='气价类型' " HasEmptyRow="True"/>
            <!--收费类型查询用-->
            <o:ObjectList Name="payfeetypelist"  EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='收费类型查询用' order by id"/>
            <o:ObjectList Name="payfeetypelistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='收费类型查询用' " HasEmptyRow="True"/>
            <!--是否有效查询用-->
            <o:ObjectList Name="youxiaolist"  EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='是否有效查询用' order by id"/>
            <o:ObjectList Name="youxiaolistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='是否有效查询用' " HasEmptyRow="True"/>
            <o:ObjectList Name="yinhanglist1"  EntityType="paramvalue" HasEmptyRow="True" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='银行名'"/>
            <o:ObjectList Name="yinhanglistnull" EntityType="paramvalue" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='银行名' " HasEmptyRow="True"/>

			 <o:ObjectList Name="SecondStairlist" EntityType="t_organization" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from t_organization where f_parentname\=\'\{search1.DataContext.FirstStair\}\'$}" />
            <o:ObjectList Name="ThirdStairStairlist" EntityType="t_organization" WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from t_organization where f_parentname\=\'\{search1.DataContext.SecondStair\}\'$}" />
            <o:ObjectList Name="FourthStairlist"  WebClientInfo="{StaticResource dbclient}"  Path="{m:Exp Str=$from t_organization where f_parentname\=\'\{search1.DataContext.ThirdStair\}\'$}" />
                       
        </m:ResourceLoad>
        <sdk:TabControl >
            <sdk:TabItem Header="    收费信息列表    " >
                <Grid x:Name="LayoutRoot">
                    <m:ResourceLoad Margin="0,0,-12,0">
                        <o:ChildWindowObj Name="openchild1" CWName="/Pages/信息列表/个人信息界面" ParamObj="daninfos.SelectedItem" />
                  
                    </m:ResourceLoad>
                    <Grid> 
                        <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=daninfos.ItemsSource.IsBusy or toExcel.IsBusy}">
                            <toolkit:DockPanel>
                                <Grid x:Name="search1"  toolkit:DockPanel.Dock="Top"  Background="#FFCFEFFF" Width="Auto" HorizontalAlignment="Stretch" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                        <RowDefinition Height="28"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="90"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <a:Data.Context>
                                        <o:SearchObject />
                                    </a:Data.Context>

                                    <TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="0,2,0,1" Height="25" TextAlignment="Right" />
                                    <TextBox Name="tj1" TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_userinfoid}" Width="Auto" Margin="0,2,0,1" Height="25" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_userinfoid"  Operator="f_userinfoid like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Text="卡　　号：" Grid.Row="1" Margin="0,2,0,1" Height="25" TextAlignment="Right"/>
                                    <TextBox Name="tj2" TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" Text="{m:Bind Path=f_cardid}" Width="Auto" Margin="0,2,0,1" Height="25" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_cardid"  Operator="f_cardid='this'"/>
                                        </a:Data.Prop>
                                    </TextBox>

                                    <TextBlock TextWrapping="Wrap" Text="用户姓名：" Grid.Row="2" Margin="0,2,0,1" Height="25" TextAlignment="Right"/>
                                    <TextBox Name="tj7" TextWrapping="Wrap" Grid.Row="2" Grid.Column="1" Text="{m:Bind Path=f_username}" Width="Auto" Margin="0,2,0,1" Height="25" Grid.ColumnSpan="1" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_username"  Operator="f_username like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                      <TextBlock TextWrapping="Wrap" Text="气表品牌：" Grid.Column="4"  Margin="0,2,0,1" Height="25" TextAlignment="Right" Grid.Row="1" />
                                    <ComboBox Name="tj5" SelectedValue="{m:Bind Path=f_gaswatchbrand}" Grid.Column="5" ItemsSource="{m:Exp Str=pinpailist}"  Margin="0,2,0,1"  SelectedValuePath="f_pinpai"  Height="25" Grid.Row="1">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding f_pinpai}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_gaswatchbrand"  Operator="f_gaswatchbrand = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Grid.Row="4" Grid.Column="0" Margin="0,2,0,1" Height="25" TextAlignment="Right" Text="收费日期 从："/>
                                    <sdk:DatePicker Name="tj4" SelectedDate="{m:Bind Path=f_yytdate1}" Grid.Row="4" Grid.Column="1" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_yytdate1"   
											Operator="f_deliverydate >= substring('this',1,10)"/>
                                        </a:Data.Prop>
                                    </sdk:DatePicker>
                                    <sdk:DatePicker Name="tj8" SelectedDate="{m:Bind Path=f_yytdate2}" Grid.Row="4" Grid.Column="3" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" Grid.ColumnSpan="1" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_yytdate2"    
											Operator="f_deliverydate ^= substring('this',1,10) + ' 23:59:59'"/>
                                        </a:Data.Prop>
                                    </sdk:DatePicker>
                                    <TextBlock TextWrapping="Wrap" Text="到：" Grid.Column="2" Grid.Row="4" Margin="0,2,0,1" Height="25" TextAlignment="Right"/>
                                    <TextBlock TextWrapping="Wrap" Text="用户类型：" Grid.Column="2"  Margin="0,2,0,1" Height="25" TextAlignment="Right" Grid.Row="1" />
                                    <ComboBox x:Name="tja"  SelectedValue="{m:Bind Path=f_usertype}" Grid.Row="1" Grid.Column="3" ItemsSource="{m:Exp Str=usertypelistnull}"  Margin="0,2,0,1"  SelectedValuePath="name"  Height="25" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto" VerticalAlignment="Stretch">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding name}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_usertype"  Operator="f_usertype = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Text="收费类型：" Grid.Column="2"  Margin="0" Height="25" TextAlignment="Right" Grid.Row="2" />
                                    <ComboBox x:Name="tjb"  SelectedValue="{m:Bind Path=f_payfeetype}" Grid.Row="2" Grid.Column="3" ItemsSource="{m:Exp Str=payfeetypelistnull}"  Margin="0"  SelectedValuePath="name"  Height="25" HorizontalAlignment="Stretch" Width="Auto">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding name}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_payfeetype"  Operator="f_payfeetype = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Text="是否有效：" Grid.Column="6"  Margin="0" Height="25" TextAlignment="Right" Grid.Row="1" />
                                    <ComboBox x:Name="tjc"  SelectedValue="{m:Bind Path=f_payfeevalid}" Grid.Row="1" Grid.Column="7" ItemsSource="{m:Exp Str=youxiaolistnull}"  Margin="0"  SelectedValuePath="name"  Height="25" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding name}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_payfeevalid"  Operator="f_payfeevalid = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Text="操 作 员：" Grid.Column="6"  Margin="0" Height="25" TextAlignment="Right" />
                                    <ComboBox x:Name="tjczy"  SelectedValue="{m:Bind Path=f_sgoperator}" Grid.Column="7" ItemsSource="{m:Exp Str=czylistnull}"  Margin="0"  SelectedValuePath="name"  Height="25" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding name}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_sgoperator"  Operator="f_sgoperator = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBlock TextWrapping="Wrap" Text="付款方式：" Grid.Column="4"  Margin="0" Height="25" TextAlignment="Right" Grid.Row="4" />
                                    <TextBlock TextWrapping="Wrap" Grid.Column="4" Margin="-1,2,0,1" Height="25" TextAlignment="Right" Text="发票号 从：" HorizontalAlignment="Left" Width="90" Grid.Row="2"/>
                                    <TextBox x:Name="tj4_Copy" Text="{m:Bind Path=startinvoicenum}" Grid.Row="2" Grid.Column="5" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0" Height="25" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="startinvoicenum"  Operator="f_invoicenum >= this"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBox x:Name="tj8_Copy" Text="{m:Bind Path=endinvoicenum}" Grid.Row="2" Grid.Column="7" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,0,0,1" Height="25" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="endinvoicenum"  Operator="f_invoicenum ^= this"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Text="到：" Grid.Column="6" Margin="0,2,0,1" Height="25" TextAlignment="Right" RenderTransformOrigin="2.625,1.32" HorizontalAlignment="Center" Grid.Row="2"/>
                                    <TextBlock TextWrapping="Wrap" Text="小区名称：" Grid.Column="4"  Margin="9,3,0,0" Height="25" TextAlignment="Right" HorizontalAlignment="Left" Width="80" />
                                    <TextBox x:Name="tjwhf_Copy" TextWrapping="Wrap" Grid.Row="0" Grid.Column="5" Text="{m:Bind Path=f_districtname}" Width="Auto" Margin="0" Height="25" Grid.ColumnSpan="1" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_districtname"  Operator="f_districtname like '%this%'"/>
                                        </a:Data.Prop>
                                    </TextBox>
                                    <ComboBox x:Name="tjc_Copy"  SelectedValue="{m:Bind Path=f_payment}" Grid.Row="4" Grid.Column="5" ItemsSource="{m:Exp Str=fukuanfangshinull}"  Margin="0"  SelectedValuePath="name"  Height="25" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Width="Auto">
                                        <ComboBox.ItemTemplate>
                                            <DataTemplate>
                                                <TextBlock Text="{Binding name}" Height="20"></TextBlock>
                                            </DataTemplate>
                                        </ComboBox.ItemTemplate>
                                        <a:Data.Prop>
                                            <o:PropertySetter PropertyName="f_payment"  Operator="f_payment = 'this'"/>
                                        </a:Data.Prop>
                                    </ComboBox>
                                    <TextBox x:Name="tj1_Copy" TextWrapping="Wrap" Grid.Column="3" Text="{m:Bind Path=f_userid}" Width="Auto" Margin="0,2,0,1" Height="25" HorizontalAlignment="Stretch" >
                                    	<a:Data.Prop>
                                    		<o:PropertySetter PropertyName="f_userid" Default="{m:Exp Str=LoginUser.character}" Operator="f_userid like '%this%'"/>
                                    	</a:Data.Prop>
                                    </TextBox>
                                    <TextBlock TextWrapping="Wrap" Text="表编号：" Grid.Column="2"  Margin="0,2,0,1" Height="25" TextAlignment="Right" />
                               <Button Content=" 查  询" x:Name="dansearchbutton" Click="{m:Exp Str=search1.DataContext.Search()}" Margin="0,2,0,1" FontSize="14" Width="60" Grid.Column="6" Grid.Row="4" HorizontalAlignment="Right" />

                                        <Button Content=" 置  空"  Width="60" Margin="5,0,0,0" Click="{m:Exp Str=search1.DataContext.New()}" FontSize="14" HorizontalAlignment="Left" Grid.Column="7" Grid.Row="4"  />
                                        <Button Content="打印" Click="{m:Exp Str=print.Print()}" Margin="70,0,0,0" Width="60" Grid.Row="4" Grid.Column="7" HorizontalAlignment="Left" />
                                        <!--Button Content="导出Excel" Click="{m:Exp Str=excel.Export()}"	toolkit:DockPanel.Dock="Right" Background="white" Grid.Column="7" Grid.Row="3"/-->

                                        <c:Excel x:Name="toExcel" HQL="{m:Exp Str=$sql:\{daninfos.ItemsSource.HQL\}}" 
                                    		Completed="{m:Exp Str=downLoad.Down()}"
                                    		Path="{m:Exp Str=$\{server.BaseAddress\}/excel/\{daninfos.ItemsSource.Count\}/f_userid:用户编号|f_username:用户姓名|f_address:地址|f_districtname:小区名称|f_apartment:门牌号|f_cardid:卡号|f_gasprice:气价|f_pregas:气量|f_preamount:金额|f_zhinajin:违约金|f_grossproceeds:充值|f_cardfees:发卡费|f_payfeevalid:是否有效|f_sgnetwork:网点|f_sgoperator:操作员|f_invoicenum:发票号|f_deliverydate:收费日期|f_usertype:用户类型|f_gaswatchbrand:气表品牌|f_gaspricetype:气价类型|f_beginfee:维护费起始日期|f_endfee:维护费截止日期|f_amountmaintenance:维护费|f_payment:付款方式|f_zhye:上期余额|f_benqizhye:本期余额|f_invoicenum:发票号|f_payfeetype:收费类型|f_filiale:所属公司|f_comtype:公司类型|f_deliverytime:交费时间|id:序号$}"/>
                                        <c:DownLoad x:Name="downLoad" Path="{m:Exp Str=$\{server.BaseAddress\}/file/\{toExcel.FileName\}}" Filter="(*.xls)|*.xls"/>
                                        <Button Content="导出Excel" IsEnabled="{m:Exp Str=daninfos.ItemsSource.Count!\=0:True\,False}" Click="{m:Exp Str=toExcel.ToExcel()}" RenderTransformOrigin="1.2,0.1" Grid.Column="7" Grid.Row="4" Width="70" HorizontalAlignment="Left" Margin="140,0,0,0" />
                                        <TextBlock TextWrapping="Wrap" Text="总公司：" Grid.Column="0" Grid.Row="3"  Height="25" TextAlignment="Right" Margin="0,2,0,1"/>
                                        <TextBox x:Name="FirstStair" TextWrapping="Wrap" Grid.Row="3" Grid.Column="1" Text="{m:Bind Path=FirstStair}"   Width="Auto" Margin="0" Height="25" HorizontalAlignment="Stretch" Grid.ColumnSpan="1" IsReadOnly="True" >
                                        	<a:Data.Prop>
                                        		<o:PropertySetter PropertyName="FirstStair" Default="{m:Exp Str=zonggongsi.value}" NoDependValue="{m:Exp Str=zonggongsi.value}" Operator="f_OrgStr like 'this%'"/>
                                        	</a:Data.Prop>
                                        </TextBox>
                                        <TextBlock TextWrapping="Wrap" Text="二级公司：" Grid.Column="2" Grid.Row="3"  Height="25" TextAlignment="Right" Margin="0"/>
                                        <ComboBox x:Name="SecondStair"  IsEnabled="{m:Exp Str=LoginUser.orgpathstr.Split($.$\,1)\=\=null}"  SelectedValue="{m:Bind Path=SecondStair}" Grid.Row="3" Grid.Column="3" ItemsSource="{m:Exp Str=SecondStairlist}" DisplayMemberPath="name" SelectedValuePath="name"  Margin="0" Height="25" HorizontalAlignment="Stretch" Width="Auto">
                                        	<a:Data.Prop>
                                        		<o:PropertySetter PropertyName="SecondStair"  Default="{m:Exp Str=SecondStairlist.Count&gt;0:LoginUser.orgpathstr.Split($.$\,1)\,null}" Operator="f_OrgStr like '%this%'"/>
                                        	</a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock TextWrapping="Wrap" Text="三级公司：" Grid.Column="4" Grid.Row="3"  Height="25" TextAlignment="Right" Margin="0"/>
                                        <ComboBox x:Name="ThirdStair"  IsEnabled="{m:Exp Str=LoginUser.orgpathstr.Split($.$\,2)\=\=null}" SelectedValue="{m:Bind Path=ThirdStair}" Grid.Row="3" Grid.Column="5" ItemsSource="{m:Exp Str=ThirdStairStairlist}"  Margin="0" DisplayMemberPath="name" SelectedValuePath="name"  Height="25" HorizontalAlignment="Stretch" Width="Auto">

                                        	<a:Data.Prop>
                                        		<o:PropertySetter PropertyName="ThirdStair" Default="{m:Exp Str=ThirdStairStairlist.Count&gt;0:LoginUser.orgpathstr.Split($.$\,2)\,null}" Operator="f_OrgStr like '%this%'"/>
                                        	</a:Data.Prop>
                                        </ComboBox>
                                        <TextBlock TextWrapping="Wrap" Text="四级公司：" Grid.Column="6" Grid.Row="3"  Height="25" TextAlignment="Right" Margin="0"/>
                                        <ComboBox x:Name="FourthStair" IsEnabled="{m:Exp Str=LoginUser.orgpathstr.Split($.$\,3)\=\=null}" SelectedValue="{m:Bind Path=FourthStair}" Grid.Row="3" Grid.Column="7" ItemsSource="{m:Exp Str=FourthStairlist}"  Margin="0" DisplayMemberPath="name" SelectedValuePath="name"  Height="25" HorizontalAlignment="Stretch" Width="Auto">
                                        	<a:Data.Prop>
                                        		<o:PropertySetter PropertyName="FourthStair" Default="{m:Exp Str=FourthStairlist.Count&gt;0:LoginUser.orgpathstr.Split($.$\,3)\,null}" Operator="f_OrgStr like '%this%'"/>
                                        	</a:Data.Prop>
                                        </ComboBox>
                                   </Grid>

                                <toolkit:DockPanel   Background="#FFC3E4FD" toolkit:DockPanel.Dock="Bottom" Height="51">
                                    <sdk:DataPager x:Name="pager2" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=daninfos}" Margin="0,0,0,0" PageSize="42" toolkit:DockPanel.Dock="Top" Height="24"   />
