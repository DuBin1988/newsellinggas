<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13"
	x:Class="Com.Aote.Pages.安检计划查询">

    <Grid x:Name="LayoutRoot">
        <m:ResourceLoad Margin="0,0,0,0">
            <!--安检列表-->
            <o:PagedObjectList Name="areafileslist" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=$select distinct t from T_INSPECTION t  left join fetch  t.CHECKPLAN_ID c  where \{areafilesearch.DataContext.Condition\}$}"
				MultiPath="{m:Exp Str=$from T_INSPECTION t where \{areafilesearch.DataContext.Condition\}$}"
				SumNames=","
				PageSize="{m:Exp Str=pager.PageSize}" PageIndex="{m:Exp Str=pager.PageIndex}"/>
            <!--安检状态-->
            <o:ObjectList Name="conditionlist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='安检状态'"/>

        </m:ResourceLoad>

        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="6*"/>
            </Grid.ColumnDefinitions>
            <c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=areafiles.ItemsSource.IsBusy}" >
                <Border BorderThickness="1" BorderBrush="#FF9AA2AB" Margin="0" Padding="2" Width="Auto" Height="Auto" VerticalAlignment="Stretch" >
                    <toolkit:DockPanel Width="Auto"   >
                        <toolkit:DockPanel   Background="#94a8cd" toolkit:DockPanel.Dock="Bottom" Margin="0,0,0,5" Height="25" >
                            <TextBlock TextWrapping="Wrap" Text="{m:Exp Str=$共\{areafiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Right"  VerticalAlignment="Bottom" Margin="0,0,0,1" Foreground="#FF140000" />
                            <sdk:DataPager x:Name="pager" Source="{Binding ItemsSource.Count, Converter={StaticResource DataPagerConverter}, ElementName=areafiles}"  Margin="0,0,0,-2" PageSize="20"/>
                        </toolkit:DockPanel>
                        <toolkit:DockPanel  x:Name="areafilesearch" toolkit:DockPanel.Dock="Top"  Margin="0,0,0,5"   >
                            <a:Data.Context>
                                <o:SearchObject />
                            </a:Data.Context>
                            <Grid Background="#FF94A8CD">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="70"/>
                                    <ColumnDefinition Width="50*"/>
                                    <ColumnDefinition Width="70"/>
                                    <ColumnDefinition Width="50*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="30"/>
                                </Grid.RowDefinitions>
                                <TextBlock TextWrapping="Wrap" Text="计划名称：" Margin="5,4,0,4" Grid.Column="0"/>
                                <TextBox TextWrapping="Wrap" Grid.Column="1" Text="{m:Bind Path=f_name}" Width="Auto" Margin="0" Height="25" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_name"  Operator="c.f_name like 'this%'"/>
                                    </a:Data.Prop>
                                </TextBox>
                                <TextBlock TextWrapping="Wrap" Text="安检情况：" Margin="5,4,0,4" Grid.Column="2"/>
                                <ComboBox Name="condition"   SelectedValue="{m:Bind Path=CONDITION}" Grid.Row="0" Grid.Column="3" ItemsSource="{m:Exp Str=conditionlist}" SelectedValuePath="code"  Margin="0" DisplayMemberPath="name"   Height="25" >

                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="CONDITION"  Operator="t.CONDITION like 'this'"/>
                                    </a:Data.Prop>
                                </ComboBox>
                                <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="从：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_startedate}" Margin="0"  Height="25" Grid.Row="1" Grid.Column="1" VerticalAlignment="Stretch" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_startedate" Value="{m:Exp Str=SysTime.Today}"
								Operator=" DEPARTURE_TIME >= substring('this',1,10)"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>
                                <TextBlock TextWrapping="Wrap" Grid.Row="1" Grid.Column="2" Text="到：" Margin="5,4,0,4" Height="25" FontSize="13.333" HorizontalAlignment="Right" />
                                <sdk:DatePicker SelectedDate="{m:Bind Path=f_enddate}" Margin="0"  Height="25" VerticalAlignment="Stretch" Grid.Row="1" Grid.Column="3" >
                                    <a:Data.Prop>
                                        <o:PropertySetter PropertyName="f_enddate" Value="{m:Exp Str=SysTime.Today}"
								Operator=" substring(DEPARTURE_TIME,1,10) ^= substring('this',1,10)"/>
                                    </a:Data.Prop>
                                </sdk:DatePicker>

                                <Button Content="查询" x:Name="areasearchbutton" Grid.Column="3" Grid.Row="3"  Click="{m:Exp Str=areafilesearch.DataContext.Search()}" Margin="0,0,5,0" HorizontalAlignment="Right" d:LayoutOverrides="Height" Width="70" Height="30" />
                            </Grid>
                        </toolkit:DockPanel>
                        <sdk:DataGrid x:Name="areafiles" Margin="0" ItemsSource="{m:Exp Str=areafileslist}"
							AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True">
                            <sdk:DataGrid.Columns>
                                <sdk:DataGridTextColumn d:IsLocked="True" Header="序号" Binding="{Binding Index}"/>
                                <sdk:DataGridTextColumn Header="计划名称" Binding="{Binding CHECKPLAN_ID.f_name}"/>
                                <sdk:DataGridTextColumn Header="安检情况" Binding="{Binding CONDITION}"/>
                                <sdk:DataGridTextColumn Header="小区名称" Binding="{Binding UNIT_NAME}"/>
                                <sdk:DataGridTextColumn Header="楼号" Binding="{Binding CUS_DOM}"/>
                                <sdk:DataGridTextColumn Header="单元" Binding="{Binding CUS_DY}"/>
                                <sdk:DataGridTextColumn Header="楼层" Binding="{Binding CUS_FLOOR}"/>
                                <sdk:DataGridTextColumn Header="房号" Binding="{Binding CUS_ROOM}"/>
                                <sdk:DataGridTextColumn Header="安检员" Binding="{Binding f_anjianren}"/>
                                <sdk:DataGridTextColumn Header="用户名称" Binding="{Binding f_consumername}"/>
                            </sdk:DataGrid.Columns>
                        </sdk:DataGrid>

                    </toolkit:DockPanel>
                </Border>
            </c:BusyIndicatorEx>
            <Grid x:Name="userfile" DataContext="{m:Exp Str=areafiles.SelectedItem}" Margin="0,0,0,0"  Grid.Column="1" Height="700" >
                <!--非民用主操作区-->
                <sdk:TabControl HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0" Width="810">
                    <sdk:TabItem Header="基本信息">
                        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Width="auto">
                            <Grid Margin="0" HorizontalAlignment="Stretch"  >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="8"/>

                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>

                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>

                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="25"/>
                                    <RowDefinition Height="30"/>
                                    <RowDefinition Height="30"/>

                                    <RowDefinition Height="0"/>
                                    <RowDefinition Height="30"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="120"/>
                                    <ColumnDefinition Width="120"/>
                                    <ColumnDefinition Width="120"/>
                                    <ColumnDefinition Width="164"/>
                                    <ColumnDefinition Width="96"/>
                                    <ColumnDefinition Width="140"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Text="检查情况：" Grid.Column="0" Margin="0" Grid.Row="0" TextWrapping="Wrap" TextAlignment="Right" />
                                <TextBox  Text="{m:Bind Path=CONDITION}" Grid.Column="1"  Grid.Row="0" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Margin="0" Grid.RowSpan="1" IsEnabled="False"/>
                                <TextBlock Text="到达时间：" Grid.Column="2" Margin="0,2,0,23" Grid.Row="0" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=ARRIVAL_TIME}" Grid.Column="3"  Grid.Row="0" TextWrapping="NoWrap" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Margin="0" Grid.RowSpan="1" />
                                <TextBlock Text="离开时间：" Grid.Column="4" Margin="0,2,0,23" Grid.Row="0" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=DEPARTURE_TIME}" Grid.Column="5"  Grid.Row="0" TextWrapping="NoWrap" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Margin="0,0,-18,0" Grid.RowSpan="1" />
                                <TextBlock Text="是否发到访不遇卡：" Grid.Column="2" Margin="0,5,0,20" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=HASNOTIFIED}" Grid.Column="3"  Grid.Row="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto" Margin="0" Grid.RowSpan="1" />
                                <TextBlock Text="用户名：" Grid.Column="0" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" />
                                <TextBox IsEnabled="False" Text="{m:Bind Path=f_consumername}" Grid.Column="1"  Grid.Row="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Margin="0" Grid.RowSpan="1" />
                                <TextBlock Text="IC卡号：" Grid.Column="2" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Margin="0,8,0,17" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_kahao}" Grid.Column="3" Grid.Row="2" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Width="Auto" Margin="0,3,0,22" Grid.RowSpan="2"  />
                                <TextBlock Text="电话：" Grid.Column="0" Margin="0,8,3,17" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right"  HorizontalAlignment="Right" Width="95" Grid.RowSpan="2"  />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_consumerphone}" Grid.Column="1" Margin="0" Grid.Row="2" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Width="Auto"/>
                                <TextBlock Text="市：" Grid.Column="0" Margin="0,2,0,23" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=city}" Grid.Column="1"  Grid.Row="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Width="Auto" Margin="0,3,0,22" Grid.RowSpan="2" />
                                <TextBlock Text="区：" Grid.Column="2" Margin="0,2,0,23" Grid.Row="3"  TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_area}" Grid.Column="3" Margin="0" Grid.Row="3" Grid.ColumnSpan="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto"/>
                                <TextBlock Text="小区名称：" Grid.Column="0" Margin="0,2,0,23" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=UNIT_NAME}" Grid.Column="1"  Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch" Width="Auto" Margin="0,3,0,22" Grid.RowSpan="2" />
                                <TextBlock Text="街道：" Grid.Column="2" Margin="0,2,0,23" Grid.Row="4"  TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=ROAD}" Grid.Column="3" Margin="0" Grid.Row="4" Grid.ColumnSpan="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto"/>
                                <TextBlock Text="楼号：" Grid.Column="4" Margin="0,3,0,22" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=CUS_DOM}" Grid.Column="5" Margin="0" Grid.Row="4" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="单元：" Grid.Column="0" Margin="0,8,0,17" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=CUS_DY}" Grid.Column="1" Margin="0" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" />
                                <TextBlock Text="楼层：" Grid.Column="2" Margin="0,8,0,17" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=CUS_FLOOR}" Grid.Column="3" Margin="0" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" />
                                <TextBlock Text="房号：" Grid.Column="4" Margin="0,8,0,17" Grid.Row="5" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=CUS_ROOM}" Grid.Column="5" Margin="0" Grid.Row="5" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" />
                                <TextBlock Text="用户档案地址：" Grid.Column="0" Margin="0,3,0,22" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_archiveaddress}" Grid.Column="1" Margin="0" Grid.Row="6" Grid.ColumnSpan="3" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="租住：" Grid.Column="2" Margin="0,3,0,5" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_zuzhu}" Grid.Column="3" Margin="0" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="小区性质：" Grid.Column="4" Margin="0,3,0,5" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"  />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_property}" Grid.Column="5" Margin="0" Grid.Row="7" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="是否重点用户：" Grid.Column="0" Margin="0,3,0,5" Grid.Row="7" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_iszhongdian}" Grid.Column="1" Margin="0" Grid.Row="7"  TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="常住人口数：" Grid.Column="4" Margin="0,2,0,23" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox IsEnabled="False"  Text="{m:Bind Path=f_renkou}" Grid.Column="5" Margin="0,3,0,22" Grid.Row="1" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Grid.RowSpan="2" />
                                <TextBlock Text="供暖方式：" Grid.Column="4" Margin="0,3,0,22" Grid.Row="6" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_gongnuan}" Grid.Column="5" Margin="0" Grid.Row="6" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto"  />
                                <TextBlock Text="基表读数：" Grid.Column="2" Margin="0,3,0,22" Grid.Row="10" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2" />
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_jbdushu}" Grid.Column="3" Margin="0,3,0,22" Grid.Row="10" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="表内剩余气量：" Grid.Column="0" Margin="0,6,0,19" Grid.Row="10" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_shengyu}" Grid.Column="1" Margin="0,3,0,22" Grid.Row="10" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="燃气表基表号：" Grid.Column="0" Margin="0,6,0,19" Grid.Row="9" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_biaohao}" Grid.Column="1" Margin="0,3,0,22" Grid.Row="9" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="左右表：" Grid.Column="2" Margin="0,6,0,19" Grid.Row="9" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_rqbiaoxing}" Grid.Column="3" Margin="0,3,0,22" Grid.Row="9" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="基表厂家型号：" Grid.Column="0" Margin="0,3,0,22" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_biaochang}" Grid.Column="1" Margin="0,3,0,22" Grid.Row="11" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="IC卡表厂家型号：" Grid.Column="2" Margin="0,3,0,22" Grid.Row="11" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_kachangjia}" Grid.Column="3" Margin="0,3,0,22" Grid.Row="11" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <TextBlock Text="使用时间：" Grid.Column="4" Margin="0,3,0,22" Grid.Row="9" TextWrapping="Wrap" TextAlignment="Right" Grid.RowSpan="2"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_qbshiyong}" Grid.Column="5" Margin="0,3,0,22" Grid.Row="9" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />
                                <CheckBox IsEnabled="False" Content="死表" IsChecked="{m:Bind Path=f_sibiao}" Grid.Column="0" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="Right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <CheckBox IsEnabled="False" Content="常通" IsChecked="{m:Bind Path=f_changtong}" Grid.Column="1" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <CheckBox IsEnabled="False" Content="表蒙损坏" IsChecked="{m:Bind Path=f_biaomeng}" Grid.Column="2" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="72"/>
                                <CheckBox IsEnabled="False" Content="锈蚀" IsChecked="{m:Bind Path=f_qbxiushi}" Grid.Column="3" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <CheckBox IsEnabled="False" Content="反装" IsChecked="{m:Bind Path=f_fanzhuang}" Grid.Column="4" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <CheckBox IsEnabled="False" Content="漏气" IsChecked="{m:Bind Path=f_qblouqi}" Grid.Column="5" Margin="0,3,0,22" Grid.Row="12" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <CheckBox IsEnabled="False" Content="其他" IsChecked="{m:Bind Path=f_qbqita}" Grid.Column="0" Margin="0,3,0,22" Grid.Row="13" HorizontalAlignment="right" Height="Auto" Grid.RowSpan="2" Width="46"/>
                                <TextBox  IsEnabled="False" Text="{m:Bind Path=f_qibiao}" Grid.Column="1" Margin="0,3,0,22" Grid.Row="13" TextWrapping="Wrap" Height="Auto" VerticalAlignment="Stretch" RenderTransformOrigin="-1.739,-0.923" HorizontalAlignment="Stretch" Width="Auto" Grid.RowSpan="2" />

                                <TextBlock Text="立管：" Grid.Column="0" Margin="0" Grid.Row="14" TextAlignment="Right" />
                                <CheckBox  IsEnabled="False" Content="正常" IsChecked="{m:Bind Path=f_lgzhengchang}" Grid.Row="14"  Grid.Column="1"  Margin="0,5,0,0"/>
                                <CheckBox IsEnabled="False" Content="暗设" IsChecked="{m:Bind Path=f_lganshe}" Grid.Row="14"   Grid.Column="2" Margin="0,5,0,0" />
                                <CheckBox IsEnabled="False" Content="明设" IsChecked="{m:Bind Path=f_lgmingshe}" Grid.Row="14"  Grid.Column="3" Margin="0,5,0,0" />
                                <CheckBox  IsEnabled="False" Content="腐蚀"  IsChecked="{m:Bind Path=f_lgfushi}" Grid.Row="14"  Grid.Column="4" Margin="0,5,0,0" />
                                <CheckBox IsEnabled="False" Content="私改"  IsChecked="{m:Bind Path=f_lgsigai}" Grid.Row="14"  Grid.Column="5" Margin="0,5,0,0" />
                                <CheckBox  IsEnabled="False" Content="漏气"  IsChecked="{m:Bind Path=f_lglouqi}" Grid.Row="15"   Grid.Column="1" Margin="0,5,0,0" />
