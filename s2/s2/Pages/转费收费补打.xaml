<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d"
	x:Class="Com.Aote.Pages.转费收费补打"
	 >
<Grid x:Name="LayoutRoot">
	<Grid.RowDefinitions>
		<RowDefinition Height="39*"/>
		<RowDefinition Height="288*"/>
		<RowDefinition Height="353*"/>
	</Grid.RowDefinitions>
		<m:ResourceLoad Margin="0,0,-101,0" Grid.RowSpan="3">
	  	 
		 
	 
			
		 
			<o:GeneralObject Name="userfilego" WebClientInfo="{StaticResource dbclient}" 
							EntityType="t_switchmoneyrepair" 	
							IsInit="{m:Exp Str=this.State \=\=$End$}">
						</o:GeneralObject>

		<!--售气收费对象 -->
        	<o:GeneralObject Name="switchm" EntityType="t_switchmoney" WebClientInfo="{StaticResource dbclient}"
                 Path="{m:Exp Str=$one/from t_switchmoney where ID\=\'\{userfiles.SelectedItem.ID\}\'      $}" 
		 
			  >
			</o:GeneralObject>
			
		 	 <b:BatchExcuteAction Name="SaveAction"  CanSave="{m:Exp Str=printpiaoju.State\=\=$End$ |or| printout.State\=\=$End$  |or| printin.State\=\=$End$  }"  WebClientInfo="{StaticResource dbclient}">
			  	<b:BatchInfo Source="{m:Exp Str=userfile.DataContext}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
		</m:ResourceLoad>
		<Grid Grid.RowSpan="3" >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="3.5*"/>
				<ColumnDefinition Width="6.5*"/>
			</Grid.ColumnDefinitions>
			
			<c:BusyIndicatorEx BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=userfiles.ItemsSource.IsBusy}" >
				<toolkit:DockPanel >
					<toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" toolkit:DockPanel.Dock="Top" Margin="0,0,0,3" Height="59" >
						<a:Data.Context>
							<o:SearchObject />        	
						</a:Data.Context>
						
						<TextBlock TextWrapping="Wrap" Text="转出用户ID号：" Margin="5,6,0,25"/>
						<TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" Width="108" Margin="0,3,0,25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_userid" Operator="f_userid = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
												<TextBlock TextWrapping="Wrap" Text="转入用户ID号：" Margin="5,6,0,25"/>
						<TextBox TextWrapping="Wrap" Text="{m:Bind Path=f_inid}" a:ControlAttach.DefaultButton="searchbutton" Width="108" Margin="0,3,0,25" >
							<a:Data.Prop>
								<o:PropertySetter PropertyName="f_inid" Operator="f_inid = 'this'"/>               
							</a:Data.Prop>
						</TextBox>
						<Button Content="查  询" x:Name="searchbutton" Click="{m:Exp Str=userfilesearch.DataContext.Search()}" Margin="0,2,5,25" HorizontalAlignment="Right" Width="50" />

					</toolkit:DockPanel>
					<toolkit:DockPanel   toolkit:DockPanel.Dock="Bottom" Height="24">
						<TextBlock TextWrapping="Wrap" Margin="0,-6,0,6" Text="{m:Exp Str=$共\{userfiles.ItemsSource.Count\}项}" Width="Auto" HorizontalAlignment="Stretch" Foreground="#FF140000"/>

					</toolkit:DockPanel>
					<sdk:DataGrid x:Name="userfiles" Margin="0,2,0,35"  
						AutoGenerateColumns="False" Width="Auto" Background="{x:Null}" IsReadOnly="True" >
						<a:Data.Items>
							<o:PagedObjectList x:Name="userfileslist" WebClientInfo="{StaticResource dbclient}"
								Path="{m:Exp Str=$from t_switchmoney where \{userfilesearch.DataContext.Condition\}}"
								IsOld="{m:Exp Str=userfile.DataContext.State\=\=$End$}"
								PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames="yi"/>
						</a:Data.Items>
						<sdk:DataGrid.Columns>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="序  号" Binding="{Binding Index}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户ID号" Binding="{Binding f_userid}"/>
							<sdk:DataGridTextColumn d:IsLocked="True" Header="用户姓名" Binding="{Binding f_username}"/>
							<sdk:DataGridTextColumn Header="转费日期" Binding="{Binding f_outfeetime,StringFormat=yyyy-MM-dd}"/>
			 		 	</sdk:DataGrid.Columns>
					</sdk:DataGrid>
					<!--提交按钮-->
				</toolkit:DockPanel>
			</c:BusyIndicatorEx>
			<c:BusyIndicatorEx BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=userfile.DataContext.IsBusy or SaveAction.IsBusy}" Margin="10,0,0,0" BorderThickness="0" BorderBrush="{x:Null}" Background="{x:Null}" Grid.Column="1" >
				<Grid  x:Name="userfile"  DataContext="{m:Exp Str=userfilego}">
					<!--非民用主操作区-->
					<ScrollViewer Height="680" VerticalAlignment="Top">
					<toolkit:DockPanel >
						<toolkit:DockPanel toolkit:DockPanel.Dock="Bottom"  Background="#FFC3E4FD" >
							<StackPanel Orientation="Horizontal" >
								<Button x:Name="printzhe" Content="打印发票" Margin="5" 
									IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors}"
									Click="{m:Exp Str=printpiaoju.TipPrint()}" Width="71" />
                                    <!--Button x:Name="printfp" Content="打印转出折子" Margin="5" 
									IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors}"
									Click="{m:Exp Str=printout.TipPrint()}" Width="71" />
					
								 
									<Button x:Name="printwxf" Content="打印转入折子" Margin="5" 
									IsEnabled="{m:Exp Str=!userfile.DataContext.HasErrors}"
									Click="{m:Exp Str=printin.TipPrint()}" Width="99" />
								<Button x:Name="save" Content="保  存" Margin="5" 
									
									Click="{m:Exp Str=print.Print()}" Width="50" /-->
								<Button Content="撤  销" Margin="5" FontSize="12" Click="{m:Exp Str=userfile.DataContext.New()}" Width="50"/>
							</StackPanel>
							<StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
								
							</StackPanel>
						</toolkit:DockPanel>
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=userfilego}" Height="Auto" VerticalAlignment="Stretch" >
							<Grid.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FFF2F8FF" Offset="0"/>
									<GradientStop Color="#FFDDF2FD" Offset="1"/>
								</LinearGradientBrush>
							</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="20"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="34"/>
								<RowDefinition Height="3"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="140"/>
								<ColumnDefinition Width="150"/>
								<ColumnDefinition Width="140"/>
								<ColumnDefinition Width="150"/>
								<ColumnDefinition Width="160"/>
								<ColumnDefinition Width="150"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="转出编号：" Margin="0,0,0,-5" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="转出用户/单位名称：" Margin="0,9,0,3" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="转出原因：" Margin="-1,8,1,4" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right"/>
							<TextBlock Text="转气手续费：" Margin="0,6" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="4"/>
							<TextBlock Text="转费日期：" Margin="0,6" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="4" Height="25"/>
							<TextBlock Text="气    价："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Margin="11,4,1,8" Height="25"/>
							<TextBlock Text="经办人：" Margin="14,8,0,4" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="2" Grid.Column="2" Height="25"/>
							<TextBlock Text="余 气 量：" Margin="-1,8,1,4" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="底    数：" Margin="14,8,0,4" Grid.Row="4" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBlock Text="转出气量：" Margin="0,8,0,4" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="4" Height="25" Grid.Column="4"/>
							<TextBlock Text="转出气费金额：" Margin="12,7,0,5" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Grid.Column="2" Height="25"/>
							<TextBlock Text="付手续费方：" Grid.Column="4" Margin="4,6,0,6" Grid.Row="12" TextWrapping="Wrap" TextAlignment="Right" Height="25" RenderTransformOrigin="0.503,0.76"/>
							<TextBox  Text="{m:Bind Path=f_userid}"  IsReadOnly="True" Grid.Column="1" Margin="0,0,0,-5" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_userid"   NoDependValue="{m:Exp Str=switchm.f_userid \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							 
							<TextBox Text="{m:Bind Path=f_outfeereason}"   IsReadOnly="True"  Grid.Column="1" Margin="-1,8,1,4" Grid.Row="2" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="Auto" VerticalAlignment="Stretch" d:LayoutOverrides="Height" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outfeereason"   NoDependValue="{m:Exp Str=switchm.f_outfeereason \=&gt;Completed}"   />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_movepoundage}" Grid.Column="5" Margin="0,6" Grid.Row="2" IsReadOnly="False" Height="25" VerticalAlignment="Stretch"   >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_movepoundage"   NoDependValue="{m:Exp Str=switchm.f_movepoundage \=&gt;Completed}"   />
								</a:Data.Prop>
							</TextBox>
							 
							<TextBox Text="{m:Bind Path=f_allgasnum}" Grid.Column="3" Margin="0,4,2,8" Grid.Row="3" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_allgasnum"   NoDependValue="{m:Exp Str=switchm.f_allgasnum \=&gt;Completed}"   />
								</a:Data.Prop>
							</TextBox>
							<!--从卡上读购气次数{m:Exp Str=card.BuyTimes\=>ReadCompleted+1}-->
							<TextBox Text="{m:Bind Path=f_outgasno}"   IsReadOnly="True"  FontWeight="Bold" Foreground="Red" Grid.Column="5" Margin="0,8,0,4" TextWrapping="Wrap" Grid.ColumnSpan="1" Grid.Row="4" Height="25" VerticalAlignment="Stretch"  >
									<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outgasno"   NoDependValue="{m:Exp Str=switchm.f_outgasno \=&gt;Completed}" 
								 
									  />
								</a:Data.Prop>
							</TextBox>	
							
							<TextBox Text="{m:Bind Path=f_outmoney}" FontWeight="Bold" Foreground="Red" IsReadOnly="True" Grid.Column="3" Margin="0,7,0,5" Grid.Row="6" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outmoney"  NoDependValue="{m:Exp Str=switchm.f_outmoney \=&gt;Completed}" 
								 	 />
								</a:Data.Prop>
							</TextBox>

							<TextBlock Margin="2,8,0,4" Grid.Row="11" TextWrapping="Wrap" Text="转入表号：" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBox Text="{m:Bind Path=f_inwatchno}" Grid.Column="3" Margin="0,8,0,4" Grid.Row="11" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_inwatchno"    NoDependValue="{m:Exp Str=switchm.f_inwatchno \=&gt;Completed}" 
									 />
								</a:Data.Prop>
							</TextBox>
							 
							
										
							<TextBox Text="{m:Bind Path=f_meternumber}"  Grid.Column="3" Margin="0,8,0,4" Grid.Row="1" HorizontalAlignment="Stretch" Width="Auto" Height="25" IsReadOnly="True" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_meternumber"   NoDependValue="{m:Exp Str=switchm.f_meternumber \=&gt;Completed}" />
								</a:Data.Prop>
							</TextBox>
						
					
						

							<TextBlock Margin="1,5,-1,7" Grid.Row="9" TextWrapping="Wrap" Text="转入编号：" TextAlignment="Right" Height="25"/>
							<TextBox Text="{m:Bind Path=f_inid}"  IsReadOnly="True"  Grid.Column="1" Margin="1,5,-1,7" Grid.Row="9" TextWrapping="Wrap"  VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_inid"     NoDependValue="{m:Exp Str=switchm.f_inid \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							
							<TextBlock Text="转出表号：" Margin="2,8,0,4" Grid.Row="1" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBlock Text="写卡类型：" Margin="0,4,0,8" Grid.Row="14" TextWrapping="Wrap" TextAlignment="Right" Height="25" RenderTransformOrigin="-3.647,-2.12"/>
							<TextBox Text="{m:Bind Path=f_repairnumfei}" Grid.Column="5" Margin="0,18,0,-6" Grid.Row="7" TextWrapping="Wrap" IsReadOnly="True" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Right" Width="31" Visibility="Collapsed">
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_repairnumfei"    NoDependValue="{m:Exp Str=switchm.f_repairnumfei \=&gt;Completed}" 
							 />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="转入用户/单位名称：" Grid.Column="2" Margin="0,6" Grid.Row="9" TextWrapping="Wrap" TextAlignment="Right"/>
							<TextBox Text="{m:Bind Path=f_inarea}"  Grid.Column="3" Margin="0,4,0,8" TextWrapping="Wrap" Grid.ColumnSpan="1" IsReadOnly="True" Grid.Row="9"  HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_inarea"  NoDependValue="{m:Exp Str=switchm.f_inarea \=&gt;Completed}"   />
								</a:Data.Prop>
							</TextBox>
							<sdk:DatePicker  SelectedDate="{m:Bind Path=f_outfeetime}" IsEnabled="False"  Grid.Column="5" Margin="0,6" Grid.Row="1"  IsTodayHighlighted="True" IsDropDownOpen="False" SelectedDateFormat="Short" VerticalAlignment="Stretch" Height="25"  FontWeight="Bold" Foreground="Red" Grid.RowSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_outfeetime"  NoDependValue="{m:Exp Str=switchm.f_outfeetime \=&gt;Completed}"   />
								</a:Data.Prop>
							</sdk:DatePicker>
							<TextBox Text="{m:Bind Path=f_remaingas}" Grid.Column="1" Margin="-1,8,1,4" Grid.Row="4" TextWrapping="Wrap" IsReadOnly="True" HorizontalAlignment="Stretch" Height="25" VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_remaingas"    NoDependValue="{m:Exp Str=switchm.f_remaingas \=&gt;Completed}" />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_basenumber}"  Grid.Column="3" Margin="0,8,0,4" Grid.Row="4" HorizontalAlignment="Stretch" Width="Auto" Height="25" IsReadOnly="True" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_basenumber"    NoDependValue="{m:Exp Str=switchm.f_basenumber \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_gasprice}"  Grid.Column="1" Margin="-1,4,0,8" Grid.Row="3" HorizontalAlignment="Stretch" Width="Auto" Height="25" IsReadOnly="True" VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_gasprice"   NoDependValue="{m:Exp Str=switchm.f_gasprice \=&gt;Completed}" />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="累计购气量：" Margin="2,4,0,8" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2"/>
							<TextBox Text="{m:Bind Path=f_switchin}"   IsReadOnly="True"    Grid.Column="5" Margin="0,6" Grid.Row="12" HorizontalAlignment="Stretch" Width="Auto" Height="25" VerticalAlignment="Stretch" > 
								<a:Data.Prop>
									<o:PropertySetter  PropertyName="f_switchin" NoDependValue="{m:Exp Str=switchm.f_switchin \=&gt;Completed}" 
										 />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Margin="0,4,0,8" Grid.Row="12" TextWrapping="Wrap" Text="转入原累计购气量：" TextAlignment="Right" Height="25"/>
							<TextBox Text="{m:Bind Path=f_oldsumgasno}" Grid.Column="1" Margin="0,4,0,8" Grid.Row="12" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_oldsumgasno"    NoDependValue="{m:Exp Str=switchm.f_oldsumgasno \=&gt;Completed}" />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="转入现累计购气量：" Grid.Column="2" Margin="10,0,0,4" Grid.Row="12" TextWrapping="Wrap" TextAlignment="Right" Height="25" VerticalAlignment="Bottom" d:LayoutOverrides="Height"/>
							<TextBox Text="{m:Bind Path=f_newsumgasno}"   IsReadOnly="True" Grid.Column="3" Margin="0,8,0,4" Grid.Row="12" TextWrapping="Wrap" Height="25" VerticalAlignment="Stretch" Grid.RowSpan="1" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_newsumgasno"    NoDependValue="{m:Exp Str=switchm.f_newsumgasno \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBox   Text="{m:Bind Path=f_writecardtype}" IsReadOnly="True"  Grid.Column="1" Margin="0,4,-1,8" Grid.Row="14" HorizontalAlignment="Stretch" Width="Auto" Height="25" VerticalAlignment="Stretch" > 
								<a:Data.Prop>
									<o:PropertySetter  PropertyName="f_writecardtype"  NoDependValue="{m:Exp Str=switchm.f_writecardtype \=&gt;Completed}" 
										  />
								</a:Data.Prop>
							</TextBox>
							 
							<TextBox Text="{m:Bind Path=f_byman}"   IsReadOnly="True"  Grid.Column="3" Margin="0,8,0,4" Grid.Row="2" HorizontalAlignment="Stretch" Width="Auto" Height="25"   VerticalAlignment="Stretch" Grid.ColumnSpan="1" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_byman"     NoDependValue="{m:Exp Str=switchm.f_byman \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="楼号：" Margin="4,8,0,4" TextWrapping="Wrap"  TextAlignment="Right" Height="25" Grid.Column="4"/>
							<TextBox x:Name="f_cusDom" Text="{m:Bind Path=f_cusDom}"  Grid.Column="5" Margin="0,8,3,4" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" Grid.Row="0" IsReadOnly="True" HorizontalAlignment="Stretch" Width="Auto" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_cusDom"      NoDependValue="{m:Exp Str=switchm.f_cusDom \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBox Text="{m:Bind Path=f_username}" IsReadOnly="True" Grid.Column="3" Margin="0,8,0,4" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Width="Auto"  >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_username"    NoDependValue="{m:Exp Str=switchm.f_username \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="门牌号：" Margin="13,6,0,6" TextWrapping="Wrap"  TextAlignment="Right" Height="25" Grid.Row="1"/>
							<TextBox x:Name="f_apartment" Text="{m:Bind Path=f_apartment}"  Grid.Column="1" Margin="0,8,0,4" TextWrapping="Wrap" Grid.ColumnSpan="1" Height="25" VerticalAlignment="Stretch" Grid.Row="1" IsReadOnly="True" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_apartment"       NoDependValue="{m:Exp Str=switchm.f_apartment \=&gt;Completed}" />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="当前用气指数："  TextWrapping="Wrap" TextAlignment="Right" Grid.Row="3" Margin="0,8,-2,4" Height="25" Grid.Column="4" HorizontalAlignment="Right" Width="118"/>
							<TextBox Text="{m:Bind Path=f_watchreading}"   IsReadOnly="True"  Grid.Column="5" Margin="0,8,0,4" Grid.Row="3" HorizontalAlignment="Stretch" Width="Auto" Height="25"  VerticalAlignment="Stretch" >
								<a:Data.Prop>
									<o:PropertySetter PropertyName="f_watchreading"     NoDependValue="{m:Exp Str=switchm.f_watchreading \=&gt;Completed}"  />
								</a:Data.Prop>
							</TextBox>
							<TextBlock Text="开户日期：" Margin="0,7,0,5" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="6" Width="130" Height="25"/>
