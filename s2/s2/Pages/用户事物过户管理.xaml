<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
	  	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
	
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
    x:Class="Com.Aote.Pages.用户事物过户管理" FontSize="13"
    mc:Ignorable="d" >
    <Grid x:Name="LayoutRoot">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <m:ResourceLoad Margin="0,0,-12,0" x:Name="loader" Grid.ColumnSpan="1">

             <oo:NewGeneralICCard Name="card" >
            </oo:NewGeneralICCard>
            <o:GeneralObject Name="ghfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='过户费' "></o:GeneralObject>
            <o:ObjectList Name="jiaofeilist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='交费类型'"/>
            <!--用户对象Path="{m:Exp Str=applys.SelectedItem.id\!\=null:$one/from t_userinfo where f_userid\=\'\{applys.SelectedItem.f_userid\}\'$\,apply.DataContext.f_userid \!\= null:$one/from t_userinfo where f_userid\=\'\{apply.DataContext.f_userid\}\'$\,$qqq$}"    -->
            <o:GeneralObject Name="userfiles" EntityType="t_userinfo" WebClientInfo="{StaticResource dbclient}"
                 Path="{m:Exp Str=
			card.CardId\=>ReadCompleted!\=null and card.Factory!\=null:$one/from t_userinfo where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,
			applys.SelectedItem.f_userid\=>SelectionChanged\!\=null:$one/from t_userinfo where f_userid\=\'\{applys.SelectedItem.f_userid\}\' and f_filiale\=\'\{LoginUser.f_fengongsi\}\'$\,$null$}">
                <o:PropertySetter PropertyName="f_username" NoDependValue="{m:Exp Str=apply.DataContext.f_tnewusername}"/>
                <o:PropertySetter PropertyName="f_idnumber" NoDependValue="{m:Exp Str=apply.DataContext.f_tnewidnumber}"/>
                <o:PropertySetter PropertyName="f_phone" NoDependValue="{m:Exp Str=apply.DataContext.f_tnewphone}"/>
                <o:PropertySetter PropertyName="f_danganstatus" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$过户$\,$过户$}"/>
                <o:PropertySetter PropertyName="f_danganreturnvalue" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$1$\,$1$}"/>
                <o:PropertySetter PropertyName="refreshCache" NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$0$\,$0$}"/>
                <!--o:PropertySetter PropertyName="f_usermc" NoDependValue="{m:Exp Str=apply.DataContext.f_newnusermc}"/>
				<o:PropertySetter PropertyName="f_bankname" NoDependValue="{m:Exp Str=apply.DataContext.f_newnbankname}"/>
				<o:PropertySetter PropertyName="f_idofcard" NoDependValue="{m:Exp Str=apply.DataContext.f_newnidofcard}"/>
				<o:PropertySetter PropertyName="f_payment" NoDependValue="{m:Exp Str=apply.DataContext.f_payment}"/-->
            </o:GeneralObject>


            <b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
                <b:BatchInfo Source="{m:Exp Str=apply.DataContext}" MethodName="SaveToJson"/>
                <b:BatchInfo Source="{m:Exp Str=userfiles}" MethodName="SaveToJson"/>
            </b:BatchExcuteAction>

        </m:ResourceLoad>
        <Grid Grid.ColumnSpan="1" >
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="3*"/>
                <ColumnDefinition Width="7*"/>
            </Grid.ColumnDefinitions>

            <toolkit:BusyIndicator BusyContent="正在查询，请稍等" IsBusy="{m:Exp Str=applys.ItemsSource.IsBusy}">
                <toolkit:DockPanel Margin="0,0,0,89" >
                    <!--信息列表-->

                    <!--信息列表按钮-->
                    <!--信息列表按钮-->
                    <toolkit:DockPanel  toolkit:DockPanel.Dock="Bottom" Margin="0" Height="45">
                        <!--提交审核-->
                        <!--Button Content=" 提交审核 " Click="{m:Exp Str=submittip.Show()}"
                        IsEnabled="{m:Exp Str='pipelines.SelectedItems.Count=>SelectionChanged!=0'}"  Height="25" Width="100" Margin="0,10" RenderTransformOrigin="1.18,0.56" /-->
                        <!--信息列表翻页-->
                        <TextBlock  TextWrapping="Wrap" Text="{m:Exp Str=$共\{applys.ItemsSource.Count\}项}" Width="197" HorizontalAlignment="Stretch" Foreground="#FF140000" VerticalAlignment="Bottom" Margin="5,0,5,-78" Height="35"/>
                    </toolkit:DockPanel>
                    <toolkit:DockPanel x:Name="applysearch" toolkit:DockPanel.Dock="Top" Margin="0,0,0,5" Background="#FFCFEFFF" Height="85" >
                        <a:Data.Context>
                            <o:SearchObject  />
                        </a:Data.Context>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="100"/>
                                <ColumnDefinition Width="50*"/>
                                <ColumnDefinition Width="50*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                                <RowDefinition Height="28"/>
                            </Grid.RowDefinitions>
                            <TextBlock TextWrapping="Wrap" Text="用户编号：" Margin="5,0,0,0" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Column="1" Height="25" Text="{m:Bind Path=f_userid}" a:ControlAttach.DefaultButton="searchbutton" >
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_userid" Operator="f_userid like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <TextBlock TextWrapping="Wrap" Grid.Row="1" Text="用户姓名：" Margin="5,0,0,0" HorizontalAlignment="Right"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="1" Height="25" Grid.Column="1" Text="{m:Bind Path=f_username}" a:ControlAttach.DefaultButton="searchbutton">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_username" Operator="f_username like 'this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                            <Button Content="查 询" x:Name="searchbutton" Grid.Column="2" Grid.Row="2" Click="{m:Exp Str=applysearch.DataContext.Search()}" Margin="0,0,45,1" HorizontalAlignment="Right" Width="60"/>
                            <TextBlock TextWrapping="Wrap" Grid.Row="2" Text="用户地址：" HorizontalAlignment="Right" Margin="0,1,0,-1"/>
                            <TextBox TextWrapping="Wrap" Grid.Row="2" Height="25" Grid.Column="1" Text="{m:Bind Path=f_address}" a:ControlAttach.DefaultButton="searchbutton" Margin="0,1,0,2" VerticalAlignment="Stretch">
                                <a:Data.Prop>
                                    <o:PropertySetter PropertyName="f_address" Operator="f_address like '%this%'"/>
                                </a:Data.Prop>
                            </TextBox>
                        </Grid>
                    </toolkit:DockPanel>
                    <sdk:DataGrid x:Name="applys" Margin="0,0,0,-64"  
        			AutoGenerateColumns="False"  IsReadOnly="True">
                        <a:Data.Items>
                            <o:PagedObjectList x:Name="applyslist" WebClientInfo="{StaticResource dbclient}"
        					Path="{m:Exp Str=$from t_userinfo where \{applysearch.DataContext.Condition\} and f_userstate \=\'正常\'}"
        					IsOld="{m:Exp Str=print.State\=\=$End$}"
        					PageIndex="{m:Exp Str=pager.PageIndex}" PageSize="{m:Exp Str=pager.PageSize}" SumNames=","/>
                        </a:Data.Items>

                        <sdk:DataGrid.Columns>
                            <sdk:DataGridTextColumn Header="序号" Binding="{Binding Index}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户编号" Binding="{Binding f_userid}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户姓名" Binding="{Binding f_username}" d:IsLocked="True"/>

                            <sdk:DataGridTextColumn Header="地址" Binding="{Binding f_address}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="小区" Binding="{Binding f_districtname}" d:IsLocked="True"/>
                            <sdk:DataGridTextColumn Header="用户类别" Binding="{Binding f_usertype}" d:IsLocked="True"/>


                        </sdk:DataGrid.Columns>
                    </sdk:DataGrid>
                </toolkit:DockPanel>
            </toolkit:BusyIndicator>
            <!--操作区按钮-->
            <toolkit:BusyIndicator x:Name="ui_apply" BusyContent="正在工作，请稍等" IsBusy="{m:Exp Str=apply.DataContext.IsBusy}"  Grid.Column="1" Margin="8,0,0,0">
                <Grid Background="{x:Null}" x:Name="apply" Height="415" >
                    <a:Data.Context>
                        <!--o:GeneralObject  Name="applygo" WebClientInfo="{StaticResource dbclient}" 
		EntityType="t_transfer" Source="{m:Exp Str=applys.SelectedItem}" IsInit="{m:Exp Str='SaveAction.State==$End'}">
            			
						</o:GeneralObject-->
                        <o:GeneralObject  Name="applygo" WebClientInfo="{StaticResource dbclient}" EntityType="t_transfer"  
                                          IsInit="{m:Exp Str=print.State\=\=$End$}" IsOld="{m:Exp Str=print.State\=\=$End$}" />

                    </a:Data.Context>

                    <!--主操作区-->

                    <StackPanel HorizontalAlignment="Right" VerticalAlignment="Bottom" Orientation="Horizontal" Margin="0,0,0,80">
                        <Button x:Name="save" Content="保 存" Margin="5" 
            			IsEnabled="{m:Exp Str=apply.DataContext.IsModified and !apply.DataContext.HasErrors}"
            			Click="save_Click" Width="60"/>
                        <Button Content="撤 销" Margin="5" Click="{m:Exp Str=apply.DataContext.New()}" Width="60"/>

                    </StackPanel>
                    <Grid HorizontalAlignment="Stretch" Height="Auto" d:LayoutOverrides="Width" Margin="0,-105,0,115">
                        <Grid.Background>
                            <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                <GradientStop Color="#FFF2F8FF" Offset="0"/>
                                <GradientStop Color="#FFDDF2FD" Offset="1"/>
                            </LinearGradientBrush>
                        </Grid.Background>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="0.36*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="0.315*"/>
                            <ColumnDefinition Width="110"/>
                            <ColumnDefinition Width="0.325*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="1"/>
                            <RowDefinition Height="1" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="45" />
                            <RowDefinition Height="30"/>
                            <RowDefinition Height="158"/>
                        </Grid.RowDefinitions>
                        <TextBlock Margin="0,3,0,2" TextWrapping="Wrap" Text="用户编号：" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="1,3,1,2" TextWrapping="Wrap" Text="用户姓名：" Grid.Row="2" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="0,3,0,2" Grid.Row="1" TextWrapping="Wrap" Text="用户地址：" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Text="电     话：" Grid.Column="4" TextAlignment="Right" Height="25"/>
                        <TextBlock Grid.Column="2" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" Text="身份证号：" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="1,3,1,2" Grid.Row="4" TextWrapping="Wrap" Text="新用户姓名：" TextAlignment="Right" Height="25"/>
                        <TextBlock Grid.Column="4" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" Text="新 电 话：" TextAlignment="Right" Height="25" />
                        <TextBlock Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" Text="新身份证号：" Grid.Column="2" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="1,3,1,2" Grid.Row="5" TextWrapping="Wrap" Text="新银行开户名：" TextAlignment="Right" Height="25"/>
                        <TextBlock Grid.Column="4" Margin="0,3,0,2" Grid.Row="5" TextWrapping="Wrap" Text="新银行名：" TextAlignment="Right" Height="25" />
                        <TextBlock Margin="0,3,0,2" Grid.Row="5" TextWrapping="Wrap" Text="新银行账号：" Grid.Column="2" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="1,3,1,2" Grid.Row="8" TextWrapping="Wrap" Text="过 户 费：" TextAlignment="Right" Height="25"/>
                        <TextBlock Margin="0,13,6,7" Grid.Row="9" TextWrapping="Wrap" Text="备     注：" HorizontalAlignment="Right" Height="25"/>
                        <TextBlock Margin="0,3,0,2" Grid.Row="8" TextWrapping="Wrap" Text="过户日期:" TextAlignment="Right" Grid.Column="2" Height="25"/>
                        <TextBlock Margin="0,3,7,2" Grid.Row="10" TextWrapping="Wrap" Text="部     门：" HorizontalAlignment="Right" Height="25"/>
                        <TextBlock Margin="0,3,4,2" Grid.Row="10" TextWrapping="Wrap" Text="操 作 人：" Grid.Column="2" Height="25" TextAlignment="Right"/>
                        <TextBlock Grid.Column="4" Margin="0,1" Grid.Row="10" TextWrapping="Wrap" Text="操作日期：" d:LayoutOverrides="Height" TextAlignment="Right"/>
                        <TextBox Text="{m:Bind Path=f_userid}" IsReadOnly="True" Grid.Column="1" HorizontalAlignment="Stretch" Margin="0,3,0,2" TextWrapping="Wrap" VerticalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Height="25" >
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_userid" NoDependValue="{m:Exp Str=userfiles.f_userid\=&gt;Completed}"/>

                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_username}"  Grid.Column="1" Margin="0,1" TextWrapping="Wrap" VerticalAlignment="Center" Grid.Row="2" IsReadOnly="True" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=userfiles.f_username\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_address}" Grid.ColumnSpan="3" Grid.Column="1" Margin="0,3,0,2" Grid.Row="1" TextWrapping="Wrap" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" IsReadOnly="True" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=userfiles.f_address\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_phone}" Grid.Column="5" Margin="0,3,6,2" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Stretch" IsReadOnly="True" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_phone" Value="{m:Exp Str=userfiles.f_phone\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_idnumber}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="2" TextWrapping="Wrap" VerticalAlignment="Stretch" IsReadOnly="True" Height="25"  >
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_idnumber" Value="{m:Exp Str=userfiles.f_idnumber\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_tnewusername}" Grid.Column="1" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Stretch" d:LayoutOverrides="HorizontalAlignment" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_tnewusername" Value="{m:Exp Str=userfiles.f_tnewusername\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_tnewphone}" Grid.Column="5" Margin="0,3,6,2" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_tnewphone" Value="{m:Exp Str=userfiles.f_tnewphone\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_tnewidnumber}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="4" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_tnewidnumber" Value="{m:Exp Str=userfiles.f_tnewidnumber\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_newnusermc}" Grid.Column="1" Margin="0,3,0,2" Grid.Row="5" TextWrapping="Wrap" VerticalAlignment="Stretch" d:LayoutOverrides="HorizontalAlignment" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_newnusermc" Value="{m:Exp Str=userfiles.f_newnusermc\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_newnbankname}" Grid.Column="3" Margin="0,3,6,2" Grid.Row="5" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_newnbankname" Value="{m:Exp Str=userfiles.f_newnbankname\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_newnidofcard}" Grid.Column="5" Margin="0,3,0,2" Grid.Row="5" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_newnidofcard" Value="{m:Exp Str=userfiles.f_newnidofcard\=>Completed}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_transferfee}" Grid.Column="1" Margin="0,3,0,2" Grid.Row="8" TextWrapping="Wrap" VerticalAlignment="Stretch" IsReadOnly="True" RenderTransformOrigin="0.474,1" HorizontalAlignment="Left" Width="138" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_transferfee" Value="{m:Exp Str=ghfobj.value}"  />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_notnull}" Grid.ColumnSpan="3" Grid.Column="1" Margin="0,1,0,4" Grid.Row="9" TextWrapping="Wrap" VerticalAlignment="Stretch"  HorizontalAlignment="Stretch">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_notnull" />
                            </a:Data.Prop>
                        </TextBox>
                        <sdk:DatePicker SelectedDate="{m:Bind Path=f_f_tdate}" Grid.Column="3" IsEnabled="False" Margin="2,3,0,2" Grid.Row="8" VerticalAlignment="Stretch" Grid.ColumnSpan="1" Height="25">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_f_tdate"
            					Default="{m:Exp Str=SysTime.Now}"/>
                            </a:Data.Prop>
                        </sdk:DatePicker>
                        <TextBox Text="{m:Bind Path=f_departments}" Grid.Column="1" Margin="0,1" Grid.Row="10" TextWrapping="Wrap" VerticalAlignment="Stretch" IsReadOnly="True" RenderTransformOrigin="0.579,5.348" d:LayoutOverrides="Height" Grid.ColumnSpan="1">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_departments" Default="{m:Exp Str=LoginUser.f_parentname}" />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBox Text="{m:Bind Path=f_toperator}" Grid.Column="3" Margin="0,3,0,2" Grid.Row="10" TextWrapping="Wrap" VerticalAlignment="Stretch" Height="25" HorizontalAlignment="Stretch" IsReadOnly="True" Width="Auto">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_toperator" Default="{m:Exp Str=LoginUser.name}" />
                            </a:Data.Prop>
                        </TextBox>
                        <sdk:DatePicker SelectedDate="{m:Bind Path=f_tdate}" Grid.Column="5" Margin="0,0,6,3" Grid.Row="10" VerticalAlignment="Bottom" Height="25" HorizontalAlignment="Stretch" IsEnabled="False">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_tdate"
            					Default="{m:Exp Str=SysTime.Today}"/>
                            </a:Data.Prop>
                        </sdk:DatePicker>
                        <!--Button Content="读卡" Click="{m:Exp Str=card.ReadCard()}" Grid.Column="2" Margin="10,0,0,0" HorizontalAlignment="Left" Width="55" Foreground="#FF1F46BE" VerticalAlignment="Center"/-->
                        <!--隐藏雨-->
                        <!--表厂别名-->
                        <TextBox Text="{m:Bind Path=f_aliasname}" Visibility="Collapsed" VerticalAlignment="Top" Height="Auto" HorizontalAlignment="Stretch" IsReadOnly="True" Grid.Row="10" Margin="0,30,0,0">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_aliasname"  />
                            </a:Data.Prop>
                        </TextBox>
                        <!--卡内气量-->
                        <TextBox Text="{m:Bind Path=f_gas}" Visibility="Collapsed" VerticalAlignment="Top" Height="Auto" HorizontalAlignment="Stretch" IsReadOnly="True" Grid.Row="10" Margin="0,30,0,0">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_gas"  />
                            </a:Data.Prop>
                        </TextBox>
                        <!--购气次数-->
                        <TextBox Text="{m:Bind Path=f_times}" Visibility="Collapsed" VerticalAlignment="Top" Height="Auto" HorizontalAlignment="Stretch" IsReadOnly="True" Grid.Row="10" Margin="0,30,0,0">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_times"  />
                            </a:Data.Prop>
                        </TextBox>
                        <!--卡号-->
                        <TextBox Text="{m:Bind Path=f_cardid}" Visibility="Collapsed" VerticalAlignment="Top" Height="Auto" HorizontalAlignment="Stretch" IsReadOnly="True" Grid.Row="10" Margin="0,30,0,0">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_cardid"  />
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock Margin="0,3,7,0" Grid.Row="11" TextWrapping="Wrap" Text="分  公  司：" TextAlignment="Right" HorizontalAlignment="Right" Height="25" VerticalAlignment="Top" d:LayoutOverrides="Height"/>
                        <TextBox Text="{m:Bind Path=f_filiale}" Grid.Column="1" Margin="0,1,0,0" Grid.Row="11" TextWrapping="Wrap" IsReadOnly="True" VerticalAlignment="Top" HorizontalAlignment="Stretch" Height="28" RenderTransformOrigin="0.368,4.185" Grid.ColumnSpan="1">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_filiale" 
								Default="{m:Exp Str=LoginUser.f_fengongsi}"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock Text="分公司编号：" Visibility="Collapsed" Margin="0,4,0,-1" TextWrapping="Wrap" TextAlignment="Right" Grid.Row="8" HorizontalAlignment="Right"  Grid.Column="2" Height="27" />
                        <TextBox Text="{m:Bind Path=f_fengongsinum}" Visibility="Collapsed"  Grid.Column="3" Margin="0,4" Grid.Row="8" TextWrapping="Wrap" IsReadOnly="True"  VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Height="Auto" d:LayoutOverrides="Width">
                            <a:Data.Prop>
                                <o:PropertySetter PropertyName="f_fengongsinum" NotNull="True"
								Default="{m:Exp Str=LoginUser.f_fengongsinum}"/>
                            </a:Data.Prop>
                        </TextBox>
                        <TextBlock Margin="1,3,1,2" TextWrapping="Wrap" Text="银行开户名：" Grid.Row="3" TextAlignment="Right" Height="25"/>
                        <TextBox Text="{m:Bind Path=f_nusermc}"  Grid.Column="1" Margin="0,3,0,2" TextWrapping="Wrap" VerticalAlignment="Stretch" Grid.Row="3" IsReadOnly="True" d:LayoutOverrides="HorizontalAlignment" HorizontalAlignment="Stretch" Width="Auto" Grid.ColumnSpan="1" Height="25">
