<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:a="clr-namespace:Com.Aote.Attachs;assembly=s2DLL"
    xmlns:o="clr-namespace:Com.Aote.ObjectTools;assembly=s2DLL"
	xmlns:oo="clr-namespace:Com.Aote.ObjectTools"
    xmlns:b="clr-namespace:Com.Aote.Behaviors;assembly=s2DLL"
	xmlns:c="clr-namespace:Com.Aote.Controls;assembly=s2DLL"
    xmlns:m="clr-namespace:Com.Aote.Marks;assembly=s2DLL"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
    xmlns:data="clr-namespace:System.Windows.Data;assembly=System.Windows"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"
    xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit"
	xmlns:System="clr-namespace:System;assembly=mscorlib"
	mc:Ignorable="d" FontSize="13.333"
	x:Class="Com.Aote.Pages.售气收费" FontFamily="Microsoft YaHei">

	<Grid x:Name="LayoutRoot">
        <m:ResourceLoad x:Name="loader" Margin="0,0,-101,0">
			<!--限购月数-->
			<o:GeneralObject Name="xgys" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='限购月数' "   ></o:GeneralObject>
			<!--非民用限购参数对象-->
        	<o:GeneralObject Name="gsxiangou" EntityType="t_gsxiangou" WebClientInfo="{StaticResource dbclient}"
                Path="{m:Exp Str=$one/from t_gsxiangou where f_fanwei &lt;\= \{kbusers.f_flownum\} and f_daofanwei &gt; \{kbusers.f_flownum\}}">
			</o:GeneralObject>
			<!--民用壁挂炉用户采暖季参数对象-->
        	<o:GeneralObject Name="cainuanji" EntityType="t_addons" WebClientInfo="{StaticResource dbclient}"
             	Path="{m:Exp Str=$one/from cainuanji }">
			</o:GeneralObject>	
			<!--历史交费列表-->
			<o:ObjectList Name="kbsellgas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=kbusers.f_userid !\= null:$from t_sellinggas where f_userid\=\'\{kbfee.f_userid\}\' order by f_deliverydate desc \, id desc $\,null}"
				IsOld="{m:Exp Str=SaveAction.State\=\=$End$}" />
			<!--本月有效费列表,目的是计算用户本月已经购买的气量-->
			<o:ObjectList Name="kbbysellgas" WebClientInfo="{StaticResource dbclient}"
				Path="{m:Exp Str=kbusers.f_userid !\= null:$from t_sellinggas where f_userid\=\'\{kbfee.f_userid\}\' and (f_payfeetype \=\'卡表收费\' or f_payfeetype \=\'发卡售气\') and DateDiff(d\,f_deliverydate\,\'\{SysTime.Today.ToString($yyyy-MM-dd$)\}\') &lt;\=\{xgys.value\}*30 and f_payfeevalid\=\'有效\'$\,null}"
			/>
			<!--付款方式列表-->
			<o:ObjectList Name="paylist" WebClientInfo="{StaticResource dbclient}" Path="from paramvalue where param.name='付款方式' order by id"/>
			<!--打开窗口对象-->
            <o:ChildWindowObj Name="openxiangou" CWName="/Pages/管理员登录" ParamObj="{m:Exp Str=kbusers}" />
			
			<!--混合气价对象 -->
        	<o:GeneralObject Name="hunhes" EntityType="t_hunhe" WebClientInfo="{StaticResource dbclient}"
             	Path="{m:Exp Str=$one/from t_hunhe }">
			</o:GeneralObject>	
			
			<!--单值对象-->
            <o:GeneralObject Name="mzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用单次最大购气量'"   ></o:GeneralObject>
            <o:GeneralObject Name="fzdgasobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用单次最大购气量'"   ></o:GeneralObject>
            <o:GeneralObject Name="ljql1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='临界气量1' "   ></o:GeneralObject>
            <o:GeneralObject Name="ljql2" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='临界气量2' "   ></o:GeneralObject>
            <o:GeneralObject Name="dbhjtqj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='低保户阶梯气价1' "   ></o:GeneralObject>
            <o:GeneralObject Name="dbhjtqj2" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='低保户阶梯气价2' "   ></o:GeneralObject>
            <o:GeneralObject Name="dbhjtqj3" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='低保户阶梯气价3' "   ></o:GeneralObject>
            <o:GeneralObject Name="jtqj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='阶梯气价1' "   ></o:GeneralObject>
            <o:GeneralObject Name="jtqj2" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='阶梯气价2' "   ></o:GeneralObject>
            <o:GeneralObject Name="jtqj3" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='阶梯气价3' "   ></o:GeneralObject>
            <o:GeneralObject Name="fmyljql1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用临界气量1' "   ></o:GeneralObject>
            <o:GeneralObject Name="fmyljql2" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用临界气量2' "   ></o:GeneralObject>
            <o:GeneralObject Name="fmyjtqj1" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用阶梯气价1' "   ></o:GeneralObject>
            <o:GeneralObject Name="fmyjtqj2" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用阶梯气价2' "   ></o:GeneralObject>
            <o:GeneralObject Name="fmyjtqj3" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用阶梯气价3' "   ></o:GeneralObject>
            <o:GeneralObject Name="xiangys" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='限购月数' "   ></o:GeneralObject>


            <o:GeneralObject Name="fbffobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用低保户月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="mwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='民用月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="fwhfobj" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  		Path="one/from t_singlevalue where name='非民用月维护费' "   ></o:GeneralObject>
            <o:GeneralObject Name="fapiaogeshi" EntityType="t_singlevalue" WebClientInfo="{StaticResource dbclient}" 
		  	Path="one/from t_singlevalue where name='发票格式'"   ></o:GeneralObject>	
			
			<!--用户对象-->
			<o:GeneralObject Name="kbusers" EntityType="t_userfiles" WebClientInfo="{StaticResource dbclient}"
             Path="{m:Exp Str=card.CardId !\= null and card.Factory !\=null and card.Factory \=\=$jinka$ :$one/from t_userfiles where f_userstate !\=\'注销\' and f_cardid\=\'\{card.CardId\}\' and dqdm \= \'\{card.Dqdm\}\' and f_aliasname\=\'\{card.Factory\}\'$\,card.CardId !\= null and card.Factory !\=null:$one/from t_userfiles where f_cardid\=\'\{card.CardId\}\' and f_aliasname\=\'\{card.Factory\}\'$\,$null$}" 
			 IsInit="{m:Exp Str=SaveAction\=\=$Error$}">
				<!--折子号-->
				<o:PropertySetter PropertyName="f_zherownum" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_zherownum.ToInt()\,0}"></o:PropertySetter>        		
				<!--初始化卡购气标记-->
				<o:PropertySetter PropertyName="f_initcardsellgas" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:$$\,null}"></o:PropertySetter>        		
				<!--当前表累计购气量-->
				<o:PropertySetter PropertyName="f_metergasnums" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_premetergasnums+kbfee.f_pregas+kbfee.f_surplusgas\,null}"></o:PropertySetter>        		
				<!--用户总累计购气量-->
				<o:PropertySetter PropertyName="f_cumulativepurchase" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_upbuynum+kbfee.f_pregas\,null}"></o:PropertySetter>        		
				<!--剩余补气量-->
				<o:PropertySetter PropertyName="f_surplusgas" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:0\,null}"></o:PropertySetter>        		
				<!--最后一次写入卡里的气量-->
				<o:PropertySetter PropertyName="f_finallygas" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:card.Gas\,null}"></o:PropertySetter>        		
				<!--结余-->
				<o:PropertySetter PropertyName="f_zhye" 
				NoDependValue="{m:Exp Str=SaveAction\=>Saving!\=null:kbfee.f_benqizhye\,null}"/>
			</o:GeneralObject>
			
			<!--收费对象-->
        	<o:GeneralObject Name="kbfee" EntityType="t_sellinggas" WebClientInfo="{StaticResource dbclient}"
            IsInit="{m:Exp Str=SaveAction\=\=$Error$}">
	  			<o:PropertySetter PropertyName="f_zherownum" NoDependValue="{m:Exp Str=kbusers !\= null and kbusers.f_zherownum !\= null and kbusers.f_zherownum.ToInt()^24:kbusers.f_zherownum+1\,1}"/>
				<o:PropertySetter PropertyName="f_pregas" NotNull="True"  
					Validation="{m:Exp Str= value ^\= kbfee1.DataContext.f_limitbuygas}"  ErrorMessage="购买气量超出限购气量，不能购气！"
				    ValidationVal="{m:Exp Str= kbfee.f_limitbuygas !\=null: kbfee.f_pregas ^\= kbfee.f_limitbuygas\,True}"
				/>
				<!--所包含的补气对象，剩余补气量为0时，为空-->
	  			<o:PropertySetter PropertyName="f_fillgas" 
				NoDependValue="{m:Exp Str=kbfee.f_surplusgas>0:fillgas\,null}"/>
			</o:GeneralObject>
			
			<!--发票对象-->
        	<o:GeneralObject Name="fpinfosobj" EntityType="t_fapiaoinfos" WebClientInfo="{StaticResource dbclient}" 
            Path="{m:Exp Str=$one/from t_fapiaoinfos where f_invoicenum\=\'\{kbfee1.DataContext.f_invoicenum\}\'}"  
			IsInit="{m:Exp Str=SaveAction\=\=$Error$ |or| card.State\=\=$Error$}">
        		<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=kbfee1.DataContext.f_username}"/>
				<o:PropertySetter PropertyName="f_invoicenum" Value="{m:Exp Str=kbfee1.DataContext.f_invoicenum}"/>
				<o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbfee1.DataContext.f_userid}" />
				<o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str=kbfee1.DataContext.f_cardid}"/>
				<o:PropertySetter PropertyName="f_gas" Value="{m:Exp Str=kbfee1.DataContext.f_pregas}" />
				<o:PropertySetter PropertyName="f_money" Value="{m:Exp Str=kbfee1.DataContext.f_preamount}" />
				<o:PropertySetter PropertyName="f_repairfee" Value="{m:Exp Str=kbfee1.DataContext.f_amountmaintenance}" />
				<o:PropertySetter PropertyName="f_totalfee" Value="{m:Exp Str=kbfee1.DataContext.f_totalcost}" />
				<o:PropertySetter PropertyName="f_sgnetwork" Value="{m:Exp Str=kbfee1.DataContext.f_sgnetwork}" />
				<o:PropertySetter PropertyName="f_sgoperator" Value="{m:Exp Str=kbfee1.DataContext.f_sgoperator}" />
				<o:PropertySetter PropertyName="f_fapiaotime" Value="{m:Exp Str=kbfee1.DataContext.f_deliverytime}" />
				<o:PropertySetter PropertyName="f_fapiaodate" Value="{m:Exp Str=kbfee1.DataContext.f_deliverydate}" />
				<o:PropertySetter PropertyName="f_filiale" Value="{m:Exp Str=kbfee1.DataContext.f_filiale}" />
				<o:PropertySetter PropertyName="f_fapiaotype" Default="购气"  />
				<o:PropertySetter PropertyName="f_fapiaostatue" Default="已用"  />
			</o:GeneralObject>
			
			<!--补气对象-->	
			<o:GeneralObject  Name="fillgas" WebClientInfo="{StaticResource dbclient}" 
    		EntityType="t_fillgas" 
    		IsInit="{m:Exp Str=SaveAction\=\=$Error$}" >
				<o:PropertySetter PropertyName="f_username" Value="{m:Exp Str=kbusers.f_username}" />
				<o:PropertySetter PropertyName="f_usertype" Value="{m:Exp Str=kbusers.f_usertype}" />
				<o:PropertySetter PropertyName="f_userid" Value="{m:Exp Str=kbusers.f_userid}"/>
				<o:PropertySetter PropertyName="f_address" Value="{m:Exp Str=kbusers.f_address}" />
				<o:PropertySetter PropertyName="f_cardid" Value="{m:Exp Str=kbusers.f_cardid}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_gaswatchbrand" Value="{m:Exp Str=kbusers.f_gaswatchbrand}" />
				<o:PropertySetter PropertyName="f_metertype" Value="{m:Exp Str=kbusers.f_metertype}" />
				<o:PropertySetter PropertyName="f_cmnewmeternumber" Value="{m:Exp Str=kbusers.f_meternumber}" />
				<o:PropertySetter PropertyName="f_cmaddgas" Value="{m:Exp Str=kbusers.f_bugas\=&gt;Completed}" />
				<o:PropertySetter PropertyName="f_nextaddgasamount" Value="{m:Exp Str=kbusers.f_surplusgas\=&gt;Completed}" />
				<o:PropertySetter PropertyName="f_thisnextaddgasamount" Default="0" Value="0"/>
				<o:PropertySetter PropertyName="f_gasprice" Value="{m:Exp Str=kbusers.f_gasprice}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_pregas" NoDependValue="{m:Exp Str=kbusers.f_surplusgas}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_aliasname" Value="{m:Exp Str=kbusers.f_aliasname}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_times" Value="{m:Exp Str=kbusers.f_times}"/>
				<o:PropertySetter PropertyName="f_newmaichong" Value="{m:Exp Str=kbusers.f_maichong}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_newwatchnum" Value="{m:Exp Str=kbusers.f_watchnum}" NotNull="True"/>
				<o:PropertySetter PropertyName="f_addgasdate" Default="{m:Exp Str=SysTime.Today}"/>
				<o:PropertySetter PropertyName="f_parentname5" Default="{m:Exp Str=LoginUser.f_parentname}"/>
				<o:PropertySetter PropertyName="f_foperator" Default="{m:Exp Str=LoginUser.name}"/>
				<o:PropertySetter PropertyName="f_filiale6" Default="{m:Exp Str=LoginUser.f_fengongsi}"/>
				<o:PropertySetter PropertyName="f_yi" Default="0"/>
				<o:PropertySetter PropertyName="f_addgastype" Default="换表补气"/>
				<o:PropertySetter PropertyName="f_comtype" Default="{m:Exp Str=LoginUser.f_comtype}"/>
    		</o:GeneralObject>
		
		    <!--保存-->
			<b:BatchExcuteAction Name="SaveAction" WebClientInfo="{StaticResource dbclient}">
				<!--用户对象-->
				<b:BatchInfo Source="{m:Exp Str=kbusers}" MethodName="SaveToJson"/>
				<!--更新维护费开始日期-->
			   	<!--b:BatchInfo Source="{m:Exp Str=UpdateBeginFeeAction}" MethodName="InvokeToJson"/-->
				<!--收费对象，可能包含补气对象-->
				<b:BatchInfo Source="{m:Exp Str=kbfee1.DataContext}" MethodName="SaveToJson"/>
				<!--发票对象-->
				<b:BatchInfo Source="{m:Exp Str=fpinfosobj}" MethodName="SaveToJson"/>
			</b:BatchExcuteAction>
		
					
		<!--卡对象-->	
		<oo:NewGeneralICCard Name="card"   
		 CanSellGas="{m:Exp Str='SaveAction.State=>Completed==$End$ and kbfee.f_initcardsellgas!=$初始化卡购气标记$ and kbfee.f_initcardsellgas!=$补卡$' }"
		 CanRewriteCard="{m:Exp Str='SaveAction.State=>Completed==$End$ and (kbfee.f_initcardsellgas==$初始化卡购气标记$ or kbfee.f_initcardsellgas==$补卡$)'}"
         CardId="{m:Exp Str='card=>Writing!= null:kbusers.f_cardid,null'}"
		 Gas="{m:Exp Str=(kbfee.f_pregas+kbfee.f_surplusgas).ToInt()}"
		 Klx="{m:Exp Str='kbusers != null\:kbusers.klx\,null'}"		
		 Kzt="{m:Exp Str='kbusers=>DataLoaded!=null:(kbfee.f_initcardsellgas==$初始化卡购气标记$:0,1),0'}"
		 Dqdm="{m:Exp Str='kbusers != null \:kbusers.dqdm\,null'}"
		 Yhh="{m:Exp Str='kbusers != null \:kbusers.yhh\,null'}"
		 Tm="0"
		 Ljgql="{m:Exp Str='kbusers != null \:kbusers.f_metergasnums\,null'}"
		 Bkcs="0"
		 Bjql="{m:Exp Str='kbusers != null \:kbusers.bjql\,null'}"
		 Czsx="{m:Exp Str='kbusers != null \:kbusers.czsx\,null'}"
		 Tzed="{m:Exp Str='kbusers != null \:kbusers.tzed\,null'}"	
		 Kmm="{m:Exp Str='kbusers != null \:kbusers.kmm\,null'}"
		 Sqrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
	 	 NewPrice="{m:Exp Str='kbusers != null\:kbusers.f_gasprice*100\,0'}"
		 Sxrq="{m:Exp Str='SysTime.Now.ToString($yyyyMMdd$)'}"
		 Sxbj="{m:Exp Str=card.Factory \=\= $ZhenLan$ \:null\,$1$}"
		 Completed = "NewGeneralICCard_Completed"
		>
		</oo:NewGeneralICCard>
		</m:ResourceLoad>
		<Grid >
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="1*"/>
				<ColumnDefinition Width="3*"/>
			</Grid.ColumnDefinitions >
			<c:BusyIndicatorEx x:Name="busy" BusyContent="正在进行卡操作，请稍等" IsBusy="{m:Exp Str=card.IsBusy}" Width="Auto" HorizontalAlignment="Stretch" >
				<toolkit:DockPanel x:Name="userfilesearch"  Background="#FFCFEFFF" Margin="0" Width="Auto" HorizontalAlignment="Stretch" d:LayoutOverrides="Height" >
					<Grid Width="Auto">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="100"/>
							<ColumnDefinition/>
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
							<RowDefinition Height="28"/>
						</Grid.RowDefinitions>
						<TextBlock TextWrapping="Wrap" Text="卡　　号：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBox Text="{m:Exp Str=card.CardId.NullToEmpty()\=>ReadCompleted}" IsReadOnly="True" TextWrapping="Wrap" Grid.Column="1"  a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" d:LayoutOverrides="GridBox" />
						<TextBlock TextWrapping="Wrap" Grid.Row="1" Text="厂　　家：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBox TextWrapping="Wrap" Grid.Row="1" Grid.Column="1" IsReadOnly="True" Text="{m:Exp Str=card.Factory.NullToEmpty()\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,2,0,1" Height="25" d:LayoutOverrides="GridBox" />					
						<TextBlock TextWrapping="Wrap" Grid.Row="2" Text="卡上气量：" Margin="0,6" HorizontalAlignment="Right"/>
						<TextBlock TextWrapping="Wrap" Grid.Row="3" Text="购气次数：" Margin="0,6" HorizontalAlignment="Right"/>
				
						<TextBox TextWrapping="Wrap" IsReadOnly="True" Grid.Row="3" Grid.Column="1" Text="{m:Exp Str=card.BuyTimes\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" d:LayoutOverrides="GridBox" />
				
						<TextBox TextWrapping="Wrap" IsReadOnly="True" Grid.Row="2" Grid.Column="1" Text="{m:Exp Str=card.Gas\=>ReadCompleted}" a:ControlAttach.DefaultButton="searchbutton" Width="Auto" Margin="0,3,0,0" Height="25" d:LayoutOverrides="GridBox" />

					</Grid>
				</toolkit:DockPanel>
			</c:BusyIndicatorEx>
			<c:BusyIndicatorEx 
				BusyContent="{m:Exp Str=$正在访问网络，请稍等...$}" 
				IsBusy="{m:Exp Str=kbusers.IsBusy or kbfee1.DataContext.IsBusy or SaveAction.IsBusy}"  Grid.Column="1" Margin="0" Grid.ColumnSpan="1" d:LayoutOverrides="GridBox">
				<toolkit:DockPanel>
					<StackPanel toolkit:DockPanel.Dock="Top">
						<Grid   Margin="0" x:Name="kbfee1" DataContext="{m:Exp Str=kbfee}" >
							<Grid.Background>
								<LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
									<GradientStop Color="#FFF2F8FF" Offset="0"/>
									<GradientStop Color="#FFDDF2FD" Offset="1"/>
								</LinearGradientBrush>
							</Grid.Background>
							<Grid.RowDefinitions>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="1"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
								<RowDefinition Height="37"/>
							</Grid.RowDefinitions>
							<Grid.ColumnDefinitions>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
								<ColumnDefinition Width="130"/>
								<ColumnDefinition Width="1*"/>
							</Grid.ColumnDefinitions>
							<TextBlock Text="用户编号：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Height="25"/>
							<TextBlock Text="用户/单位名称：" Margin="0" TextWrapping="Wrap"  TextAlignment="Right" Grid.Column="2" Height="25"/>
							<TextBlock Text="地　　址：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25" Grid.Column="2" RenderTransformOrigin="1.638,0.44"/>
							<TextBlock Text="小区名称：" Margin="0" Grid.Row="2" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表品牌：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Height="25"/>
							<TextBlock Text="气表型号：" Margin="0" Grid.Row="3" TextWrapping="Wrap" TextAlignment="Right" Grid.Column="2" Height="25"/>
